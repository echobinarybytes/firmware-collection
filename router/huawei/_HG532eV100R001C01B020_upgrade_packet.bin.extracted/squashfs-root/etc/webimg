for (var i = 0; i < StaticRoute.length - 1; i++)
{
if((StaticRoute[i].Interface != 'br0') && (StaticRoute[i].Interface != ""))
{
for (var j = 0; j < WanInfo.length; j++)
{
if (WanInfo[j].domain == StaticRoute[i].Interface)
{
break;
}
}
if(WanInfo.length == j)
{
break;
}

<html>
<head>
<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/util.js"></script>
<script language="JavaScript" src="/lang/ddns.res"></script>
<title>DDNS</title>
<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" type="text/javascript">
var temp = 'InternetGatewayDevice.WANDevice.1.WANConnectionDevice.';
var prefixLength = temp.length;
var selctIndex = -1;
function stWan(domain,Name,ServiceList,ConnectionType, NATEnabled)
{
this.domain = domain;
this.key = this.domain;
this.NATEnabled = NATEnabled;
this.InterfaceName = Name;
this.ServiceList = ServiceList;
this.ConnectionType = ConnectionType;
this.wanDomain = domain;
}
var WanPPP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|X_ServiceList|ConnectionType|NATEnabled,stWan);%>;
var WanIP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Name|X_ServiceList|ConnectionType|NATEnabled,stWan);%>;
var Wan = new Array();
for (j = 0; j < WanPPP.length - 1; j++)
{
Wan[j] = WanPPP[j];
}
for (k = 0; k < WanIP.length - 1; k++,j++)
{
Wan[j] = WanIP[k];
}
function stDDNS(domain,Enable,Provider,Username,Password,Port,Protocol,Server,DomainName,HostName, wandomain)
{
this.domain = domain;
this.key = this.domain;
this.Enable = Enable;
this.Provider = Provider;
this.Username = Username;
this.Password = Password;
this.Port = Port;
this.Protocol = Protocol;
this.Server = Server;
this.DomainName = DomainName;
this.wanDomain = '';
this.InterfaceName = '';
this.HostName = HostName;
this.wancfgdomain = wandomain;
}
var WanPPPDDNS = <%webGetCfgEntry(InternetGatewayDevice.Services.X_DDNSConfiguration.{i}, Enable|Provider|Username|Password|ServicePort|Protocol|Server|DomainName|HostName|WanPath, stDDNS);%>;
var DDNS = new Array();
for (j = 0; j < WanPPPDDNS.length - 1; j++)
{
DDNS[j] = WanPPPDDNS[j];
}
AssociateParam('DDNS','Wan','wanDomain|InterfaceName');
var WanDDNS = new Array();
function WriteOption()
{
var k;
var m = 0;
for (k = 0; k < Wan.length; k++)
{
if ((Wan[k].ServiceList.indexOf('INTERNET') >= 0)
&& (Wan[k].ConnectionType == 'IP_Routed')
&& ((Wan[k].NATEnabled == 1) || (Wan[k].NATEnabled == 2)))
{
document.write('<option value="' + Wan[k].domain + '">'
+ Wan[k].InterfaceName + '</option>');
WanDDNS[m] = Wan[k];
++m;
}
}
document.write('<option value="' + '' + '">'
+ '' + '</option>');
}
function isTValidName(name) {
var i = 0;
var unsafeString = "\"<>%\\^[]`\+\$\,='#&:;*/{} \t";
for ( i = 0; i < name.length; i++ )
{
for( j = 0; j < unsafeString.length; j++)
if ( (name.charAt(i)) == unsafeString.charAt(j) )
return false;
}
return true;
}
function isVaildDomainHost(val)
{
for (j = 0; j < val.length; j++)
{
var c = val.charAt(j);
if (c >= 'a' && c <= 'z')
{
continue;
}
else if (c >= 'A' && c <= 'Z')
{
continue;
}
else if (c >= '0' && c <= '9')
{
continue;
}
else if (c == '-')
{
continue;
}
else
{
return false;
}
}
return true;
}
function isVaildDomain(val)
{
if (val.length > 67)
{
return false;
}
if (val.indexOf('.') == -1)
{
return false;
}
if (val == '' || val.charAt(0) == '.' || val.charAt(val.length-1) == '.')
{
return false;
}
for (j = 0; j < val.length; j++)
{
var c = val.charAt(j);
if (c >= 'a' && c <= 'z')
{
continue;
}
else if (c >= 'A' && c <= 'Z')
{
continue;
}
else if (c >= '0' && c <= '9')
{
continue;
}
else if (c == '-' || c == '.')
{
continue;
}
else
{
return false;
}
}
return true;
}
function btnAdd(place)
{
var loc = place + '?action=add';
with ( document.forms[0] )
{
loc += '&service=' + ispname[ispname.selectedIndex].value;
loc += '&iface=' + ifc[ifc.selectedIndex].value;
loc += '&hostname=' + domainname.value;
loc += '&username=' + username.value;
loc += '&password=' + password.value;
loc += '&domainname=' + domainname.value;
if (ispname.selectedIndex == 1)
{
loc += '&protocol=' + infoprotocol[infoprotocol.selectedIndex].value;
loc += '&serveraddr=' + infoserveraddress.value;
}
else
{
loc += '&protocol=' + 'GNUDip.http';
loc += '&serveraddr=' + Server.value;
}
}
var code = 'location="' + loc + '"';
eval(code);
}
function btnRemove()
{
var rml = getElement('rml');
if (rml == null)
return;
var SubmitForm = new webSubmitForm();
with (document.forms[0])
{
if (rml.length > 0)
{
var i;
for (i = 0; i < rml.length; i++)
{
if (rml[i].checked == true)
{
SubmitForm.addParameter(rml[i].value,'');
}
}
}
else if (rml.checked == true)
{
SubmitForm.addParameter(rml.value,'');
}
}
SubmitForm.setAction('del.cgi?RequestFile=/html/application/ddns.asp');
SubmitForm.submit();
}
function LoadFrame()
{
var i = GetmenuName("ddns.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
if (typeof(top.DDNSProvider) != 'undefined' && top.DDNSProvider != '')
setSelect('DDNSProviderType',top.DDNSProvider);
ISPChange();
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_0');
}
}
function checkeMail(mail)
{
return(new RegExp(/^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/).test(mail));
}
function CheckForm()
{
var ProviderType = getSelectVal('DDNSProviderType');
if(DDNS.length >= 32)
{
AddErrInfo(eval("gErrStr6"), '')
return false;
}
with (getElement('ConfigForm'))
{
if (HostName.value == "" || isVaildDomainHost(HostName.value) == false)
{
AddErrInfo(eval("gErrStr7"), 'HostName');
return false;
}
if (isVaildDomain(DomainName.value) == false)
{
AddErrInfo(eval("gErrStr5"), 'DomainName');
return false;
}
if ((ProviderType == "Other") && (Provider.value == ''))
{
AddErrInfo(eval("gErrStr17"), 'Provider');
return false;
}
for (i = 0; i < DDNS.length; i++)
{
if ( (DomainName.value == DDNS[i].DomainName) &&
(HostName.value == DDNS[i].HostName) && selctIndex != i )
{
AddErrInfo(eval("gErrStr8"), 'DomainName');
return false;
}
}
if (DDNSProviderType.value == 'TZO')
{
if (checkeMail(Username.value) == false)
{
AddErrInfo(eval("gErrStr9") + getSafeString(Username.value) + eval("gErrStr10"), 'Username');
return false;
}
}
else
{
if (Username.value == "" || isTValidName(Username.value) == false)
{
AddErrInfo(eval("gErrStr11"), 'Username');
return false;
}
}
if (Password.value == "")
{
AddErrInfo(eval("gErrStr12"), 'Password');
return false;
}
if("Other" == ProviderType)
{
if((Server.value == "") || (false == isCorrectIpAddress(Server.value))
|| (false == isValidDomain(Server.value, 64)))
{
AddErrInfo(eval("gErrStr13"), 'Server');
return false;
}
}
if (isValidPort(ServicePort.value) == false)
{
AddErrInfo(eval("gErrStr15"), 'ServicePort');
return false;
}
if (Interface.value == "")
{
AddErrInfo(eval("gErrStr16"), 'Interface');
return false;
}
}
return true;
}
function AddSubmitParam(SubmitForm)
{
setDisable('DDNSProviderType',1);
setDisable('Interface',1);
SubmitForm.addDiv('ConfigureDiv','x');
var ProviderType = getSelectVal('DDNSProviderType');
SubmitForm.delParameter('x.Password');
if (getValue('Password') != '@1GV)Z<!')
{
SubmitForm.addParameter('x.Password',getValue('Password'));
}
if (ProviderType == 'TZO')
{
SubmitForm.addParameter('x.Provider', 'TZO');
SubmitForm.addParameter('x.Protocol', 'TZO');
}
else if(ProviderType == 'DynDNS.org')
{
SubmitForm.addParameter('x.Provider', 'DynDNS.org');
SubmitForm.addParameter('x.Protocol', 'dyndns');
}
var DomainPrefix = getSelectVal('Interface');
SubmitForm.addParameter('x.WanPath', DomainPrefix);
if( selctIndex == -1 )
{
SubmitForm.setAction('addcfg.cgi?x=' + 'InternetGatewayDevice.Services.X_DDNSConfiguration'
+ '&RequestFile=/html/application/ddns.asp');
}
else
{
SubmitForm.setAction('setcfg.cgi?x=' + DDNS[selctIndex].domain
+ '&RequestFile=/html/application/ddns.asp');
}
top.aplyLine = selctIndex;
top.DDNSProvider = ProviderType;
setDisable('btnApply',1);
}
function ISPChange()
{
with (document.forms[0])
{
if (DDNSProviderType.value == 'TZO')
{
HostName.value = '';
DomainName.value = '';
Username.value = '';
Password.value = '';
getElement('tdUserType').innerHTML = eval("gVarEmail");
setDisplay('ispinfoprotocol', 0);
setDisplay('isDDNSServer', 0);
Protocol.value = 'TZO';
Server.value = 'cgi.tzo.com';
ServicePort.value = '80';
}
else if (DDNSProviderType.value == 'DynDNS.org')
{
HostName.value = '';
DomainName.value = '';
Username.value = '';
Password.value = '';
getElement('tdUserType').innerHTML = eval("gVarUsername");
setDisplay('ispinfoprotocol', 0);
setDisplay('isDDNSServer', 0);
Protocol.value = 'dyndns';
Server.value = 'DynDNS.org';
ServicePort.value = '80';
}
else
{
getElement('tdUserType').innerHTML = eval("gVarUsername");
Server.value = '';
ServicePort.value = '';
HostName.value = '';
DomainName.value = '';
Username.value = '';
Password.value = '';
setDisplay('ispinfoprotocol', 1);
setDisplay('isDDNSServer', 1);
}
}
}
function writewan(wancfgdomain)
{
var i = 0;
for (i = 0; i < WanDDNS.length; i++)
{
if (wancfgdomain == WanDDNS[i].domain)
{
break;
}
}
var obj = getElementByName('Interface');
obj.selectedIndex = i;
}
function setCtlDisplay(record)
{
if (setSelect('DDNSProviderType', record.Provider) == false)
{
setSelect('DDNSProviderType', 'Other');
}
with (getElement('ConfigForm'))
{
var ProviderType = getElement('DDNSProviderType');
if (ProviderType.value == 'TZO')
{
getElement('tdUserType').innerHTML = eval("gVarEmail");
setDisplay('ispinfoprotocol', 0);
setDisplay('isDDNSServer', 0);
writewan(record.wancfgdomain);
setText('HostName', record.HostName);
setText('DomainName', record.DomainName);
setText('Username', record.Username);
setText('Password', record.Password);
}
else if (ProviderType.value == 'DynDNS.org')
{
setText('HostName', record.HostName);
setText('DomainName', record.DomainName);
setText('Username', record.Username);
setText('Password', record.Password);
getElement('tdUserType').innerHTML = eval("gVarUsername");
setDisplay('ispinfoprotocol', 0);
setDisplay('isDDNSServer', 0);
writewan(record.wancfgdomain);
setSelect('Protocol', 'DynDNS.org');
Server.value = 'DynDNS.org';
ServicePort.value = '80';
}
else
{
getElement('tdUserType').innerHTML = eval("gVarUsername");
setText('Server', record.Server);
setText('ServicePort', record.Port);
setText('HostName', record.HostName);
setText('DomainName', record.DomainName);
setText('Username', record.Username);
setText('Password', record.Password);
setText('Provider', record.Provider);
writewan(record.wancfgdomain);
setDisplay('ispinfoprotocol', 1);
setSelect('Protocol', 'GNUDip.http');
setDisplay('isDDNSServer', 1);
}
}
}
function setControl(index)
{
var record;
selctIndex = index;
if (index == -1)
{
record = new stDDNS('','','DynDNS.org','','','','','','','');
setDisable('Interface',0);
}
else
{
setDisable('Interface',1);
record = DDNS[index];
}
setCtlDisplay(record);
setDisable('Interface', 0);
}
function clickRemove()
{
removeInst('/html/application/ddns.asp');
}

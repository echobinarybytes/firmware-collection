AddErrInfo(eval("gErrStr3"), 'QueuePIR3');
return false;
}
if (QueueCBS3.value == ''
|| isInteger(QueueCBS3.value) == false)
{
AddErrInfo(eval("gErrStr2"), 'QueueCBS3');
return false;
}
if (parseInt(QueueCBS3.value) < 0
|| parseInt(QueueCBS3.value) > 200000)
{
AddErrInfo(eval("gErrStr3"), 'QueueCBS3');
return false;
}
if (QueueMBS3.value == ''
|| isInteger(QueueMBS3.value) == false)
{
AddErrInfo(eval("gErrStr2"), 'QueueMBS3');
return false;
}
if (parseInt(QueueMBS3.value) < 0
|| parseInt(QueueMBS3.value) > 200000)
{
AddErrInfo(eval("gErrStr3"), 'QueueMBS3');
return false;
}
if (QueuePIR4.value == ''
|| isInteger(QueuePIR4.value) == false)
{
AddErrInfo(eval("gErrStr2"), 'QueuePIR4');
return false;
}
if (parseInt(QueuePIR4.value) < 0
|| parseInt(QueuePIR4.value) > 200000)
{
AddErrInfo(eval("gErrStr3"), 'QueuePIR4');
return false;
}
if (QueueCBS4.value == ''
|| isInteger(QueueCBS4.value) == false)
{
AddErrInfo(eval("gErrStr2"), 'QueueCBS4');
return false;
}
if (parseInt(QueueCBS4.value) < 0
|| parseInt(QueueCBS4.value) > 200000)
{
AddErrInfo(eval("gErrStr3"), 'QueueCBS4');
return false;
}
if (QueueMBS4.value == ''
|| isInteger(QueueMBS4.value) == false)
{
AddErrInfo(eval("gErrStr2"), 'QueueMBS4');
return false;
}
if (parseInt(QueueMBS4.value) < 0
|| parseInt(QueueMBS4.value) > 200000)
{
AddErrInfo(eval("gErrStr3"), 'QueueMBS4');
return false;
}
if (parseInt(QueuePIR1.value) < parseInt(QueueCIR1.value))
{
AddErrInfo(eval("gErrStr5"), 'QueuePIR1');
return false;
}
if (parseInt(QueueMBS1.value) < parseInt(QueueCBS1.value))
{
AddErrInfo(eval("gErrStr6"), 'QueueMBS1');
return false;
}
if (parseInt(QueuePIR2.value) < parseInt(QueueCIR2.value))
{
AddErrInfo(eval("gErrStr5"), 'QueuePIR2');
return false;
}
if (parseInt(QueueMBS2.value) < parseInt(QueueCBS2.value))
{
AddErrInfo(eval("gErrStr6"), 'QueueMBS2');
return false;
}
if (parseInt(QueuePIR3.value) < parseInt(QueueCIR3.value))
{
AddErrInfo(eval("gErrStr5"), 'QueuePIR3');
return false;
}
if (parseInt(QueueMBS3.value) < parseInt(QueueCBS3.value))
{
AddErrInfo(eval("gErrStr6"), 'QueueMBS3');
return false;
}
if (parseInt(QueuePIR4.value) < parseInt(QueueCIR4.value))
{
AddErrInfo(eval("gErrStr5"), 'QueuePIR4');
return false;
}
if (parseInt(QueueMBS4.value) < parseInt(QueueCBS4.value))
{
AddErrInfo(eval("gErrStr6"), 'QueueMBS4');
return false;
}
return true;
}
}
function AddGlobleData(Form)
{
with (document.forms[0])
{
if (GloableData[0].Enable != getValueDft('Enable'))
{
Form.addParameter('x.Enable', getValueDft('Enable'));
glbChange = true;
}
if (QosCIR.value == '')
{
Form.addParameter('x.X_BandWidth', 0); 
glbChange = true;
}
else if (GloableData[0].Bandwith != getValueDft('QosCIR'))
{
Form.addParameter('x.X_BandWidth', getValueDft('QosCIR'));
glbChange = true;
}
if (GloableData[0].QueueType != getValueDft('QueueType'))
{
Form.addParameter('x.X_QueueType', getValueDft('QueueType'));
glbChange = true;
}
}
}
function AddQueueData(Form)
{
with (document.forms[0])
{
if (QueueType.value == 'PQ')
{
if ((QueueData[0].Enable != getValueDft('PQEbable1'))
|| (QueueData[1].Enable != getValueDft('PQEbable2'))
|| (QueueData[2].Enable != getValueDft('PQEbable3'))
|| (QueueData[3].Enable != getValueDft('PQEbable4')))
{
Form.addParameter('a.QueueEnable',getValueDft('PQEbable1'));
Form.addParameter('b.QueueEnable',getValueDft('PQEbable2'));
Form.addParameter('c.QueueEnable',getValueDft('PQEbable3'));
Form.addParameter('d.QueueEnable',getValueDft('PQEbable4'));
queueChange = true;
}
}
else if (QueueType.value == 'WFQ')
{
if ((QueueData[0].Enable != getValueDft('WRREbable1'))
|| (QueueData[1].Enable != getValueDft('WRREbable2'))
|| (QueueData[2].Enable != getValueDft('WRREbable3'))
|| (QueueData[3].Enable != getValueDft('WRREbable4')))
{
Form.addParameter('a.QueueEnable',getValueDft('WRREbable1'));
Form.addParameter('b.QueueEnable',getValueDft('WRREbable2'));
Form.addParameter('c.QueueEnable',getValueDft('WRREbable3'));
Form.addParameter('d.QueueEnable',getValueDft('WRREbable4'));
queueChange = true;
}
if (getValueDft('QueueWeight1') == '')
{
Form.addParameter('a.QueueWeight', 0);
}
else if (QueueData[0].QueWeight != getValueDft('QueueWeight1'))
{
Form.addParameter('a.QueueWeight',getValueDft('QueueWeight1'));
queueChange = true;
}
if (getValueDft('QueueWeight2') == '')
{
Form.addParameter('b.QueueWeight', 0);
}
else if (QueueData[1].QueWeight != getValueDft('QueueWeight2'))
{
Form.addParameter('b.QueueWeight',getValueDft('QueueWeight2'));
queueChange = true;
}
if (getValueDft('QueueWeight3') == '')
{
Form.addParameter('c.QueueWeight',0);
}
else if (QueueData[2].QueWeight != getValueDft('QueueWeight3'))
{
Form.addParameter('c.QueueWeight',getValueDft('QueueWeight3'));
queueChange = true;
}
if (getValueDft('QueueWeight4') == '')
{
Form.addParameter('d.QueueWeight',0);
}
else if (QueueData[3].QueWeight != getValueDft('QueueWeight4'))
{
Form.addParameter('d.QueueWeight',getValueDft('QueueWeight4'));
queueChange = true;
}
}
else if (QueueType.value == 'WFQ,PQ')
{
if ((QueueData[0].Enable != getValueDft('WFQPQEbable1'))
|| (QueueData[1].Enable != getValueDft('WFQPQEbable2'))
|| (QueueData[2].Enable != getValueDft('WFQPQEbable3'))
|| (QueueData[3].Enable != getValueDft('WFQPQEbable4')))
{
Form.addParameter('a.QueueEnable',getValueDft('WFQPQEbable1'));
Form.addParameter('b.QueueEnable',getValueDft('WFQPQEbable2'));
Form.addParameter('c.QueueEnable',getValueDft('WFQPQEbable3'));
Form.addParameter('d.QueueEnable',getValueDft('WFQPQEbable4'));
queueChange = true;
}
if (getValueDft('WFQPQQueueWeight1') == '')
{
Form.addParameter('a.QueueWeight', 0);
}
else if (QueueData[0].QueWeight != getValueDft('WFQPQQueueWeight1'))
{
Form.addParameter('a.QueueWeight', getValueDft('WFQPQQueueWeight1'));
queueChange = true;
}
if (getValueDft('WFQPQQueueWeight2') == '')
{
Form.addParameter('b.QueueWeight', 0);
}
else if (QueueData[1].QueWeight != getValueDft('WFQPQQueueWeight2'))
{
Form.addParameter('b.QueueWeight', getValueDft('WFQPQQueueWeight2'));
queueChange = true;
}
if (getValueDft('WFQPQQueueWeight3') == '')
{
Form.addParameter('c.QueueWeight', 0);
}
else if (QueueData[2].QueWeight != getValueDft('WFQPQQueueWeight3'))
{
Form.addParameter('c.QueueWeight',getValueDft('WFQPQQueueWeight3'));
queueChange = true;
}
if (getValueDft('WFQPQQueueWeight4') == '')
{
Form.addParameter('d.QueueWeight', 0);
}
else if (QueueData[3].QueWeight != getValueDft('WFQPQQueueWeight4'))
{
Form.addParameter('d.QueueWeight', getValueDft('WFQPQQueueWeight4'));
queueChange = true;
}
}
}
}
function AddPolicerData(Form)
{
with (document.forms[0])
{
if ((PolicerData[0].Enable != getValueDft('CAREbable1'))
|| (PolicerData[1].Enable != getValueDft('CAREbable2'))
|| (PolicerData[2].Enable != getValueDft('CAREbable3'))
|| (PolicerData[3].Enable != getValueDft('CAREbable4')))
{
Form.addParameter('h.PolicerEnable', getValueDft('CAREbable1'));
Form.addParameter('i.PolicerEnable', getValueDft('CAREbable2'));
Form.addParameter('j.PolicerEnable', getValueDft('CAREbable3'));
Form.addParameter('k.PolicerEnable', getValueDft('CAREbable4'));
policerChange = true;
}
if (getValueDft('QueueCIR1') == '')
{
Form.addParameter('h.CommittedRate', 0);
}
else if (PolicerData[0].CommittedRate != getValueDft('QueueCIR1'))
{
Form.addParameter('h.CommittedRate', getValueDft('QueueCIR1'));
policerChange = true;
}
if (getValueDft('QueueCIR2') == '')
{
Form.addParameter('i.CommittedRate', 0);
}
else if (PolicerData[1].CommittedRate != getValueDft('QueueCIR2'))
{
Form.addParameter('i.CommittedRate', getValueDft('QueueCIR2'));
policerChange = true;
}
if (getValueDft('QueueCIR3') == '')
{
Form.addParameter('j.CommittedRate', 0);
}
else if (PolicerData[2].CommittedRate != getValueDft('QueueCIR3'))
{
Form.addParameter('j.CommittedRate', getValueDft('QueueCIR3'));
policerChange = true;
}
if (getValueDft('QueueCIR4') == '')
{
Form.addParameter('k.CommittedRate', 0);
}
else if (PolicerData[3].CommittedRate != getValueDft('QueueCIR4'))
{
Form.addParameter('k.CommittedRate', getValueDft('QueueCIR4'));
policerChange = true;
}
if (getValueDft('QueuePIR1') == '')
{
Form.addParameter('h.PeakRate',0);
}
else if (PolicerData[0].PeakRate != getValueDft('QueuePIR1'))
{
Form.addParameter('h.PeakRate',getValueDft('QueuePIR1'));
policerChange = true;
}
if (getValueDft('QueueCBS1') == '')
{
Form.addParameter('h.CommittedBurstSize',0);
}
else if (PolicerData[0].CommittedBurstSize != getValueDft('QueueCBS1'))
{
Form.addParameter('h.CommittedBurstSize',getValueDft('QueueCBS1'));
policerChange = true;
}
if (getValueDft('QueueMBS1') == '')
{
Form.addParameter('h.PeakBurstSize',0);
}
else if (PolicerData[0].PeakBurstSize != getValueDft('QueueMBS1'))
{
Form.addParameter('h.PeakBurstSize',getValueDft('QueueMBS1'));
policerChange = true;
}
if (getValueDft('QueuePIR2') == '')
{
Form.addParameter('i.PeakRate',0);
}
else if (PolicerData[1].PeakRate != getValueDft('QueuePIR2'))
{
Form.addParameter('i.PeakRate',getValueDft('QueuePIR2'));
policerChange = true;
}
if (getValueDft('QueueCBS2') == '')
{
Form.addParameter('i.CommittedBurstSize',0);
}
else if (PolicerData[1].CommittedBurstSize != getValueDft('QueueCBS2'))
{
Form.addParameter('i.CommittedBurstSize',getValueDft('QueueCBS2'));
policerChange = true;
}
if (getValueDft('QueueMBS2') == '')
{
Form.addParameter('i.PeakBurstSize',0);
}
else if (PolicerData[1].PeakBurstSize != getValueDft('QueueMBS2'))
{
Form.addParameter('i.PeakBurstSize',getValueDft('QueueMBS2'));
policerChange = true;
}
if (getValueDft('QueuePIR3') == '')
{
Form.addParameter('j.PeakRate',0);
}
else if (PolicerData[2].PeakRate != getValueDft('QueuePIR3'))
{
Form.addParameter('j.PeakRate',getValueDft('QueuePIR3'));
policerChange = true;
}
if (getValueDft('QueueCBS3') == '')
{
Form.addParameter('j.CommittedBurstSize',0);
}
else if (PolicerData[2].CommittedBurstSize != getValueDft('QueueCBS3'))
{
Form.addParameter('j.CommittedBurstSize',getValueDft('QueueCBS3'));
policerChange = true;
}
if (getValueDft('QueueMBS3') == '')
{
Form.addParameter('j.PeakBurstSize',0);
}
else if (PolicerData[2].PeakBurstSize != getValueDft('QueueMBS3'))
{
Form.addParameter('j.PeakBurstSize',getValueDft('QueueMBS3'));
policerChange = true;
}
if (getValueDft('QueuePIR4') == '')
{
Form.addParameter('k.PeakRate',0);
}
else if (PolicerData[3].PeakRate != getValueDft('QueuePIR4'))
{
Form.addParameter('k.PeakRate',getValueDft('QueuePIR4'));
policerChange = true;
}
if (getValueDft('QueueCBS4') == '')
{
Form.addParameter('k.CommittedBurstSize',0);
}
else if (PolicerData[3].CommittedBurstSize != getValueDft('QueueCBS4'))
{
Form.addParameter('k.CommittedBurstSize',getValueDft('QueueCBS4'));
policerChange = true;
}
if (getValueDft('QueueMBS4') == '')
{
Form.addParameter('k.PeakBurstSize',0);
}
else if (PolicerData[3].PeakBurstSize != getValueDft('QueueMBS4'))
{
Form.addParameter('k.PeakBurstSize',getValueDft('QueueMBS4'));
policerChange = true;
}
}
}
function QosSetAction(Form)
{
if ((false == glbChange) && (false == queueChange) && (false == policerChange))
{
Form.addParameter('x.Enable', getValueDft('Enable'));
Form.setAction('setcfg.cgi?' + 'x=InternetGatewayDevice.QueueManagement'
+ '&RequestFile=/html/network/qos.asp');
}
if ((false == glbChange) && (false == queueChange) && (true == policerChange))
{
Form.setAction('setcfg.cgi?'
+ 'h=InternetGatewayDevice.QueueManagement.Policer.1'
+ '&i=InternetGatewayDevice.QueueManagement.Policer.2'
+ '&j=InternetGatewayDevice.QueueManagement.Policer.3'
+ '&k=InternetGatewayDevice.QueueManagement.Policer.4'
+ '&RequestFile=/html/network/qos.asp');
}
if ((false == glbChange) && (true == queueChange) && (false == policerChange))
{
Form.setAction('setcfg.cgi?'
+ 'a=InternetGatewayDevice.QueueManagement.Queue.1'
+ '&b=InternetGatewayDevice.QueueManagement.Queue.2'
+ '&c=InternetGatewayDevice.QueueManagement.Queue.3'
+ '&d=InternetGatewayDevice.QueueManagement.Queue.4'
+ '&RequestFile=/html/network/qos.asp');
}
if ((false == glbChange) && (true == queueChange) && (true == policerChange))
{
Form.setAction('setcfg.cgi?'
+ 'a=InternetGatewayDevice.QueueManagement.Queue.1'
+ '&b=InternetGatewayDevice.QueueManagement.Queue.2'
+ '&c=InternetGatewayDevice.QueueManagement.Queue.3'
+ '&d=InternetGatewayDevice.QueueManagement.Queue.4'
+ '&h=InternetGatewayDevice.QueueManagement.Policer.1'
+ '&i=InternetGatewayDevice.QueueManagement.Policer.2'
+ '&j=InternetGatewayDevice.QueueManagement.Policer.3'
+ '&k=InternetGatewayDevice.QueueManagement.Policer.4'
+ '&RequestFile=/html/network/qos.asp');
}
if ((true == glbChange) && (false == queueChange) && (false == policerChange))
{
Form.setAction('setcfg.cgi?' + 'x=InternetGatewayDevice.QueueManagement'
+ '&RequestFile=/html/network/qos.asp');
}
if ((true == glbChange) && (false == queueChange) && (true == policerChange))
{
Form.setAction('setcfg.cgi?'
+ 'x=InternetGatewayDevice.QueueManagement'
+ '&h=InternetGatewayDevice.QueueManagement.Policer.1'
+ '&i=InternetGatewayDevice.QueueManagement.Policer.2'
+ '&j=InternetGatewayDevice.QueueManagement.Policer.3'
+ '&k=InternetGatewayDevice.QueueManagement.Policer.4'
+ '&RequestFile=/html/network/qos.asp');
}
if ((true == glbChange) && (true == queueChange) && (false == policerChange))
{
Form.setAction('setcfg.cgi?'
+ 'x=InternetGatewayDevice.QueueManagement'
+ '&a=InternetGatewayDevice.QueueManagement.Queue.1'
+ '&b=InternetGatewayDevice.QueueManagement.Queue.2'
+ '&c=InternetGatewayDevice.QueueManagement.Queue.3'
+ '&d=InternetGatewayDevice.QueueManagement.Queue.4'
+ '&RequestFile=/html/network/qos.asp');
}
if ((true == glbChange) && (true == queueChange) && (true == policerChange))
{
Form.setAction('setcfg.cgi?'
+ 'x=InternetGatewayDevice.QueueManagement'
+ '&a=InternetGatewayDevice.QueueManagement.Queue.1'
+ '&b=InternetGatewayDevice.QueueManagement.Queue.2'
+ '&c=InternetGatewayDevice.QueueManagement.Queue.3'
+ '&d=InternetGatewayDevice.QueueManagement.Queue.4'
+ '&h=InternetGatewayDevice.QueueManagement.Policer.1'
+ '&i=InternetGatewayDevice.QueueManagement.Policer.2'
+ '&j=InternetGatewayDevice.QueueManagement.Policer.3'
+ '&k=InternetGatewayDevice.QueueManagement.Policer.4'
+ '&RequestFile=/html/network/qos.asp');
}
glbChange = false;
queueChange = false;
policerChange = false;
}
function AddQosData(Form)
{
AddGlobleData(Form);
AddQueueData(Form);
AddPolicerData(Form);
QosSetAction(Form);
}
function Qos_Submit()
{
top.errStrStr = '';
top.errIdCtrlStr = '';
if (CheckGlobleData() == false)
{
if (top.errStrStr != '')
{
setDisplay('erroinfo', 1)
setErrInfo();
}
return false;
}
if (CheckQueueData() == false)
{
if (top.errStrStr != '')
{
setDisplay('erroinfo', 1)
setErrInfo();
}
return false;
}
if (CheckPolicerData() == false)
{
if (top.errStrStr != '')
{
setDisplay('erroinfo', 1)
setErrInfo();
}
return false;
}
var Form = new webSubmitForm();
AddQosData(Form);
Form.submit();
}
function clickAction()
{
setDisplay('QosConfig', 1);
if (getCheckVal('Enable') == 0)
{
setDisplay('QosEnableDisplayTr1', 0);
setDisplay('QosEnableDisplayTr2', 0);
setDisplay('ForceWeight', 0);
setDisplay('QosQueue', 0);
setDisplay('PQueque', 0);
setDisplay('WRRQueque', 0);
setDisplay('WFQPQQueque', 0);
setDisplay('PolicerQueque', 0);
setDisplay('appOption', 0);
}
else
{
var type = getElement('QueueType');
setDisplay('QosQueue', 1);
if (type.value == 'WFQ')
{
setDisplay('ForceWeight', 0);
setDisplay('WRRQueque', 1);
setDisplay('PQueque', 0);
setDisplay('WFQPQQueque', 0);
}
else if (type.value == 'PQ')
{
setDisplay('ForceWeight', 0);
setDisplay('PQueque', 1);
setDisplay('WRRQueque', 0);
setDisplay('WFQPQQueque', 0);
}
else if (type.value == 'WFQ,PQ')
{
setDisplay('ForceWeight', 0);
setDisplay('PQueque', 0);
setDisplay('WRRQueque', 0);
setDisplay('WFQPQQueque', 1);
}
setDisplay('QosEnableDisplayTr1', 1);
setDisplay('QosEnableDisplayTr2', 1);
setDisplay('PolicerQueque', 1);
setDisplay('appOption', 1);
}
Qos_Submit();
}
</script>
</head>

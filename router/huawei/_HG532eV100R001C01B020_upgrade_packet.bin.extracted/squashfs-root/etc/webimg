|| (WanInfo[j].ServiceList == 'TR069')
|| 0 == WanInfo[j].ipv6enable)
{
continue;
}
}
document.write('<TR id="record_' + i
+ '" class="trTabContent" onclick="selectLine(this.id);">');
document.write('<TD>' + StaticRoute[i].DestIPAddress + '/' + StaticRoute[i].PrefixLength + '</TD>');
document.write('<TD>' + StaticRoute[i].GatewayIPAddress + '</TD>');
document.write('<TD>' + ShowIfName(StaticRoute[i].Interface) + '</TD>');
if ((StaticRoute[i].Status == "Active") && (1 == StaticRoute[i].enable))
{
document.write('<TD>' + eval("gVarPubActive") + '</TD>');
}
else
{
document.write('<TD>' + eval("gVarPubInActive") + '</TD>');
}
if (StaticRoute[i].enable == 1)
{
document.write('<TD>' + eval("gVarPubEnable") + '</TD>');
}
else
{
document.write('<TD>' + eval("gVarPubDisable") + '</TD>');
}
document.write('<TD>' + '<input type="checkbox" name="rml"'  + ' value='
+ StaticRoute[i].Domain  + '>' + '</TD>');
document.write('</TR>');
}

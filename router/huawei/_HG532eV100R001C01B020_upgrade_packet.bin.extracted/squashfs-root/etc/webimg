var ssidIdx = 0;
var wlHide = Wlan[0].wlHide;
var band = 2;
var phy = 'g';
var wepEnable = getWepSwitch();
function isvalidpin(val)
{
var ret = false;
var len = val.length;
var pinSize = 8;
var pinvalue = new Array();
if ( len !=  pinSize)
return false;
else
{
for ( i = 0; i < pinSize; i++ )
{
pinvalue[i] = parseInt(val.charAt(i),10);
}
var accum = 0;
accum = (pinvalue[0] + pinvalue[2] + pinvalue[4] + pinvalue[6]) * 3 + pinvalue[1] + pinvalue[3] + pinvalue[5] + pinvalue[7];
if (0 == (accum % 10))
{
return true; 
}
else
{
return false; 
}
}
return ret;
}
function get_cookie (cookie_name)
{
var results = document.cookie.match ( '(^|;) ?' + cookie_name + '=([^;]*)(;|$)' );
if (results)
{
return (results[2]);
}
else
{
return "";
}
}
function getWepSwitch()
{
if (Wlan[0].Encryption == 'WEPEncryption')
{
return 1;
}
else if (Wlan[0].Encryption == 'None')
{
return 0;
}
}
function hideOption(selObject, index)
{
var oldOption = selObject.children(index);
var oldStr = oldOption.innerText;
var newOption = document.createElement('<div' + oldOption.outerHTML.match(/(<\w*)([^>]*)(>)/)[2] +'>');
newOption.innerText  = oldStr;
newOption.swapNode(oldOption);
}
function hideWlSecInfo(hidden)
{
var status = 'visible';
with ( document.forms[0] )
{
var i = 0;
if ( hidden == 1 )
{
status = 'hidden';
wlHide.disabled = 1;
wlSsid.disabled = 1;
}
else
{
wlHide.disabled = 0;
wlSsid.disabled = 0;
}
}
}
function wlSecCbClick(cb)
{
if (cb.checked == true)
{
hideWlSecInfo(0);
}
else
{
hideWlSecInfo(1);
}
}
function getPreauth(wlPreauth)
{
var ret;
if (wlPreauth == "on")
ret = 1;
else
ret = 0;
return ret;
}
function isValidWPAPskKey(val)
{
var ret = false;
var len = val.length;
var maxSize = 64;
var minSize = 8;
if (len >= minSize && len < maxSize)
{
if (isValidAscii(val) == '')
{
ret = true;
}
}
else if (len == maxSize)
{
for (i = 0; i < maxSize; i++)
{
if (isHexaDigit(val.charAt(i)) == false)
{
break;
}
}
if (i == maxSize)
{
ret = true;
}
}
else
{
ret = false;
}
return ret;
}
function hideAllElements()
{
setDisplay("wlWpaD", 0);
setDisplay("wpaPreShareKey", 0);
setDisplay("wpaGTKRekey", 0);
setDisplay("wlRadius", 0);
setDisplay("Preauth", 0);
setDisplay("NetReauth", 0);
setDisplay("wlWepD", 0);
setDisplay("keyInfo", 0);
setDisplay("wps", 0);
}
function wlKeyIndexChange()
{
var index = parseInt(getSelectVal('wlKeyIndex'));
var i = 1;
for(i = 1; i <= 4; i++)
{
setDisplay('WepKey'+ i, 0);
if(index == i)
{
setDisplay('WepKey'+ i, 1);
}
}
}
function encrypChange()
{
var ssidindex = getSelectVal('wlSsidIdx');
ssidIdx = parseInt(ssidindex,10);
with (document.forms[0])
{
var cwep = getSelectVal('wlWep');
var authMode = getSelectVal('wlnAuthMode');
var mode = getValue('wlgnMode');
if (cwep == 'WEPEncryption' && authMode == 'Basic')
{
setDisplay("keyInfo", 1);
wlKeys[0].disabled = 0;
wlKeys[1].disabled = 0;
wlKeys[2].disabled = 0;
wlKeys[3].disabled = 0;
wlKeyIndex.length = 0;
wlKeyIndex[0] = new Option("1", "1");
wlKeyIndex[1] = new Option("2", "2");
wlKeyIndex[2] = new Option("3", "3");
wlKeyIndex[3] = new Option("4", "4");
setSelect('wlKeyIndex', Wlan[ssidIdx].KeyIndex) ;
setDisplay('wlWepAuth', 1);
if (mode == 'n')
{
setDisplay('wlWepAuth', 0);
setDisplay("keyInfo", 0);
}
}
else
{
setDisplay('wlWepAuth', 0);
setDisplay("keyInfo", 0);
}
if(((authMode == 'WPA') ||(authMode == '11i') || (authMode == 'WPAand11i')) && (ssidIdx == 0) )
{
setCheck("wpsControl", WPSInfo.WPSControl);
setDisable("wpsControl", 0);
var wpaen= getSelectVal('wlWpa');
if (wpaen == 'TKIPEncryption' )
{
setCheck("wpsControl", 0);
setDisplay("wps", 0);
}
else
{
setDisplay("wps", 1);
}
}
}
wlKeyIndexChange();
}
function wlKeyBitChange()
{
}
function authModeChange(OnPageLoad)
{
var ssidindex = getSelectVal('wlSsidIdx');
ssidIdx = parseInt(ssidindex,10);
with (document.forms[0])
{
var authMode = getSelectVal('wlnAuthMode');
var mode = '';
var i, algos;
hideAllElements();
switch (authMode)
{
case 'Basic':
case 'shared':
setDisplay("wlWepD", 1);
setDisplay("wlWepAuth", 1);
setDisplay("keyInfo", 1);
break;
case 'radius':
setDisplay("wlRadius", 1);
setDisplay("wlWepD", 1);
break;
case 'wpa2':
case 'wpa2mix':
setDisplay("Preauth", 1);
setDisplay("NetReauth", 1);
case 'wpa':
setDisplay("wlWpaD", 1);
setDisplay("wlRadius", 1);
break;
case 'WPAand11i':
case '11i':
case 'psk2mix':
case 'WPA':
setDisplay("wlWpaD", 1);
setDisplay("wpaPreShareKey", 1);
if(ssidIdx == 0)
{
var ishide = getCheckVal('wlHide');
if (ishide == '1')
{
setCheck("wpsControl", 0);
}
else
{
setDisplay("wps", 1);
}
}
break;
}
if (OnPageLoad == 1)
{
algos = new Array(new Array('TKIP','TKIPEncryption'), new Array('AES',
'AESEncryption'),new Array('TKIP+AES',
'TKIPandAESEncryption'));
wlWpa.length = algos.length;
for (var i in algos)
{
wlWpa[i] = new Option(algos[i][0], algos[i][1]);
wlWpa[i].value = algos[i][1];
if (Wlan[ssidIdx].BeaconType == 'WPA'
&& wlWpa[i][1] == Wlan[ssidIdx].WPAModes)
{
wlWpa[i].selected = true;
}
else if (Wlan[ssidIdx].BeaconType == '11i'
&& wlWpa[i][1] ==  Wlan[ssidIdx].IEEE11iEModes)
{
wlWpa[i].selected = true;
}
else if (Wlan[ssidIdx].BeaconType == 'WPAand11i'
&& wlWpa[i][1] ==  Wlan[ssidIdx].WPAWPA2Modes)
{
wlWpa[i].selected = true;
}
}
}
else if (OnPageLoad == 2)
{
algos = new Array(new Array('AES','AESEncryption'));
wlWpa.length = algos.length;
for (var i in algos)
{
wlWpa[i] = new Option(algos[i][0], algos[i][1]);
wlWpa[i].value = algos[i][1];
if (Wlan[ssidIdx].BeaconType == 'WPA'
&& wlWpa[i][1] == Wlan[ssidIdx].WPAModes)
{
wlWpa[i].selected = true;
}
else if (Wlan[ssidIdx].BeaconType == '11i'
&& wlWpa[i][1] ==  Wlan[ssidIdx].IEEE11iEModes)
{
wlWpa[i].selected = true;
}
else if (Wlan[ssidIdx].BeaconType == 'WPAand11i'
&& wlWpa[i][1] ==  Wlan[ssidIdx].WPAWPA2Modes)
{
wlWpa[i].selected = true;
}
}
}
else {
if (authMode == 'WPA') {
setSelect('wlWpa', 'AESEncryption');
}
else if (authMode == '11i') {
setSelect('wlWpa', 'AESEncryption');
}
else if (authMode == 'WPAand11i') {
setSelect('wlWpa', 'AESEncryption');
}
}
if (Wlan[ssidIdx].BeaconType == 'WPA' && authMode == 'WPA')
{
setSelect('wlWpa',Wlan[ssidIdx].WPAModes);
}
else if (Wlan[ssidIdx].BeaconType == '11i' && authMode == '11i')
{
setSelect('wlWpa',Wlan[ssidIdx].IEEE11iEModes);
}
else if(Wlan[ssidIdx].BeaconType == 'WPAand11i' && authMode == 'WPAand11i')
{
setSelect('wlWpa',Wlan[ssidIdx].WPAWPA2Modes);
}
wlWep.length = 0;
if (getValue('wlgnMode') == "n")
{
wlWep[0] = new Option("Disabled", "None")
}
else
{
wlWep[0] = new Option("Disabled", "None")
wlWep[1] = new Option("Enabled", "WEPEncryption");
}
WepAuthMode[0] = new Option("OPEN", "None");
WepAuthMode[1] = new Option("SHARE", "SharedAuthentication");
WepAuthMode[2] = new Option("BOTH", "BothAuthentication");
setSelect('wlWep', Wlan[ssidIdx].Encryption);
setSelect('WepAuthMode', Wlan[ssidIdx].AuthMode);
}
encrypChange();
}
function isValidKey(val, size)
{
var ret = false;
var len = val.length;
var dbSize = size * 2;
if (len == size)
{
if (isValidAscii(val) == '')
{
ret = true;
}
}
else if (len == dbSize)
{
for (i = 0; i < dbSize; i++)
{
if (isHexaDigit(val.charAt(i)) == false)
{
break;
}
}
if (i == dbSize)
{
ret = true;
}
}
else
{
ret = false;
}
return ret;
}
function getGmodeIndex(mode)
{
var ret = 0;
switch(mode)
{
case '0':
ret = 3;
break;
case '4':
ret = 1;
break;
case '5':
ret = 2;
break;
default:
ret = 0;
break;
}
return ret;
}
function gModeChange()
{
var ssidindex = getSelectVal('wlSsidIdx');
ssidIdx = parseInt(ssidindex, 10);
with ( document.forms[0] )
{
var mode = getValue('wlgnMode');
if ((mode == 'b/g/n') || (mode == 'n'))
{
setDisplay('wlTransferRate', 0);
setDisplay('11N', 1);
loadMCSList();
setSelect('wlMcs', Wlan[ssidIdx].MCSValue);
}
else
{
setDisplay('wlTransferRate', 1);
setDisplay('11N', 0);
loadRateList();
setSelect('wlRate', Wlan[ssidIdx].rate);
}
if (mode == 'n')
{
authModeChange(2);
setDisable('wlQos',1);
setCheck('wlQos',1);
}
else
{
setDisable('wlQos',0);
authModeChange(1);
setCheck('wlQos',Wlan[ssidIdx].WMMEnable);
}
if('b' == mode && 'JP' == getValue('RegulatoryDomain')){
loadChannelList(getSelectVal('RegulatoryDomain'));
}else{
loadChannelList(getSelectVal('RegulatoryDomain'));
}
}
}
function loadChannelList(country)
{
var ssidindex = getSelectVal('wlSsidIdx');
var mode = getValue('wlgnMode');
ssidIdx = parseInt(ssidindex,10);
document.forms[0].wlChannel[0] = new Option("Auto", "0");
if (country == "IL")
{
document.forms[0].wlChannel[1] = new Option("3", "3");
document.forms[0].wlChannel[2] = new Option("4", "4");
document.forms[0].wlChannel[3] = new Option("5", "5");
document.forms[0].wlChannel[4] = new Option("6", "6");
document.forms[0].wlChannel[5] = new Option("7", "7");
document.forms[0].wlChannel[6] = new Option("8", "8");
document.forms[0].wlChannel[7] = new Option("9", "9");
}
else if (country == "FR" || country == "JO" || country == "ES")
{
document.forms[0].wlChannel[1] = new Option("10", "10");
document.forms[0].wlChannel[2] = new Option("11", "11");
if (country == "FR" || country == "JO" )
{
document.forms[0].wlChannel[3] = new Option("12", "12");
document.forms[0].wlChannel[4] = new Option("13", "13");
}
}
else
{
document.forms[0].wlChannel[1] = new Option("1", "1");
document.forms[0].wlChannel[2] = new Option("2", "2");
document.forms[0].wlChannel[3] = new Option("3", "3");
document.forms[0].wlChannel[4] = new Option("4", "4");
document.forms[0].wlChannel[5] = new Option("5", "5");
document.forms[0].wlChannel[6] = new Option("6", "6");
document.forms[0].wlChannel[7] = new Option("7", "7");
document.forms[0].wlChannel[8] = new Option("8", "8");
document.forms[0].wlChannel[9] = new Option("9", "9");
document.forms[0].wlChannel[10] = new Option("10", "10");
document.forms[0].wlChannel[11] = new Option("11", "11");
if (country != "CA" && country != "DO" && country != "GT" && country != "MX" && country != "PA" && country != "PR" && country != "US")
{
document.forms[0].wlChannel[12] = new Option("12", "12");
document.forms[0].wlChannel[13] = new Option("13", "13");
if(country == "JP")
document.forms[0].wlChannel[14] = new Option("14", "14");
}
}
if(mode == 'b' && country == 'JP'){
document.forms[0].wlChannel[14] = new Option("14", "14");
}else{
document.forms[0].wlChannel[14] = null;
}
if(Wlan[ssidIdx].AutoChannel=="1")
setSelect('wlChannel',"0");
else
setSelect('wlChannel',Wlan[ssidIdx].channel);
}
function loadMCSList()
{
document.forms[0].wlMcs.options.length = 0;
document.forms[0].wlMcs[0] = new Option("Auto", "33");
if (Wlan[0].TxRxStream == '1')
{
document.forms[0].wlMcs[1] = new Option("0", "0");
document.forms[0].wlMcs[2] = new Option("1", "1");
document.forms[0].wlMcs[3] = new Option("2", "2");
document.forms[0].wlMcs[4] = new Option("3", "3");
document.forms[0].wlMcs[5] = new Option("4", "4");
document.forms[0].wlMcs[6] = new Option("5", "5");
document.forms[0].wlMcs[7] = new Option("6", "6");
document.forms[0].wlMcs[8] = new Option("7", "7");
}
if (Wlan[0].TxRxStream == '2')
{
document.forms[0].wlMcs[1] = new Option("8", "8");
document.forms[0].wlMcs[2] = new Option("9", "9");
document.forms[0].wlMcs[3] = new Option("10", "10");
document.forms[0].wlMcs[4] = new Option("11", "11");
document.forms[0].wlMcs[5] = new Option("12", "12");
document.forms[0].wlMcs[6] = new Option("13", "13");
document.forms[0].wlMcs[7] = new Option("14", "14");
document.forms[0].wlMcs[8] = new Option("15", "15");
}
}
function loadRateList()
{
document.forms[0].wlRate.options.length = 0;
var mode = getValue('wlgnMode');
if (band == "2")
{ 
if (mode == "b")
{ 
document.forms[0].wlRate[0] = new Option("Auto", "Auto");
document.forms[0].wlRate[1] = new Option("1 Mbps", "1");
document.forms[0].wlRate[2] = new Option("2 Mbps", "2");
document.forms[0].wlRate[3] = new Option("5.5 Mbps", "5.5");
document.forms[0].wlRate[4] = new Option("11 Mbps", "11");
}
else if (mode == 'b/g/n')
{
document.forms[0].wlRate[0] = new Option("Auto", "Auto");
document.forms[0].wlRate[1] = new Option("MCS0", "6.5");
document.forms[0].wlRate[2] = new Option("MCS1", "13");
document.forms[0].wlRate[3] = new Option("MCS2", "19.5");
document.forms[0].wlRate[4] = new Option("MCS3", "26");
document.forms[0].wlRate[5] = new Option("MCS4", "39");
document.forms[0].wlRate[6] = new Option("MCS5", "52");
document.forms[0].wlRate[7] = new Option("MCS6", "58.5");
document.forms[0].wlRate[8] = new Option("MCS7", "65");
}
else if (mode == 'n')
{
document.forms[0].wlRate[0] = new Option("Auto", "Auto");
document.forms[0].wlRate[1] = new Option("MCS0", "6.5");
document.forms[0].wlRate[2] = new Option("MCS1", "13");
document.forms[0].wlRate[3] = new Option("MCS2", "19.5");
document.forms[0].wlRate[4] = new Option("MCS3", "26");
document.forms[0].wlRate[5] = new Option("MCS4", "39");
document.forms[0].wlRate[6] = new Option("MCS5", "52");
document.forms[0].wlRate[7] = new Option("MCS6", "58.5");
document.forms[0].wlRate[8] = new Option("MCS7", "65");
}else if(mode == 'g')
{
document.forms[0].wlRate[0] = new Option("Auto", "Auto");
document.forms[0].wlRate[1] = new Option("6 Mbps", "6");
document.forms[0].wlRate[2] = new Option("9 Mbps", "9");
document.forms[0].wlRate[3] = new Option("12 Mbps", "12");
document.forms[0].wlRate[4] = new Option("18 Mbps", "18");
document.forms[0].wlRate[5] = new Option("24 Mbps", "24");
document.forms[0].wlRate[6] = new Option("36 Mbps", "36");
document.forms[0].wlRate[7] = new Option("48 Mbps", "48");
document.forms[0].wlRate[8] = new Option("54 Mbps", "54");
}
else
{
document.forms[0].wlRate[0] = new Option("Auto", "Auto");
document.forms[0].wlRate[1] = new Option("1 Mbps", "1");
document.forms[0].wlRate[2] = new Option("2 Mbps", "2");
document.forms[0].wlRate[3] = new Option("5.5 Mbps", "5.5");
document.forms[0].wlRate[4] = new Option("6 Mbps", "6");
document.forms[0].wlRate[5] = new Option("9 Mbps", "9");
document.forms[0].wlRate[6] = new Option("11 Mbps", "11");
document.forms[0].wlRate[7] = new Option("12 Mbps", "12");
document.forms[0].wlRate[8] = new Option("18 Mbps", "18");
document.forms[0].wlRate[9] = new Option("24 Mbps", "24");
document.forms[0].wlRate[10] = new Option("36 Mbps", "36");
document.forms[0].wlRate[11] = new Option("48 Mbps", "48");
document.forms[0].wlRate[12] = new Option("54 Mbps", "54");
}
}
else if (band == "1")
{ 
document.forms[0].wlRate[0] = new Option("Auto", "Auto");
document.forms[0].wlRate[1] = new Option("6 Mbps", "6");
document.forms[0].wlRate[2] = new Option("9 Mbps", "9");
document.forms[0].wlRate[3] = new Option("12 Mbps", "12");
document.forms[0].wlRate[4] = new Option("18 Mbps", "18");
document.forms[0].wlRate[5] = new Option("24 Mbps", "24");
document.forms[0].wlRate[6] = new Option("36 Mbps", "36");
document.forms[0].wlRate[7] = new Option("48 Mbps", "48");
document.forms[0].wlRate[8] = new Option("54 Mbps", "54");
}
else
{
document.forms[0].wlRate[0] = new Option("Auto", "Auto");
}
}

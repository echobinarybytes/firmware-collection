<script language="JavaScript" type="text/javascript">
var EnableCWMP = <%webGetParaByDomainName(InternetGatewayDevice.ManagementServer.EnableCWMP);%>;
var judgepassword = '<%webjudgeuserpassword();%>';
var judgeConnectionpassword = '<%webjudgeConnectionpassword();%>';
function stCWMP(domain,InformEnb,Interval,URL,Username,CntReqName,sslCertEnbl,CntReqPort)
{
this.domain = domain;
this.InformEnb = InformEnb;
this.Interval = Interval;
this.URL = URL;
this.Username = Username;
if (judgepassword == 1)
{
this.Password = '';
}
else
{
this.Password = '@1GV)Z<!';
}
this.CntReqName = CntReqName;
if (judgeConnectionpassword == 1)
{
this.CntReqPwd = '';
}
else
{
this.CntReqPwd = '@1GV)Z<!';
}
this.sslCertEnbl = sslCertEnbl;
this.CntReqPort = CntReqPort;
}
var temp = <%webGetCfgEntry(InternetGatewayDevice.ManagementServer,PeriodicInformEnable|PeriodicInformInterval|URL|Username|ConnectionRequestUsername|X_SSLCertEnable|X_ConnReqPort,stCWMP);%>;
var PeriodicInformEnableAcc = '<%webGetAccList(InternetGatewayDevice.ManagementServer.PeriodicInformEnable);%>';
var PeriodicInformIntervalAcc = '<%webGetAccList(InternetGatewayDevice.ManagementServer.PeriodicInformInterval);%>';
var URLAcc = '<%webGetAccList(InternetGatewayDevice.ManagementServer.URL);%>';
var UsernameAcc = '<%webGetAccList(InternetGatewayDevice.ManagementServer.Username);%>';
var PasswordAcc = '<%webGetAccList(InternetGatewayDevice.ManagementServer.Password);%>';
var ConnectionRequestUsernameAcc = '<%webGetAccList(InternetGatewayDevice.ManagementServer.ConnectionRequestUsername);%>';
var ConnectionRequestPasswordAcc = '<%webGetAccList(InternetGatewayDevice.ManagementServer.ConnectionRequestPassword);%>';
var X_SSLCertEnableAcc = '<%webGetAccList(InternetGatewayDevice.ManagementServer.X_SSLCertEnable);%>';
var cwmp = temp[0];
var host = <%webGetCfgArray(InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.IPInterface.1,IPInterfaceIPAddress|IPInterfaceSubnetMask);%>;
var AddrPrimary;
var MaskPrimary;
if (host.length > 1)
{
AddrPrimary = host[0][1];
MaskPrimary = host[0][2];
}
var X_ConnReqPortAcc = '<%webGetAccList(InternetGatewayDevice.ManagementServer.X_ConnReqPort);%>';
function stPortMap(domain,ExPort, ExPortEnd)
{
this.ExPort = ExPort;
this.ExPortEnd = ExPortEnd;
}
var WanIPPortMapping = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i}.PortMapping.{i},ExternalPort|ExternalPortEndRange,stPortMap);%>;
var WanPPPPortMapping = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i}.PortMapping.{i},ExternalPort|ExternalPortEndRange,stPortMap);%>;
var PortMapping = new Array();
for (i = 0; i < WanIPPortMapping.length-1; i++)
{
PortMapping[i] = WanIPPortMapping[i];
}
for (j = 0; j < WanPPPPortMapping.length-1; j++,i++)
{
PortMapping[i] = WanPPPPortMapping[j];
}
function LoadFrame()
{
var i = GetmenuName("tr069.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
setCheck('EnableCWMP', EnableCWMP);
setCheck('PeriodicInformEnable',cwmp.InformEnb);
setText('PeriodicInformInterval',cwmp.Interval);
setText('URL',cwmp.URL);
setText('Username',cwmp.Username);
setText('Password',cwmp.Password);
setText('ConnectionRequestUsername',cwmp.CntReqName);
setText('ConnectionRequestPassword',cwmp.CntReqPwd);
setText('X_ConnReqPort',cwmp.CntReqPort);
if(X_ConnReqPortAcc == "")
{
setDisable('X_ConnReqPort', 1);
}
setCheck('X_SSLCertEnable',cwmp.sslCertEnbl);
if(PeriodicInformEnableAcc == "")
{
setDisable('PeriodicInformEnable',1);
}
if(PeriodicInformIntervalAcc == "")
{
setDisable('PeriodicInformInterval',1);
}
if(URLAcc == "")
{
setDisable('URL',1);
}
if(UsernameAcc == "")
{
setDisable('Username',1);
}
if(PasswordAcc == "")
{
setDisable('Password',1);
}
if(ConnectionRequestUsernameAcc == "")
{
setDisable('ConnectionRequestUsername',1);
}
if(ConnectionRequestPasswordAcc == "")
{
setDisable('ConnectionRequestPassword',1);
}
if(X_SSLCertEnableAcc == "")
{
setDisable('X_SSLCertEnable',1);
}
}
function isSafeStringSN(val)
{
if ( val == "" )
{
return false;
}
for ( var j = 0 ; j < val.length ; j++ )
{
if ( !isSafeCharSN(val.charAt(j)) )
{
return false;
}
}
return true;
}
function isSafeCharSN(val)
{
if (((val >= 'A')&&(val <= 'Z'))
||((val >= 'a')&&(val <= 'z'))
||((val >= '0')&&(val <= '9'))
||(val == ':')||(val == '/')
||(val == '-')||(val == '.')||(val == '_'))
{
return true;
}
else
{
return false;
}
}
function isSafeStringSN(val)
{
if ( val == "" )
{
return false;
}
for ( var j = 0 ; j < val.length ; j++ )
{
if ( !isSafeCharSN(val.charAt(j)) )
{
return false;
}
}
return true;
}
function CheckForm(type)
{
with (getElement ("ConfigForm"))
{
if (getCheckVal("EnableCWMP") == 0)
{
if(confirm("Do you really want to disable the Remote Management function? This function allows modem to remote firmware upgrade by operator."))
{
return true;
}
else
{
return false;
}
}
if (URL.value == '')
{
AddErrInfo(eval("gErrStr0"), 'URL');
return false;
}
if (!isSafeStringSN(URL.value))
{
AddErrInfo(eval("gErrStr1"), 'URL');
return false;
}
if(URL.value.indexOf("://") >= 0)
{
var urlParts = URL.value.split("://");
if(urlParts[0] != "http" && urlParts[0] != "https")
{
AddErrInfo(eval("gErrStr15"), 'URL');
return false;
}
}
else
{
AddErrInfo(eval("gErrStr15"), 'URL');
return false;
}
var ipdomain = "";
if (urlParts.length > 1)
{
ipdomain = urlParts[1].split("/")[0];
}
else
{
ipdomain = urlParts[0].split("/")[0];
}
var ipdomainParts = ipdomain.split(":");
ipdomain = ipdomainParts[0];
if (ipdomain == "")
{
AddErrInfo(eval("gErrStr14"), 'URL');
return false;
}
if (!isValidIPDomain(ipdomain, 64))
{
AddErrInfo(eval("gErrStr14"), 'URL');
return false;
}
if (isIpFormat(ipdomain) == true)
{
if (isAbcIpAddress(ipdomain) == true)
{
if (isInSameSubnet(ipdomain, AddrPrimary, MaskPrimary) == true)
{
AddErrInfo(eval("gErrStr14"), 'URL');
return false;
}
}
}
if (getCheckVal("PeriodicInformEnable") == 1 &&PeriodicInformInterval.value =='' )
{
AddErrInfo(eval("gErrStr2"), 'PeriodicInformInterval');
return false;
}
if ((PeriodicInformInterval.value == '') || (isPlusInteger(PeriodicInformInterval.value) == false))
{
AddErrInfo(eval("gErrStr3"), 'PeriodicInformInterval');
return false;
}
var info = parseFloat(PeriodicInformInterval.value );
if (info <= 0 || info > 2147483647)
{
AddErrInfo(eval("gErrStr4"), 'PeriodicInformInterval');
return false;
}
if (isValidString(Username.value) == false )
{
AddErrInfo(eval("gErrStr5"), 'Username');
return false;
}
if (isValidString(Password.value) == false )
{
AddErrInfo(eval("gErrStr6"), 'Password');
return false;
}
if (isValidString(ConnectionRequestUsername.value) == false )
{
AddErrInfo(eval("gErrStr7"), 'ConnectionRequestUsername');
return false;
}
if (isValidString(ConnectionRequestPassword.value) == false )
{
AddErrInfo(eval("gErrStr8"), 'ConnectionRequestPassword');
return false;
}
if ((X_ConnReqPort.value < 1)||(X_ConnReqPort.value > 65535))
{
AddErrInfo(eval("gErrStr11"), 'X_ConnReqPort');
return false;
}
for(i = 0; i < PortMapping.length; i++)
{
if((parseInt(PortMapping[i].ExPort) <= parseInt(X_ConnReqPort.value)) && (parseInt(PortMapping[i].ExPortEnd) >= parseInt(X_ConnReqPort.value)))
{
AddErrInfo(eval("gErrStr12"), 'X_ConnReqPort');
return false;
}
}
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
SubmitForm.addForm('ConfigForm','x');
var Password = document.getElementById('Password');
var ConnectionRequestPassword = document.getElementById('ConnectionRequestPassword');
SubmitForm.delParameter('x.Password');
SubmitForm.delParameter('x.ConnectionRequestPassword');
if (Password.value != '@1GV)Z<!')
{
SubmitForm.addParameter('x.Password', getValue('Password'));
}
if (ConnectionRequestPassword.value != '@1GV)Z<!')
{
SubmitForm.addParameter('x.ConnectionRequestPassword', getValue('ConnectionRequestPassword'));
}
SubmitForm.setAction('setcfg.cgi?x=InternetGatewayDevice.ManagementServer&'
+ 'RequestFile=/html/network/tr069.asp');
setDisable('btnApply',1);
}
function CheckSslForm()
{
with (getElement ("SslForm"))
{
if(cwmpcert.value.length == 0)
{
AddErrInfo(eval("gErrStr13"), '');
setDisplay('erroinfo', 1);
setErrInfo();
return false;
}
}
return true;
}
function SubmitCert()
{
if ('' != top.errStrStr)
{
ClearErro();
}
top.errStrStr = '';
top.errIdCtrlStr = '';
if (CheckSslForm() == false)
{
return false;
}
var SslForm =getElement('SslForm');
setDisable('btnUpload',1);
SslForm.submit();
top.previousPage = '/html/network/tr069.asp';
}

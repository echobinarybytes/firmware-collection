WlanSrcMacAddressAcc[3] = <%webGetCfgEntryAcc(InternetGatewayDevice.LANDevice.1.WLANConfiguration.4.X_WlanMacFilter.{i},X_WlanSrcMacAddress, stWlanMacflt);%>;
function stWlanMacflt(Domain,MacAddresss)
{
this.Domain = Domain;
this.MacAddresss = MacAddresss;
}
function clickRemove()
{
var ssidindex = getSelectVal('wlSsidName');
ssidIdx = parseInt(ssidindex,10);
var rmname = 'rm' + (ssidIdx+1);
var rmselect = getElement(rmname);
if (rmselect == null)
{
alert(eval("gVarAlertNot_Add"));
return;
}
var SubmitForm = new webSubmitForm();
var cnt = 0;
with (document.forms[0])
{
if (rmselect.length > 0)
{
for (var i = 0; i < rmselect.length; i++)
{
if (rmselect[i].checked == true)
{
SubmitForm.addParameter(rmselect[i].value,'');
cnt++;
}
}
}
else if (rmselect.checked == true)
{
SubmitForm.addParameter(rmselect.value,'');
cnt++;
}
if(cnt==0)
{
alert(eval("gVarAlertNot_Select"));
return;
}
}
SubmitForm.setAction('del.cgi?' + 'RequestFile=/html/ntwkall/wlmacflt.asp');
SubmitForm.submit();
}
function LoadFrame()
{
var i = GetmenuName("wlmacflt.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
hideAllElements();
setCheck('isFilter',enableFilter);
setDisplay('wlMacFliter',enableFilter);
setRadio('FilterMode',Mode[0]);
displaySelectElements(0);
if(enableFilterAcc == "")
{
setDisable( "isFilter", 1);
}
if(ModeAcc[0] == "")
{
setDisable("FilterMode1", 1);
setDisable("FilterMode2", 1);
}
}
function hideAllElements()
{
for (var i = 0; i < 4; i++)
{
for(var j = 0; j < WlanSrcMacAddress[i].length - 1; j++)
{
var recordname = 'record_' + (i * 16 + j);
setDisplay(recordname, 0);
}
}
}
function displaySelectElements(val)
{
for(var i = 0; i < WlanSrcMacAddress[val].length - 1; i++)
{
var recordname = 'record_' + (val * 16 + i);
setDisplay(recordname, 1);
}
if(WlanSrcMacAddress[val].length == 1)
{
setControl(-1);
}
else
{
var selectname = 'record_' + (val * 16);
selectLine(selectname);
}
}
function ChangeMode()
{
var Form = new webSubmitForm();
var FilterMode = getElement("FilterMode");
if ('' != top.errStrStr)
{
ClearErro();
}
var ssidindex = getSelectVal('wlSsidName');
ssidIdx = parseInt(ssidindex,10);
if (FilterMode[0].checked == true)
{
if (confirm(eval("gVarBlackList")))
{
Form.addParameter('InternetGatewayDevice.LANDevice.1.WLANConfiguration.' + (ssidIdx + 1) + '.X_WlanMacFilterpolicy',0);
}
else
{
FilterMode[0].checked = false;
FilterMode[1].checked = true;
return;
}
}
else if (FilterMode[1].checked == true)
{
if (confirm(eval("gVarWhiteList")))
{
Form.addParameter('InternetGatewayDevice.LANDevice.1.WLANConfiguration.' + (ssidIdx + 1) + '.X_WlanMacFilterpolicy',1);
}
else
{
FilterMode[0].checked = true;
FilterMode[1].checked = false;
return;
}
}
Form.setAction('setcfg.cgi?RequestFile=/html/ntwkall/wlmacflt.asp');
Form.submit();
}
function SubmitForm()
{
var Form = new webSubmitForm();
if ('' != top.errStrStr)
{
alert("clear err");
ClearErro();
}
Form.addParameter('InternetGatewayDevice.LANDevice.1.WLANConfiguration.1.MACAddressControlEnabled',getCheckVal('isFilter'));
var i = 1;
for (i = 1; i < 5; i++)
{
Form.addParameter('InternetGatewayDevice.LANDevice.1.WLANConfiguration.' + i + '.X_WlanMacFilterpolicy', Mode[i - 1]);
}
Form.setAction('setcfg.cgi?RequestFile=/html/ntwkall/wlmacflt.asp');
setDisable('isFilter',1);
Form.submit();
}
function CheckForm(type)
{
var ssidindex = getSelectVal('wlSsidName');
ssidIdx = parseInt(ssidindex,10);
with (getElement('ConfigForm'))
{
if (isValidMacAddress(X_WlanSrcMacAddress.value) == false )
{
AddErrInfo(eval("gErrStr2") + getSafeString(X_WlanSrcMacAddress.value) + eval("gErrStr3"), 'X_WlanSrcMacAddress');
return false;
}
if(WlanSrcMacAddress[ssidIdx].length >= 17)
{
if(selctIndex == -1)
{
AddErrInfo(eval("gErrStr4"), '');
return false;
}
}
for(var i = 0; i < WlanSrcMacAddress[ssidIdx].length-1; i++)
{
if(selctIndex == i)
{
continue;
}
if((getValue('X_WlanSrcMacAddress').toUpperCase()) == WlanSrcMacAddress[ssidIdx][i].MacAddresss)
{
AddErrInfo(eval("gErrStr5"), 'X_WlanSrcMacAddress');
return false;
}
}
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
var ssidindex = getSelectVal('wlSsidName');
ssidIdx = parseInt(ssidindex,10);
SubmitForm.addParameter('x.X_WlanSrcMacAddress',
getValue('X_WlanSrcMacAddress').toUpperCase());
if( selctIndex == -1 )
{
SubmitForm.setAction('addcfg.cgi?x=InternetGatewayDevice.LANDevice.1.WLANConfiguration.' + (ssidIdx+1) + '.X_WlanMacFilter'
+ '&RequestFile=/html/ntwkall/wlmacflt.asp');
setDisable('btnApply',1);
}
else
{
SubmitForm.setAction('setcfg.cgi?x=' + WlanSrcMacAddress[ssidIdx][selctIndex].Domain
+ '&RequestFile=/html/ntwkall/wlmacflt.asp');
setDisable('btnApply',1);
}
}
function Changessid()
{
var i = GetmenuName("wlmacflt.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
var ssidindex = getSelectVal('wlSsidName');
ssidIdx = parseInt(ssidindex,10);
hideAllElements();
setCheck('isFilter',enableFilter);
setDisplay('wlMacFliter',enableFilter);
setRadio('FilterMode',Mode[ssidIdx]);
displaySelectElements(ssidIdx);
}
function setControl(index)
{
var ssidindex = getSelectVal('wlSsidName');
ssidIdx = parseInt(ssidindex,10);
if (index == -1)
{
selctIndex = index;
setText('X_WlanSrcMacAddress','');
setDisable('X_WlanSrcMacAddress', 0);
}
else
{
selctIndex = (index%16);
setText('X_WlanSrcMacAddress',WlanSrcMacAddress[ssidIdx][selctIndex].MacAddresss);
setDisable('X_WlanSrcMacAddress', TransAcc(WlanSrcMacAddressAcc[ssidIdx][selctIndex].MacAddresss));
}
}

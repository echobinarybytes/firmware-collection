<script language="JavaScript" type="text/javascript">
function WanWdConstruction(Domain, AccessType)
{
this.WdDomain = Domain;
var list = Domain.split('.');
this.WdInstId = list[2];
this.AccessType = AccessType;
}
var WD = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANCommonInterfaceConfig, WANAccessType, WanWdConstruction);%>;
function stWanIp(Domain, Name, Status, ConnType, IpAddress, Dns, SubnetMask, Gateway, Uptime
, xIpv4Enable, xIpv6Enable, xIpv6Status, xIpv6Addr, xIpv6PrefixLen, xIpv6Gateway
, xIpv6DynamicGateway
, xIpv6Dns, xIpv6PrefixList, xIPv6AddrType
)
{
this.Domain = Domain;
var list = Domain.split('.');
this.WdInstId = list[2];
this.Name = Name;
this.Status = Status;
this.ConnType = ConnType;
this.IpAddress = IpAddress;
this.SubnetMask = SubnetMask;
this.Gateway = Gateway;
this.Dns = Dns;
this.Uptime = Uptime;
if (this.IpAddress == '')
{
this.Dns = '';
}
this.xIpv4Enable = xIpv4Enable;
this.xIpv6Enable = xIpv6Enable;
this.xIpv6Status = xIpv6Status;
if ((xIpv6Addr != '') && (xIpv6PrefixLen != '0'))
{
this.xIpv6Addr = xIpv6Addr + '/' + xIpv6PrefixLen;
}
else
{
this.xIpv6Addr = xIpv6Addr;
}
this.xIpv6Gateway = xIpv6Gateway;
this.xIpv6DynamicGateway = xIpv6DynamicGateway;
if ((xIPv6AddrType == '6to4')
&& (this.xIpv6Gateway == '')
&& (this.xIpv6Status == 'Connected'))
{
this.xIpv6Gateway = '192.88.99.1';
}
this.xIpv6Dns = xIpv6Dns;
if (this.xIpv6Addr == '')
{
this.xIpv6Dns = '';
}
var xIpv6DnsServer = this.xIpv6Dns.split(',');
this.xIpv6Dns1 = '';
this.xIpv6Dns2 = '';
if (typeof(xIpv6DnsServer[0]) != 'undefined')
{
this.xIpv6Dns1 = xIpv6DnsServer[0];
}
if (typeof(xIpv6DnsServer[1]) != 'undefined')
{
this.xIpv6Dns2 = xIpv6DnsServer[1];
}
this.xIpv6PrefixList = xIpv6PrefixList;
}
var WanIp = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i}, Name|ConnectionStatus|ConnectionType|ExternalIPAddress|DNSServers|SubnetMask|DefaultGateway|Uptime
|X_IPv4Enable|X_IPv6Enable|X_IPv6ConnectionStatus|X_IPv6Address|X_IPv6PrefixLength|X_IPv6DefaultGateway
|X_IPv6DynamicDefaultGateway
|X_IPv6DNSServers|X_IPv6PrefixList|X_IPv6AddressingType
, stWanIp);%>;
function stWanPPP(Domain, Name, Status, ConnType, IpAddress, Dns, RemoteIpAddress, Uptime
, xIpv4Enable, xIpv6Enable, xIpv6Status, xIpv6Addr, xIpv6PrefixLen, xIpv6Gateway
, xIpv6DynamicGateway
, xIpv6Dns, xIpv6PrefixList, xIPv6AddrType
)
{
this.Domain = Domain;
var list = Domain.split('.');
this.WdInstId = list[2];
this.Name = Name;
this.Status = Status;
this.IpAddress = IpAddress;
if (IpAddress != '')
{
this.SubnetMask = '255.255.255.255';
}
else
{
this.SubnetMask = '';
}
this.Dns = Dns;
this.Gateway = RemoteIpAddress;
this.Uptime = Uptime;
if (this.IpAddress == '')
{
this.Dns = '';
}
this.xIpv4Enable = xIpv4Enable;
this.xIpv6Enable = xIpv6Enable;
this.xIpv6Status = xIpv6Status;
if ((xIpv6Addr != '') && (xIpv6PrefixLen != '0'))
{
this.xIpv6Addr = xIpv6Addr + '/' + xIpv6PrefixLen;
}
else
{
this.xIpv6Addr = xIpv6Addr;
}
this.xIpv6Gateway = xIpv6Gateway;
this.xIpv6DynamicGateway = xIpv6DynamicGateway;
if ((xIPv6AddrType == '6to4')
&& (this.xIpv6Gateway == '')
&& (this.xIpv6Status == 'Connected'))
{
this.xIpv6Gateway = '192.88.99.1';
}
this.xIpv6Dns = xIpv6Dns;
if (this.xIpv6Addr == '')
{
this.xIpv6Dns = '';
}
var xIpv6DnsServer = this.xIpv6Dns.split(',');
this.xIpv6Dns1 = '';
this.xIpv6Dns2 = '';
if (typeof(xIpv6DnsServer[0]) != 'undefined')
{
this.xIpv6Dns1 = xIpv6DnsServer[0];
}
if (typeof(xIpv6DnsServer[1]) != 'undefined')
{
this.xIpv6Dns2 = xIpv6DnsServer[1];
}
this.xIpv6PrefixList = xIpv6PrefixList;
}
var WanPPP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|ConnectionStatus|ConnectionType|ExternalIPAddress|DNSServers|RemoteIPAddress|Uptime
|X_IPv4Enable|X_IPv6Enable|X_IPv6ConnectionStatus|X_IPv6Address|X_IPv6PrefixLength|X_IPv6DefaultGateway
|X_IPv6DynamicDefaultGateway
|X_IPv6DNSServers|X_IPv6PrefixList|X_IPv6AddressingType
, stWanPPP);%>;
var Wan = Array();
for (i = 0; i < WanIp.length-1; i++)
{
Wan[i] = WanIp[i];
}
for (j = 0; j < WanPPP.length-1; j++,i++)
{
Wan[i] = WanPPP[j];
}
function GetAccessType(WdInstId)
{
for (var i = 0; i < WD.length - 1; i++)
{
if (WD[i].WdInstId == WdInstId)
{
return WD[i].AccessType;
}
}
return '';
}
function AutoRefresh()
{
window.location.replace("internetstatus.asp");
}
function LoadFrame()
{
var i = GetmenuName("internetstatus.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
setTimeout("AutoRefresh()", 10000);
}
function CheckForm(type)
{
with (getElement ("ConfigForm"))
{
}
return true;
}
function FormatTime(timeVal)
{
var timeString = "";
var UpDay =  parseInt(eval(timeVal/86400));
var UpHour = parseInt(eval(timeVal%86400/3600));
var UpMin = parseInt(eval(timeVal%3600/60));
var UpSec = eval(timeVal%60);
UpHour += UpDay*24;
if (UpHour >=0 && UpHour<=9 )
UpHour = '0' + UpHour;
if (UpMin >=0 && UpMin<=9 )
UpMin = '0' + UpMin;
if (UpSec >=0 && UpSec<=9 )
UpSec = '0' + UpSec;
timeString = UpHour + ':' + UpMin + ':' + UpSec;
return timeString;
}

<script language="JavaScript" type="text/javascript">
var SelctIndex = -1;
var PageData = null;
var WAN_NUM_MAX = 8;
var WAN_COMMON_CONN_NUM = 1;
var WAN_PPPOE_CONN_NUM = 1;
var WAN_CONN_TYPE = '';
function WanWcdConstruction(Domain, IpNum, PppNum, Name,
atmPvc, atmQoS, atmPeakCellRate, LinkType, atmSustainedCellRate, atmMaxBurstSize, encapMode, vlanIDMark, prioIDMark
)
{
this.WcdDomain = Domain;
var list = Domain.split('.');
this.WcdName = Name;
this.IpNum = IpNum;
this.PppNum = PppNum;
this.atmPvc = atmPvc;
if(atmPvc.charAt(0) == 'P')
{
atmPvc = atmPvc.substr(4);
}
var pvc = atmPvc.split('/');
this.atmVpi = parseInt(pvc[0],10).toString();
this.atmVci = parseInt(pvc[1],10).toString();
this.LinkType = LinkType;
this.atmServiceCategory = atmQoS;
this.atmPeakCellRate = atmPeakCellRate;
this.atmSustainedCellRate = atmSustainedCellRate;
this.atmMaxBurstSize = atmMaxBurstSize;
this.encapMode = encapMode;
this.vlanIDMark = vlanIDMark;
this.prioIDMark = prioIDMark;
}
function WanIPConstruction(Domain, Enable, Status, ConnType, Name, NatEnabled, AddrType,IpAddress, DefaultGateway, Dns,
ConnTrigger,
xBindList,
xServiceList, SubnetMask, xDhcpRelay,
xIpv4Enable, xIpv6Enable,
xTunnelType,
xTunnelEndpoint,
xIpv6Status, xIpv6AddrType, xIpv6Addr, xIpv6PrefixLen, xIpv6Gateway, xIpv6Dns,
MaxMTUSize, TCPMSS, MACAddress, MACAddressOverride, DHCPOption60
)
{
this.index = -1;
this.Cmo = 'WANIPConnection';
this.Domain = Domain;
if (Domain != '')
{
var list = Domain.split('.');
this.WcdDomain = list[0] + '.' + list[1] + '.' + list[2] + '.' + list[3] + '.' + list[4];
}
else
{
this.WcdDomain = '';
}
this.ManualFlag = 0;
this.Enable = Enable;
this.Status = Status;
this.ConnType = ConnType;
this.BridgeType = "";
if ((ConnType == 'IP_Bridged') || (ConnType == 'PPPoE_Bridged'))
{
this.BridgeType = "Bridge";
}
this.Name = Name;
this.NatEnabled = NatEnabled;
this.AddrType = AddrType;
this.IpAddress = IpAddress;
this.DefaultGateway = DefaultGateway;
var DnsServer = Dns.split(',');
this.dnsPrimary = DnsServer[0];
if (DnsServer.length > 1)
{
this.dnsSecondary = DnsServer[1];
}
else
{
this.dnsSecondary = '';
}
this.ConnTrigger = ConnTrigger;
this.xBindList = xBindList;
this.xServiceList = xServiceList;
this.SubnetMask = SubnetMask;
this.xDhcpRelay = xDhcpRelay;
this.Auth = '';
this.UserName = '';
this.Password = '';
this.IdleTime = 0;
this.xIpv4Enable = xIpv4Enable;
this.xIpv6Enable = xIpv6Enable;
this.xTunnelType = xTunnelType;
this.xTunnelEndpoint = xTunnelEndpoint;
this.xIpv6Status = xIpv6Status;
this.xIpv6AddrType = xIpv6AddrType;
this.xIpv6Addr = xIpv6Addr;
this.xIpv6PrefixLen = xIpv6PrefixLen;
if (this.xIpv6PrefixLen == '0')
{
this.xIpv6PrefixLen = '';
}
this.xIpv6Gateway = xIpv6Gateway;
if (typeof(xIpv6Dns) == 'undefined')
{
xIpv6Dns = '';
}
var Ipv6DnsServer = xIpv6Dns.split(',');
this.xIpv6Dns1 = Ipv6DnsServer[0];
if (Ipv6DnsServer.length > 1)
{
this.xIpv6Dns2 = Ipv6DnsServer[1];
}
else
{
this.xIpv6Dns2 = '';
}
this.LinkType = GetLinkType(this.WcdDomain);
if(this.LinkType == 'IPoA')
{
this.ConnType = 'IPoA';
}
this.LinkName = '';
this.MaxMTUSize = MaxMTUSize;
this.TCPMSS = TCPMSS;
this.MACAddress = MACAddress;
this.MACAddressOverride = MACAddressOverride;
this.DHCPOption60 = DHCPOption60;
}
function GetLinkType(WcdDomain)
{
for (var i = 0; i < WCD.length - 1; i++)
{
if (WcdDomain == WCD[i].WcdDomain)
{
if (WCD[i].WcdName.indexOf('ETH') >= 0)
{
return 'EoA';
}
else
{
return WCD[i].LinkType;
}
}
}
return 'EoA';
}
function WanPPPConstruction(Domain, Enable, Status, ConnType, Name, IdleTime, NatEnabled, UserName, PasswordIsNull, Auth
, IpAddress, DefaultGateway, Dns
, ConnTrigger
, xBindList
, xServiceList, xDhcpRelay
, MaxMRUSize, MaxMTUSize, TCPMSS, MACAddress, MACAddressOverride
, xIpv4Enable, xIpv6Enable, xIpv6Status, xIpv6AddrType, xIpv6Addr, xIpv6PrefixLen, xIpv6Gateway, xIpv6Dns
, xTunnelType
, xTunnelEndpoint
)
{
this.index = -1;
this.Cmo = 'WANPPPConnection';
this.Domain = Domain;
if (Domain != '')
{
var list = Domain.split('.');
this.WcdDomain = list[0] + '.' + list[1] + '.' + list[2] + '.' + list[3] + '.' + list[4];
}
else
{
this.WcdDomain = '';
}
this.ManualFlag = 0;
this.Enable = Enable;
this.Status = Status;
this.ConnType = ConnType;
if ('IP_Routed' == ConnType)
{
this.ConnType = 'PPP_Routed';
if (GetLinkType(this.WcdDomain) == "PPPoA")
{
this.ConnType = 'PPPoA';
}
}
this.BridgeType = "";
if ((ConnType == 'IP_Bridged') || (ConnType == 'PPPoE_Bridged'))
{
this.BridgeType = "Bridge";
}
this.Name = Name;
this.NatEnabled = NatEnabled;
this.IpAddress = IpAddress;
this.DefaultGateway = DefaultGateway;
var DnsServer = Dns.split(',');
this.dnsPrimary = DnsServer[0];
if (DnsServer.length > 1)
{
this.dnsSecondary = DnsServer[1];
}
else
{
this.dnsSecondary = '';
}
this.ConnTrigger = ConnTrigger;
this.xBindList = xBindList;
this.xServiceList = xServiceList;
this.Auth = Auth;
this.UserName = UserName;
if (PasswordIsNull == 1)
{
this.Password = '';
}
else
{
this.Password = '@1GV)Z<!';
}
this.IdleTime = IdleTime;
this.xIpv4Enable = xIpv4Enable;
this.xIpv6Enable = xIpv6Enable;
this.xTunnelType = xTunnelType;
this.xTunnelEndpoint = xTunnelEndpoint;
this.xIpv6Status = xIpv6Status;
this.xIpv6AddrType = xIpv6AddrType;
this.xIpv6Addr = xIpv6Addr;
this.xIpv6PrefixLen = xIpv6PrefixLen;
if (this.xIpv6PrefixLen == '0')
{
this.xIpv6PrefixLen = '';
}
this.xIpv6Gateway = xIpv6Gateway;
if (typeof(xIpv6Dns) == 'undefined')
{
xIpv6Dns = '';
}
var Ipv6DnsServer = xIpv6Dns.split(',');
this.xIpv6Dns1 = Ipv6DnsServer[0];
if (Ipv6DnsServer.length > 1)
{
this.xIpv6Dns2 = Ipv6DnsServer[1];
}
else
{
this.xIpv6Dns2 = '';
}
this.SubnetMask = '';
this.xDhcpRelay = xDhcpRelay;
this.LinkType = GetLinkType(this.WcdDomain);;
this.LinkName = '';
this.MaxMRUSize = MaxMRUSize;
this.MaxMTUSize = MaxMTUSize;
this.TCPMSS = TCPMSS;
this.MACAddress = MACAddress;
this.MACAddressOverride = MACAddressOverride;
this.DHCPOption60 = '';
}
function stDsl(domain, UpstreamCurrRate)
{
this.domain = domain;
this.UpstreamCurrRate = UpstreamCurrRate;
}
var DSLCfg = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.1.WANDSLInterfaceConfig,UpstreamCurrRate,stDsl);%>;
var WCD = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.1.WANConnectionDevice.{i}, WANIPConnectionNumberOfEntries|WANPPPConnectionNumberOfEntries|Name|WANDSLLinkConfig.DestinationAddress|WANDSLLinkConfig.ATMQoS|WANDSLLinkConfig.ATMPeakCellRate|WANDSLLinkConfig.LinkType|WANDSLLinkConfig.ATMSustainableCellRate|WANDSLLinkConfig.ATMMaximumBurstSize|WANDSLLinkConfig.ATMEncapsulation|WANDSLLinkConfig.X_VLANIDMark|WANDSLLinkConfig.X_802-1pMark, WanWcdConstruction);%>;
var WANIP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},
Enable|ConnectionStatus|ConnectionType|Name|NATEnabled|AddressingType|ExternalIPAddress|DefaultGateway|DNSServers
|ConnectionTrigger
|X_LanInterface
|X_ServiceList|SubnetMask|X_DHCPRelay
|X_IPv4Enable|X_IPv6Enable
|X_TunnelType
|X_TunnelEndpoint
|X_IPv6ConnectionStatus|X_IPv6AddressingType|X_IPv6Address|X_IPv6PrefixLength|X_IPv6DefaultGateway|X_IPv6DNSServers
|MaxMTUSize|X_TCP_MSS|MACAddress|MACAddressOverride|X_WANDHCPOption60, WanIPConstruction);%>;
var WANPPP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},
Enable|ConnectionStatus|ConnectionType|Name|IdleDisconnectTime|NATEnabled|Username|X_PasswordIsNull|PPPAuthenticationProtocol|ExternalIPAddress|DefaultGateway|DNSServers
|ConnectionTrigger
|X_LanInterface
|X_ServiceList|X_DHCPRelay
|MaxMRUSize|MaxMTUSize|X_TCP_MSS|MACAddress|MACAddressOverride
|X_IPv4Enable|X_IPv6Enable|X_IPv6ConnectionStatus|X_IPv6AddressingType|X_IPv6Address|X_IPv6PrefixLength|X_IPv6DefaultGateway|X_IPv6DNSServers
|X_TunnelType
|X_TunnelEndpoint
,WanPPPConstruction);%>;
var CloneMacAddr = "<%webGetMacAddr();%>";
var WAN = Array();
for (i = 0; i < WANIP.length-1; i++)
{
WAN[i] = WANIP[i];
WAN[i].index = i;
}
for (var j = 0; j < WANPPP.length-1; j++,i++)
{
WAN[i] = WANPPP[j];
WAN[i].index = i;
}
var upRate;
if (DSLCfg.length > 1)
{
upRate = DSLCfg[0].UpstreamCurrRate;
}
else
{
upRate = 0;
}
var pcrMax = 1024;
if (upRate != 0)
pcrMax = Math.ceil((upRate * 1024) / (53 * 8)) - 1;
function setCtlDisplay(record)
{
var InsId;
var Cmd;
var k;
for (k = 0; k < WCD.length - 1; k++)
{
if (record.WcdDomain == WCD[k].WcdDomain)
{
break;
}
}
if (k < WCD.length - 1)
{
setText('IdPVCVpi', WCD[k].atmVpi);
setText('IdPVCVci', WCD[k].atmVci);
setSelect('IDAtmServiceCategory', WCD[k].atmServiceCategory);
setText('IdAtmPeakCellRate', WCD[k].atmPeakCellRate);
setText('IdAtmSustainedCellRate', WCD[k].atmSustainedCellRate);
setText('IdAtmMaxBurstSize', WCD[k].atmMaxBurstSize);
setSelect('IDAtmEncapMode', WCD[k].encapMode);
if( WCD[k].vlanIDMark == '-1')
{
setCheck('IdVlanMarkEnable', 0);
setCheck('IdPrioMarkEnable', 0);
setDisplay('TrPrioMarkID',0);
setDisable('IdPrioMarkEnable',1);
}
else
{
setCheck('IdVlanMarkEnable', 1);
setText('IdVlanMarkID', WCD[k].vlanIDMark);
setDisable('IdPrioMarkEnable',0);
if( WCD[k].prioIDMark == '-1')
{
setCheck('IdPrioMarkEnable', 0);
setDisplay('TrPrioMarkID',0);
}
else
{
setCheck('IdPrioMarkEnable', 1);
setText('IdPrioMarkID', WCD[k].prioIDMark);
}
}
}
else
{
setText('IdPVCVpi', '');
setText('IdPVCVci', '');
setSelect('IDAtmServiceCategory', 'UBR');
setText('IdAtmPeakCellRate', '0');
setText('IdAtmSustainedCellRate', '0');
setText('IdAtmMaxBurstSize', '0');
setSelect('IDAtmEncapMode', 'LLC');
setCheck('IdVlanMarkEnable', 0);
setCheck('IdPrioMarkEnable', 0);
}
var BindArray = record.xBindList.split(',');
for (var i = 1; i <= 4; i++)
{
Cmd =  'setCheck(\'IdBindLan' + i + '\', 0)';
eval(Cmd);
Cmd =  'setCheck(\'IdBindWlan' + i + '\', 0)';
eval(Cmd);
}
for (var i = 0; i < BindArray.length; i++)
{
InsId = BindArray[i].substr(BindArray[i].lastIndexOf('.') + 1)
if (BindArray[i].indexOf('LANEthernetInterfaceConfig') > 0)
{
Cmd =  'setCheck(\'IdBindLan' + InsId + '\', 1)';
}
else if (BindArray[i].indexOf('WLANConfiguration') > 0)
{
Cmd =  'setCheck(\'IdBindWlan' + InsId + '\', 1)';
}
else
{
break;
}
eval(Cmd);
}
setSelect('IdINTERNET', record.xServiceList);
setCheck('IdEnable', record.Enable);
if (record.BridgeType == "")
{
setSelect('IdConnType', record.ConnType);
}
else
{
setSelect('IdConnType', record.BridgeType);
}
setSelect('IdNatEnabled', record.NatEnabled);
setSelect('IdAddrType', record.AddrType);
setCheck('IdDhcpRelay', record.xDhcpRelay);
setText('IdIpAddr', record.IpAddress);
setText('IdSubMask', record.SubnetMask);
setText('IdGateWay', record.DefaultGateway);
setText('IdPrimaryDns', record.dnsPrimary);
setText('IdSecondDns', record.dnsSecondary);
setText('IdDhcpOption60', record.DHCPOption60);
setSelect('IdAuth', record.Auth);
setSelect('IdConnTrigger', record.ConnTrigger);
if (record.ManualFlag == 0)
{
setDisable('IdPppDialButton', 0);
setDisable('IdPppDisconnButton', 0);
}
else
{
setDisable('IdPppDialButton', 1);
setDisable('IdPppDisconnButton', 1);
}
setText('IdIdleTime', record.IdleTime);
setText('IdPppUser', record.UserName);
setText('IdPppPasswd', record.Password);
setCheck('IdIpv4Enable', record.xIpv4Enable);
setCheck('IdIpv6Enable', record.xIpv6Enable);
setSelect('IdTunnelType', record.xTunnelType);
setText('IdTunnelEndpoint', record.xTunnelEndpoint);
setSelect('IdIpv6AddrType', record.xIpv6AddrType);
setText('IdIpv6Addr', record.xIpv6Addr);
setText('IdIpv6PrefixLen', record.xIpv6PrefixLen);
setText('IdIpv6Gateway', record.xIpv6Gateway);
setText('IdIpv6Dns1', record.xIpv6Dns1);
setText('IdIpv6Dns2', record.xIpv6Dns2);
if ((record.ConnType == 'PPP_Routed') || (record.ConnType == 'PPPoA'))
{
if ((1492 == record.MaxMTUSize) || (0 == record.MaxMTUSize))
{
setSelect('mtuMode', 'mtuAuto');
record.MaxMTUSize = 1492;
}
else
{
setSelect('mtuMode', 'mtuManual');
}
}
else if (record.ConnType == 'IP_Routed' || record.ConnType == 'IPoA')
{
if ((1500 == record.MaxMTUSize) || (0 == record.MaxMTUSize))
{
setSelect('mtuMode', 'mtuAuto');
record.MaxMTUSize = 1500;
}
else
{
setSelect('mtuMode', 'mtuManual');
}
}
setText('mtuSize', record.MaxMTUSize);
setText('TCPMSS', record.TCPMSS);
}
function mtuModeChangeDisplay()
{
var ConnType;
var MtuMode;
with ( document.forms[0])
{
ConnType = getSelectVal('IdConnType');
MtuMode = getSelectVal('mtuMode');
if ((ConnType == 'PPP_Routed') || (ConnType == 'PPPoA'))
{
if (MtuMode == 'mtuAuto')
{
setDisable("mtuSize", 1);
mtuSize.value = '1492';
}
else
{
setDisable("mtuSize", 0);
}
}
else if ((ConnType == 'IP_Routed') || (ConnType == 'IPoA'))
{
if (MtuMode == 'mtuAuto')
{
setDisable("mtuSize", 1);
mtuSize.value = '1500';
}
else
{
setDisable("mtuSize", 0);
}
}
}
}
function mtuModeChange()
{
var ConnType;
var MtuMode;
var MtuBak;
with ( document.forms[0])
{
ConnType = getSelectVal('IdConnType');
MtuMode = getSelectVal('mtuMode');
MtuBak = mtuSize.value;
if ((ConnType == 'PPP_Routed') || (ConnType == 'PPPoA'))
{
if (MtuMode == 'mtuAuto')
{
setDisable("mtuSize", 1);
mtuSize.value = 1492;
}
else
{
setDisable("mtuSize", 0);
mtuSize.value = MtuBak;
}
}
else if ((ConnType == 'IP_Routed') || (ConnType == 'IPoA'))
{
if (MtuMode == 'mtuAuto')
{
setDisable("mtuSize", 1);
mtuSize.value = 1500;
}
else
{
setDisable("mtuSize", 0);
mtuSize.value = MtuBak;
}
}
}
}
function setControl(index)
{
var record;
var Domain;
SelctIndex = index;
if (index == -1)
{
if (WAN.length > 0)
{
Domain = WAN[0].Domain;
}
else
{
Domain = '';
}
record = new WanPPPConstruction(Domain, '1', '', 'PPP_Routed', '', '300', '1',
'',
'1','AUTO', '', '', '',
'AlwaysOn'
, ''
, 'INTERNET','0'
, '1492', '1492', '', '', '0'
, '1', '0', '', 'SLAAC', '', '', '', ''
, ''
, ''
);
record.Password = '';
getElement('tdHead').innerHTML = eval("gVarPubConfigure");
setDisable('btnApply',0);
}
else
{
record = WAN[index];
getElement('tdHead').innerHTML = record.Name;
setDisable('btnApply',0);
}
WAN_CONN_TYPE = record.ConnType;
setCtlDisplay(record);
EnblVlanMarkChange();
EnblPrioMarkChange();
FinalDisplay();
}
function EnblVlanMarkChange()
{
with (document.forms[0])
{
var temp = getCheckVal("IdVlanMarkEnable");
if (temp == 0)
{
setDisplay('TrVlanMarkID',0);
setText('IdVlanMarkID',0);
setDisable('IdPrioMarkEnable',1);
setDisplay('TrPrioMarkID',0);
}
else
{
setDisplay('TrVlanMarkID',1);
setDisable('IdPrioMarkEnable',0);
}
}
}
function EnblPrioMarkChange()
{
with (document.forms[0])
{
var temp = getCheckVal("IdPrioMarkEnable");
if (temp == 0)
{
setDisplay('TrPrioMarkID',0);
}
else
{
setDisplay('TrPrioMarkID',1);
}
}
}
function atmServiceCategoryChange()
{
with (document.forms[0])
{
switch (getSelectVal('IDAtmServiceCategory'))
{
case "UBR":
setDisplay('TRAtmPeakCellRate',0);
setDisplay('TRAtmSustainedCellRate',0);
setDisplay('TRAtmMaxBurstSize',0);
break;
case "UBR+":
case "CBR":
setDisplay('TRAtmPeakCellRate',1);
setDisplay('TRAtmSustainedCellRate',0);
setDisplay('TRAtmMaxBurstSize',0);
break;
case "VBR-nrt":
case "VBR-rt":
setDisplay('TRAtmPeakCellRate',1);
setDisplay('TRAtmSustainedCellRate',1);
setDisplay('TRAtmMaxBurstSize',1);
break;
}
}
}
function LoadFrame()
{
var i = GetmenuName("wan.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
setControl(-1);
selectLine('record_0');
}
}
function GetBind(BindList, Id)
{
if (getCheckVal(Id) == 1)
{
if (BindList == '')
{
return  getValue(Id);
}
else
{
return  ',' + getValue(Id);
}
}
return '';
}
function ManualDialSubmit(Trigger)
{
with (document.forms[0])
{
setDisable('IdPppDialButton', 1);
setDisable('IdPppDisconnButton', 1);
}
if (SelctIndex != -1)
{
var xmlhttp = CreateXMLHttp();
xmlhttp.open('post','gmsgSendCmd.cgi?ModName=wan&SubType=1&RequestFile=/html/ntwkall/wan.asp', false);
xmlhttp.send('Trigger=' + Trigger + '&Path=' + WAN[SelctIndex].Domain);
WAN[SelctIndex].ManualFlag = 1;
}
setTimeout("location = '/html/ntwkall/wan.asp';",5000);
}
function ConnTriggerChanged()
{
with (getElement('Configure'))
{
if ((getSelectVal('IdConnType') == 'OnDemand')
&& (getValue('IdIdleTime') == '0'))
{
setText('IdIdleTime', '300');
}
}
FinalDisplay();
}
function ChangeConnType()
{
setText('IdPppPasswd', '');
FinalDisplay();
}
function FinalDisplay()
{
var ConnType;
var ConnTrigger;
var LinkType;
var WcdDomain;
var AddrType;
var itemRadio;
var k;
var PPPType;
with (getElement('Configure'))
{
ConnType = getSelectVal('IdConnType');
PPPType = getSelectVal('IdPPPType');
if ((ConnType == 'PPP_Routed')
|| (ConnType == 'DHCP_Spoofed')
|| (ConnType == 'PPPoA'))
{
setDisplay('TrAuth', 1);
setDisplay('TrConnTrigger', 1);
setDisplay('TrPppUser', 1);
setDisplay('TrPppPasswd', 1);
setDisplay('TrAddrType', 0);
setDisplay('TrIpAddr', 0);
setDisplay('TrSubMask', 0);
setDisplay('TrGateWay', 0);
setDisplay('TrPrimaryDns', 0);
setDisplay('TrSecondDns', 0);
setDisplay('TrDhcpRelay', 0);
setDisplay('TrDhcpOption60', 0);
LinkType = getSelectVal("IdConnType");
if (LinkType != "PPPoA")
{
LinkType = "EoA";
}
if (LinkType == 'PPP')
{
}
else if (LinkType == 'EoA')
{
}
else if (LinkType == 'PPPoA')
{
}
else
{
}
if ((ConnType == 'PPP_Routed') || (ConnType == 'PPPoA') || (ConnType == 'IP_Routed'))
{
setDisplay('TrMtuMode', 1);
setDisplay('TrTCPMSS', 1);
}
else
{
setDisplay('TrMtuMode', 0);
setDisplay('TrTCPMSS', 0);
}
if ((ConnType == 'PPP_Routed') || (ConnType == 'PPPoA'))
{
if((WAN_CONN_TYPE == 'IP_Bridged')||(WAN_CONN_TYPE == 'PPPoE_Bridged'))
{
setSelect('IdNatEnabled',1);
}
setDisplay('TrNatEnabled', 1);
}
else if (ConnType == 'DHCP_Spoofed')
{
setDisplay('TrNatEnabled', 0);
}
ConnTrigger = getSelectVal('IdConnTrigger');
if (ConnTrigger == 'AlwaysOn')
{
setDisplay('IdPppDialButton', 0);
setDisplay('IdPppDisconnButton', 0);
setDisplay('TrIdleTime', 0);
}
else if (ConnTrigger == 'OnDemand')
{
setDisplay('IdPppDialButton', 0);
setDisplay('IdPppDisconnButton', 0);
setDisplay('TrIdleTime', 1);
}
else if (ConnTrigger == 'Manual')
{
if (SelctIndex == -1)
{
setDisplay('IdPppDialButton', 0);
setDisplay('IdPppDisconnButton', 0);
}
else
{
if (WAN[SelctIndex].ConnTrigger != 'Manual')
{
setDisplay('IdPppDialButton', 0);
setDisplay('IdPppDisconnButton', 0);
}
else if (WAN[SelctIndex].Status == 'Connected')
{
setDisplay('IdPppDialButton', 0);
setDisplay('IdPppDisconnButton', 1);
}
else
{
setDisplay('IdPppDialButton', 1);
setDisplay('IdPppDisconnButton', 0);
}
}
setDisplay('TrIdleTime', 0);
}
if(getValue('TCPMSS')== '')
{
setValueDft('TCPMSS', '0');
}
}
else if (ConnType == 'IP_Routed' || ConnType == 'IPoA')
{
if((WAN_CONN_TYPE == 'IP_Bridged')||(WAN_CONN_TYPE == 'PPPoE_Bridged'))
{
setSelect('IdNatEnabled',1);
}
setDisplay('TrNatEnabled', 1);
setDisplay('TrAuth', 0);
setDisplay('TrConnTrigger', 0);
setDisplay('IdPppDialButton', 0);
setDisplay('IdPppDisconnButton', 0);
setDisplay('TrIdleTime', 0);
setDisplay('TrPppUser', 0);
setDisplay('TrPppPasswd', 0);
if(ConnType == 'IPoA')
{
setSelect('IdAddrType', 'Static');
setDisplay('TrAddrType', 0);
}
else
{
setDisplay('TrAddrType', 1);
}
AddrType = getSelectVal('IdAddrType');
if (AddrType == 'DHCP')
{
setDisplay('TrIpAddr', 0);
setDisplay('TrSubMask', 0);
setDisplay('TrGateWay', 0);
setDisplay('TrPrimaryDns', 0);
setDisplay('TrSecondDns', 0);
setDisplay('TrDhcpRelay', 0);
setDisplay('TrDhcpOption60', 1);
}
else if (AddrType == 'Static')
{
setDisplay('TrIpAddr', 1);
setDisplay('TrSubMask', 1);
setDisplay('TrGateWay', 1);
setDisplay('TrPrimaryDns', 1);
setDisplay('TrSecondDns', 1);
setDisplay('TrDhcpRelay', 0);
setDisplay('TrDhcpOption60', 0);
}
setDisplay('TrMtuMode', 1);
setDisplay('TrTCPMSS', 1);
if(getValue('TCPMSS')== '')
{
setValueDft('TCPMSS', '0');
}
}
else if (ConnType == 'Bridge')
{
setDisplay('TrNatEnabled', 0);
setDisplay('TrAuth', 0);
setDisplay('TrConnTrigger', 0);
setDisplay('IdPppDialButton', 0);
setDisplay('IdPppDisconnButton', 0);
setDisplay('TrIdleTime', 0);
setDisplay('TrPppUser', 0);
setDisplay('TrPppPasswd', 0);
setDisplay('TrAddrType', 0);
setDisplay('TrIpAddr', 0);
setDisplay('TrSubMask', 0);
setDisplay('TrGateWay', 0);
setDisplay('TrPrimaryDns', 0);
setDisplay('TrSecondDns', 0);
setDisplay('TrDhcpRelay', 1);
setDisplay('TrDhcpOption60', 0);
setDisplay('TrMtuMode', 0);
setDisplay('TrTCPMSS', 0);
}
}
Ipv6Display();
Ipv4Display();
mtuModeChangeDisplay()
atmServiceCategoryChange();
clickServiceListCheck();
}
function stPageData()
{
this.LinkType = "EoA";
this.Enable = getCheckVal('IdEnable');
this.ConnType = getSelectVal('IdConnType');
if (this.ConnType == "PPPoA")
{
this.LinkType = "PPPoA";
}
if (this.ConnType == "IPoA")
{
this.LinkType = "IPoA";
}
this.BridgeType = "";
if (this.ConnType == 'Bridge')
{
this.BridgeType = "Bridge";
if (SelctIndex == -1)
{
this.Cmo = 'WANPPPConnection';
}
else
{
if (WAN[SelctIndex].ConnType == 'IP_Bridged')
{
this.Cmo = 'WANIPConnection';
}
else
{
this.Cmo = 'WANPPPConnection';
}
}
}
else if ((this.ConnType == 'IP_Routed'))
{
this.Cmo = 'WANIPConnection';
}
else if (this.ConnType == 'IPoA')
{
this.Cmo = 'WANIPConnection';
}
else
{
this.Cmo = 'WANPPPConnection';
}
this.NatEnabled = getSelectVal('IdNatEnabled');
this.ConnTrigger = getSelectVal('IdConnTrigger');
this.AtmVpi = getValue('IdPVCVpi');
this.AtmVci = getValue('IdPVCVci');
this.AtmServiceCategory = getSelectVal('IDAtmServiceCategory');
this.AtmPeakCellRate = getValue('IdAtmPeakCellRate');
this.AtmSustainedCellRate = getValue('IdAtmSustainedCellRate');
this.AtmMaxBurstSize = getValue('IdAtmMaxBurstSize');
this.AtmEncapMode = getSelectVal('IDAtmEncapMode');
this.VlanMarkEnable = getCheckVal('IdVlanMarkEnable');
this.VlanMarkID = getValue('IdVlanMarkID');
this.PrioMarkEnable = getCheckVal('IdPrioMarkEnable');
this.PrioMarkID = getValue('IdPrioMarkID');
var BindList = '';
for (var i = 1; i <= 4; i++)
{
BindList = BindList + GetBind(BindList, 'IdBindLan' + i);
BindList = BindList + GetBind(BindList, 'IdBindWlan' + i);
}
this.xBindList = BindList;
var ServiceList = getSelectVal('IdINTERNET');
this.xServiceList = ServiceList;
this.AddrType = getSelectVal('IdAddrType');
this.xDhcpRelay = getCheckVal('IdDhcpRelay');
this.IpAddress = getValue('IdIpAddr');
this.SubnetMask = getValue('IdSubMask');
this.DefaultGateway = getValue('IdGateWay');
this.dnsPrimary = getValue('IdPrimaryDns');
this.dnsSecondary = getValue('IdSecondDns');
this.DhcpOption60 = getValue('IdDhcpOption60');
this.Auth = getSelectVal('IdAuth');
this.IdleTime = getValue('IdIdleTime');
this.UserName = getValue('IdPppUser');
this.Password = getValue('IdPppPasswd');
this.MaxMTUSize = getValue('mtuSize');
this.TCPMSS = getValue('TCPMSS');
if (TrIpv4Enable.style.display != "none")
{
this.xIpv4Enable = getCheckVal('IdIpv4Enable');
}
else
{
this.xIpv4Enable = 0;
}
if (TrIpv6Enable.style.display != "none")
{
this.xIpv6Enable = getCheckVal('IdIpv6Enable');
}
else
{
this.xIpv6Enable = 0;
}
this.xTunnelType = getSelectVal('IdTunnelType');
this.xTunnelEndpoint = getValue('IdTunnelEndpoint');
this.xIpv6AddrType = getSelectVal('IdIpv6AddrType');
this.xIpv6Addr = getValue('IdIpv6Addr');
this.xIpv6PrefixLen = getValue('IdIpv6PrefixLen');
this.xIpv6Gateway = getValue('IdIpv6Gateway');
this.xIpv6Dns1 = getValue('IdIpv6Dns1');
this.xIpv6Dns2 = getValue('IdIpv6Dns2');
}
function btnRemove()
{
var Form = new webSubmitForm();
with (document.forms[0])
{
}
Form.setAction('del.cgi?RequestFile=/html/ntwkall/wan.asp');
Form.submit();
setDisable('btnApply',1);
}
var stat = false;
function btnRemoveWanCnt()
{
if (stat == true)
return;
if(	WAN.length == 0)
{
AddErrInfo(eval("gVarAlertNot_Add"), '');
setDisplay('erroinfo', 1);
setErrInfo();
return;
}
if (SelctIndex == -1)
{
AddErrInfo(eval("gVarAlertNot_Select"), '');
setDisplay('erroinfo', 1);
setErrInfo();
return;
}
if (confirm(eval("gErrStr17")) == false)
return;
var Form = new webSubmitForm();
var domain = WAN[SelctIndex].WcdDomain;
var uloop = 0;
for(uloop = 0; uloop < WAN.length; uloop++)
{
if((WAN[uloop].WcdDomain == domain) && (uloop != SelctIndex))
{
domain = WAN[SelctIndex].Domain;
break;
}
}
Form.addParameter(domain, '');
Form.setAction('del.cgi?RequestFile=/html/ntwkall/wan.asp');
stat = true;
setDisable('btnApply1', 1);
Form.submit();
}
function clickRemove()
{
var cnt = 0;
btnRemoveWanCnt();
return ;
}
function clickServiceListCheck()
{
with (document.forms[0])
{
if (getSelectVal("IdINTERNET") == "TR069")
{
setDisplay("TrLanBindList", 0)
}
else
{
setDisplay("TrLanBindList", 1)
}
var ConnType = getSelectVal('IdConnType');
if (getSelectVal("IdINTERNET") == "OTHER")
{
setSelect('IdTunnelType', '');
setDisplay("TrTunnelType", 0);
setDisplay('TrTunnelEndpoint', 0);
}
else
{
if ((ConnType == 'PPP_Routed') || (ConnType == 'IP_Routed'))
{
setDisplay("TrTunnelType", 1);
TunnelDisplay();
}
}
}
}
function getcntwithsamewantypeINpvc(vpi, vci, type)
{
var cnt = 0;
for (j = 0; j < WCD.length - 1; j++)
{
if ((WCD[j].atmVpi == vpi) && (WCD[j].atmVci == vci))
{
for (i = 0; i < WAN.length; i++)
{
if (WAN[i].WcdDomain == WCD[j].WcdDomain)
{
if ((type == "Bridge") &&
(WAN[i].ConnType == "PPPoE_Bridged" || WAN[i].ConnType == "IP_Bridged"))
{
cnt++;
}
else if (type == "PPPoE")
{
if ((WAN[i].ConnType == "PPP_Routed") && (WAN[i].Cmo== 'WANPPPConnection'))
{
cnt++;
}
}
else if (type == "DHCP")
{
if ((WAN[i].ConnType == "IP_Routed") && (WAN[i].Cmo == 'WANIPConnection'))
{
cnt++;
}
}
else if (type == "IPoA")
{
if ((WAN[i].ConnType == "IPoA") && (WAN[i].Cmo == 'WANIPConnection'))
{
cnt++;
}
}
else if (type == "PPPoA")
{
if ((WAN[i].ConnType == "PPPoA") && (WAN[i].Cmo== 'WANPPPConnection'))
{
cnt++;
}
}
}
}
}
}
return cnt;
}
function canAddWan(vpi, vci, wanType)
{
var cntofwanwithsamewantype = 0;
var i;
if (wanType == "Bridge")
{
cntofwanwithsamewantype = getcntwithsamewantypeINpvc(vpi, vci, "Bridge");
if (cntofwanwithsamewantype >= 1)
{
alert(eval("gATMErrStr30"));
return false;
}
cntofwanwithsamewantype = getcntwithsamewantypeINpvc(vpi, vci, "PPPoA");
cntofwanwithsamewantype += getcntwithsamewantypeINpvc(vpi, vci, "IPoA");
if (cntofwanwithsamewantype >= 1)
{
alert(eval("gATMErrStr40"));
return false;
}
}
else if (wanType == "PPPoE")
{
cntofwanwithsamewantype = getcntwithsamewantypeINpvc(vpi, vci, "PPPoE");
if (cntofwanwithsamewantype >= 1)
{
alert(eval("gATMErrStr31"));
return false;
}
cntofwanwithsamewantype = getcntwithsamewantypeINpvc(vpi, vci, "DHCP");
cntofwanwithsamewantype += getcntwithsamewantypeINpvc(vpi, vci, "PPPoA");
cntofwanwithsamewantype += getcntwithsamewantypeINpvc(vpi, vci, "IPoA");
if (cntofwanwithsamewantype >= 1)
{
alert(eval("gATMErrStr32"));
return false;
}
}
else if (wanType == "PPPoA")
{
cntofwanwithsamewantype = getcntwithsamewantypeINpvc(vpi, vci, "PPPoA");
if (cntofwanwithsamewantype >= 1)
{
alert(eval("gATMErrStr38"));
return false;
}
cntofwanwithsamewantype = getcntwithsamewantypeINpvc(vpi, vci, "Bridge");
cntofwanwithsamewantype += getcntwithsamewantypeINpvc(vpi, vci, "PPPoE");
cntofwanwithsamewantype += getcntwithsamewantypeINpvc(vpi, vci, "DHCP");
cntofwanwithsamewantype += getcntwithsamewantypeINpvc(vpi, vci, "IPoA");
if (cntofwanwithsamewantype >= 1)
{
alert(eval("gATMErrStr35"));
return false;
}
}
else if (wanType == "IPoA")
{
cntofwanwithsamewantype = getcntwithsamewantypeINpvc(vpi, vci, "IPoA");
if (cntofwanwithsamewantype >= 1)
{
alert(eval("gErrStr71"));
return false;
}
cntofwanwithsamewantype = getcntwithsamewantypeINpvc(vpi, vci, "Bridge");
cntofwanwithsamewantype += getcntwithsamewantypeINpvc(vpi, vci, "PPPoE");
cntofwanwithsamewantype += getcntwithsamewantypeINpvc(vpi, vci, "DHCP");
cntofwanwithsamewantype += getcntwithsamewantypeINpvc(vpi, vci, "PPPoA");
if (cntofwanwithsamewantype >= 1)
{
alert(eval("gErrStr72"));
return false;
}
}
else 
{
cntofwanwithsamewantype = getcntwithsamewantypeINpvc(vpi, vci, "PPPoE");
cntofwanwithsamewantype += getcntwithsamewantypeINpvc(vpi, vci, "PPPoA");
cntofwanwithsamewantype += getcntwithsamewantypeINpvc(vpi, vci, "IPoA");
if (cntofwanwithsamewantype >= 1)
{
alert(eval("gATMErrStr33"));
return false;
}
cntofwanwithsamewantype = getcntwithsamewantypeINpvc(vpi, vci, "DHCP");
if (cntofwanwithsamewantype >= 1)
{
alert(eval("gATMErrStr34"));
return false;
}
}
return true;
}
function CheckWanCnt()
{
var cntofwanwithsamepvc = 0;
var i;
if (SelctIndex == -1) 
{
if (PageData.ConnType.indexOf("Bridge") >= 0)
{
if (canAddWan(PageData.AtmVpi, PageData.AtmVci, "Bridge") == false)
{
return false;
}
}
else if (PageData.ConnType== "PPP_Routed")
{
if (canAddWan(PageData.AtmVpi, PageData.AtmVci, "PPPoE") == false)
{
return false;
}
}
else if (PageData.ConnType== "IP_Routed")
{
if (canAddWan(PageData.AtmVpi, PageData.AtmVci, "DHCP") == false)
{
return false;
}
}
else if (PageData.ConnType== "DHCP_Spoofed")
{
}
else if (PageData.ConnType== "IPoA")
{
if (canAddWan(PageData.AtmVpi, PageData.AtmVci, "IPoA") == false)
{
return false;
}
}
else
{
if (canAddWan(PageData.AtmVpi, PageData.AtmVci, "PPPoA") == false)
{
return false;
}
}
}
else 
{
for (i = 0; i < WCD.length - 1; i++)
{
if (WCD[i].WcdDomain == WAN[SelctIndex].WcdDomain)
{
break;
}
}
var origvpi = WCD[i].atmVpi;
var origvci = WCD[i].atmVci;
if ((PageData.AtmVpi == origvpi) && (PageData.AtmVci == origvci)) 
{
if (PageData.ConnType.indexOf("Bridge") >= 0)
{
if (WAN[SelctIndex].ConnType == "IP_Routed")
{
cntofwanwithsamepvc = getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "Bridge");
if (cntofwanwithsamepvc >= 1)
{
alert(eval("gATMErrStr30"));
return false;
}
}
}
else if ((PageData.ConnType == "PPP_Routed") || (PageData.ConnType == "IP_Routed"))
{
if (WAN[SelctIndex].ConnType == "IP_Routed")
{
if ((PageData.ConnType == "IP_Routed") && (WAN[SelctIndex].Cmo == "WANPPPConnection"))
{
cntofwanwithsamepvc = getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "PPPoE");
cntofwanwithsamepvc--;
if (cntofwanwithsamepvc >= 1)
{
alert(eval("gATMErrStr33"));
return false;
}
}
}
else if (WAN[SelctIndex].ConnType.indexOf("Bridge") >= 0)
{
if (PageData.ConnType == "PPP_Routed")
{
cntofwanwithsamepvc = getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "PPPoE");
if (cntofwanwithsamepvc >= 1)
{
alert(eval("gATMErrStr31"));
return false;
}
cntofwanwithsamepvc = getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "DHCP");
if (cntofwanwithsamepvc >= 1)
{
alert(eval("gATMErrStr32"));
return false;
}
}
else
{
cntofwanwithsamepvc = getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "DHCP");
if (cntofwanwithsamepvc >= 1)
{
alert(eval("gATMErrStr34"));
return false;
}
cntofwanwithsamepvc = getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "PPPoE");
if (cntofwanwithsamepvc >= 1)
{
alert(eval("gATMErrStr33"));
return false;
}
}
}
}
else if (PageData.ConnType== "DHCP_Spoofed")
{
}
else if (PageData.ConnType== "PPPoA")
{
cntofwanwithsamepvc = getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "Bridge");
cntofwanwithsamepvc += getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "PPPoE");
cntofwanwithsamepvc += getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "DHCP");
cntofwanwithsamepvc += getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "PPPoA");
cntofwanwithsamepvc += getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "IPoA");
cntofwanwithsamepvc--;
if (cntofwanwithsamepvc >= 1)
{
alert(eval("gATMErrStr35"));
return false;
}
}
else
{
cntofwanwithsamepvc = getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "Bridge");
cntofwanwithsamepvc += getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "PPPoE");
cntofwanwithsamepvc += getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "DHCP");
cntofwanwithsamepvc += getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "PPPoA");
cntofwanwithsamepvc += getcntwithsamewantypeINpvc(PageData.AtmVpi, PageData.AtmVci, "IPoA");
cntofwanwithsamepvc--;
if (cntofwanwithsamepvc >= 1)
{
alert(eval("gErrStr72"));
return false;
}
}
}
else 
{
if (PageData.ConnType.indexOf("Bridge") >= 0)
{
if (canAddWan(PageData.AtmVpi, PageData.AtmVci, "Bridge") == false)
{
return false;
}
}
else if (PageData.ConnType== "PPP_Routed")
{
if (canAddWan(PageData.AtmVpi, PageData.AtmVci, "PPPoE") == false)
{
return false;
}
}
else if (PageData.ConnType== "IP_Routed")
{
if (canAddWan(PageData.AtmVpi, PageData.AtmVci, "DHCP") == false)
{
return false;
}
}
else if (PageData.ConnType== "DHCP_Spoofed")
{
}
else if (PageData.ConnType == "IPoA")
{
if (canAddWan(PageData.AtmVpi, PageData.AtmVci, "IPoA") == false)
{
return false;
}
}
else
{
if (canAddWan(PageData.AtmVpi, PageData.AtmVci, "PPPoA") == false)
{
return false;
}
}
}
}
return true;
}
function CheckForm(Form)
{
with (getElement('Configure'))
{
PageData = new stPageData();
}
var vip4 = document.getElementById("IdIpv4Enable");
var vip6 = document.getElementById("IdIpv6Enable");
if(vip4.checked== false && vip6.checked==false)
{
if (PageData.ConnType == 'Bridge')
{
setCheck('IdIpv4Enable', 1);
}
else
{
AddErrInfo(eval("gErrStr70 "), 'IdIpv4Enable');
}
}
{
var vpi = parseInt(PageData.AtmVpi, 10);
if ( vpi < 0 || vpi > 255 || isNaN(vpi))
{
AddErrInfo(eval("gATMErrStr6"), 'IdPVCVpi');
return false;
}
var vci = parseInt(PageData.AtmVci, 10);
if ( vci < 32 || vci > 65535 || isNaN(vci))
{
AddErrInfo(eval("gATMErrStr7"), 'IdPVCVci');
return false;
}
if( isPlusInteger(PageData.AtmVpi) == false )
{
AddErrInfo(eval("gATMErrStr8"), 'IdPVCVpi');
return false;
}
if( isPlusInteger(PageData.AtmVci) == false )
{
AddErrInfo(eval("gATMErrStr9"), 'IdPVCVci');
return false;
}
var vpivalue = PageData.AtmVpi;
if (vpivalue.length > 1 && vpivalue.charAt(0) == '0')
{
AddErrInfo(eval("gATMErrStr10"), 'IdPVCVpi');
return false;
}
var vcivalue = PageData.AtmVci;
if (vcivalue.length > 1 && vcivalue.charAt(0) == '0')
{
AddErrInfo(eval("gATMErrStr11"), 'IdPVCVci');
return false;
}
if (PageData.VlanMarkEnable == 1)
{
var v = PageData.VlanMarkID;
if(isPlusInteger(v) == false)
{
AddErrInfo(eval("gATMErrStr21"), 'IdVlanMarkID');
return false;
}
else
{
if ((v == "") || (v <= 0) || (v > 4094))
{
AddErrInfo(eval("gATMErrStr21"), 'IdVlanMarkID');
return false;
}
if (v.length > 1 && v.charAt(0) == '0')
{
AddErrInfo(eval("gATMErrStr21"), 'IdVlanMarkID');
return false;
}
}
if ( 1 == PageData.PrioMarkEnable)
{
var v = PageData.PrioMarkID;
if(isPlusInteger(v) == false)
{
AddErrInfo(eval("gATMErrStr22"), 'IdPrioMarkID');
return false;
}
else
{
if ((v == "") || (v < 0) || (v > 7))
{
AddErrInfo(eval("gATMErrStr22"), 'IdPrioMarkID');
return false;
}
if (v.length > 1 && v.charAt(0) == '0')
{
AddErrInfo(eval("gATMErrStr22"), 'IdPrioMarkID');
return false;
}
}
}
}
switch (PageData.AtmServiceCategory)
{
case "UBR+":
case "CBR":
if(isPlusInteger(PageData.AtmPeakCellRate) == false)
{
AddErrInfo(eval("gATMErrStr12"), 'IdAtmPeakCellRate');
return false;
}
else
{
var PeakCellRate = parseInt(PageData.AtmPeakCellRate, 10);
if (PeakCellRate <= 0 || PeakCellRate > pcrMax)
{
AddErrInfo(eval("gATMErrStr12"), 'IdAtmPeakCellRate');
return false;
}
}
break;
case "VBR-nrt":
case "VBR-rt":
if(isPlusInteger(PageData.AtmPeakCellRate) == false)
{
AddErrInfo(eval("gATMErrStr12"), 'IdAtmPeakCellRate');
return false;
}
else
{
var PeakCellRate = parseInt(PageData.AtmPeakCellRate, 10);
if (PeakCellRate <= 0 || PeakCellRate > pcrMax)
{
AddErrInfo(eval("gATMErrStr12"), 'IdAtmPeakCellRate');
return false;
}
}
if(isPlusInteger(PageData.AtmSustainedCellRate) == false)
{
AddErrInfo(eval("gATMErrStr13"), 'IdAtmSustainedCellRate');
return false;
}
else
{
var PeakCellRate = parseInt(PageData.AtmPeakCellRate, 10);
var SustainedCellRate = parseInt(PageData.AtmSustainedCellRate);
if (SustainedCellRate <= 0 || SustainedCellRate > pcrMax)
{
AddErrInfo(eval("gATMErrStr13"), 'IdAtmSustainedCellRate');
return false;
}
if (SustainedCellRate >= PeakCellRate)
{
AddErrInfo(eval("gATMErrStr15"), 'IdAtmSustainedCellRate');
return false;
}
}
if(isPlusInteger(PageData.AtmMaxBurstSize) == false)
{
AddErrInfo(eval("gATMErrStr16"), 'IdAtmMaxBurstSize');
return false;
}
else
{
var MaxBurstSize = parseInt(PageData.AtmMaxBurstSize, 10);
if (MaxBurstSize <= 0 || MaxBurstSize > 1000000)
{
AddErrInfo(eval("gATMErrStr16"), 'IdAtmMaxBurstSize');
return false;
}
}
break;
}
}
if (PageData.xBindList != '')
{
for (var i = 1; i <= 4; i++)
{
var Lan = 'IdBindLan' + i;
var Wlan = 'IdBindWlan' + i;
if (getCheckVal(Lan) == 1)
{
for (var j = 0; j < WAN.length; j++)
{
if ((j != SelctIndex)
&& (WAN[j].xBindList.indexOf(getValue(Lan)) >= 0))
{
AddErrInfo(eval("gErrStr1"), 'IdBindLan1');
return false;
}
}
}
if (getCheckVal(Wlan) == 1)
{
for (var j = 0; j < WAN.length; j++)
{
if ((j != SelctIndex)
&& (WAN[j].xBindList.indexOf(getValue(Wlan)) >= 0))
{
AddErrInfo(eval("gErrStr1"), 'IdBindWlan1');
return false;
}
}
}
}
}
if ((PageData.ConnType == 'PPP_Routed')
|| (PageData.ConnType == 'DHCP_Spoofed')
|| (PageData.ConnType == 'PPPoA'))
{
if ((PageData.xServiceList == "TR069")
&& (PageData.ConnTrigger == 'OnDemand'))
{
AddErrInfo(eval("gErrStr3"), 'IdINTERNET');
return false;
}
if ((PageData.xServiceList != "INTERNET")
&& (PageData.xProxyEnable == 1))
{
AddErrInfo(eval("gErrStr3"), 'IdINTERNET');
return false;
}
if (PageData.xServiceList == "SHARENET")
{
AddErrInfo(eval("gErrStr16"), 'IdINTERNET');
return false;
}
if (PageData.ConnType == 'DHCP_Spoofed')
{
if (PageData.xServiceList != "INTERNET")
{
AddErrInfo(eval("gErrStr3"), 'IdINTERNET');
return false;
}
if (PageData.ConnTrigger == 'OnDemand')
{
AddErrInfo(eval("gErrStr18"), 'IdConnTrigger');
return false;
}
if (PageData.xBindList != '')
{
if (PageData.xBindList.indexOf('LANEthernetInterfaceConfig') >= 0)
{
AddErrInfo(eval("gErrStr19"), 'IdBindLan1');
}
else
{
AddErrInfo(eval("gErrStr19"), 'IdBindWlan1');
}
return false;
}
}
if (PageData.ConnTrigger == 'OnDemand')
{
var v = getValue('IdIdleTime');
if(isPlusInteger(v) == false)
{
AddErrInfo(eval("gErrStr5"), 'IdIdleTime');
return false;
}
else
{
if ((v == "") || (v < 30) || (v > 31536000))
{
AddErrInfo(eval("gErrStr5"), 'IdIdleTime');
return false;
}
}
}
}
else if (PageData.ConnType == 'IP_Routed' || PageData.ConnType == 'IPoA')
{  
if (PageData.xServiceList == "SHARENET")
{
AddErrInfo(eval("gErrStr16"), 'IdINTERNET');
return false;
}
if (PageData.AddrType == 'Static')
{
if (!isValidIpAddressEx(getValue('IdIpAddr')))
{
AddErrInfo(eval("gErrStr6"), 'IdIpAddr');
return false;
}
if (!isValidSubnetMask(getValue('IdSubMask')) || getValue('IdSubMask') == '255.255.255.255')
{
AddErrInfo(eval("gErrStr7"), 'IdSubMask');
return false;
}
if (!isHostIpWithSubnetMask(getValue('IdIpAddr'), getValue('IdSubMask')))
{
AddErrInfo(eval("gErrStr8"), 'IdIpAddr');
return false;
}
if (!isAbcIpAddress(getValue('IdGateWay')))
{
AddErrInfo(eval("gErrStr9"), 'IdGateWay');
return false;
}
if (!isSameSubNet(getValue('IdIpAddr'), getValue('IdSubMask'), getValue('IdGateWay'), getValue('IdSubMask')))
{
AddErrInfo(eval("gErrStr30"), 'IdGateWay');
return false;
}
if (getValue('IdPrimaryDns') != '' && !isAbcIpAddress(getValue('IdPrimaryDns')))
{
AddErrInfo(eval("gErrStr10"), 'IdPrimaryDns');
return false;
}
if (getValue('IdSecondDns') != '' && !isAbcIpAddress(getValue('IdSecondDns')))
{
AddErrInfo(eval("gErrStr11"), 'IdSecondDns');
return false;
}
}
else
{
}
}
else
{
if ((PageData.xServiceList != "INTERNET")
&& (PageData.xServiceList != "SHARENET")
&& (PageData.xServiceList != "OTHER"))
{
AddErrInfo(eval("gErrStr12"), 'IdINTERNET');
return false;
}
}
if (SelctIndex != -1)
{
}
else 
{
if (WAN.length >= WAN_NUM_MAX)
{
AddErrInfo(eval("gErrStr14"));
return false;
}
}
if ((PageData.ConnType == 'PPP_Routed')|| (PageData.ConnType == 'PPPoA'))
{
if(PageData.UserName.length > 64 || PageData.UserName.length == 0)
{
AddErrInfo(eval("gErrStr26"),'IdPppUser');
return false;
}
if(isValidAscii(PageData.UserName) != '' || PageData.UserName.indexOf(' ') > (-1) )
{
AddErrInfo(eval("gErrStr28"),'IdPppUser');
return false;
}
if(PageData.Password.length > 64 || PageData.Password.length == 0)
{
AddErrInfo(eval("gErrStr27"),'IdPppPasswd');
return false;
}
if(isValidAscii(PageData.Password) != '' || PageData.Password.indexOf(' ') > (-1))
{
AddErrInfo(eval("gErrStr29"),'IdPppPasswd');
return false;
}
}
if (CheckWanCnt() == false)
{
return false;
}
if ((PageData.ConnType == 'PPP_Routed')
|| (PageData.ConnType == 'PPPoA')
|| (PageData.ConnType == 'IP_Routed')
|| (PageData.ConnType == 'IP_Static')
|| (PageData.ConnType == 'IPoA')
)
{
var v = PageData.MaxMTUSize;
if(isPlusInteger(v) == false)
{
AddErrInfo(eval("gErrStr65"), 'mtuSize');
return false;
}
else
{
if ((PageData.ConnType == 'IP_Routed') || (PageData.ConnType == 'IP_Static') || (PageData.ConnType == 'IPoA'))
{
if ((v == "") || (v < 576) || (v > 1500))
{
AddErrInfo(eval("gErrStr63"), 'mtuSize');
return false;
}
}
else if ((PageData.ConnType == 'PPP_Routed') ||
(PageData.ConnType == 'PPPoA'))
{
if ((v == "") || (v < 576) || (v > 1492))
{
AddErrInfo(eval("gErrStr64"), 'mtuSize');
return false;
}
}
}
var w = PageData.TCPMSS;
if(isPlusInteger(w) == false)
{
AddErrInfo(eval("gErrStr68"), 'TCPMSS');
return false;
}
else
{
if ((PageData.ConnType == 'IP_Routed') || (PageData.ConnType == 'IP_Static'))
{
if ((w == "") || ((w < 536) && (w != 0)) || (w > 1460))
{
AddErrInfo(eval("gErrStr66"), 'TCPMSS');
return false;
}
}
else if (PageData.ConnType == 'PPP_Routed')
{
if ((w == "") || ((w < 536) && (w != 0)) || (w > 1452))
{
AddErrInfo(eval("gErrStr67"), 'TCPMSS');
return false;
}
}
}
if ((parseInt(v) - parseInt(w) < 40) && (0 != parseInt(v)) && (0 != parseInt(w)))
{
AddErrInfo(eval("gErrStr69"), 'TCPMSS');
return false;
}
}
if (SelctIndex != -1)
{
if (((PageData.xServiceList != WAN[SelctIndex].xServiceList)
|| (PageData.ConnType != WAN[SelctIndex].ConnType)
|| (PageData.NatEnabled != WAN[SelctIndex].NatEnabled))
&& (WAN[SelctIndex].ConnType != 'IP_Bridged')
&& (WAN[SelctIndex].ConnType != 'PPPoE_Bridged'))
{
}
}
if (PageData.xIpv6Enable == 1)
{
if (PageData.xIpv6AddrType == "Static")
{
if (PageData.xIpv6Addr == '' || !isUnicastIpv6Address(PageData.xIpv6Addr))
{
AddErrInfo(eval("gErrStr73"), 'IdIpv6Addr');
return false;
}
var v6rdPrefixLen = PageData.xIpv6PrefixLen;
if(isPlusInteger(v6rdPrefixLen) == false)
{
AddErrInfo(eval("gErrStr74"), 'IdIpv6PrefixLen');
return false;
}
if ((v6rdPrefixLen == "") || (v6rdPrefixLen < 16) || (v6rdPrefixLen > 64))
{
AddErrInfo(eval("gErrStr74"), 'IdIpv6PrefixLen');
return false;
}
if (v6rdPrefixLen.length > 1 && v6rdPrefixLen.charAt(0) == '0')
{
AddErrInfo(eval("gErrStr74"), 'IdIpv6PrefixLen');
return false;
}
if (PageData.xIpv6Gateway != '' && !isUnicastIpv6Address(PageData.xIpv6Gateway))
{
AddErrInfo(eval("gErrStr73"), 'IdIpv6Gateway');
return false;
}
if (PageData.xIpv6Dns1 != '' && !isUnicastIpv6Address(PageData.xIpv6Dns1))
{
AddErrInfo(eval("gErrStr73"), 'IdIpv6Dns1');
return false;
}
if (PageData.xIpv6Dns2 != '' && !isUnicastIpv6Address(PageData.xIpv6Dns2))
{
AddErrInfo(eval("gErrStr73"), 'IdIpv6Dns2');
return false;
}
}
if (PageData.xIpv6AddrType == "DHCP")
{
if (PageData.xIpv6Gateway != '' && !isUnicastIpv6Address(PageData.xIpv6Gateway))
{
AddErrInfo(eval("gErrStr73"), 'IdIpv6Gateway');
return false;
}
}
}
var i = 0;
if (PageData.xIpv6Enable == 1)
{
for (i=0; i<WAN.length; i++)
{
if ((i !=SelctIndex) && (WAN[i].xTunnelType == '6to4') && (WAN[i].xIpv4Enable == 1))
{
AddErrInfo(eval("gErrStr54"), 'IdIpv6AddrType');
return false;
}
}
}
if (1 == PageData.xIpv4Enable)
{
for (i=0; i<WAN.length; i++)
{
if ((i !=SelctIndex) && (WAN[i].xTunnelType == 'DS-Lite') && (WAN[i].xIpv6Enable == 1))
{
AddErrInfo(eval("gErrStr45"), 'IdIpv4Enable');
return false;
}
}
}
if (PageData.xTunnelType == "6to4")
{
if (PageData.xServiceList == "TR069")
{
AddErrInfo(eval("gErrStr51"), 'IdINTERNET');
return false;
}
if (PageData.xIpv6Enable == 1)
{
AddErrInfo(eval("gErrStr56"), 'IdIpv6Enable');
return false;
}
if ( (PageData.ConnType == 'PPP_Routed') && (PageData.ConnTrigger == 'OnDemand'))
{
AddErrInfo(eval("gErrStr44"), 'IdConnTrigger');
}
for (i=0; i<WAN.length; i++)
{
if ((i !=SelctIndex) && (WAN[i].xIpv6Enable == 1))
{
AddErrInfo(eval("gErrStr54"), 'IdTunnelType');
return false;
}
if ((i !=SelctIndex) && (WAN[i].xTunnelType == '6to4') && (WAN[i].xIpv4Enable == 1))
{
AddErrInfo(eval("gErrStr48"), 'IdTunnelType');
return false;
}
}
if (PageData.xTunnelEndpoint != '' && !isAbcIpAddress(PageData.xTunnelEndpoint))
{
AddErrInfo(eval("gErrStr55"), 'IdTunnelEndpoint');
return false;
}
var dns1 = getValue('Id6to4Ipv6Dns1');
var dns2 = getValue('Id6to4Ipv6Dns2');
if ((dns1 != '' && !isUnicastIpv6Address(dns1))  || (dns1 == '' && dns2 != ''))
{
AddErrInfo(eval("gErrStr25"), 'Id6to4Ipv6Dns1');
return false;
}
if (dns2 != '' && !isUnicastIpv6Address(dns2))
{
AddErrInfo(eval("gErrStr26"), 'Id6to4Ipv6Dns2');
return false;
}
}
else if (PageData.xTunnelType == "DS-Lite")
{
if ((v == "") || v < 1320)
{
AddErrInfo(eval("gErrStr75"), 'mtuSize');
return false;
}
if (PageData.xIpv4Enable == 1)
{
AddErrInfo(eval("gErrStr45"), 'IdIpv4Enable');
return false;
}
if (PageData.xServiceList.indexOf("INTERNET") < 0)
{
AddErrInfo(eval("gErrStr50"), 'IdINTERNET');
return false;
}
if ( (PageData.ConnType == 'PPP_Routed') && (PageData.ConnTrigger == 'OnDemand'))
{
AddErrInfo(eval("gErrStr44"), 'IdConnTrigger');
return false;
}
if (PageData.xIpv6AddrType == "Static" && PageData.xTunnelEndpoint == "")
{
AddErrInfo(eval("gErrStr49"), 'IdTunnelEndpoint');
return false;
}
for (i=0; i<WAN.length; i++)
{
if ((i !=SelctIndex) && (WAN[i].xIpv4Enable == 1))
{
AddErrInfo(eval("gErrStr45"), 'IdTunnelType');
return false;
}
if ((i !=SelctIndex) && (WAN[i].xTunnelType == 'DS-Lite') && (WAN[i].xIpv6Enable == 1))
{
AddErrInfo(eval("gErrStr47"), 'IdTunnelType');
return false;
}
}
if (PageData.xTunnelEndpoint != '' && ((false == isValidDomain(PageData.xTunnelEndpoint, 64) && false == isGlobalIpv6Address(PageData.xTunnelEndpoint))
|| PageData.xTunnelEndpoint.indexOf(' ') > (-1)))
{
AddErrInfo(eval("gErrStr46"), 'IdTunnelEndpoint');
return false;
}
}
return true;
}
function TunnelDisplay()
{
var TunnelType = getSelectVal('IdTunnelType');
if (TunnelType == "")
{
setDisplay('TrTunnelEndpoint', 0);
}
else if (TunnelType == "6to4")
{
setDisplay('TrTunnelEndpoint', 1);
setDisplay('Tr6to4Ipv6Dns1', 1);
setDisplay('Tr6to4Ipv6Dns2', 1);
}
else if (TunnelType == "DS-Lite")
{
setDisplay('TrTunnelEndpoint', 1);
}
}
function Ipv4Display()
{
var Ipv4Enable;
var ConnType;
var PPPType;
with (getElement('Configure'))
{
ConnType = getSelectVal('IdConnType');
if ((ConnType != 'PPP_Routed')
&& (ConnType != 'IP_Routed'))
{
setDisplay('TrIpv4Enable', 0);
return;
}
setDisplay('TrIpv4Enable', 1);
Ipv4Enable = getCheckVal('IdIpv4Enable');
Ipv6Enable = getCheckVal('IdIpv6Enable');
if ( 0 == Ipv4Enable )
{
setSelect('IdAddrType', 'DHCP');
setDisplay('TrNatEnabled', 0);
setDisplay('TrAddrType', 0);
setDisplay('TrIpAddr', 0);
setDisplay('TrSubMask', 0);
setDisplay('TrGateWay', 0);
setDisplay('TrPrimaryDns', 0);
setDisplay('TrSecondDns', 0);
if (0 == Ipv6Enable)
{
setSelect('IdTunnelType','');
TunnelDisplay();
setDisplay('TrTunnelType', 0);
}
else
{
setDisplay('TrTunnelType', 1);
setDisplay('Tunnel_dslite', 1);
var flag = false;
for (var i = 0; i < document.getElementById("IdTunnelType").options.length; i++)
{
if ("none" != document.getElementById("IdTunnelType").options[i].style.display && "" != document.getElementById("IdTunnelType").options[i].value)
{
flag = true;
}
}
if (false == flag)
{
setDisplay('TrTunnelType', 0);
}
}
return;
}
else
{
setDisplay('TrNatEnabled', 1);
if ( 'IP_Routed' == getSelectVal('IdConnType') )
{
setDisplay('TrAddrType', 1);
}
if ( 'IP_Routed' == getSelectVal('IdConnType') && 'Static' == getSelectVal('IdAddrType') )
{
setDisplay('TrIpAddr', 1);
setDisplay('TrSubMask', 1);
setDisplay('TrGateWay', 1);
setDisplay('TrPrimaryDns', 1);
setDisplay('TrSecondDns', 1);
}
else
{
setDisplay('TrIpAddr', 0);
setDisplay('TrSubMask', 0);
setDisplay('TrGateWay', 0);
setDisplay('TrPrimaryDns', 0);
setDisplay('TrSecondDns', 0);
}
if (0 == Ipv6Enable)
{
setDisplay('TrTunnelType', 1);
setDisplay('Tunnel_dslite', 0);
var flag = false;
for (var i = 0; i < document.getElementById("IdTunnelType").options.length; i++)
{
if ("none" != document.getElementById("IdTunnelType").options[i].style.display && "" != document.getElementById("IdTunnelType").options[i].value)
{
flag = true;
}
}
if (false == flag)
{
setDisplay('TrTunnelType', 0);
}
}
else
{
setSelect('IdTunnelType','');
TunnelDisplay();
setDisplay('TrTunnelType', 0);
}
}
}
}
function Ipv6Display()
{
var Ipv6Enable;
var AddrType;
var ConnType;
var PPPType;
with (getElement('Configure'))
{
ConnType = getSelectVal('IdConnType');
if ((ConnType != 'PPP_Routed')
&& (ConnType != 'IP_Routed'))
{
setDisplay('TrIpv6Enable', 0);
setDisplay('TrIpv6AddrType', 0);
setDisplay('TrIpv6Addr', 0);
setDisplay('TrIpv6Dns1', 0);
setDisplay('TrIpv6Dns2', 0);
setDisplay('TrIpv6Gateway', 0);
setDisplay('TrTunnelType', 0);
setDisplay('TrTunnelEndpoint', 0);
return;
}
setDisplay('TrIpv6Enable', 1);
setDisplay('TrTunnelType', 1);
TunnelDisplay();
Ipv6Enable = getCheckVal('IdIpv6Enable');
Ipv4Enable = getCheckVal('IdIpv4Enable');
if (Ipv6Enable == 0)
{
setSelect('IdIpv6AddrType', 'SLAAC');
setDisplay('TrIpv6AddrType', 0);
setDisplay('TrIpv6Addr', 0);
setDisplay('TrIpv6Dns1', 0);
setDisplay('TrIpv6Dns2', 0);
setDisplay('TrIpv6Gateway', 0);
if (0 == Ipv4Enable)
{
setSelect('IdTunnelType','');
TunnelDisplay();
setDisplay('TrTunnelType', 0);
}
else
{
setDisplay('TrTunnelType', 1);
setDisplay('Tunnel_dslite', 0);
var flag = false;
for (var i = 0; i < document.getElementById("IdTunnelType").options.length; i++)
{
if ("none" != document.getElementById("IdTunnelType").options[i].style.display && "" != document.getElementById("IdTunnelType").options[i].value)
{
flag = true;
}
}
if (false == flag)
{
setDisplay('TrTunnelType', 0);
}
}
}
else
{
setDisplay('TrIpv6AddrType', 1);
AddrType = getSelectVal('IdIpv6AddrType');
if (AddrType == 'SLAAC')
{
setDisplay('TrIpv6Addr', 0);
setDisplay('TrIpv6Dns1', 0);
setDisplay('TrIpv6Dns2', 0);
setDisplay('TrIpv6Gateway', 0);
}
else if (AddrType == 'DHCP')
{
setDisplay('TrIpv6Addr', 0);
setDisplay('TrIpv6Dns1', 0);
setDisplay('TrIpv6Dns2', 0);
if (ConnType == 'PPP_Routed')
{
setDisplay('TrIpv6Gateway', 0);
}
else
{
setDisplay('TrIpv6Gateway', 1);
}
}
else if (AddrType == 'Static')
{
setDisplay('TrIpv6Addr', 1);
setDisplay('TrIpv6Dns1', 1);
setDisplay('TrIpv6Dns2', 1);
setDisplay('TrIpv6Gateway', 1);
}
if (0 == Ipv4Enable)
{
setDisplay('TrTunnelType', 1);
setDisplay('Tunnel_dslite', 1);
var flag = false;
for (var i = 0; i < document.getElementById("IdTunnelType").options.length; i++)
{
if ("none" != document.getElementById("IdTunnelType").options[i].style.display && "" != document.getElementById("IdTunnelType").options[i].value)
{
flag = true;
}
}
if (false == flag)
{
setDisplay('TrTunnelType', 0);
}
}
else
{
setSelect('IdTunnelType','');
TunnelDisplay();
setDisplay('TrTunnelType', 0);
}
}
}
}
function AddSubmitParam(Form, type)
{
Form.addParameter('y.Enable', PageData.Enable);
Form.addParameter('y.X_ServiceList', PageData.xServiceList);
if (PageData.xServiceList == "TR069")
{
Form.addParameter('y.X_LanInterface', '');
}
else
{
Form.addParameter('y.X_LanInterface', PageData.xBindList);
}
if ((PageData.ConnType == 'PPP_Routed') || (PageData.ConnType == 'PPPoA')
|| (PageData.ConnType == 'IPoA')
)
{
Form.addParameter('y.ConnectionType', 'IP_Routed');
}
else if (PageData.ConnType == 'Bridge')
{
if (PageData.Cmo == 'WANIPConnection')
{
Form.addParameter('y.ConnectionType', 'IP_Bridged');
}
else
{
Form.addParameter('y.ConnectionType', 'PPPoE_Bridged');
}
}
else
{
Form.addParameter('y.ConnectionType', PageData.ConnType);
}
if ((PageData.ConnType == 'PPP_Routed')
|| (PageData.ConnType == 'DHCP_Spoofed')
|| (PageData.ConnType == 'PPPoA'))
{
Form.addParameter('y.NATEnabled', PageData.NatEnabled);
Form.addParameter('y.ConnectionTrigger', PageData.ConnTrigger);
Form.addParameter('y.IdleDisconnectTime', PageData.IdleTime);
Form.addParameter('y.Username', PageData.UserName);
if ('@1GV)Z<!' != PageData.Password)
{
Form.addParameter('y.Password', PageData.Password);
}
Form.addParameter('y.PPPAuthenticationProtocol', PageData.Auth);
Form.addParameter('y.DNSEnabled', '1');
Form.addParameter('y.DNSOverrideAllowed', '1');
Form.addParameter('y.X_DHCPRelay', '0');
}
else if (PageData.ConnType == 'IP_Routed'
|| PageData.ConnType == 'IPoA'
)
{
Form.addParameter('y.NATEnabled', PageData.NatEnabled);
Form.addParameter('y.AddressingType', PageData.AddrType);
if (PageData.AddrType == 'Static')
{
Form.addParameter('y.ExternalIPAddress', PageData.IpAddress);
Form.addParameter('y.SubnetMask', PageData.SubnetMask);
Form.addParameter('y.DefaultGateway', PageData.DefaultGateway);
if (PageData.dnsSecondary == '')
{
Form.addParameter('y.DNSServers', PageData.dnsPrimary);
}
else
{
Form.addParameter('y.DNSServers', PageData.dnsPrimary + ',' + PageData.dnsSecondary);
}
Form.addParameter('y.DNSEnabled', '1');
Form.addParameter('y.DNSOverrideAllowed', '0');
}
else
{
Form.addParameter('y.X_WANDHCPOption60', PageData.DhcpOption60);
Form.addParameter('y.DNSEnabled', '1');
Form.addParameter('y.DNSOverrideAllowed', '1');
Form.addParameter('y.DNSServers', "");
}
Form.addParameter('y.X_DHCPRelay', '0');
}
else if (PageData.ConnType == 'Bridge')
{
Form.addParameter('y.X_DHCPRelay', PageData.xDhcpRelay);
if (PageData.Cmo == 'WANPPPConnection')
{
Form.addParameter('y.ConnectionTrigger', 'AlwaysOn');
Form.addParameter('y.IdleDisconnectTime', '0');
}
}
if (0 > PageData.xServiceList.indexOf('INTERNET'))
{
Form.addParameter('y.RouteProtocolRx', 'Off');
}
if ((PageData.ConnType != 'PPP_Routed') && (PageData.ConnType != 'IP_Routed'))
{
Form.addParameter('y.RouteProtocolRx', 'Off');
}
if (0 == PageData.NatEnabled)
{
Form.addParameter('y.RouteProtocolRx', 'Off');
}
if ((PageData.ConnType == 'PPP_Routed')
|| (PageData.ConnType == 'IP_Routed'))
{
Form.addParameter('y.X_IPv4Enable', PageData.xIpv4Enable);
Form.addParameter('y.X_IPv6Enable', PageData.xIpv6Enable);
Form.addParameter('y.X_IPv6AddressingType', PageData.xIpv6AddrType);
if (('SLAAC' == PageData.xIpv6AddrType) || (('PPP_Routed' == PageData.ConnType) && ('DHCP' == PageData.xIpv6AddrType)))
{
Form.addParameter('y.X_IPv6DefaultGateway','');
}
else
{
Form.addParameter('y.X_IPv6DefaultGateway', PageData.xIpv6Gateway);
}
if (PageData.xIpv6Enable == 1 && PageData.xIpv6AddrType == 'Static')
{
Form.addParameter('y.X_IPv6Address', PageData.xIpv6Addr);
Form.addParameter('y.X_IPv6PrefixLength', PageData.xIpv6PrefixLen);
if (PageData.xIpv6Dns2 == '')
{
Form.addParameter('y.X_IPv6DNSServers', PageData.xIpv6Dns1);
}
else
{
Form.addParameter('y.X_IPv6DNSServers', PageData.xIpv6Dns1 + ',' + PageData.xIpv6Dns2);
}
Form.addParameter('y.X_IPv6DNSEnabled', '1');
Form.addParameter('y.X_IPv6DNSOverrideAllowed', '0');
}
else
{
Form.addParameter('y.X_IPv6Address', "");
Form.addParameter('y.X_IPv6PrefixLength', '0');
Form.addParameter('y.X_IPv6DNSEnabled', '1');
Form.addParameter('y.X_IPv6DNSOverrideAllowed', '1');
Form.addParameter('y.X_IPv6DNSServers', "");
}
Form.addParameter('y.X_TunnelType', PageData.xTunnelType);
if ("" != PageData.xTunnelType)
Form.addParameter('y.X_TunnelEndpoint', PageData.xTunnelEndpoint);
}
else
{
Form.addParameter('y.X_TunnelType', '');
Form.addParameter('y.X_IPv4Enable', '1');
Form.addParameter('y.X_IPv6Enable', '0');
Form.addParameter('y.X_IPv6Address', "");
Form.addParameter('y.X_IPv6PrefixLength', '0');
Form.addParameter('y.X_IPv6DNSEnabled', '1');
Form.addParameter('y.X_IPv6DNSOverrideAllowed', '1');
Form.addParameter('y.X_IPv6DNSServers', "");
}
Form.addParameter('y.MaxMTUSize', PageData.MaxMTUSize);
if ((PageData.ConnType == 'PPP_Routed')
|| (PageData.ConnType == 'PPPoA')
|| (PageData.ConnType == 'IP_Routed')
|| (PageData.ConnType == 'IP_Static')
|| (PageData.ConnType == 'IPoA')
)
{
Form.addParameter('y.X_TCP_MSS', PageData.TCPMSS);
}
Form.addParameter('x.DestinationAddress','PVC:'
+ parseInt(PageData.AtmVpi, 10) + '/' + parseInt(PageData.AtmVci, 10));
Form.addParameter('x.ATMQoS', PageData.AtmServiceCategory);
Form.addParameter('x.Enable',1);
switch (PageData.AtmServiceCategory)
{
case "UBR+":
case "CBR":
Form.addParameter('x.ATMPeakCellRate',PageData.AtmPeakCellRate);
break;
case "VBR-nrt":
case "VBR-rt":
Form.addParameter('x.ATMPeakCellRate',PageData.AtmPeakCellRate);
Form.addParameter('x.ATMSustainableCellRate',PageData.AtmSustainedCellRate);
Form.addParameter('x.ATMMaximumBurstSize',PageData.AtmMaxBurstSize);
break;
}
if (PageData.ConnType == "PPPoA")
{
Form.addParameter('x.LinkType',"PPPoA");
}
else if (PageData.ConnType == "IPoA")
{
Form.addParameter('x.LinkType',"IPoA");
}
else
{
Form.addParameter('x.LinkType',"EoA");
}
Form.addParameter('x.ATMEncapsulation', PageData.AtmEncapMode);
if (PageData.VlanMarkEnable == 1)
{
Form.addParameter('x.X_VLANIDMark', PageData.VlanMarkID);
if (PageData.PrioMarkEnable == 1)
{
Form.addParameter('x.X_802-1pMark', PageData.PrioMarkID);
}
else
{
Form.addParameter('x.X_802-1pMark', "-1");
}
}
else
{
Form.addParameter('x.X_VLANIDMark', "-1");
Form.addParameter('x.X_802-1pMark', "-1");
}
top.aplyLine = SelctIndex;
var url = '';
if(SelctIndex == -1 )
{
url = 'changewantype.cgi?dellist=NULL&x=InternetGatewayDevice.WANDevice.1.WANConnectionDevice.&y=' + PageData.Cmo;
url += "&RequestFile=/html/ntwkall/wan.asp";
}
else
{
for (i = 0; i < WCD.length - 1; i++)
{
if (WCD[i].WcdDomain == WAN[SelctIndex].WcdDomain)
{
break;
}
}
var origvpi = WCD[i].atmVpi;
var origvci = WCD[i].atmVci;
var origLinkType = WCD[i].LinkType;
var cntwanwithsamePVC = 0;
for (var j = 0; j < WAN.length; j++)
{
if (j != SelctIndex)
{
if (WAN[SelctIndex].WcdDomain == WAN[j].WcdDomain)
{
break;
}
}
}
if (j < WAN.length)
{
cntwanwithsamePVC = 1;
}
if ((((PageData.ConnType == "PPPoA") || (origLinkType == "PPPoA")) &&
(PageData.ConnType != origLinkType))
||(((PageData.ConnType == "IPoA") || (origLinkType == "IPoA")) && (PageData.ConnType != origLinkType))
)
{
if (cntwanwithsamePVC >= 1)
{
url = "changewantype.cgi?" + 'dellist=' + WAN[SelctIndex].Domain + '&x=InternetGatewayDevice.WANDevice.1.WANConnectionDevice.'
+ '&y=' + PageData.Cmo
+ "&RequestFile=/html/ntwkall/wan.asp";
}
else
{
url = "changewantype.cgi?" + 'dellist=' + WAN[SelctIndex].WcdDomain + '&x=InternetGatewayDevice.WANDevice.1.WANConnectionDevice.'
+ '&y=' + PageData.Cmo
+ "&RequestFile=/html/ntwkall/wan.asp";
}
}
else
{
if ((origvpi == PageData.AtmVpi) && (origvci == PageData.AtmVci))
{
if (PageData.Cmo != WAN[SelctIndex].Cmo)
{
url = "changewantype.cgi?" + 'dellist=' + WAN[SelctIndex].Domain + '&x=' + WAN[SelctIndex].WcdDomain
+ '&y=' + PageData.Cmo
+ "&RequestFile=/html/ntwkall/wan.asp";
}
else
{
url = "setcfg.cgi?x=" + WAN[SelctIndex].WcdDomain + ".WANDSLLinkConfig&y=" + WAN[SelctIndex].Domain + "&RequestFile=/html/ntwkall/wan.asp";
}
}
else
{
if (PageData.Cmo != WAN[SelctIndex].Cmo)
{
if (cntwanwithsamePVC >= 1)
{
url = "changewantype.cgi?" + 'dellist=' + WAN[SelctIndex].Domain + '&x=InternetGatewayDevice.WANDevice.1.WANConnectionDevice.'
+ '&y=' + PageData.Cmo
+ "&RequestFile=/html/ntwkall/wan.asp";
}
else
{
url = "changewantype.cgi?" + 'dellist=' + WAN[SelctIndex].Domain + '&x=' + WAN[SelctIndex].WcdDomain
+ '&y=' + PageData.Cmo
+ "&RequestFile=/html/ntwkall/wan.asp";
}
}
else
{
if (cntwanwithsamePVC >= 1)
{
url = "changewantype.cgi?" + 'dellist=' + WAN[SelctIndex].Domain + '&x=InternetGatewayDevice.WANDevice.1.WANConnectionDevice.'
+ '&y=' + PageData.Cmo
+ "&RequestFile=/html/ntwkall/wan.asp";
}
else
{
url = "setcfg.cgi?x=" + WAN[SelctIndex].WcdDomain + ".WANDSLLinkConfig&y=" + WAN[SelctIndex].Domain + "&RequestFile=/html/ntwkall/wan.asp";
}
}
}
}
}
Form.setAction(url);
setDisable('btnApply',1);
}

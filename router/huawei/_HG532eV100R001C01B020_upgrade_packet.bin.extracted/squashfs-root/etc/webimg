<script language="JavaScript" type="text/javascript">
function stMLDCfg(domain,MLDProxyEnable,MLDSnoopingEnable,Wan6List)
{
this.domain = domain;
this.MLDProxyEnable = MLDProxyEnable;
this.MLDSnoopingEnable = MLDSnoopingEnable
this.Wan6List = Wan6List;
}
function stWan(domain,Name,ConnectionType,ServerList, ipv6enable)
{
this.domain = domain;
this.Name = Name;
this.CntType = ConnectionType;
this.ServerList = ServerList;
this.ipv6enable = ipv6enable;
}
var vMldProxy = <%webGetCfgEntry(InternetGatewayDevice.Services.X_IPTV6,MLDProxyEnable|MLDSnoopingEnable|Wan6List,stMLDCfg);%>;
var mld = vMldProxy[0];
var WanPPP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|ConnectionType|X_ServiceList|X_IPv6Enable,stWan);%>;
var WanIP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Name|ConnectionType|X_ServiceList|X_IPv6Enable,stWan);%>;
var Wan = new Array();
for (i = 0; i < WanPPP.length-1; i++)
{
Wan[i] = WanPPP[i];
}
for (j = 0; j < WanIP.length-1; j++,i++)
{
Wan[i] = WanIP[j];
}
function LoadFrame()
{
var i = GetmenuName("mld.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
with ( document.forms[0])
{
setCheck('chkMldProxy',mld.MLDProxyEnable);
setCheck('chkMldSnp',mld.MLDSnoopingEnable);
proxyClick();
if(MLDWanIsExist())
{
setValueDft('Interface',mld.Wan6List);
}
else
{
setValueDft('Interface',"");
}
}
}
function CheckForm(type)
{
if((getSelectVal('Interface')=="")&&(getCheckVal('chkMldProxy') == 1))
{
AddErrInfo(eval("gErrStr2"), 'Interface');
return false;
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
var MldSnp = getCheckVal('chkMldSnp');
SubmitForm.addParameter('InternetGatewayDevice.Services.X_IPTV6.MLDSnoopingEnable',MldSnp);
var MldProxy = getCheckVal('chkMldProxy');
SubmitForm.addParameter('InternetGatewayDevice.Services.X_IPTV6.MLDProxyEnable',MldProxy);
var Wan6Info = "";
if(MldProxy == 1)
{
var Wan6Info  = getSelectVal('Interface');
SubmitForm.addParameter('InternetGatewayDevice.Services.X_IPTV6.Wan6List',Wan6Info);
}
else
{
SubmitForm.addParameter('InternetGatewayDevice.Services.X_IPTV6.Wan6List',Wan6Info);
}
SubmitForm.setAction('setcfg.cgi?RequestFile=/html/application/mld.asp');
setDisable('btnApply',1);
}
function proxyClick()
{
setDisplay('proxyEnable',getCheckVal('chkMldProxy'));
}
function WriteRouteOption()
{
document.write('<option value="" >' + "" + '</option>');
for (i = 0; i < Wan.length; i++)
{
if (Wan[i].CntType.indexOf('IP_Routed') < 0)
{
continue;
}
if ((Wan[i].ServerList.indexOf('INTERNET') < 0)
&& (Wan[i].ServerList.indexOf('Other') < 0))
{
continue;
}
if (Wan[i].ipv6enable != 1)
{
continue;
}
document.write('<option id="wan_' + i + '" value="' + Wan[i].domain + '">'
+ Wan[i].Name + '</option>');
}
}
function MLDWanIsExist()
{
var MLDWanList = mld.Wan6List;
for (i = 0; i < Wan.length; i++)
{
if(MLDWanList == Wan[i].domain)
{
if((Wan[i].CntType.indexOf('Bridge') > -1)||(Wan[i].ServerList == 'TR069'))
{
return false;
}
else
{
return true;
}
}
}
return false;
}
function WanChose()
{
with ( document.forms[0])
{
if((Interface.options.length > 0)&&(Interface.options[0].value == ""))
{
Interface.options[0] = null;
}
}
}
</script>
</head>

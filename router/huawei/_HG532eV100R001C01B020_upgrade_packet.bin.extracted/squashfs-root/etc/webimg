<script language="JavaScript" type="text/javascript">
var ifstatus = 0;
var ifnetwork = 0;
var ifsecurity = 0;
var ifapp = 0;
var ifman = 0;
var globalFirItem = 'status_Link_1';
var globalSecItem = 'status_Link_1';
var item = '';
function FormLoad()
{
if(top.Link == '')
{
funstatus();
}
else
{
item = opener.top.Link;
var pose = item.lastIndexOf('#');
if (pose != -1)
{
var option = item.substr(pose+1);
openAllHelp(option);
}
}
}
function hiddenFirContent()
{
getElement('Device_Div').style.display = "none";
getElement('WAN_Basic').style.display = "none";
getElement('Advance_Route1_div').style.display = "none";
getElement('Advance_Route2_div').style.display = "none";
getElement('Advance_Route3_div').style.display = "none";
getElement('Advance_Account_div').style.display = "none";
}
function hiddenStatus()
{
getElement('status_1').style.display = "none";
getElement('status_2').style.display = "none";
getElement('status_3').style.display = "none";
getElement('status_4').style.display = "none";
getElement('HelpStatusImag').src = "/images/collapsemenu.gif";
getElement('Status_Link').style.color = '#000000';
ifstatus = 0;
}
function hiddenBasic()
{
getElement('basic_4').style.display = "none";
getElement('basic_3').style.display = "none";
getElement('HelpBasicImag').src = "/images/collapsemenu.gif";
getElement('Basic_Link').style.color = '#000000';
ifnetwork = 0;
}
function hiddenAdvance()
{
getElement('advance_17').style.display = "none";
getElement('advance_3').style.display = "none";
getElement('advance_4').style.display = "none";
getElement('advance_9').style.display = "none";
getElement('HelpAdvanceImag').src = "/images/collapsemenu.gif";
getElement('Advance_Link').style.color = '#000000';
ifsecurity = 0;
}
function hiddenMan()
{
getElement('man_1').style.display = "none";
getElement('man_2').style.display = "none";
getElement('man_3').style.display = "none";
getElement('man_4').style.display = "none";
getElement('HelpMMImag').src = "/images/collapsemenu.gif";
getElement('Management_Link').style.color = '#000000';
ifman = 0;
}
function displayStatus()
{
getElement('status_1').style.display = "block";
getElement('status_2').style.display = "block";
getElement('status_3').style.display = "block";
getElement('status_4').style.display = "block";
getElement('Status_Link').style.color = '#990000';
getElement('HelpStatusImag').src = "/images/expandmenu.gif";
ifstatus = 1;
}
function displayBasic()
{
getElement('basic_4').style.display = "block";
getElement('basic_3').style.display = "block";
getElement('Basic_Link').style.color = '#990000';
getElement('HelpBasicImag').src = "/images/expandmenu.gif";
ifnetwork = 1;
}
function displayAdvance()
{
getElement('advance_17').style.display = "block";
getElement('advance_3').style.display = "block";
getElement('advance_4').style.display = "block";
getElement('advance_9').style.display = "block";
getElement('Advance_Link').style.color = '#990000';
getElement('HelpAdvanceImag').src = "/images/expandmenu.gif"
ifsecurity = 1;
}
function displayMan()
{
getElement('man_1').style.display = "block";
getElement('man_2').style.display = "block";
getElement('man_3').style.display = "block";
getElement('man_4').style.display = "block";
getElement('Management_Link').style.color = '#990000';
getElement('HelpMMImag').src = "/images/expandmenu.gif"
ifman = 1;
}
function funstatus()
{
hiddenBasic();
hiddenAdvance();
hiddenMan();
with (document.forms[0])
{
if (ifstatus == 0)
{
displayStatus();
getElement(globalFirItem).style.color = '#000000';
getElement(globalSecItem).style.color = '#000000';
hiddenFirContent();
getElement('status_Link_1').style.color = '#990000';
globalSecItem = 'status_Link_1';
openAllHelp(globalSecItem);
globalFirItem = 'Status_Link';
}
else
{
hiddenStatus();
}
}
}
function funnetwork()
{
hiddenStatus();
hiddenAdvance();
hiddenMan();
with (document.forms[0])
{
if (ifnetwork == 0)
{
displayBasic();
getElement(globalFirItem).style.color = '#000000';
getElement(globalSecItem).style.color = '#000000';
globalFirItem = 'Basic_Link';
hiddenFirContent();
getElement('basic_Link_1').style.color = '#990000';
globalSecItem = 'basic_Link_1';
openAllHelp(globalSecItem);
globalFirItem = 'Basic_Link';
}
else
{
hiddenBasic();
}
}
}
function funsecui()
{
hiddenBasic();
hiddenStatus();
hiddenMan();
with (document.forms[0])
{
if (ifsecurity == 0)
{
displayAdvance();
getElement(globalFirItem).style.color = '#000000';
getElement(globalSecItem).style.color = '#000000';
globalFirItem = 'Advance_Link';
hiddenFirContent();
getElement('advance_Link_17').style.color = '#990000';
globalSecItem = 'advance_Link_17';
openAllHelp(globalSecItem);
globalFirItem = 'Advance_Link';
}
else
{
hiddenAdvance();
}
}
}
function funman()
{
hiddenBasic();
hiddenAdvance();
hiddenStatus();
with (document.forms[0])
{
if (ifman == 0)
{
displayMan();
getElement(globalFirItem).style.color = '#000000';
getElement(globalSecItem).style.color = '#000000';
hiddenFirContent();
getElement('management_Link_1').style.color = '#990000';
globalSecItem = 'management_Link_1';
openAllHelp(globalSecItem);
globalFirItem = 'Management_Link';
}
else
{
hiddenMan();
}
}
}

var curUserType = "<%webGetUserType();%>";
function stSyslogCfg(domain,DisplayLevel,Enable)
{
this.domain = domain;
this.DisplayLevel = DisplayLevel;
this.Enable = Enable;
}
var temp = <%webGetCfgEntry(InternetGatewayDevice.X_SyslogConfig,DisplayLevel|Enable,stSyslogCfg);%>;
var SyslogCfg = temp[0];
function LoadFrame()
{
var i = GetmenuName("logview.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
setSelect('DisplayLevel',SyslogCfg.DisplayLevel);
if(SyslogCfg.Enable != 0)
{
setDisable('saveLog',0);
setDisable('clearLog',0);
setDisable('DisplayLevel',0);
}else{
setDisable('saveLog',1);
setDisable('clearLog',1);
setDisable('DisplayLevel',1);
}
if (curUserType != "0")
{
setDisplay("clearLog","0");
}
}
function ViewLog()
{
var Form = new webSubmitForm();
Form.addParameter('InternetGatewayDevice.X_SyslogConfig.DisplayLevel',
getSelectVal('DisplayLevel'));
Form.setAction('setcfg.cgi?RequestFile=/html/management/logview.asp');
Form.submit();
setDisable('DisplayLevel',1);
}
function clearLog()
{
if(!confirm(gvarClearconfirm))
return;
var Form = new webSubmitForm();
Form.setAction('clearLogBuffer.cgi?RequestFile=/html/management/logview.asp');
Form.submit();
setDisable('clearLog',1);
}
function saveLog()
{
var Form = new webSubmitForm();
Form.setAction('savelog.txt?RequestFile=/html/content.asp');
Form.submit();
}

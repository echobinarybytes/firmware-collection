<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" type="text/javascript">
var selctIndex = -1;
var AclRule = new Array();
function stAclInfo(Service,Direction,IPStart,IPEnd)
{
this.Service = Service;
this.Direction = Direction;
this.IPStart = IPStart;
this.IPEnd = IPEnd;
}
var AclServiceConstEng = new Array(
new stAclInfo('ICMP','LAN','','')
, new stAclInfo('HTTP','LAN','','')
, new stAclInfo('HTTPS','LAN','','')
);
function stACL(domain,Service,Direction,IPStart,IPEnd)
{
this.domain = domain;
this.Service = Service;
this.Direction = Direction;
this.IPStart = IPStart;
this.IPEnd = IPEnd;
}
var AclRule = <%webGetCfgEntry(InternetGatewayDevice.ACL.{i},X_Service|X_Direction|X_StartIpAddr|X_EndIpAddr,stACL);%>
function CheckForm()
{
with(document.forms[0])
{
if (getElement('AclService').value == "FIRST_APP")
{
AddErrInfo(eval("gErrStr7"), 'AclService');
return false;
}
if (selctIndex == -1 && AclRule.length > 32)
{
AddErrInfo(eval("gErrStrLast"), '');
return false;
}
for (i = 0; i < AclRule.length && null != AclRule[i]; i++)
{
if ((AclRule[i].Service == getElement('AclService').value)
&& (AclRule[i].Direction == getElement('AclDirection').value)
&& (AclRule[i].IPStart == getElement('AclIPStart').value)
&& (AclRule[i].IPEnd == getElement('AclIPEnd').value)
&&selctIndex != i)
{
AddErrInfo(eval("gErrStr8") + getSafeString(getElement('AclService').value) + eval("gErrStr9"), 'AclService');
return false;
}
}
if(getElement('AclService').value == 'HTTP'  && getElement('AclDirection').value == 'WAN')
{
AddErrInfo(eval("gErrStr15"), 'AclService');
return false;
}
if (getElement('AclIPStart').value != ""  && (isAbcIpAddress(getElement('AclIPStart').value) == false) )
{
AddErrInfo(eval("gErrStr10") + getSafeString(getElement('AclIPStart').value) + eval("gErrStr11"), 'AclIPStart');
return false;
}
if ( getElement('AclIPEnd').value != "" && (isAbcIpAddress(getElement('AclIPEnd').value) == false))
{
AddErrInfo(eval("gErrStr12") + getSafeString(getElement('AclIPEnd').value) + eval("gErrStr13"), 'AclIPEnd');
return false;
}
if (getElement('AclIPStart').value != ""
&& getElement('AclIPEnd').value != ""
&& (IpAddress2DecNum(getElement('AclIPStart').value) > IpAddress2DecNum(getElement('AclIPEnd').value)))
{
AddErrInfo(eval("gErrStr14"), 'AclIPStart');
return false;
}
if (getElement('AclIPStart').value == "" && getElement('AclIPEnd').value != "" )
{
AddErrInfo(eval("gMsg"), 'AclIPStart');
return false;
}
if (getElement('AclIPStart').value != ""
&& getElement('AclIPEnd').value == "")
{
AclIPEnd.value = AclIPStart.value;
}
if(selctIndex != -1)
{
if((AclRule[selctIndex].Service == 'HTTP')&&(AclRule[selctIndex].Service != getElement('AclService').value))
{
if(confirm(eval("gVarAclHttpChangeWarning"),'')==false)
{
return false;
}
}
}
}
return true;
}
function AddSubmitParam(SubmitForm)
{
SubmitForm.addParameter('x.X_Service',getSelectVal('AclService'));
SubmitForm.addParameter('x.X_Direction',getSelectVal('AclDirection'));
SubmitForm.addParameter('x.X_StartIpAddr',getValue('AclIPStart'));
SubmitForm.addParameter('x.X_EndIpAddr',getValue('AclIPEnd'));
if( selctIndex == -1 )
{
SubmitForm.setAction('addcfg.cgi?x=InternetGatewayDevice.ACL'
+ '&RequestFile=/html/security/acl.asp');
}
else
{
SubmitForm.setAction('setcfg.cgi?x='+AclRule[selctIndex].domain
+ '&RequestFile=/html/security/acl.asp');
}
top.aplyLine = selctIndex;
setDisable('btnApply',1);
}
function setCtlDisplay(record)
{
with(document.forms[0])
{
setSelect('AclService',record.Service);
setSelect('AclDirection',record.Direction);
setText('AclIPStart',record.IPStart);
setText('AclIPEnd',record.IPEnd);
}
}
function setControl(index)
{
var record;
selctIndex = index;
if (index == -1)
{
record = new stACL('','FIRST_APP', '', '', '');
}
else
{
record = AclRule[index];
}
setCtlDisplay(record);
}
function AclSelect(sName)
{
}
function LoadFrame()
{
top.errStrStr = '';
top.errIdCtrlStr = '';
with(document.forms[0])
{
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_0');
}
}
}
function clickRemove()
{
RemoveAclRule('/html/security/acl.asp');
}
function RemoveAclRule(url)
{
if (remove_state == true)
return;
var rml = getElement('rml');
if (rml == null)
{
alert(eval("gVarAlertNot_Add"))
return;
}
var SubmitForm = new webSubmitForm();
var cnt = 0;
with (document.forms[0])
{
var removehttpnum = RemoveHttpNum(rml);
var httptotal = GetHttpTotalNum();
if(removehttpnum > 0)
{
if(confirm(eval("gVarAclHttpWarning"),'')==false)
{
return false;
}
}
if (rml.length > 0)
{
for (var i = 0; i < rml.length; i++)
{
if (rml[i].checked == true)
{
SubmitForm.addParameter(rml[i].value,'');
cnt++;
}
}
}
else if (rml.checked == true)
{
SubmitForm.addParameter(rml.value,'');
cnt++;
}
if(cnt==0)
{
alert(eval("gVarAlertNot_Select"))
return;
}
}
setDisable('btnApply',1);
SubmitForm.setAction('del.cgi?RequestFile=' + url);
remove_state = true;
SubmitForm.submit();
}
function RemoveHttpNum(rml)
{
var num=0 ;
if(rml.length>0)
{
for (var i = 0; i < rml.length; i++)
{
if(rml[i].checked==true)
{
for (var j = 0; j < AclRule.length && null != AclRule[j]; j++)
{
if(rml[i].value ==AclRule[j].domain)
{
if(AclRule[j].Service == 'HTTP')
num++;
}
}
}
}
}
else
{
for (j = 0; j < AclRule.length && null != AclRule[j]; j++)
{
if(rml.value ==AclRule[j].domain)
{
if(AclRule[j].Service == 'HTTP')
num++;
}
}
}
return num;
}
function GetHttpTotalNum()
{
var num = 0;
for(var i=0; i < AclRule.length && null != AclRule[i];i++)
{
if(AclRule[i].Service=='HTTP')
{
num++;
}
}
return num;
}
function FW_ACL_CfgHeader()
{
writeTabCfgHeaderEx(eval("gVarAcl"),'Acl',"90%");
}
</script>

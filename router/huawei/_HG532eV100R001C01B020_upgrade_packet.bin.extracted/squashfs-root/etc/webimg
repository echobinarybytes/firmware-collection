<html>
<head>
<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/util.js"></script>
<script language="JavaScript" src="/lang/dhcp6s.res"></script>
<title>DHCP6S Configure</title>
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" type="text/javascript">
function dhcpv6cnst(domain,Enable,AllocateWan,AssConn,Prefix,PrefixLength,Preferredlifetime,Validlifetime,DnsSrver,DomainName)
{
this.domain            = domain;
this.allocateWan       = AllocateWan;
this.AssConn           = AssConn;
this.Prefix            = Prefix;
this.PrefixLength      = PrefixLength;
this.Enable            = Enable;
this.DomainName        = DomainName;
this.Preferredlifetime = Preferredlifetime;
this.Validlifetime     = Validlifetime;
var dns           = DnsSrver.split(',');
this.DnsSrvOne    = dns[0];
if ( dns.length > 1 )
{
this.DnsSrvTwo = dns[1];
}
else
{
this.DnsSrvTwo = '';
}
}
var dhcpv6st = <%webGetCfgEntry(InternetGatewayDevice.LANDevice.{i}.LANHostConfigManagement.X_DHCPv6,DHCPServerEnable|UseAllocatedWAN|AssociatedConnection|Prefix|PrefixLength|Preferredlifetime|Validlifetime|DNSServer|DomainName,dhcpv6cnst);%>;
var dhcpEnbl = dhcpv6st[0].Enable;
var UseAllc  = dhcpv6st[0].allocateWan;
var AssCon   = dhcpv6st[0].AssConn;
var DnsOne   = dhcpv6st[0].DnsSrvOne;
var DnsTwo   = dhcpv6st[0].DnsSrvTwo;
var Prefix   = dhcpv6st[0].Prefix;
var Prelen   = dhcpv6st[0].PrefixLength;
var Domana   = dhcpv6st[0].DomainName;
var Time1    = dhcpv6st[0].Preferredlifetime;
var Time2    = dhcpv6st[0].Validlifetime;
var changeAddrFlag = false;
function disableDhcpSrv()
{
with ( document.forms[0] )
{
setCheck("dhcpSrvType" ,0);
setDisplay('AssConnection',0);
setDisplay('DHCP6SPrefixCfg',0);
setDisplay('DHCP6SGenerateIPPoolMode', 0);
setDisable('dhcpPrefix',1);
setDisable('PrefixLen' ,1);
setDisable('t1',1);
setDisable('t2',1);
setDisable('DnsSrvOne',1);
setDisable('DnsSrvTwo',1);
setDisable('DomainNameText'    ,1);
setDisable('NameDomainNameText',1);
}
}
function enableDhcpSrv(formLoad)
{
with ( document.forms[0] )
{
setCheck("dhcpSrvType",1);
setDisplay('AssConnection',1);
if ( "Normal" == UseWan.value )
{
setDisplay('DHCP6SPrefixCfg', 1);
}
setDisplay('DHCP6SGenerateIPPoolMode', 0);
setDisable('dhcpPrefix',0);
setDisable('PrefixLen' ,0);
setDisable('t1',0);
setDisable('t2',0);
setDisable('DnsSrvOne',0);
setDisable('DnsSrvTwo',0);
setDisable('DomainNameText'    ,1);
setDisable('NameDomainNameText',1);
}
}
function typeClick()
{
with ( document.forms[0] )
{
if (getCheckVal('dhcpSrvType') == 0)
{
disableDhcpSrv();
}
else
{
enableDhcpSrv(1);
}
}
}
function UseWanChange()
{
with (document.forms[0])
{
if ( "Normal" == UseWan.value && 1 == getCheckVal('dhcpSrvType') )
{
setDisplay('DHCP6SPrefixCfg', 1);
}
else
{
setDisplay('DHCP6SPrefixCfg', 0);
}
}
}
function EnablePrefixParameter()
{
with (document.forms[0])
{
if ( 0 != getCheckVal('dhcpSrvType'))
{
setDisable('dhcpPrefix',0);
setDisable('NamePrefix',0);
setDisable('PrefixLen'    ,0);
setDisable('NamePrefixLen',0);
}
}
}
function DisablePrefixParameter()
{
with (document.forms[0])
{
if ( 0 != getCheckVal('dhcpSrvType'))
{
setDisable('dhcpPrefix',1);
setDisable('NamePrefix',1);
setDisable('PrefixLen'    ,1);
setDisable('NamePrefixLen',1);
}
}
}
function EnableIPPoolParameter()
{
with (document.forms[0])
{
}
}
function DisableIPPoolParameter()
{
with (document.forms[0])
{
}
}
function IPPoolGenerateOperation()
{
with (document.forms[0])
{
if ( "UsePrefix" == UsePrefixOrIPPool.value )
{
DisableIPPoolParameter();
EnablePrefixParameter();
}
else if ( "UseIPPool" == UsePrefixOrIPPool.value )
{
DisablePrefixParameter();
EnableIPPoolParameter();
}
else
{
DisablePrefixParameter();
DisableIPPoolParameter();
}
}
}
function LoadFrame()
{
var i        = GetmenuName("dhcp6s.asp");
var menuName = UpdateMenuByFileName(i);
var menuItem = top.g_oMenuTree.curMenuItem;
var cookie   = document.cookie;
ReLocation(cookie,menuName);
with ( document.forms[0] )
{
if ( 1 == dhcpEnbl )
{
setCheck("dhcpSrvType",1);
setDisplay('AssConnection',1);
}
else
{
setCheck("dhcpSrvType",0);
setDisplay('AssConnection',0);
}
UseWan.value = UseAllc;
UseWanChange();
if ( 0 != Prefix )
{
dhcpPrefix.value = Prefix;
}
if ( 0 != Prelen )
{
PrefixLen.value  = Prelen;
}
if ( 0 != Time1 )
{
t1.value = Time1;
}
if ( 0 != Time2 )
{
t2.value = Time2;
}
DnsSrvOne.value = DnsOne;
DnsSrvTwo.value = DnsTwo;
DomainNameText.value = Domana;
}
}
function CheckPrefix(Prefix,type)
{
var IpAddress1 = Prefix.split(':');
var IpAddress3 = Prefix;
if ( 1 == type && '' == IpAddress3)
{
return true;
}
if ( true != isUnicastIpv6Address(IpAddress3) )
{
return false;
}
switch ( type )
{
case 0 :
if ( 15 == parseInt(IpAddress1[0].substring(0, 1),16) )
{
switch ( parseInt(IpAddress1[0].substring(1, 2),16) )
{
case 15 :
case 13 :
return false;
case 14 :
if ( 8 <= parseInt(IpAddress1[0].substring(2, 3),16) && 12 > parseInt(IpAddress1[0].substring(2, 3),16) )
{
return false;
}
break;
default:
break;
}
}
var addrParts = Prefix.split('::');
if(addrParts[0] == '')
{
return false;
}
break;
case 1 :
if ( 15 == parseInt(IpAddress1[0].substring(0, 1),16) )
{
switch ( parseInt(IpAddress1[0].substring(1, 2),16) )
{
case 15 :
return false;
default:
break;
}
}
var addrParts = Prefix.split('::');
if(addrParts[0] == '')
{
return false;
}
break;
default:
return false;
}
return true;
}
function CheckLength(Length)
{
var TemLen = parseInt(Length);
if ( true == isNaN(TemLen) || TemLen > 64 || TemLen < 16)
{
return false;
}
return true;
}
function CheckHex(Hex)
{
var TemHex  = Hex;
var Loopj   = 0;
var Tempi   = 0;
var Tempj   = 1;
var section = 0;
var num     = 0;
for ( Loopj = 0 ; Loopj < TemHex.length ; Loopj++ )
{
section = TemHex.substring(Tempi, Tempj);
num = parseInt(section,16);
if ( true == isNaN(num) )
{
return false;
}
Tempi++;
Tempj++;
}
return true;
}
function CheckTime(Time1,Time2)
{
var TemTime1 = Time1;
var TemTime2 = Time2;
if ( TemTime1.length > 10 || '' == TemTime1 )
{
return 1;
}
if ( TemTime2.length > 10 || '' == TemTime2 )
{
return 2;
}
if ( true != isPlusInteger(TemTime1))
{
return 1;
}
if ( true != isPlusInteger(TemTime2))
{
return 2;
}
TemTime1 = parseInt(Time1);
TemTime2 = parseInt(Time2);
if ( TemTime1 > 4294967295 || TemTime1 < 60 )
{
return 1;
}
if ( TemTime2 > 4294967295 || TemTime2 < 60 )
{
return 2;
}
if ( TemTime2 < TemTime1 )
{
return 3;
}
return true;
}
function CheckForm(type)
{
with ( document.forms[0] )
{
switch(type)
{
case 0:
{
if ( 1 != getCheckVal('dhcpSrvType') || "Normal" != UseWan.value )
{
return true;
}
if ( "" != dhcpPrefix.value )
{
if ( true != CheckPrefix(dhcpPrefix.value,0) )
{
AddErrInfo(eval("gErrStr0") + getSafeString(dhcpPrefix.value) + eval("gErrStr1"), 'dhcpPrefix');
return false;
}
}
if ( "" != dhcpPrefix.value || ("" != PrefixLen.value && 0 != PrefixLen.value) )
{
if ( true != CheckLength(PrefixLen.value) )
{
AddErrInfo(eval("gErrStr2") + getSafeString(PrefixLen.value) + eval("gErrStr3"), 'PrefixLen');
return false;
}
}
if ( "" != dhcpPrefix.value || ("" != t1.value && 0 != t1.value) || ("" != t2.value && 0 != t2.value) )
{
switch ( CheckTime(t1.value,t2.value) )
{
case 1 :
{
AddErrInfo(eval("gErrStr6") + getSafeString(t1.value) + eval("gErrStr5"), 't1');
return false;
}
case 2 :
{
AddErrInfo(eval("gErrStr4") + getSafeString(t2.value) + eval("gErrStr5"), 't2');
return false;
}
case 3 :
{
AddErrInfo(eval("gErrStr6") + getSafeString(t2.value) + eval("gErrStr7") + eval("gErrStr8") + t1.value, 't2');
return false;
}
default:
break;
}
}
if ( "" == DnsSrvOne.value && "" != DnsSrvTwo.value )
{
AddErrInfo(eval("gErrStr9"), 'DnsSrvOne');
return false;
}
if ( true != CheckPrefix(DnsSrvOne.value,1) )
{
AddErrInfo(eval("gVarPrimaryDNSServ") + getSafeString(DnsSrvOne.value) + eval("gErrStr1"), 'DnsSrvOne');
return false;
}
if ( true != CheckPrefix(DnsSrvTwo.value,1) )
{
AddErrInfo(eval("gVarSecondaryDNSSe ") + getSafeString(DnsSrvTwo.value) + eval("gErrStr1"), 'DnsSrvTwo');
return false;
}
break;
}
case 1:
{
}
break;
case 2:
{
}
break;
default:
break;
}
}
return true;
}
function AddSubmitParam(Form,type)
{
with (document.forms[0])
{
switch(type)
{
case 0:
{
Form.addParameter('y.DHCPServerEnable',getCheckVal('dhcpSrvType'));
if ( 1 == getCheckVal('dhcpSrvType') )
{
Form.addParameter('y.UseAllocatedWAN',getValue('UseWan'));
}
if ( 1 == getCheckVal('dhcpSrvType') && "Normal" == UseWan.value )
{
Form.addParameter('y.Prefix',getValue('dhcpPrefix'));
if ( '' == getValue('PrefixLen') )
{
Form.addParameter('y.PrefixLength',0);
}
else
{
Form.addParameter('y.PrefixLength',getValue('PrefixLen'));
}
if ( '' == getValue('t1') )
{
Form.addParameter('y.Preferredlifetime',0);
}
else
{
Form.addParameter('y.Preferredlifetime',getValue('t1'));
}
if ( '' == getValue('t2') )
{
Form.addParameter('y.Validlifetime',0);
}
else
{
Form.addParameter('y.Validlifetime',getValue('t2'));
}
var DnsStr = '';
DnsStr = '';
if (DnsSrvOne.value != '')
{
DnsStr = getValue('DnsSrvOne');
if (DnsSrvTwo.value != '')
{
DnsStr = getValue('DnsSrvOne') + ',' + getValue('DnsSrvTwo');
}
}
else if (DnsSrvTwo.value != '') 
{
DnsStr = getValue('DnsSrvTwo');
}
Form.addParameter('y.DNSServer',DnsStr);
Form.addParameter('y.DomainName',getValue('DomainNameText'));
}
var RequestFile = '/html/ntwkall/dhcp6s.asp';
var url = 'setcfg.cgi?'
+ 'y=InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.X_DHCPv6'
+ '&RequestFile=' + RequestFile;
Form.setAction(url);
break;
}
case 1:
break;
case 2:
break;
default:
return;
}
}
}

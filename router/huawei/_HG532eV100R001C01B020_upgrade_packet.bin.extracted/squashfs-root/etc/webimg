<form id="Configure">
<script language="JavaScript" type="text/javascript">
writeTabCfgHeaderEx(eval("gVarStaticDhcp"),'Static DHCP',"90%");
</script>
<table class="tabInfo">
<tr class="trTabHead">
<td><script language="JavaScript" type="text/javascript">document.write(eval("gVarMacAddrTitle"));</script></td>
<td><script language="JavaScript" type="text/javascript">document.write(eval("gVarIPAddrTitle"));</script></td>
<td><script language="JavaScript" type="text/javascript">document.write(eval("gVarStatusTitle"));</script></td>
<td><script language="JavaScript" type="text/javascript">document.write(eval("gVarPubRemove"));</script></td>
</tr>
<script language="JavaScript" type="text/javascript">
for (var i = 0; i < StaticAddr.length - 1; i++)
{
document.write('<TR id=record_' + i
+' class="trTabContent" onclick="selectLine(this.id);">');
document.write('<TD align="center">' + StaticAddr[i].Chaddr + '</TD>');
document.write('<TD align="center">' + StaticAddr[i].Yiaddr + '</TD>');
if (StaticAddr[i].Enable == 1)
{
document.write('<TD align="center">' + eval("gVarPubsEnable") + '</TD>');
}
else
{
document.write('<TD align="center">' + eval("gVarPubDisable") + '</TD>');
}
document.write('<TD align="center"><input type="checkbox"' + 'name="rml"'
+ ' value="' + StaticAddr[i].domain + '"></td>\n');
document.write('</TR>');
}
</script>
</table>

function stDosAttack(domain,SynFlood,IcmpFlood,ArpAttack)
{
this.domain = domain;
this.SynFlood = SynFlood;
this.IcmpFlood = IcmpFlood;
this.ArpAttack = ArpAttack;
}
var DosAttackData = <%webGetCfgEntry(InternetGatewayDevice.DosAttack,SynFlooding|IcmpFlooding|ArpAttack,stDosAttack);%>
function DosAttackApply()
{
with(document.forms[0])
{
var SubmitForm = new webSubmitForm();
SubmitForm.addParameter('x.SynFlooding',getCheckVal('SynFlood'));
SubmitForm.addParameter('x.IcmpFlooding',getCheckVal('IcmpFlood'));
SubmitForm.addParameter('x.ArpAttack',getCheckVal('ArpAttack'));
SubmitForm.setAction('setcfg.cgi?x=' + DosAttackData[0].domain
+ '&RequestFile=/html/security/dosattack.asp');
SubmitForm.submit();
}
setDisable('btnApply',1);
}
function LoadFrame()
{
top.errStrStr = '';
top.errIdCtrlStr = '';
with(document.forms[0])
{
setDisplay('DivDosAttack',1);
setDisplay('DivDosApply',1);
setCheck("SynFlood",DosAttackData[0].SynFlood);
setCheck("IcmpFlood",DosAttackData[0].IcmpFlood);
setCheck("ArpAttack",DosAttackData[0].ArpAttack);
}
}

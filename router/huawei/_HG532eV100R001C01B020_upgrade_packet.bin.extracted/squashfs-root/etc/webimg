function getAfterBurnerIndex(afterburner)
{
var ret;
if (afterburner == "auto")
ret = 1;
else
ret = 0;
return ret;
}
function getTpcDbIndex(val)
{
var ret;
if ( val == "0" )
ret = 0;
else if ( val == "2" )
ret = 1;
else if ( val == "3" )
ret = 2;
else if ( val == "4" )
ret = 3;
else ret = 0;
return ret;
}
function ChannelChange()
{
}
function getBandIndex(b)
{
var ret;
if ( b == '1' )
ret = 1;
else
ret = 0;
return ret;
}
function wl_afterburner_options(OnPageLoad)
{
}
function wl_recalc(OnPageLoad)
{
with ( document.forms[0] )
{
wl_afterburner_options(OnPageLoad);
}
}
function updateCurChannel()
{
}
function LoadFrame()
{
var ssidindex = getSelectVal('wlSsidIdx');
ssidIdx = parseInt(ssidindex, 10);
var i = GetmenuName("wlan.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
with ( document.forms[0] )
{
if (top.curUserType != top.sptUserType)
{
setDisplay('11N', 0);
setDisplay('div_gnMode', 0);
}
setDisplay('nauthen', 1);
setCheck('wlEnbl', enbl);
setCheck('wlHide', Wlan[ssidIdx].wlHide);
if (phy == "g")
{
setSelect('wlgnMode', Wlan[ssidIdx].mode);
}
setSelect('bwControl', Wlan[ssidIdx].BWControl);
setSelect('giControl', Wlan[ssidIdx].GIControl);
setCountry();
ChangeCountry();
setText('X_AssociateDeviceNum', Wlan[ssidIdx].DeviceNum);
gModeChange();
setText('wlTxPwr', Wlan[ssidIdx].power);
setCheck('wlHide', Wlan[ssidIdx].wlHide);
setCheck('enableSsid', Wlan[ssidIdx].enable);
setText('wlSsid', Wlan[ssidIdx].ssid);
if(ssidIdx == 0)
{
setDisable('enableSsid', 1);
}
else
{
setDisable('enableSsid', 0);
}
if(getSelectVal('wlgnMode') == "n")
{
setDisable('wlQos',1);
setCheck('wlQos',1);
}
else
{
setDisable('wlQos',0);
setCheck('wlQos',Wlan[ssidIdx].WMMEnable);
}
setCheck('wlApiso', Wlan[ssidIdx].APISOControl);
if(Wlan[ssidIdx].BeaconType == 'Basic' && Wlan[ssidIdx].Encryption == 'None')
{
setSelect('wlnAuthMode', 'Basic_None');
}
else if(Wlan[ssidIdx].BeaconType == 'Basic' && Wlan[ssidIdx].Encryption == 'WEPEncryption')
{
setSelect('wlnAuthMode', 'Basic_WEPEncryption');
}
else
{
setSelect('wlnAuthMode', Wlan[ssidIdx].BeaconType);
}
for ( i = 0; i < 4; i++)
{
wlKeys[i].value = keys[(ssidIdx * 4) + i][1];
}
setDisplay('wlanCfg', enbl);
setSelect('wlKeyBit', parseInt(Wlan[ssidIdx].EncypBit)+24);
wlKeyBitChange();
setText('wlWpaPsk', wpaPskKey[ssidIdx][1]);
var authMode = getSelectVal('wlnAuthMode');
setCheck("wpsControl", WPSInfo.WPSControl);
setDisable("wpsControl", 0);
setDisplay('mode', 1);
setDisplay('pinVal', 1);
setSelect("wpsMode", WPSInfo.WPSMode);
if (getSelectVal("wpsMode") == 'ap-pin')
{
setText("pinValue", Wlan[ssidIdx].PinValue);
}
if (getSelectVal("wpsMode") == 'ap-appin')
{
setText("appinValue",Wlan[ssidIdx].APPinValue);
}
}
wpsContChange();
authModeChange(1);
var mode = getValue('wlgnMode');
if(mode == 'n')
{
authModeChange(2);
}
var ishide = getCheckVal('wlHide');
if (ishide == '1')
{
setCheck("wpsControl", 0);
setDisplay("wps", 0);
}
}
function setCountry()
{
var ssidindex = getSelectVal('wlSsidIdx');
ssidIdx = parseInt(ssidindex,10);
var ret = setSelect('RegulatoryDomain',Wlan[ssidIdx].Country);
if (ret == false)
{
setSelect('RegulatoryDomain','Other');
}
}
function repeatChar(char,times)
{
var string = '';
var i = 0;
for(i = 0; i < times; i++)
{
string += char;
}
return string;
}
function AddSubmitParam(Form,type)
{
var url = '';
var temp = '';
with (document.forms[0])
{
Form.usingPrefix('y');
var phymode = getSelectVal('wlgnMode');
Form.addParameter('Standard',phymode);
if(phymode == 'b' || phymode == 'g' || phymode== 'b/g')
{
Form.addParameter('MaxBitRate',getSelectVal('wlRate'));
Form.addParameter('X_Wlan11NHtMcs','33');
}
else
{
Form.addParameter('MaxBitRate','Auto');
Form.addParameter('X_Wlan11NHtMcs',getSelectVal('wlMcs'));
}
Form.addParameter('Enable',getCheckVal('enableSsid'));
var channel= getValue('wlChannel');
if (channel == '0')
{
Form.addParameter('AutoChannelEnable','1');
Form.addParameter('Channel','6');
}
else
{
Form.addParameter('AutoChannelEnable','0');
Form.addParameter('Channel',getSelectVal('wlChannel'));
}
Form.addParameter('SSIDAdvertisementEnabled',(1-parseInt(getCheckVal('wlHide'))));
Form.addParameter('X_WlanIsolateControl',getCheckVal('wlApiso'));
Form.addParameter('WMMEnable',getCheckVal('wlQos'));
Form.addParameter('X_Wlan11NBWControl',getSelectVal('bwControl'));
Form.addParameter('X_Wlan11NGIControl',getSelectVal('giControl'));
var ssid = getValue('wlSsid');
Form.addParameter('SSID',ssid);
var country = getSelectVal('RegulatoryDomain');
if (country == 'Other')
{
country = getValue('countryName');
}
Form.addParameter('RegulatoryDomain',country);
var deviceNum = getValue('X_AssociateDeviceNum');
Form.addParameter('X_AssociateDeviceNum',deviceNum);
var power = getValue('wlTxPwr');
Form.addParameter('X_PowerValue',power);
var AuthMode = getSelectVal('wlnAuthMode');
if (AuthMode == 'Basic_None')
{
Form.addParameter('BeaconType', 'Basic');
}
else if (AuthMode == 'Basic_WEPEncryption')
{
Form.addParameter('BeaconType', 'Basic');
}
else
{
Form.addParameter('BeaconType', AuthMode);
}
Form.endPrefix();
if (AuthMode == 'Basic_None' || AuthMode == 'Basic_WEPEncryption')
{
var wepauthmode = getSelectVal('WepAuthMode');
if (AuthMode == 'Basic_WEPEncryption')
{
var KeyBit = getSelectVal('wlKeyBit');
var index = parseInt(getSelectVal('wlKeyIndex'));
var Keys = getElement('wlKeys');
Form.addParameter('y.WEPEncryptionLevel', (KeyBit-24)+'-bit');
Form.addParameter('y.WEPKeyIndex', index);
var ssidindex = getSelectVal('wlSsidIdx');
ssidIdx = parseInt(ssidindex,10);
if(Keys[index-1].value == '@1GV)Z<!')
{
}
else
{
for (l = 1; l <= 4; l++)
{
if(index == l)
{
Form.addParameter('k' +'.WEPKey.' + l +'.WEPKey',  Keys[l-1].value);
}
else
{
if((KeyBit - 24) == Wlan[ssidIdx].EncypBit)
{
}
else
{
if(KeyBit == 64)
{
Form.addParameter('k' +'.WEPKey.' + l +'.WEPKey',  repeatChar(l, 5));
}
else
{
Form.addParameter('k' +'.WEPKey.' + l +'.WEPKey',  repeatChar(l, 13));
}
}
}
}
}
}
if (AuthMode == 'Basic_None')
{
Form.addParameter('y.BasicAuthenticationMode', 'None');
Form.addParameter('y.BasicEncryptionModes', 'None');
}
else
{
Form.addParameter('y.BasicAuthenticationMode', wepauthmode);
Form.addParameter('y.BasicEncryptionModes', 'WEPEncryption');
}
}
if (AuthMode == 'WPA' || AuthMode == '11i' || AuthMode == 'WPAand11i')
{
var ssidindex = getSelectVal('wlSsidIdx');
ssidIdx = parseInt(ssidindex,10);
if (ssidIdx == 0)
{
Form.addParameter('z.Enable', getCheckVal('wpsControl'));
Form.addParameter('z.X_WPSMode', getSelectVal('wpsMode'));
if(getSelectVal("wpsMode") == 'ap-pin')
{
Form.addParameter('z.PinPhrase', getValue('pinValue'));
}
if(getSelectVal("wpsMode") == 'ap-appin')
{
Form.addParameter('z.DevicePassword', getValue('appinValue'));
}
}
value = getValue('wlWpaPsk');
if (value != '@1GV)Z<!')
{
Form.addParameter('k.PreSharedKey', value);
}
if (AuthMode == 'WPA')
{
Form.addParameter('y.WPAEncryptionModes', getSelectVal('wlWpa'));
}
else if (AuthMode == '11i')
{
Form.addParameter('y.IEEE11iEncryptionModes', getSelectVal('wlWpa'));
}
else  if (AuthMode == 'WPAand11i')
{
Form.addParameter('y.X_MixedEncryptionModes',getSelectVal('wlWpa'));
}
}
else
{
Form.addParameter('z.Enable', '0');
}
}
var ssidindex = getSelectVal('wlSsidIdx');
ssidIdx = parseInt(ssidindex, 10);
enable = getCheckVal('wlEnbl');
Form.addParameter('x.X_WLANEnable', enable);
var wlandomain = 'InternetGatewayDevice.LANDevice.1.WLANConfiguration.'
+(ssidIdx + 1);
var wpsdomain = 'InternetGatewayDevice.LANDevice.1.WLANConfiguration.1.WPS';
AuthMode = getSelectVal('wlnAuthMode');
if (AuthMode == 'Basic_None' || AuthMode == 'Basic_WEPEncryption')
{
temp = 'InternetGatewayDevice.LANDevice.1.WLANConfiguration.' +(ssidIdx+1);
Form.setAction('setcfg.cgi?x=InternetGatewayDevice.LANDevice.1'
+ '&y=' + wlandomain + '&k=' + temp
+ '&RequestFile=/html/ntwkall/wlan.asp');
}
else if (AuthMode == 'WPA' || AuthMode == '11i' || AuthMode == 'WPAand11i')
{
temp = 'InternetGatewayDevice.LANDevice.1.WLANConfiguration.' +(ssidIdx+1) +'.PreSharedKey.1';
if(ssidIdx == 0)
{
Form.setAction('setcfg.cgi?x=InternetGatewayDevice.LANDevice.1'
+ '&y=' + wlandomain + '&k=' + temp + '&z=' + wpsdomain
+ '&RequestFile=/html/ntwkall/wlan.asp');
}
else
{
Form.setAction('setcfg.cgi?x=InternetGatewayDevice.LANDevice.1'
+ '&y=' + wlandomain + '&k=' + temp
+ '&RequestFile=/html/ntwkall/wlan.asp');
}
}
setDisable('wlEnbl', 1);
setDisable('btnApply',1);
}
function CheckForm(type)
{
with (document.forms[0])
{
var ssid = getValue('wlSsid');
if (ssid == '' )
{
AddErrInfo(eval("gErrStr4"), 'wlSsid');
setCheck('wlEnbl',1);
wlClickEnble(getElement('wlEnbl'));
}
if (ssid.length > 32)
{
AddErrInfo(eval("gErrStr5") + getSafeString(getValue('wlSsid')) + eval("gErrStr6"), 'wlSsid');
setCheck('wlEnbl',1);
wlClickEnble(getElement('wlEnbl'));
}
if (isValidAscii(ssid) != '')
{
AddErrInfo(eval("gErrStr7") + getSafeString(isValidAscii(wlSsid.value)) + eval("gErrStr8"), 'wlSsid');
setCheck('wlEnbl',1);
wlClickEnble(getElement('wlEnbl'));
}
if (ssid.charAt(ssid.length - 1) == ' ')
{
AddErrInfo(eval("gErrStr9"), 'wlSsid');
setCheck('wlEnbl',1);
wlClickEnble(getElement('wlEnbl'));
}
else if(ssid.charAt(0) == ' ')
{
AddErrInfo(eval("gErrStr27"), 'wlSsid');
setCheck('wlEnbl',1);
wlClickEnble(getElement('wlEnbl'));
}
var country = getSelectVal('RegulatoryDomain');
if (country == 'Other')
{
country = getValue('countryName');
if (isValidCfgStr('Regulatory Domain',country,16) == false)
{
AddErrInfo(eval("gErrStr12"), 'countryName');
}
}
var deviceNum = getValue('X_AssociateDeviceNum');
if (isPlusInteger(deviceNum) == false || parseInt(deviceNum) < 1 || parseInt(deviceNum) > 32)
{
AddErrInfo(eval("gErrStr10") + eval("gErrStr11"), 'X_AssociateDeviceNum');
}
var power = getValue('wlTxPwr');
if(isPlusInteger(power) == false || parseInt(power) < 1 || parseInt(power) > 20)
{
AddErrInfo(eval("gErrStr13"), 'wlTxPwr');
setCheck('wlEnbl',1);
wlClickEnble(getElement('wlEnbl'));
}
var AuthMode = getSelectVal('wlnAuthMode');
if (AuthMode == 'Basic_None' || AuthMode == 'Basic_WEPEncryption')
{
if (AuthMode == 'Basic_WEPEncryption')
{
var KeyBit = getSelectVal('wlKeyBit');
var index = parseInt(getSelectVal('wlKeyIndex'));
var Keys = getElement('wlKeys');
var val;
var vKey = 0;
for (vKey = 0; vKey < 4; vKey++)
{
if(vKey == (index-1))
{
val = Keys[vKey].value;
if ( val != '' )
{
if ( val == '@1GV)Z<!' )
{
}
else if ( KeyBit == '128' )
{
if (isValidKey(val, 13) == false )
{
AddErrInfo(eval("gErrStr15"), 'WepKey'+index);
setCheck('wlEnbl',1);
wlClickEnble(getElement('wlEnbl'));
}
}
else
{
if ( isValidKey(val, 5) == false )
{
AddErrInfo(eval("gErrStr17"),  'WepKey'+index);
setCheck('wlEnbl',1);
wlClickEnble(getElement('wlEnbl'));
}
}
}
else
{
AddErrInfo(eval("gErrStr18"),  'WepKey'+index);
setCheck('wlEnbl',1);
wlClickEnble(getElement('wlEnbl'));
}
}
}
}
else
{
if(getSelectVal("wpsMode") == 'ap-pin')
{
var pinVal = getElement('pinValue');
if(getCheckVal('wpsControl') == 1)
{
if(isSafeStringIn(pinVal.value,'0123456789') == false)
{
AddErrInfo(eval("gErrStr19"), 'pinValue');
}
}
}
}
}
else if (AuthMode == 'WPA' || AuthMode == '11i'|| AuthMode == 'WPAand11i')
{
if(getSelectVal("wpsMode") == 'ap-pin')
{
var pinVal = getElement('pinValue');
if(getCheckVal('wpsControl') == 1)
{
if(isSafeStringIn(pinVal.value,'0123456789') == false)
{
AddErrInfo(eval("gErrStr20"), 'pinValue');
}
else if(isvalidpin(pinVal.value) == false)
{
AddErrInfo(eval("gErrStr20"), 'pinValue');
}
}
}
value = getValue('wlWpaPsk');
if (isValidWPAPskKey(value) == false )
{
AddErrInfo(eval("gErrStr21"), 'wlWpaPsk');
setCheck('wlEnbl',1);
wlClickEnble(getElement('wlEnbl'));
}
}
if ((getCheckVal('wlHide')) == 1 && (getCheckVal('wpsControl')) == 1)
{
alert(eval("gErrStr26"));
return false;
}
}
}
function SubmitForm()
{
var Form = new webSubmitForm();
enable = getCheckVal('wlEnbl');
Form.addParameter('x.X_WLANEnable', enable);
Form.setAction('setcfg.cgi?x=InternetGatewayDevice.LANDevice.1'
+ '&RequestFile=/html/ntwkall/wlan.asp');
setDisable('wlEnbl', 1);
Form.submit();
}
function wlClickEnble(wlEnbleCl)
{
if (wlEnbleCl.checked == true)
{
if (state == 1)
{
setDisplay('wlanCfg', 1);
}
}
else
{
setDisplay('wlanCfg', 0);
}
}
function ChangeCountry()
{
var ssidindex = getSelectVal('wlSsidIdx');
ssidIdx = parseInt(ssidindex, 10);
var country = getSelectVal('RegulatoryDomain');
if (country == 'Other')
{
setDisplay('trCountry', 1);
setText('countryName', Wlan[ssidIdx].Country);
}
else
{
setDisplay('trCountry', 0);
}
getElement('wlChannel').length = 0;
loadChannelList(country);
}
function onBandChange()
{
}
function wpsModeChange()
{
with ( document.forms[0])
{
var temp;
temp = getSelectVal("wpsMode");
if (temp == 'ap-pbc')
{
setDisplay("pinVal", 0);
setDisplay("appinVal", 0);
}
else if (temp == 'ap-pin')
{
setDisplay("pinVal", 1);
setDisplay("appinVal", 0);
setText("pinValue",WPSInfo.PinValue);
}
else if (temp == 'ap-appin')
{
setDisplay("pinVal", 0);
setDisplay("appinVal", 1);
setText("appinValue",WPSInfo.APPinValue);
}
}
wpsContChange();
}
function wpsContChange()
{
with ( document.forms[0])
{
var temp = getCheckVal("wpsControl");
if (temp == 0)
{
setDisable("wpsMode", 1);
setDisable("pinValue", 1);
setDisable("appinValue", 1);
}
else
{
setDisable("wpsMode", 0);
setDisable("pinValue", 0);
setDisable("appinValue", 1);
}
}
}
function Changessid()
{
var ssidindex = getSelectVal('wlSsidIdx');
ssidIdx = parseInt(ssidindex,10);
with (document.forms[0])
{
setDisplay('11N', 1);
setDisplay('div_gnMode', 1);
setDisplay('nauthen', 1);
setCheck('wlEnbl', enbl);
setCheck('wlHide', Wlan[ssidIdx].wlHide);
setSelect('wlgnMode', Wlan[ssidIdx].mode);
setSelect('bwControl', Wlan[ssidIdx].BWControl);
setSelect('giControl', Wlan[ssidIdx].GIControl);
setCountry();
ChangeCountry();
setText('X_AssociateDeviceNum', Wlan[ssidIdx].DeviceNum);
gModeChange();
setText('wlTxPwr', Wlan[ssidIdx].power);
setCheck('enableSsid', Wlan[ssidIdx].enable);
setText('wlSsid', Wlan[ssidIdx].ssid);
if(getSelectVal('wlgnMode') == "n")
{
setDisable('wlQos',1);
setCheck('wlQos',1);
}
else
{
setDisable('wlQos',0);
setCheck('wlQos',Wlan[ssidIdx].WMMEnable);
}
setCheck('wlApiso', Wlan[ssidIdx].APISOControl);
if(Wlan[ssidIdx].BeaconType == 'Basic' && Wlan[ssidIdx].Encryption == 'None')
{
setSelect('wlnAuthMode', 'Basic_None');
}
else if(Wlan[ssidIdx].BeaconType == 'Basic' && Wlan[ssidIdx].Encryption == 'WEPEncryption')
{
setSelect('wlnAuthMode', 'Basic_WEPEncryption');
}
else
{
setSelect('wlnAuthMode', Wlan[ssidIdx].BeaconType);
}
for ( i = 0; i < 4; i++)
{
var index = (ssidIdx * 4) + i;
wlKeys[i].value = keys[index][1];
}
setSelect('wlKeyBit', parseInt(Wlan[ssidIdx].EncypBit)+24);
wlKeyBitChange();
setText('wlWpaPsk', wpaPskKey[ssidIdx][1]);
if(ssidIdx == 0)
{
setDisable('enableSsid', 1);
setCheck("wpsControl", WPSInfo.Control);
setDisplay("wps", 1);
setDisable("wpsControl", 0);
setSelect("wpsMode", WPSInfo.WPSMode);
if(getSelectVal("wpsMode") == 'ap-pin')
{
setText("pinValue", WPSInfo.PinValue);
}
if(getSelectVal("wpsMode") == 'ap-appin')
{
setText("appinValue",WPSInfo.APPinValue);
}
wpsModeChange();
}
else
{
setCheck("wpsControl", 0);
setDisplay("wps", 0);
setDisable('enableSsid', 0);
}
}
wpsContChange();
authModeChange(1);
}
function ResetPIN()
{
setText("appinValue",WpsDefaultPin);
}
function GenerateAPPIN()
{
var xmlhttp = CreateXMLHttp();
xmlhttp.onreadystatechange = function()
{
if (xmlhttp.readyState == 4)
{
if (xmlhttp.status == 200)
{
var result = xmlhttp.responseText;
{
setText("appinValue",result);
}
}
}
}
xmlhttp.open('post','GeneratePIN.cgi', true);
xmlhttp.send(null);
}
function hideBroadcast()
{
var ssidindex = getSelectVal('wlSsidIdx');
ssidIdx = parseInt(ssidindex,10);
if(ssidIdx == 0)
{
var hide = getCheckVal('wlHide');
if(hide == '1')
{
setCheck("wpsControl", 0);
setDisplay("wps", 0);
}
else
{
var AuthMode = getSelectVal('wlnAuthMode');
switch ( AuthMode )
{
case '11i':
case 'WPAand11i':
case 'WPA':
setDisplay("wps", 1);
setCheck("wpsControl", 0);
setDisable("wpsControl", 0);
setSelect("wpsMode", WPSInfo.WPSMode);
if(getSelectVal("wpsMode") == 'ap-pin')
{
setText("pinValue", WPSInfo.PinValue);
}
if(getSelectVal("wpsMode") == 'ap-appin')
{
setText("appinValue",WPSInfo.APPinValue);
}
wpsModeChange();
break;
}
}
}
}

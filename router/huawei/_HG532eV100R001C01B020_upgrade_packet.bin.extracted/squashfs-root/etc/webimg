<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/util.js"></script>
<script language="JavaScript" src="/lang/urlfilter.res"></script>
<script language="JavaScript" src="/lang/firewall.res"></script>
<title>URL FILTER</title>
<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" type="text/javascript">
var selctIndex = -1;
var k = 0;
var DisplayIndex = 0;
function stUrlFilter(domain,Url,Status,Priority)
{
this.domain = domain;
this.Url = Url;
this.Status = Status;
this.ulPriority = Priority;
}
var UrlCurrList = <%webGetCfgEntry(InternetGatewayDevice.X_FireWall.UrlFilter.{i},Url|Status|Priority, stUrlFilter);%>
var j = 0;
var UrlSwapData = new Array();
for (i = 1; i < UrlCurrList.length && null != UrlCurrList[i]; i++)
{
if (parseInt(UrlCurrList[i].ulPriority) < parseInt(UrlCurrList[i-1].ulPriority))
{
UrlSwapData[0] = UrlCurrList[i];
j = i - 1;
do
{
UrlCurrList[j+1] = UrlCurrList[j];
j--;
}while((j >= 0)&&(parseInt(UrlSwapData[0].ulPriority) < parseInt(UrlCurrList[j].ulPriority)));
if (j < 0)
{
UrlCurrList[0] = UrlSwapData[0];
}
else
{
UrlCurrList[j+1] = UrlSwapData[0];
}
}
}
var AllDropOrAcceptIndex = -1;
var DisplayNumOneIndex = -1;
for (i = 0; i < UrlCurrList.length && null != UrlCurrList[i]; i ++)
{
if (UrlCurrList[i].Url == "")
{
if (UrlCurrList[i].Status != 0) 
{
AllDropOrAcceptIndex = i;
}
break;
}
}
function isValidUrlName(url)
{
var i=0;
var invalidArray = new Array();
invalidArray[i++] = "www";
invalidArray[i++] = "com";
invalidArray[i++] = "org";
invalidArray[i++] = "net";
invalidArray[i++] = "edu";
invalidArray[i++] = "www.";
invalidArray[i++] = ".com";
invalidArray[i++] = ".org";
invalidArray[i++] = ".net";
invalidArray[i++] = ".edu";
if (url == "")
{
return true;
}
if (isValidAscii(url) != '')
{
return false;
}
for (i = 0; i < url.length; i++)
{
if (url.charAt(i) == '\\')
{
return false;
}
}
if (url.length < 3)
{
return false;
}
for(j=0; j< invalidArray.length; j++)
{
if (url == invalidArray[j])
{
return false;
}
}
return true;
}
function CheckForm(type)
{
with ( document.forms[0])
{
if (urlitem.value.length > 63)
{
AddErrInfo(eval("gErrStr7"), '');
return;
}
if(isValidUrlName(urlitem.value) == false)
{
AddErrInfo(eval("gErrStr8"), 'urlitem');
return;
}
if (selctIndex == -1 && UrlCurrList.length > 32)
{
AddErrInfo(eval("gErrStrLast"), '');
return false;
}
if (isPlusInteger(getElement('PriValue').value) == false)
{
AddErrInfo(eval("gErrStr9"), 'PriValue');
return;
}
if (parseInt(getElement('PriValue').value) != 0
&& parseInt(getElement('PriValue').value) != 255)
{
for(k = 0; k < UrlCurrList.length && null != UrlCurrList[k]; k++)
{
if (parseInt(UrlCurrList[k].ulPriority) == parseInt(getElement('PriValue').value)
&& selctIndex != k)
{
AddErrInfo(eval("gErrStr11") + getElement('PriValue').value + eval("gErrStr12"), 'PriValue');
return;
}
}
}
var str = urlitem.value.toLowerCase();
var i;
if (-1 != (i = str.indexOf("http://")))
{
str = str.substring(i+7);
}
if (str == "")
{
if (parseInt(getElement('PriValue').value) != 255)
{
AddErrInfo(eval("gErrStr13"), 'PriValue');
return;
}
}
else
{
if (getElement('PriValue').value == "" || parseInt(getElement('PriValue').value) < 0 || parseInt(getElement('PriValue').value) > 254)
{
AddErrInfo(eval("gErrStr14"), 'PriValue');
return;
}
}
for(k = 0; k < UrlCurrList.length && null != UrlCurrList[k]; k++)
{
if(UrlCurrList[k].Url.toLowerCase() == urlitem.value.toLowerCase() && selctIndex != k)
{
AddErrInfo(eval("gErrStr15"), '');
return;
}
}
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
var url = getValue('urlitem').toLowerCase();
SubmitForm.addParameter('x.Url',url);
SubmitForm.addParameter('x.Priority',getValue('PriValue'));
SubmitForm.addParameter('x.Status',getValue('UrlStatus'));
top.aplyLine = selctIndex;
if ( selctIndex == -1 )
{
SubmitForm.setAction('addcfg.cgi?x=InternetGatewayDevice.X_FireWall.UrlFilter'
+ '&RequestFile=/html/security/urlfilter.asp');
}
else
{
SubmitForm.setAction('setcfg.cgi?x='+UrlCurrList[selctIndex].domain
+'&RequestFile=/html/security/urlfilter.asp');
}
setDisable('btnApply',1);
}
function LoadFrame()
{
top.errStrStr = '';
top.errIdCtrlStr = '';
with(document.forms[0])
{
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_'+DisplayNumOneIndex);
}
}
}
function setCtlDisplay(record)
{
setText('urlitem',record.Url);
setSelect('UrlStatus',record.Status);
setText('PriValue',record.ulPriority);
}
function setControl(index)
{
var record;
selctIndex = index;
if (index == -1)
{
record = new stUrlFilter('','','0','0');
}
else
{
record = UrlCurrList[index];
}
setDisable('urlitem', 0);
setDisable('UrlStatus', 0);
setDisable('PriValue', 0);
setCtlDisplay(record);
}
function clickRemove()
{
removeInst('/html/security/urlfilter.asp');
}
function ShowUrl(Url)
{
if (Url == "")
{
return "--";
}
return getSafeString(Url);
}
function ShowUrlStatus(Status)
{
if (Status == 0)
{
return eval("gVarDisable");
}
else if (Status == 1)
{
return eval("gVarAccept");
}
else if (Status == 2)
{
return eval("gVarDeny");
}
}
function FW_URL_CfgHeader()
{
writeTabCfgHeaderEx(eval("gVarUrlFilter"),'Url Filter',"90%");
}
</script>

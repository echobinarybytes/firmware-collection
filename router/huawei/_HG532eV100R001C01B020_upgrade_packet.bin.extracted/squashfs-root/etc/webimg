<script language="JavaScript" type="text/javascript">
var selctIndex = -1;
function stDsl(domain,LinkType)
{
this.domain = domain;
var list = domain.split('.');
this.key = 	'.' + list[4] + '.';
this.LinkType = LinkType;
}
function stWanInfo(domain, Name, CntType,ServiceList,Enable, ipv6enable)
{
this.domain = domain;
var list = domain.split('.');
this.key = 	'.' + list[4] + '.';
this.Name = Name;
this.CntType = CntType;
this.LinkType = 'EoA';
this.ServiceList = ServiceList;
this.Enable = getBoolValue(Enable);
this.ipv6enable = ipv6enable;
}
var WanIPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Name|ConnectionType|X_ServiceList|Enable|X_IPv6Enable,stWanInfo);%>;
var WanPPPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|ConnectionType|X_ServiceList|Enable|X_IPv6Enable,stWanInfo);%>;
var WanInfo = new Array();
for (i = 0; i < WanIPInfo.length-1; i++)
{
WanInfo[i] = WanIPInfo[i];
}
for (j = 0; j < WanPPPInfo.length-1; i++,j++)
{
WanInfo[i] =  WanPPPInfo[j];
}
var host = <%webGetCfgArray(InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.IPInterface.1,IPInterfaceIPAddress|IPInterfaceSubnetMask);%>;
var IpAddress = host[0][1];
var SubnetMask = host[0][2];
function ShowIfName(val)
{
for (var i = 0; i < WanInfo.length; i++)
{
if (WanInfo[i].domain == val)
{
return WanInfo[i].Name;
}
else if ('br0' == val)
{
return 'br0';
}
}
return '&nbsp;';
}
function stStaticRoute(Domain, Enable, DestIPAddress, PrefixLength, GatewayIPAddress, Interface, Status)
{
this.Domain = Domain;
this.enable = Enable;
this.DestIPAddress = DestIPAddress;
this.PrefixLength = PrefixLength;
this.GatewayIPAddress = GatewayIPAddress;
this.Status = Status;
this.Interface = Interface;
}
var StaticRoute = <%webGetCfgEntry(InternetGatewayDevice.Layer3Forwarding.X_IPv6Forwarding.{i},Enable|DestIPAddress|PrefixLength|GatewayIPAddress|Interface|Status,stStaticRoute);%>;
function LoadFrame()
{
var i = GetmenuName("route6.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_0');
}
}
function AddSubmitParam(SubmitForm,type)
{
}
function WriteOption()
{
for (i = 0; i < WanInfo.length; i++)
{
if ((WanInfo[i].CntType == 'IP_Routed')
&& ((WanInfo[i].ServiceList.indexOf('INTERNET') >= 0) || (WanInfo[i].ServiceList.indexOf('OTHER') >= 0))
&& (WanInfo[i].ipv6enable != 0))
{
document.write('<option id="wan_' + i + '" value="' + WanInfo[i].domain + '">'
+ WanInfo[i].Name + '</option>');
}
}
}
function btnClear()
{
with ( document.forms[0] )
{
DestIPAddress.value = '';
DestSubnetMask.value = '';
GatewayIPAddress.value = '';
}
}
function clickRemove()
{
removeInst('/html/network/route6.asp');
}
function CheckForm(type)
{
var i = 0;
if ((-1 == selctIndex) && (StaticRoute.length >= 33))
{
AddErrInfo(eval("gErrStr14"), 'DestIPAddress');
return false;;
}
for (i = 0; i < StaticRoute.length - 1; i++)
{
if ((StaticRoute[i].DestIPAddress == getValue('DestIPAddress'))
&& (StaticRoute[i].PrefixLength == getValue('prefixlength'))
&& (selctIndex != i) )
{
AddErrInfo(eval("gErrStr0"), 'DestIPAddress');
return false;;
}
}
with ( document.forms[0] )
{
var DestIp = DestIPAddress.value;
if (DestIp == '')
{
AddErrInfo(eval("gErrStr1") + getSafeString(DestIPAddress.value) + eval("gErrStr2"), 'DestIPAddress');
return false;
}
if (isUnicastIpv6Address(DestIp) == false)
{
AddErrInfo(eval("gErrStr1") + getSafeString(DestIPAddress.value) + eval("gErrStr2"), 'DestIPAddress');
return false;
}
if ((prefixlength.value == '')
|| (!isInteger(prefixlength.value))
|| (parseInt(prefixlength.value) > 128)
|| (parseInt(prefixlength.value) <= 0))
{
AddErrInfo(eval("gErrStr4") + getSafeString(prefixlength.value) + eval("gErrStr5"), 'prefixlength');
return false;
}
if ('' == getSelectVal('Interface'))
{
AddErrInfo(eval("gErrStr13"), 'Interface');
return false;
}
var addrTmp = getValue('DestIPAddress');
var plen1 = getValue('prefixlength');
var plen2 = 0;
for (i = 0; i < StaticRoute.length - 1; i++)
{
if ((StaticRoute[i].PrefixLength == '')
|| (StaticRoute[i].DestIPAddress == ''))
{
continue;
}
plen2 = StaticRoute[i].PrefixLength;
if (selctIndex != i)
{
if (true == CompareTwoAddress(addrTmp, plen1, StaticRoute[i].DestIPAddress, plen2))
{
AddErrInfo(eval("gErrStr0"), 'DestIPAddress');
return false;
}
}
}
if (gwStr.checked == true)
{
if (isIpv6Address(GatewayIPAddress.value) == false)
{
AddErrInfo(eval("gErrStr10") + getSafeString(GatewayIPAddress.value) + eval("gErrStr11"), 'gwStr');
return false;
}
if (isUnicastIpv6Address(GatewayIPAddress.value) == false)
{
AddErrInfo(eval("gErrStr10") + getSafeString(GatewayIPAddress.value) + eval("gErrStr11"), 'gwStr');
return false;
}
}
{
var selectObj = getElement('Interface');
var index = parseInt(selectObj.selectedIndex);
var idx = selectObj.options[index].id.split('_')[1];
if (getSelectVal('Interface') != 'br0' && WanInfo[idx].Enable != 1)
{
AddErrInfo(eval("gErrStr13"), 'Interface');
return false;
}
}
}
if (0 == getValue('prefixlength'))
{
for (i = 0; i < StaticRoute.length - 1; i++)
{
if ((0 == StaticRoute[i].PrefixLength) && (selctIndex != i))
{
AddErrInfo(eval("gErrStr0"), 'DestIPAddress');
return false;;
}
}
}
return true;
}
function AddSubmitParam(Form,type)
{
with ( document.forms[0] )
{
Form.addParameter('x.DestIPAddress', DestIPAddress.value);
Form.addParameter('x.PrefixLength', prefixlength.value);
if (gwStr.checked == true)
{
Form.addParameter('x.GatewayIPAddress',GatewayIPAddress.value);
}
else
{
Form.addParameter('x.GatewayIPAddress', '');
}
Form.addParameter('x.Enable', getCheckVal('gEnable'));
Form.addParameter('x.Interface', getSelectVal('Interface'));
}
if (selctIndex == -1)
{
Form.setAction("addcfg.cgi?x=InternetGatewayDevice.Layer3Forwarding.X_IPv6Forwarding"
+ "&RequestFile=/html/network/route6.asp");
}
else
{
Form.setAction("setcfg.cgi?x="+ StaticRoute[selctIndex].Domain
+ "&RequestFile=/html/network/route6.asp");
}
top.aplyLine = selctIndex;
setDisable('btnApply',1);
}
function setCtlDisplay(record)
{
setText('DestIPAddress',record.DestIPAddress);
setText('GatewayIPAddress',record.GatewayIPAddress);
setText('prefixlength',record.PrefixLength);
setSelect('Interface', record.Interface);
setCheck('gEnable', record.enable);
}
function setControl(index)
{
var record;
selctIndex = index;
if (index == -1)
{
record = new stStaticRoute('', '', '', '', '');
setDisable('btnApply',0);
setDisable('GatewayIPAddress',0);
setDisable('Interface',0);
setDisable('prefixlength', 0);
setDisable('gwStr',0);
setCheck('gwStr',0);
setCheck('gEnable', 0);
}
else
{
record = StaticRoute[index];
setDisable('btnApply',0);
setDisable('GatewayIPAddress',0);
setDisable('Interface',0);
setDisable('prefixlength', 0);
setDisable('gwStr',0);
if (record.GatewayIPAddress == "")
{
setCheck('gwStr',0);
}
else
{
setCheck('gwStr',1);
}
setCheck('gEnable', record.enable);
}
setCtlDisplay(record);
}

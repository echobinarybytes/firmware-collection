<script language="JavaScript" type="text/javascript">
for (i = 0; i < Wan.length; i++)
{
document.write('<tr class="trTabContent">');
document.write('<td>' + Wan[i].Name + '</td>');
if ((Wan[i].Status == 'Connected') || (Wan[i].xIpv6Status == 'Connected'))
{
document.write('<td>' + eval("gVarConnected") + '&nbsp;</td>');
}
else if ((Wan[i].Status == 'Connecting') || (Wan[i].xIpv6Status == 'Connecting'))
{
document.write('<td>' + eval("gVarConnecting") + '&nbsp;</td>');
}
else
{
document.write('<td>' + eval("gVarDisConnected") + '&nbsp;</td>');
}
document.write('<td>' + FormatTime(Wan[i].Uptime)+ '&nbsp;</td>');
if (GetAccessType(Wan[i].WdInstId)== 'Ethernet')
{
document.write('<td>' + eval("gVarAcc_eth") + '&nbsp;</td>');
}
else
{
document.write('<td>' + GetAccessType(Wan[i].WdInstId) + '&nbsp;</td>');
}
document.write('</tr>');
}
</script>

<head>
<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/util.js"></script>
<script language="JavaScript" src="/lang/porttrigger.res"></script>
<title>PortTrigger</title>
<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" type="text/javascript">
var WanIndex = -1;
var selctIndex = -1;
function stWanInfo(domain,Name,Enable,NATEnabled,ConnectionType,ServiceList,DMZEnable,DMZIP)
{
this.domain = domain;
this.key = this.domain;
this.Name = Name;
this.ConnectionType = ConnectionType;
this.ServiceList = ServiceList;
this.DMZEnable = DMZEnable;
this.DMZIP = DMZIP;
this.Enable = Enable;
this.NATEnabled = NATEnabled;
this.CntStatus = '';
}
var WanIPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Name|Enable|NATEnabled|ConnectionType|X_ServiceList|X_DMZ.DMZEnable|X_DMZ.DMZHostIPAddress,stWanInfo);%>;
var WanPPPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|Enable|NATEnabled|ConnectionType|X_ServiceList|X_DMZ.DMZEnable|X_DMZ.DMZHostIPAddress,stWanInfo);%>;
function stCntStatus(domain,CntStatus)
{
this.key = domain;
this.CntStatus = CntStatus;
}
var WanIPCntStatusInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},ConnectionStatus,stCntStatus);%>;
var WanPPPCntStatusInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},ConnectionStatus,stCntStatus);%>;
var CntStatusInfo = new Array();
for (i = 0; i < WanIPCntStatusInfo.length-1; i++)
{
CntStatusInfo[i] = WanIPCntStatusInfo[i];
}
for (j = 0; j < WanPPPCntStatusInfo.length-1; j++,i++)
{
CntStatusInfo[i] = WanPPPCntStatusInfo[j];
}
var WanInfo = new Array();
for (i = 0; i < WanIPInfo.length-1; i++)
{
WanInfo[i] = WanIPInfo[i];
}
for (j = 0; j < WanPPPInfo.length-1; j++,i++)
{
WanInfo[i] = WanPPPInfo[j];
}
AssociateParam('WanInfo','CntStatusInfo','CntStatus');
function stPortTrigger(domain,Enable,TPort,TPortEnd,TProto,OPort,OPortEnd, despName)
{
this.domain = domain;
this.key = this.domain;
this.PortTriggerEnable = Enable;
this.TPort = TPort;
this.TPortEnd = TPortEnd;
this.TProto = TProto;
this.OPort = OPort;
this.OPortEnd = OPortEnd;
this.Description = despName;
var index = domain.lastIndexOf('X_PortTrigger');
this.Interface = domain.substr(0,index - 1);
this.NATEnabled = '';
this.Name = '';
this.Enable = '';
}
var WanIPPortTrigger = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i}.X_PortTrigger.{i},PortTriggerEnable|TriggerPort|TriggerPortEnd|TriggerProtocol|OpenPort|OpenPortEnd|PortTriggerDescription, stPortTrigger);%>;
var WanPPPPortTrigger = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i}.X_PortTrigger.{i},PortTriggerEnable|TriggerPort|TriggerPortEnd|TriggerProtocol|OpenPort|OpenPortEnd|PortTriggerDescription, stPortTrigger);%>;
var PortTrigger = new Array();
for (i = 0; i < WanIPPortTrigger.length-1; i++)
{
PortTrigger[i] = WanIPPortTrigger[i];
}
for (j = 0; j < WanPPPPortTrigger.length-1; j++,i++)
{
PortTrigger[i] = WanPPPPortTrigger[j];
}
if (PortTrigger.length > 0)
{
AssociateParam('PortTrigger', 'WanInfo', 'NATEnabled|Enable|id|Name');
}
function AddSubmitParam(SubmitForm,type)
{
var Interface = getSelectVal('PortTriggerInterface');
setDisable('PortTriggerInterface',1);
SubmitForm.addDiv('PortTriggerDiv','x');
if( selctIndex == -1 )
{
SubmitForm.setAction('addcfg.cgi?x=' + Interface + '.X_PortTrigger'
+'&RequestFile=/html/application/porttrigger.asp');
}
else
{
SubmitForm.setAction('setcfg.cgi?x=' + PortTrigger[selctIndex].domain
+'&RequestFile=/html/application/porttrigger.asp');
}
top.aplyLine = selctIndex;
setDisable('btnApply',1);
}
var TriggerPortSum = 0;
var OpenPortSum = 0;
function CountPortSum()
{
TriggerPortSum = 0;
OpenPortSum = 0;
for (k = 0;(k < PortTrigger.length); k++)
{
if (selctIndex == k)
{
continue;
}
if ((parseInt(PortTrigger[k].TPortEnd) <= 0)
|| (parseInt(PortTrigger[k].TPortEnd) == parseInt(PortTrigger[k].TPort)))
{
TriggerPortSum += 1;
}
else
{
TriggerPortSum += (parseInt(PortTrigger[k].TPortEnd) - parseInt(PortTrigger[k].TPort) + 1);
}
if ((parseInt(PortTrigger[k].OPortEnd) <= 0)
|| (parseInt(PortTrigger[k].OPortEnd) == parseInt(PortTrigger[k].OPort)))
{
OpenPortSum += 1;
}
else
{
OpenPortSum += (parseInt(PortTrigger[k].OPortEnd) - parseInt(PortTrigger[k].OPort) + 1);
}
}
}
function CheckForm(type)
{
switch (type)
{
case 3:
return CheckPortTrigger();
break;
}
return true;
}
function same_inst()
{
var i = 0;
var Triggerportendvalue = parseInt(getElement('TriggerPortEnd').value);
var Triggerportvalue = parseInt(getElement('TriggerPort').value);
var comTriggerportendvalue = 0;
var comTriggerportvalue = 0;
for (i=0; i<PortTrigger.length; i++)
{
if (selctIndex == i)
{
continue;
}
if (parseInt(getElement('TriggerPortEnd').value) != parseInt(PortTrigger[i].TPortEnd))
{
continue;
}
if (parseInt(getElement('TriggerPort').value) != parseInt(PortTrigger[i].TPort))
{
continue;
}
if (parseInt(getElement('OpenPortEnd').value) != parseInt(PortTrigger[i].OPortEnd))
{
continue;
}
if (parseInt(getElement('OpenPort').value) != parseInt(PortTrigger[i].OPort))
{
continue;
}
if (getElement('TriggerProtocol').value != PortTrigger[i].TProto)
{
continue;
}
var endIndex = PortTrigger[i].domain.lastIndexOf('X_PortTrigger') - 1;
var Interface = PortTrigger[i].domain.substring(0,endIndex);
if (Interface != getElement('PortTriggerInterface').value)
{
continue;
}
return false;
}
for (i=0; i<PortTrigger.length; i++)
{
if (selctIndex == i)
{
continue;
}
var endIndex = PortTrigger[i].domain.lastIndexOf('X_PortTrigger') - 1;
var Interface = PortTrigger[i].domain.substring(0,endIndex);
if (Interface != getElement('PortTriggerInterface').value)
{
continue;
}
if ((getElement('TriggerProtocol').value != "TCP/UDP")
&& (PortTrigger[i].TProto != "TCP/UDP")
&& (getElement('TriggerProtocol').value != PortTrigger[i].TProto))
{
continue;
}
comTriggerportendvalue = parseInt(PortTrigger[i].TPortEnd);
comTriggerportvalue = parseInt(PortTrigger[i].TPort);
if (Triggerportendvalue != -1)
{
if (comTriggerportendvalue != -1)
{
if ((Triggerportendvalue < comTriggerportvalue)
|| (Triggerportvalue > comTriggerportendvalue))
{
continue;
}
return false;
}
else
{
if ((Triggerportendvalue < comTriggerportvalue)
|| (Triggerportvalue > comTriggerportvalue))
{
continue;
}
return false;
}
}
else
{
if (comTriggerportendvalue != -1)
{
if ((Triggerportvalue < comTriggerportvalue)
|| (Triggerportvalue > comTriggerportendvalue))
{
continue;
}
return false;
}
else
{
if (Triggerportendvalue != comTriggerportvalue)
{
continue;
}
return false;
}
}
}
return true;
}
function CheckPortTrigger()
{
if((selctIndex == -1) && (PortTrigger.length >= 32))
{
AddErrInfo(eval("gErrStr0"), '')
return false;
}
if (getElement('PortTriggerInterface').length == 0)
{
AddErrInfo(eval("gErrStr1"), 'PortTriggerInterface');
return false;
}
if (getElement('PortTriggerDescription').value == '')
{
AddErrInfo(eval("gErrStr34"), 'PortTriggerDescription');
return false;
}
for( var i=0; i<PortTrigger.length; i++ )
{
if(PortTrigger[i].Description == getElement('PortTriggerDescription').value && selctIndex != i)
{
AddErrInfo(getElement('PortTriggerDescription').value + eval("gErrStr1"), 'PortTriggerDescription');
return false;
}
}
with (getElById('PortTriggerForm'))
{
CountPortSum();
if (TriggerPortSum > 256)
{
AddErrInfo(eval("gErrStr3"), '');
return false;
}
if (OpenPortSum > 256)
{
AddErrInfo(eval("gErrStr4"), '');
return false;
}
if ((TriggerPortSum + parseInt(getElement('TriggerPortEnd').value) - parseInt(getElement('TriggerPort').value)+ 1) > 256)
{
AddErrInfo(eval("gErrStr5"), 'TriggerPortEnd');
return false;
}
if ((OpenPortSum + parseInt(getElement('OpenPortEnd').value) - parseInt(getElement('OpenPort').value)+ 1) > 256)
{
AddErrInfo(eval("gErrStr6"), 'OpenPortEnd');
return false;
}
if (getElement('TriggerPort').value == "" || isValidPort(getElement('TriggerPort').value) == false)
{
AddErrInfo(eval("gErrStr7") + getElement('TriggerPort').value + eval("gErrStr8"), 'TriggerPort');
return false;
}
if (getElement('TriggerPortEnd').value == "")
{
getElement('TriggerPortEnd').value = getElement('TriggerPort').value;
}
if (isValidPort(getElement('TriggerPortEnd').value) == false)
{
AddErrInfo(eval("gErrStr9") + getElement('TriggerPortEnd').value + eval("gErrStr10"), 'TriggerPortEnd');
return false;
}
if ((parseInt(getElement('TriggerPort').value) <= 21) && (parseInt(getElement('TriggerPortEnd').value) >= 21))
{
AddErrInfo(eval("gErrStr11"), 'TriggerPort');
return false;
}
if ((parseInt(getElement('TriggerPort').value) <= 69) && (parseInt(getElement('TriggerPortEnd').value) >= 69))
{
AddErrInfo(eval("gErrStr12"), 'TriggerPort');
return false;
}
if ((parseInt(getElement('TriggerPort').value) <= 80) && (parseInt(getElement('TriggerPortEnd').value) >= 80))
{
AddErrInfo(eval("gErrStr13"), 'TriggerPort');
return false;
}
if ((parseInt(getElement('TriggerPort').value) <= 161) && (parseInt(getElement('TriggerPortEnd').value) >= 161))
{
AddErrInfo(eval("gErrStr14"), 'TriggerPort');
return false;
}
if ((parseInt(getElement('TriggerPort').value) <= 162) && (parseInt(getElement('TriggerPortEnd').value) >= 162))
{
AddErrInfo(eval("gErrStr15"), 'TriggerPort');
return false;
}
if ((parseInt(getElement('TriggerPort').value) <= 517) && (parseInt(getElement('TriggerPortEnd').value) >= 517))
{
AddErrInfo(eval("gErrStr16"), 'TriggerPort');
return false;
}
if ((parseInt(getElement('TriggerPort').value) <= 518) && (parseInt(getElement('TriggerPortEnd').value) >= 518))
{
AddErrInfo(eval("gErrStr17"), 'TriggerPort');
return false;
}
if ((parseInt(getElement('TriggerPort').value) <= 1720) && (parseInt(getElement('TriggerPortEnd').value) >= 1720))
{
AddErrInfo(eval("gErrStr18"), 'TriggerPort');
return false;
}
if ((parseInt(getElement('TriggerPort').value) <= 1723) && (parseInt(getElement('TriggerPortEnd').value) >= 1723))
{
AddErrInfo(eval("gErrStr19"), 'TriggerPort');
return false;
}
if ((parseInt(getElement('TriggerPort').value) <= 6667) && (parseInt(getElement('TriggerPortEnd').value) >= 6667))
{
AddErrInfo(eval("gErrStr20"), 'TriggerPort');
return false;
}
if ((parseInt(getElement('TriggerPortEnd').value) - parseInt(getElement('TriggerPort').value)) > 256)
{
AddErrInfo(eval("gErrStr21"), 'TriggerPort');
return false;
}
if (getElement('OpenPort').value == "" || isValidPort(getElement('OpenPort').value) == false)
{
AddErrInfo(eval("gErrStr22") + getElement('OpenPort').value + eval("gErrStr23"), 'OpenPort');
return false;
}
if (getElement('OpenPortEnd').value == "")
{
getElement('OpenPortEnd').value = getElement('OpenPort').value;
}
if (isValidPort(getElement('OpenPortEnd').value) == false)
{
AddErrInfo(eval("gErrStr24") + getElement('OpenPortEnd').value + eval("gErrStr25"), 'OpenPortEnd');
return false;
}
if ((parseInt(getElement('OpenPortEnd').value) - parseInt(getElement('OpenPort').value)) > 256)
{
AddErrInfo(eval("gErrStr26"), 'OpenPortEnd');
return false;
}
if ((parseInt(getElement('OpenPort').value) <= 8081) && (parseInt(getElement('OpenPortEnd').value) >= 8081))
{
AddErrInfo(eval("gErrStr31"), 'OpenPortEnd');
return false;
}
if ((parseInt(getElement('OpenPort').value) <= 8082) && (parseInt(getElement('OpenPortEnd').value) >= 8082))
{
AddErrInfo(eval("gErrStr32"), 'OpenPortEnd');
return false;
}
if (getElement('PortTriggerDescription').value == "" )
{
AddErrInfo(eval("gErrStr27") + getElement('PortTriggerDescription').value + eval("gErrStr28"), 'PortTriggerDescription');
return false;
}
tPortS = parseInt(getElement('TriggerPort').value);
tPortE = parseInt(getElement('TriggerPortEnd').value);
if (tPortS > tPortE)
{
AddErrInfo(eval("gErrStr29"), 'TriggerPort');
return false;
}
oPortS = parseInt(getElement('OpenPort').value);
oPortE = parseInt(getElement('OpenPortEnd').value);
if (oPortS > oPortE)
{
AddErrInfo(eval("gErrStr30"), 'OpenPort');
return false;
}
if (false == same_inst())
{
AddErrInfo(eval("gErrStr33"));
return false;
}
}
return true;
}
function LoadFrame()
{
var i = GetmenuName("porttrigger.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_0');
}
}
function ShowPortTrigger()
{
var Interface = getElement('PortTriggerInterface');
var optionID = Interface.options[Interface.selectedIndex].id;
var index = optionID.split('_')[1];
if (Interface.length > 0)
{
if (WanInfo[index].Enable == 0 || WanInfo[index].NATEnabled == 0)
{
setDisable('btn1',1);
}
else
{
setDisable('btn1',0);
}
}
var html = '<table border="1" cellpadding="0" cellspacing="0" width="70%" style="table-layout:fixed;word-break:break-all">'
+ '<tr align="middle">'
+  '<td width="9%" class="table_title">Prptocol</td>'
+  '<td width="13%" class="table_title">Source IP Address</td>'
+  '<td width="16%" class="table_title">External Port</td>'
+  '<td width="16%" class="table_title">External End Port</td>'
+  '<td width="16%" class="table_title">Internal Port</td>'
+  '<td width="13%" class="table_title">Internal Host</td>'
+  '<td width="15%" class="table_title">Trigger Name</td>'
+  '<td width="10%" class="table_title">Enable</td>'
+  '<td width="10%" class="table_title">Remove</td>'
+ '</tr>'
for (i = 0; i < PortTrigger.length; i++)
{
if (PortTrigger[i].domain.indexOf(getSelectVal('PortTriggerInterface'))
> -1)
{
html += '<TR align="middle">'
html +=  '<TD align="center">' + PortTrigger[i].TProto + '&nbsp;</TD>';
html += '<TD align="center">' + PortTrigger[i].TPort + '&nbsp;</TD>';
if (PortTrigger[i].TPortEnd == 0)
{
html += '<TD align="center">' + PortTrigger[i].TPort + '&nbsp;</TD>';
}
else
{
html += '<TD align="center">' + PortTrigger[i].TPortEnd + '&nbsp;</TD>';
}
html += '<TD align="center">' + PortTrigger[i].Oport + '&nbsp;</TD>';
if (PortTrigger[i].OPortEnd == 0)
{
html += '<TD align="center">' + PortTrigger[i].Oport + '&nbsp;</TD>';
}
else
{
html += '<TD align="center">' + PortTrigger[i].OPortEnd + '&nbsp;</TD>';
}
html += '<TD align="center">'
+ getDisplayText(PortTrigger[i].Description) + '&nbsp;</TD>';
if (PortTrigger[i].Enable == 1)
{
html += '<TD align="center">' + eval('gVarPubEnable') + '&nbsp;</TD>';
}
else
{
html += '<TD align="center">' + eval('gVarPubDisable') + '&nbsp;</TD>';
}
html += '<TD align="center"><input type="checkbox" name="rml" value="'
+ PortTrigger[i].domain + '"></TD>';
html += '</TR>';
}
}
html += '</table>'
}
function ShowPortTriggerCfg()
{
var html = ''
for (i = 0; i < PortTrigger.length; i++)
{
for(var j=0;j < WanInfo.length; j++)
{
if(WanInfo[j].Name == PortTrigger[i].Name)
{
break;
}
}
if(WanInfo.length == j)
{
break;
}
if ( (WanInfo[j].ConnectionType != 'IP_Routed')
|| (WanInfo[j].ServiceList.indexOf('INTERNET') < 0 && WanInfo[j].ServiceList.indexOf('OTHER') < 0)
|| (WanInfo[j].NATEnabled == 0))
{
continue;
}
{
html += '<TR id=record_' + i
+' class="trTabContent" onclick="selectLine(this.id);">'
html += '<TD align="center">' + getSafeString(PortTrigger[i].Description) + '&nbsp;</TD>';
html +=  '<TD align="center">' + getSafeString(PortTrigger[i].Name) + '&nbsp;</TD>';
html +=  '<TD align="center">' + PortTrigger[i].TProto + '&nbsp;</TD>';
html += '<TD align="center">' + PortTrigger[i].TPort + '&nbsp;</TD>';
if (PortTrigger[i].TPortEnd == 0)
{
html += '<TD align="center">' + PortTrigger[i].TPort + '&nbsp;</TD>';
}
else
{
html += '<TD align="center">' + PortTrigger[i].TPortEnd + '&nbsp;</TD>';
}
html += '<TD align="center">' + PortTrigger[i].OPort + '&nbsp;</TD>';
if (PortTrigger[i].OPortEnd == 0)
{
html += '<TD align="center">' + PortTrigger[i].OPort + '&nbsp;</TD>';
}
else
{
html += '<TD align="center">' + PortTrigger[i].OPortEnd + '&nbsp;</TD>';
}
if (PortTrigger[i].PortTriggerEnable == 1)
{
html += '<TD align="center">' + eval('gVarPubEnable') + '&nbsp;</TD>';
}
else
{
html += '<TD align="center">' + eval('gVarPubDisable') + '&nbsp;</TD>';
}
html += '<TD align="center"><input type="checkbox" name="rml" value="'
+ PortTrigger[i].domain + '"></TD>';
html += '</TR>';
}
}
document.write(html);
}
TOTAL_APP = 5;
var FIRST_APP = "Choose...";
var v = new Array(TOTAL_APP);
v[0] = new cV("Aim Talk", 1);
v[0].e[0] = new iVe("4099", "4099", "1", "5191", "5191");//, "1" );
v[1] = new cV("Asheron's Call", 1);
v[1].e[0] = new iVe("9000", "9013", "2", "9000", "9013");//, "2" );
v[2] = new cV("Calista IP Phone", 1);
v[2].e[0] = new iVe("5190", "5190", "0", "3000", "3000");//, "2" );
v[3] = new cV("Net2Phone", 1);
v[3].e[0] = new iVe("6801", "6801", "2", "6801", "6801");//, "2" );
v[4] = new cV("Rainbow Six/Rogue Spea", 1);
v[4].e[0] = new iVe("2346", "2346", "0", "2436", "2436");//, "0" );
function cV(name, entryNum)
{
this.name = name;
this.eNum = entryNum;
this.e = new Array(6);
}
function iVe(eStart, eEnd, proto, iStart, iEnd)
{
this.eStart = eStart;
this.eEnd = eEnd;
this.proto = proto;
this.iStart = iStart;
this.iEnd = iEnd;
}
function clearAll()
{
}
function trimspace(str)
{
var strTemp = new String(str);
while (-1 != strTemp.search(" "))
{
strTemp = strTemp.replace(" ", "");
}
return strTemp;
}
function appSelect(sName)
{
clearAll();
with (getElement('PortTriggerForm'))
{
if (sName == FIRST_APP)
{
return;
}
for(var i = 0; i < TOTAL_APP; i++)
{
if(v[i].name == sName)
{
switch (v[i].e[0].proto)
{
case '0':
setSelect('TriggerProtocol', 'TCP/UDP');
break;
case '1':
setSelect('TriggerProtocol', 'TCP');
break;
case '2':
setSelect('TriggerProtocol', 'UDP');
break;
}
getElement('TriggerPort').value = v[i].e[0].eStart;
getElement('TriggerPortEnd').value = v[i].e[0].eEnd;
getElement('OpenPort').value = v[i].e[0].iStart;
getElement('OpenPortEnd').value = v[i].e[0].iEnd;
getElement('PortTriggerDescription').value = sName; 
}
}
}
}
function radioClick()
{
if(getElement("radiosrv")[0].checked == true)
{
setDisable("constsrvName", 1);
}
else
{
setDisable("constsrvName", 0);
}
}
function setCtlDisplay(record)
{
var endIndex = record.domain.lastIndexOf('X_PortTrigger') - 1;
var Interface = record.domain.substring(0,endIndex);
setSelect('PortTriggerInterface',Interface);
setSelect('PortTriggerEnable',	 record.ProtMapEnabled);
setSelect('TriggerProtocol', record.TProto);
setText('TriggerPort',record.TPort);
if ((record.TPortEnd == 0) || (record.TPortEnd == ''))
{
setText('TriggerPortEnd', record.TPort);
}
else
{
setText('TriggerPortEnd', record.TPortEnd);
}
setText('OpenPort',record.OPort);
if ((record.OPortEnd == 0) || (record.OPortEnd == ''))
{
setText('OpenPortEnd', record.OPort);
}
else
{
setText('OpenPortEnd', record.OPortEnd);
}
setText('PortTriggerDescription', record.Description);
}
function setControl(index)
{
var record;
selctIndex = index;
if (index == -1)
{
record = new stPortTrigger('','','','','', '', '','');
setDisable('PortTriggerInterface', 0);
}
else
{
record = PortTrigger[index];
setDisable('PortTriggerInterface', 1);
}
setCtlDisplay(record);
}
function clickRemove()
{
removeInst('/html/application/porttrigger.asp');
}
</script>

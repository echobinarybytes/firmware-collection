<script language="JavaScript" type="text/javascript">
var curDiv = 1;
var ssidIdx = 0;
var firstSetFlag = 0;
function stWlan(domain,ssid,BeaconType,Encryption,AuthMode,KeyIndex,EncryptionLevel,WPAModes,IEEE11iEModes,MixedModes)
{
this.domain = domain;
this.ssid = ssid;
this.BeaconType = BeaconType;
this.Encryption = Encryption;
this.AuthMode = AuthMode;
this.KeyIndex = KeyIndex;
this.EncypBit = parseInt(EncryptionLevel);
this.WPAModes = WPAModes;
this.IEEE11iEModes = IEEE11iEModes;
this.MixedModes = MixedModes;
}
var Wlan = <%webGetCfgEntry(InternetGatewayDevice.LANDevice.1.WLANConfiguration.1,SSID|BeaconType|BasicEncryptionModes|BasicAuthenticationMode|WEPKeyIndex|WEPEncryptionLevel|WPAEncryptionModes|IEEE11iEncryptionModes|X_MixedEncryptionModes,stWlan);%>;
var keys = <%webGetCfgArray(InternetGatewayDevice.LANDevice.1.WLANConfiguration.1.WEPKey.{i},WEPKey);%>;
var ssidAccessAttr = 'Subscriber';
var wpaPskKey = <%webGetCfgArray(InternetGatewayDevice.LANDevice.1.WLANConfiguration.1.PreSharedKey.1,PreSharedKey);%>;
var enbl = <%webGetParaByDomainName(InternetGatewayDevice.LANDevice.1.X_WLANEnable);%>
var state = enbl;
function WanPPPConstruction(Domain, Name, UserName)
{
this.Domain = Domain;
this.Name = Name;
this.UserName = UserName;
this.Password = '@1GV)Z<!';
}
var WANPPP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},
Name|Username, WanPPPConstruction);%>;
var WAN;
if (WANPPP.length > 1)
{
WAN = WANPPP[0];
}
else
{
WAN = new WanPPPConstruction('', '', '');
WAN.Password = '';
WAN.Name = gVarName;
}
function LoadFrame()
{
var i = GetmenuName("wizard.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
disSetpX(1);
setText('pppUserName',WAN.UserName);
setText('pppPassword',WAN.Password);
}
function isValidKey(val, size) {
var ret = false;
var len = val.length;
var dbSize = size * 2;
if ( len == size )
ret = true;
else if ( len == dbSize ) {
for ( i = 0; i < dbSize; i++ )
if ( isHexaDigit(val.charAt(i)) == false )
break;
if ( i == dbSize )
ret = true;
} else
ret = false;
return ret;
}
function xusetDisable(sId, flag)
{
var item;
if (null == (item = getElement(sId)))
{
debug(sId + " is not existed" );
return false;
}
var isEnable = false;
if ( flag == 1 || flag == '1' )
{
isEnable = true;
}
else
{
isEnable = false;
}
for (var i = 0; i < item.length; i++)
{
item[i].disabled = isEnable;
}
return true;
}
function disSetpX(index)
{
setDisplay('step_'+1+'_'+0,0);
setDisplay('step_'+2+'_'+0,0);
setDisplay('step_'+3+'_'+0,0);
setDisplay('step_'+4+'_'+0,0);
setDisplay('step_'+1+'_'+1,0);
setDisplay('step_'+2+'_'+1,0);
setDisplay('step_'+3+'_'+1,0);
setDisplay('step_submit',0);
setDisplay('step_buton1',1);
setDisplay('step_buton',0);
switch( index )
{
case 1 :
setDisplay('step_'+1+'_'+0,1);
setDisplay('step_'+1+'_'+1,1);
break;
case 2 :
setDisplay('step_'+2+'_'+0,1);
setDisplay('step_'+2+'_'+1,1);
setDisplay('step_submit',0);
setDisplay('step_buton1',0);
setDisplay('step_buton',1);
if (WAN.Domain != '')
{
setDisable('pppUserName', 0);
setDisable('pppPassword', 0);
}
else
{
setDisable('pppUserName', 1);
setDisable('pppPassword', 1);
}
break;
case 3 :
setDisplay('step_'+3+'_'+0,1);
setDisplay('step_'+3+'_'+1,1);
setDisplay('step_submit',0);
setDisplay('step_buton1',0);
setDisplay('step_buton',1)
setDisable('wlEnbl', 0);
setDisable('wlSsid', 0);
setDisable('wlAuthMode', 0);
setDisable('wlPreauth', 0);
setDisable('wlNetReauth', 0);
setDisable('wlWpaPsk', 0);
setDisable('wlWpaGtkRekey', 0);
setDisable('wlRadiusIPAddr', 0);
setDisable('wlRadiusPort', 0);
setDisable('wlRadiusKey', 0);
setDisable('wlWpa', 0);
setDisable('wlWepauth', 0);
setDisable('wlKeyBit', 0);
setDisable('wlKeyIndex', 0);
xusetDisable('wlKeys', 0);
if(firstSetFlag <= index)
{
setCheck('wlEnbl', enbl);;
setText('wlSsid',Wlan[ssidIdx].ssid);
setSelect('wlAuthMode',Wlan[ssidIdx].BeaconType);
for ( i = 0; i < 4; i++)
{
getElement('wlKeys')[i].value = keys[i][1];
}
setSelect('wlKeyBit',parseInt(Wlan[ssidIdx].EncypBit)+24);
setText('wlWpaPsk',wpaPskKey[0][1]);
if (ssidAccessAttr.indexOf('Subscriber') < 0)
{
setDisable('wlSsid',1);
}
authModeChange(1);
}
break;
case 4 :
setDisplay('step_'+4+'_'+0,1);
setDisplay('step_'+1+'_'+1,1);
setDisplay('step_'+2+'_'+1,1);
setDisplay('step_'+3+'_'+1,1);
setDisplay('step_submit',1);
setDisplay('step_buton',0);
setDisplay('step_buton1',0);
setDisable('pppUserName', 1);
setDisable('pppPassword', 1);
setDisable('wlEnbl', 1);
setDisable('wlSsid', 1);
setDisable('wlAuthMode', 1);
setDisable('wlPreauth', 1);
setDisable('wlNetReauth', 1);
setDisable('wlWpaPsk', 1);
setDisable('wlWpaGtkRekey', 1);
setDisable('wlRadiusIPAddr', 1);
setDisable('wlRadiusPort', 1);
setDisable('wlRadiusKey', 1);
setDisable('wlWpa', 1);
setDisable('wlWepauth', 1);
setDisable('wlKeyBit', 1);
setDisable('wlKeyIndex', 1);
xusetDisable('wlKeys', 1);
break;
default:
return false;
}
return true;
}
function opBackNext(operate)
{
if(curDiv > firstSetFlag)
{
firstSetFlag = curDiv;
}
if( operate == 'next' && curDiv < 4 )
{
if( false == iCheckForm(curDiv))
{
return false;
}
curDiv++;
disSetpX(curDiv);
}
else if( operate == 'back' && curDiv > 1 )
{
curDiv--;
disSetpX(curDiv);
switch(curDiv)
{
case 1:
case 2:
break;
}
}
}
function hideAllElements()
{
setDisplay("wlWpaD", 0);
setDisplay("wpaPreShareKey", 0);
setDisplay("wpaGTKRekey", 0);
setDisplay("wlRadius", 0);
setDisplay("Preauth", 0);
setDisplay("NetReauth", 0);
setDisplay("Wepauth", 0);
setDisplay("keyInfo", 0);
}
function encrypChange()
{
var authMode = getSelectVal('wlAuthMode')
if (authMode == 'Basic')
{
setDisplay("keyInfo", 1);
getElement('wlKeys')[0].disabled = 0;
getElement('wlKeys')[1].disabled = 0;
getElement('wlKeys')[2].disabled = 0;
getElement('wlKeys')[3].disabled = 0;
getElement('wlKeyIndex').length = 0;
getElement('wlKeyIndex')[0] = new Option("1", "1");
getElement('wlKeyIndex')[1] = new Option("2", "2");
getElement('wlKeyIndex')[2] = new Option("3", "3");
getElement('wlKeyIndex')[3] = new Option("4", "4");
setSelect('wlKeyIndex',Wlan[ssidIdx].KeyIndex) ;
}
else
{
setDisplay("keyInfo", 0);
}
}
function authModeChange(OnPageLoad)
{
var authMode = getSelectVal('wlAuthMode');
var mode = '';
var i, algos;
hideAllElements();
switch ( authMode ) {
case 'Basic':
setDisplay("Wepauth", 1);
setDisplay("keyInfo", 1);
break;
case '11i':
case 'WPAWPA2':
case 'WPA':
setDisplay("wlWpaD", 1);
setDisplay("wpaPreShareKey", 1);
break;
}
if(OnPageLoad==1)
{
algos = new Array(new Array('TKIP','TKIPEncryption'), new Array('AES',
'AESEncryption'),new Array('TKIP+AES',
'TKIPandAESEncryption'));
getElement('wlWpa').length = algos.length;
for (var i in algos)
{
getElement('wlWpa')[i] = new Option(algos[i][0], algos[i][1]);
getElement('wlWpa')[i].value = algos[i][1];
if (Wlan[ssidIdx].BeaconType == 'WPA'
&& getElement('wlWpa')[i][1] == Wlan[ssidIdx].WPAModes)
{
getElement('wlWpa')[i].selected = true;
}
else if (Wlan[ssidIdx].BeaconType == '11i'
&& getElement('wlWpa')[i][1] ==  Wlan[ssidIdx].IEEE11iEModes)
{
wlWpa[i].selected = true;
}
else if (Wlan[ssidIdx].BeaconType == 'WPAWPA2'
&& getElement('wlWpa')[i][1] ==  Wlan[ssidIdx].MixedModes)
{
wlWpa[i].selected = true;
}
}
}
if (authMode == 'WPA')
{
setSelect('wlWpa',Wlan[ssidIdx].WPAModes);
}
else if (authMode == '11i')
{
setSelect('wlWpa',Wlan[ssidIdx].IEEE11iEModes);
}
else if (authMode == 'WPAWPA2')
{
setSelect('wlWpa',Wlan[ssidIdx].MixedModes);
}
else if(authMode == 'Basic')
{
setSelect('wlWepauth',Wlan[ssidIdx].AuthMode);
}
encrypChange();
}
function enableWLAN()
{
}
function CheckForm( )
{
if( false == iCheckForm(2) || false == iCheckForm(3) )
{
return false;
}
return true;
}
function iCheckForm( curDiv )
{
switch(curDiv)
{
case 2:
break;
case 3:
var ssid = getValue('wlSsid');
if (ssid == '' )
{
AddErrInfo(eval("gErrStr12"), 'wlSsid');
return false;
}
if (ssid.length > 32)
{
AddErrInfo(eval("gErrStr13") + getValue('wlSsid') + eval("gErrStr14"), 'wlSsid');
return false;
}
if (isValidAscii(ssid) != '')
{
AddErrInfo(eval("gErrStr15") + isValidAscii(getElement('wlSsid').value) + eval("gErrStr16"), 'wlSsid');
return false;
}
if (ssid.charAt(ssid.length - 1) == ' ' )
{
AddErrInfo(eval("gErrStr17"), 'wlSsid');
return false;
}
var AuthMode = getSelectVal('wlAuthMode');
if (AuthMode == 'Basic')
{
var KeyBit = getSelectVal('wlKeyBit');
var index = parseInt(getSelectVal('wlKeyIndex'));
var wlKeys = getElement('wlKeys');
var val;
for (vKey = 0; vKey < 4; vKey++)
{
val = getElement('wlKeys')[vKey].value;
if ( val != '' )
{
if ( KeyBit == '128' )
{
if (isValidKey(val, 13) == false )
{
AddErrInfo(eval("gErrStr19") + val + eval("gErrStr20"), 'wlKeys');
return false;
}
}
else
{
if ( isValidKey(val, 5) == false )
{
AddErrInfo(eval("gErrStr21") + val + eval("gErrStr22"), 'wlKeys');
return false;
}
}
}
else
{
AddErrInfo(eval("gErrStr18"), 'wlKeys');
}
}
}
else if (AuthMode == 'WPA' || AuthMode == '11i' || AuthMode == 'WPAWPA2')
{
value = getValue('wlWpaPsk');
if (isValidWPAPskKey(value) == false )
{
AddErrInfo(eval("gErrStr23"), 'wlWpaPsk');
return false;
}
}
break;
default:
break;
}
return true;
}
function isValidWPAPskKey(val) {
var ret = false;
var len = val.length;
var maxSize = 64;
var minSize = 8;
if ( len >= minSize && len < maxSize )
ret = true;
else if ( len == maxSize ) {
for ( i = 0; i < maxSize; i++ )
if ( isHexaDigit(val.charAt(i)) == false )
break;
if ( i == maxSize )
ret = true;
} else
ret = false;
return ret;
}
function AddSubmitParam(Form,type)
{
setDisable('back1',1);
setDisable('apply',1);
if (WAN.Domain != '')
{
var url = "setcfg.cgi?x="+ WAN.Domain + "&RequestFile=/html/ntwkall/wan.asp";
var xmlParaList = '';
xmlParaList += 'x.Username=';
xmlParaList += getValue('pppUserName');
if ('@1GV)Z<!' != getValue('pppPassword'))
{
xmlParaList += '&x.Password=';
xmlParaList += getValue('pppPassword');
}
var xmlhttp = CreateXMLHttp();
xmlhttp.open('POST',url,true);
xmlhttp.send(xmlParaList);
}
enable = getCheckVal('wlEnbl');
Form.addParameter('x.X_WLANEnable',enable);
if (state == 1 || enable == 0)
{
var url = '';
var temp = '';
Form.usingPrefix('y');
var ssid = getValue('wlSsid');
Form.addParameter('SSID',ssid);
var AuthMode = getSelectVal('wlAuthMode');
Form.addParameter('BeaconType',AuthMode);
Form.endPrefix();
if (AuthMode == 'Basic')
{
var KeyBit = getSelectVal('wlKeyBit');
var index = parseInt(getSelectVal('wlKeyIndex'));
var wlKeys = getElement('wlKeys');
var BasicAuthmode = getSelectVal('wlWepauth');
Form.addParameter('y.WEPEncryptionLevel',(KeyBit-24)+'-bit');
Form.addParameter('y.BasicAuthenticationMode', BasicAuthmode);
Form.addParameter('y.WEPKeyIndex',index);
for (l = 1; l <= 4; l++)
{
Form.addParameter('k' + l +'.WEPKey.' + l + '.' + 'WEPKey',
wlKeys[l-1].value);
}
Form.addParameter('y.BasicEncryptionModes','WEPEncryption');
}
else if (AuthMode == 'WPA' || AuthMode == '11i' || AuthMode == 'WPAWPA2')
{
value = getValue('wlWpaPsk');
Form.addParameter('k.PreSharedKey.1.PreSharedKey',value);
if (AuthMode == 'WPA')
{
Form.addParameter('y.WPAEncryptionModes',getSelectVal('wlWpa'));
}
else  if (AuthMode == '11i')
{
Form.addParameter('y.IEEE11iEncryptionModes',getSelectVal('wlWpa'));
}
else  if (AuthMode == 'WPAWPA2')
{
Form.addParameter('y.X_MixedEncryptionModes',getSelectVal('wlWpa'));
}
}
}
var wlandomain = 'InternetGatewayDevice.LANDevice.1.WLANConfiguration.'
+(ssidIdx + 1);
if (AuthMode == 'Basic')
{
Form.setAction('setcfg.cgi?x=InternetGatewayDevice.LANDevice.1'
+ '&y=' + wlandomain + '&k1=' + wlandomain + '&k2=' + wlandomain
+ '&k3=' + wlandomain + '&k4=' + wlandomain
+ '&RequestFile=/html/status/deviceinfo.asp');
}
else if (AuthMode == 'WPA' || AuthMode == '11i' || AuthMode == 'WPAWPA2')
{
Form.setAction('setcfg.cgi?x=InternetGatewayDevice.LANDevice.1'
+ '&y=' + wlandomain + '&k=' + wlandomain //+ '&p=' + wlandomain
+ '&RequestFile=/html/status/deviceinfo.asp');
}
else if(AuthMode == 'None')
{
Form.setAction('setcfg.cgi?x=InternetGatewayDevice.LANDevice.1'
+ '&y=' + wlandomain
+ '&RequestFile=/html/status/deviceinfo.asp');
}
var menuItem = top.g_oMenuTree.getMenuByName(eval('gVarStatus'));
top.menufrm.expandMenu(menuItem.id);
top.menufrm.showTableMenu();
setDisable('wlEnbl',1);
Form.submit();
}
</script>

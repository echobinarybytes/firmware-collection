function DhcpServerRelTime(timeVal)
{
var timeString = "";
if (timeVal == 0)
{
return 'Lease Expired';
}
if (timeVal >= 60*60*24)
{
timeString += parseInt(timeVal/(60*60*24)) + " " + eval("gVarDay") + " ";
timeVal %= 60*60*24;
}
if (timeVal >= 60*60)
{
timeString += parseInt(timeVal / (60*60)) + " " + eval("gVarHour") + " ";
timeVal %= 60*60;
}
if (timeVal >= 60)
{
timeString += parseInt(timeVal / (60)) + " " + eval("gVarMinute") + " " ;
timeVal %= 60;
}
if (timeVal > 0)
{
timeString += timeVal + " " + eval("gVarSecond") + " ";
}
return timeString;
}
function convertSize(num)
{
if(num > 1024*1024*1024)
{
return (num/(1024*1024*1024)).toFixed(2) + ' GB';
}
else if(num > 1024*1024)
{
return (num/(1024*1024)).toFixed(2) + ' MB';
}
else if(num > 1024)
{
return (num/1024).toFixed(2) + ' KB';
}
else
{
return num + ' Bytes'
}
}

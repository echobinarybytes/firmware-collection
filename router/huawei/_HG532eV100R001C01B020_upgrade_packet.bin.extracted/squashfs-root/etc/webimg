<script language="JavaScript" src="/lang/lanname.res"></script>
<title>LAN Name</title>
<script language="JavaScript" type="text/javascript">
var selctIndex = -1;
function stHostFromDb(domain, MACAddress, HostName, LeaseTime)
{
this.domain = domain;
this.MACAddress = MACAddress;
this.HostName = HostName;
this.LeaseTime = LeaseTime;
}
var LanSrcInfo =  <%webGetCfgEntry(InternetGatewayDevice.LANDevice.1.Hosts.Host.{i},MACAddress|HostName|LeaseTimeRemaining,stHostFromDb);%>
var LanDevInfo = new Array();
var lLanIndex = 0;
var lValidFlg = 0;
var lDhcpNum = 32;
if (LanSrcInfo.length - 1 < 32)
{
lDhcpNum = LanSrcInfo.length - 1;
}
for (lLanIndex = 0; lLanIndex < lDhcpNum; lLanIndex++)
{
if (LanSrcInfo[lLanIndex].LeaseTime != 0)
{
LanDevInfo[lValidFlg] = LanSrcInfo[lLanIndex].MACAddress
+ "   " + LanSrcInfo[lLanIndex].HostName;
lValidFlg++;
}
}
var MacString ='';
var lZeroMac = 0;
var lHideFlag = 0;
function suggestOver(div_value)
{
div_value.className = 'suggest_link_over';
}
function suggestOut(div_value)
{
div_value.className = 'suggest_link';
}
function SearchSubString()
{
var i = 0;
lZeroMac = 0;
for (i = 0; i < LanDevInfo.length; i++)
{
if (-1 != LanDevInfo[i].indexOf(MacString))
{
lZeroMac = 1;
}
}
}
function FindMacFromStr(div_value)
{
var i = 0;
for (i = 0; i < LanDevInfo.length; i++)
{
if (-1 != div_value.indexOf(LanSrcInfo[i].MACAddress))
{
return i;
}
}
return -1;
}
function HideSrcDstNode()
{
if (lHideFlag)
{
document.getElementById("search_src_suggest").style.display = 'none';
lHideFlag = 0;
}
}
function setSrcMacSearch(div_value)
{
var Index = 0;
Index = FindMacFromStr(div_value);
if (Index != -1)
{
div_value = LanSrcInfo[Index].MACAddress
}
else
{
div_value = '';
}
document.getElementById("MACAddress").value = div_value;
document.getElementById("search_src_suggest").style.display = 'none';
}
function SrcMacSearchRequest()
{
var i=0;
MacString = document.getElementById("MACAddress").value;
SearchSubString();
HideSrcDstNode();
if (lZeroMac)
{
lHideFlag = 1;
document.getElementById("search_src_suggest").style.display = '';
var div = document.getElementById("search_src_suggest");
div.innerHTML = "";
for (i = 0; i < LanDevInfo.length; i++)
{
if (-1 != LanDevInfo[i].indexOf(MacString))
{
var suggest = '<div onmouseover="suggestOver(this);" ';
suggest += 'onmouseout="suggestOut(this);" ';
suggest += 'onclick="setSrcMacSearch(this.innerHTML);" ';
suggest += 'class="suggest_link">' + LanDevInfo[i] + '</div>';
div.innerHTML += suggest;
}
}
}
}
function stDataFromDb(domain, MACAddress, HostName)
{
this.domain = domain;
this.MACAddress = MACAddress;
this.HostName = HostName;
}
var LanName = <%webGetCfgEntry(InternetGatewayDevice.LANDevice.1.Hosts.X_Host.{i}, Address|Name, stDataFromDb);%>;
function LoadFrame()
{
var i = GetmenuName("lanname.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_0');
}
}
function btnRemove()
{
var Form = new webSubmitForm();
with ( document.forms[0] )
{
MACAddress.value = '';
HostName.value = '';
}
Form.setAction('del.cgi?RequestFile=/html/application/lanname.asp');
Form.submit();
setDisable('btnApply',1);
}
function clickRemove()
{
removeInst('/html/application/lanname.asp');
}
function CheckForm(Form)
{
var rulecount = 0;
with ( document.forms[0] )
{
if (getElement('MACAddress').value == '')
{
AddErrInfo(eval("gErrStr0"), 'MACAddress');
return false;
}
if ((!isAbcIpAddress(getElement('MACAddress').value))
&& (!isValidMacAddress(getElement('MACAddress').value)))
{
AddErrInfo(eval("gErrStr0"), 'MACAddress');
return false;
}
if (getElement('HostName').value == '')
{
AddErrInfo(eval("gErrStr1"), 'HostName');
return false;
}
if (getElement('HostName').value.charAt(0) == '.')
{
AddErrInfo(eval("gErrStr1"), 'HostName');
return false;
}
for (var i = 0; i < LanName.length - 1; i++)
{
if (selctIndex != i)
{
rulecount++;
if (rulecount >= 32)
{
AddErrInfo(eval("gErrStr2"), '');
return false;
}
if (getElement('HostName').value == LanName[i].HostName)
{
AddErrInfo(eval("gErrStr3"), 'HostName');
return false;
}
}
}
}
return true;
}
function  AddSubmitParam(Form,type)
{
var macaddrval = getValue('MACAddress');
if (macaddrval.indexOf(':') != -1)
{
macaddrval = macaddrval.toUpperCase();
}
Form.addParameter('x.Address',macaddrval);
Form.addParameter('x.Name',getValue('HostName'));
top.aplyLine = selctIndex;
if(selctIndex == -1 )
{
Form.setAction("addcfg.cgi?x=InternetGatewayDevice.LANDevice.1.Hosts.X_Host"
+ "&RequestFile=/html/application/lanname.asp");
}
else
{
Form.setAction("setcfg.cgi?x="+ LanName[selctIndex].domain
+ "&RequestFile=/html/application/lanname.asp");
}
setDisable('btnApply',1);
}
function setCtlDisplay(record)
{
setText('MACAddress',record.MACAddress);
setText('HostName',record.HostName);
}
function setControl(index)
{
var record;
selctIndex = index;
if (index == -1)
{
record = new stDataFromDb('', '', '', '', '');
getElement('tdHead').innerHTML = eval("gVarPubConfigure");
setDisable('btnApply',0);
}
else
{
record = LanName[index];
getElement('tdHead').innerHTML = record.HostName;
setDisable('btnApply',0);
}
setCtlDisplay(record);
}
</script>
</head>
<body onLoad="LoadFrame();">
<form id="Configure">
<script language="JavaScript" type="text/javascript">
writeTabCfgHeaderEx(eval("gVarLanName"),'Lan Name',"90%");
</script>
<table class="tabInfo">
<tr class="trTabHead">
<td><script language="JavaScript" type="text/javascript">document.write(eval("gVarListAddress"));</script></td>
<td><script language="JavaScript" type="text/javascript">document.write(eval("gVarListDomain"));</script></td>
<td><script language="JavaScript" type="text/javascript">document.write(eval("gVarPubRemove"));</script></td>
</tr>
<script language="JavaScript" type="text/javascript">
for (var i = 0; i < LanName.length - 1; i++)
{
document.write('<TR id=record_' + i
+' class="trTabContent" onclick="selectLine(this.id);">');
document.write('<TD align="center">' + LanName[i].MACAddress + '</TD>');
document.write('<TD align="center">' + LanName[i].HostName + '</TD>');
document.write('<TD align="center"><input type="checkbox"' + 'name="rml"'
+ ' value="' + LanName[i].domain + '"></td>\n');
document.write('</TR>');
}
</script>
</table>
<table class="tabCfgArea" cellpadding="3" cellspacing="3">
<tr>
<td>
<table cellpadding="2" cellspacing="0" class="tabCfgContent">
<tr align="left" class="trTabCfgHead">
<td colspan="4" id="tdHead"><script language="JavaScript" type="text/javascript">document.write(eval("gVarPubConfigure"));</script></td>
</tr>
<tr class="trTabConfigure">
<td width="241" align="right"><script language="JavaScript" type="text/javascript">document.write(eval("gVarAddress"));</script></td>
<td colspan="3">
<input type="text" maxlength="17" name="MACAddress"  id="MACAddress" autocomplete="off" onKeyUp="SrcMacSearchRequest()" onClick="HideSrcDstNode()"></td>
</tr>
<tr>
<td>
<div id="search_src_suggest" style="display:none;"></div>
</td>
</tr>
<tr class="trTabConfigure">
<td width="241" align="right"><script language="JavaScript" type="text/javascript">document.write(eval("gVarDomain"));</script></td>
<td colspan="3">
<input type="text" maxlength="64" name="HostName" onClick="HideSrcDstNode()">
</td>
</tr>
<tr class="trTabCfgHead">
<td align="left">&nbsp;</td>
<td width="377" align="left">&nbsp;</td>
<td width="108" align="left">&nbsp;</td>
<td width="114" align="left">
<button name="btnApply" type="button" value="Apply" class="changebutton" onClick="Submit();"><script language="JavaScript" type="text/javascript">document.write(eval("gVarPubApply"));</script></button>
</td>
</tr>
</table>
</td>
</tr>
</table>
<script language="JavaScript" type="text/javascript">
writeTabTail();
</script>

<script language="JavaScript" type="text/javascript">
var selctIndex = -1;
var lang = '<%webGetParaByDomainName(InternetGatewayDevice.UserInterface.X_Web.Language);%>';
if (lang == '')
{
lang = 'en';
}
function stMacFilter(domain,Name,Status,MACAddress,DestMACAddress,Days,StartTime,EndTime,TimeCtl,Priority)
{
this.domain = domain;
this.Name = Name;
this.Status = Status;
this.MACAddress = MACAddress;
this.DestMACAddress = DestMACAddress;
this.Days = Days;
this.StartTime = StartTime;
this.EndTime = EndTime;
this.TimeCtl = TimeCtl;
this.ulPriority = Priority;
}
var MacFilter = <%webGetCfgEntry(InternetGatewayDevice.X_FireWall.MacFilter.{i},Name|Status|SourceMACAddress|DestMACAddress|Days|StartTime|EndTime|TimeControl|Priority,stMacFilter);%>;
function stNewMacIndex(Index)
{
this.Index = Index;
}
var MacSwapData = new Array();
for (i = 1; i < MacFilter.length && null != MacFilter[i]; i++)
{
if (parseInt(MacFilter[i].ulPriority) < parseInt(MacFilter[i-1].ulPriority))
{
MacSwapData[0] = MacFilter[i];
j = i - 1;
do
{
MacFilter[j+1] = MacFilter[j];
j--;
}while((j >= 0)&&(parseInt(MacSwapData[0].ulPriority) < parseInt(MacFilter[j].ulPriority)));
if (j < 0)
{
MacFilter[0] = MacSwapData[0];
}
else
{
MacFilter[j+1] = MacSwapData[0];
}
}
}
var AllDropOrAcceptArray = new Array();
var DisplayNumOneIndex = -1;
var AllDropMacIndex = 0;
for (i = 0; i < MacFilter.length && null != MacFilter[i]; i ++)
{
if ((MacFilter[i].MACAddress == "")
&& (MacFilter[i].DestMACAddress == ""))
{
AllDropOrAcceptArray[AllDropMacIndex++] = new stNewMacIndex(i);
}
}
function stDataFromDb(domain, MACAddress, HostName)
{
this.domain = domain;
this.MACAddress = MACAddress;
this.HostName = HostName;
}
var LanSrcInfo = <%webGetCfgEntry(InternetGatewayDevice.LANDevice.1.Hosts.X_Host.{i}, Address|Name,stDataFromDb);%>;
var LanDevInfo = new Array();
var lLanIndex = 0;
var lValidFlg = 0;
for (lLanIndex = 0; lLanIndex < LanSrcInfo.length - 1; lLanIndex++)
{
if(isValidMacAddress(LanSrcInfo[lLanIndex].MACAddress))
{
LanDevInfo[lValidFlg] = LanSrcInfo[lLanIndex].MACAddress
+ " " + LanSrcInfo[lLanIndex].HostName;
lValidFlg++;
}
}
var MacString ='';
var lZeroMac = 0;
var lHideFlag = 0;
function suggestOver(div_value)
{
div_value.className = 'suggest_link_over';
}
function suggestOut(div_value)
{
div_value.className = 'suggest_link';
}
function SearchSubString()
{
var i = 0;
lZeroMac = 0;
for (i = 0; i < LanDevInfo.length; i++)
{
if (-1 != LanDevInfo[i].indexOf(MacString))
{
lZeroMac = 1;
}
}
}
function FindMacFromStr(div_value)
{
var i = 0;
for (i = 0; i < LanSrcInfo.length - 1; i++)
{
if (-1 != div_value.indexOf(LanSrcInfo[i].MACAddress))
{
return i;
}
}
return -1;
}
function HideSrcDstNode()
{
if (lHideFlag)
{
document.getElementById("search_src_suggest").style.display = 'none';
document.getElementById("search_dst_suggest").style.display = 'none';
lHideFlag = 0;
}
}
function setSrcMacSearch(div_value)
{
var Index = 0;
Index = FindMacFromStr(div_value);
if (Index != -1)
{
div_value = LanSrcInfo[Index].MACAddress
}
else
{
div_value = '';
}
document.getElementById("SourceMACAddress").value = div_value;
document.getElementById("search_src_suggest").style.display = 'none';
}
function SrcMacSearchRequest()
{
var i=0;
MacString = document.getElementById("SourceMACAddress").value;
SearchSubString();
HideSrcDstNode();
if (lZeroMac)
{
lHideFlag = 1;
document.getElementById("search_src_suggest").style.display = '';
var div = document.getElementById("search_src_suggest");
div.innerHTML = "";
for (i = 0; i < LanDevInfo.length; i++)
{
if (-1 != LanDevInfo[i].indexOf(MacString))
{
var suggest = '<div onmouseover="suggestOver(this);" ';
suggest += 'onmouseout="suggestOut(this);" ';
suggest += 'onclick="setSrcMacSearch(this.innerHTML);" ';
suggest += 'class="suggest_link">' + LanDevInfo[i] + '</div>';
div.innerHTML += suggest;
}
}
}
}
function setDstMacSearch(div_value)
{
var Index = 0;
Index = FindMacFromStr(div_value);
if (Index != -1)
{
div_value = LanSrcInfo[Index].MACAddress
}
else
{
div_value = '';
}
document.getElementById("DestMACAddress").value = div_value;
document.getElementById("search_dst_suggest").style.display = 'none';
}
function DstMacSearchRequest()
{
var i=0;
MacString = document.getElementById("DestMACAddress").value;
SearchSubString();
HideSrcDstNode();
if (lZeroMac)
{
lHideFlag = 1;
document.getElementById("search_dst_suggest").style.display = '';
var div = document.getElementById("search_dst_suggest");
div.innerHTML = "";
for (i = 0; i < LanDevInfo.length; i++)
{
if (-1 != LanDevInfo[i].indexOf(MacString))
{
var suggest = '<div onmouseover="suggestOver(this);" ';
suggest += 'onmouseout="suggestOut(this);" ';
suggest += 'onclick="setDstMacSearch(this.innerHTML);" ';
suggest += 'class="suggest_link">' + LanDevInfo[i] + '</div>';
div.innerHTML += suggest;
}
}
}
}
function addClick()
{
if(MacFilter.length >= 32 + 1)
{
AddErrInfo(eval("gErrStr6"), '')
return;
}
location.replace('/html/security/MacFilter.asp');
}
function LoadFrame()
{
top.errStrStr = '';
top.errIdCtrlStr = '';
with(document.forms[0])
{
setDisplay('DivTimeCtl',0);
setCheck("TimeCtl",0);
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_'+DisplayNumOneIndex);
}
}
}
function checkTime(time)
{
var timePart = time.split(':');
if (timePart.length != 2)
{
return null;
}
if (isPlusInteger(timePart[0]) == true)
{
var temp = parseInt(timePart[0]);
if (temp < 0 || temp > 23)
{
return null;
}
}
else
{
return null;
}
if (isPlusInteger(timePart[1]) == true)
{
var temp = parseInt(timePart[1]);
if (temp < 0 || temp > 59)
{
return null;
}
}
else
{
return null;
}
return timePart;
}
function isValidDstMacAddress(address)
{
var c = '';
var i = 0, j = 0;
var isAllZero = 1;
addrParts = address.split(':');
if ( addrParts.length != 6 )
return false;
for (i = 0; i < 6; i++)
{
if ( addrParts[i] == '' )
return false;
if ( addrParts[i].length != 2)
{
return false;
}
for ( j = 0; j < addrParts[i].length; j++ )
{
c = addrParts[i].toLowerCase().charAt(j);
if ( (c >= '0' && c <= '9') ||
(c >= 'a' && c <= 'f') )
{
if('0' != c)
{
isAllZero = 0;
}
continue;
}
else
{
return false;
}
}
}
if(1 == isAllZero)
{
return false;
}
return true;
}
function isValidSrcMacAddress(address)
{
var c = '';
var i = 0, j = 0;
var num = 0;
var isAllZero = 1;
addrParts = address.split(':');
if ( addrParts.length != 6 )
return false;
c = addrParts[0].toLowerCase().charAt(1);
if (c == '1'
|| c == '3'
|| c == '5'
|| c == '7'
|| c == '9'
|| c == 'b'
|| c == 'd'
|| c == 'f')
{
return false;
}
for (i = 0; i < 6; i++)
{
if ( addrParts[i] == '' )
return false;
if ( addrParts[i].length != 2)
{
return false;
}
for ( j = 0; j < addrParts[i].length; j++ )
{
c = addrParts[i].toLowerCase().charAt(j);
if ( (c >= '0' && c <= '9') ||
(c >= 'a' && c <= 'f') )
{
if('0' != c)
{
isAllZero = 0;
}
continue;
}
else
{
return false;
}
}
}
if(1 == isAllZero)
{
return false;
}
return true;
}
function CheckForm(type)
{
with(document.forms[0])
{
if (getElement('Name').length > 32)
{
AddErrInfo(eval("gErrStr8"), 'Name');
return false;
}
if (isPlusInteger(getElement('PriValue').value) == false)
{
AddErrInfo(eval("gErrStr9"), 'PriValue');
return;
}
if (selctIndex == -1 && MacFilter.length > 32)
{
AddErrInfo(eval("gErrStrLast"), '');
return false;
}
for (i = 0; i < MacFilter.length && null != MacFilter[i]; i++)
{
if (MacFilter[i].Name == getElement('Name').value && selctIndex != i )
{
AddErrInfo(eval("gErrStr11") + getSafeString(getElement('Name').value) + eval("gErrStr12"), 'Name');
return false;
}
}
if (parseInt(getElement('PriValue').value) != 0
&& parseInt(getElement('PriValue').value) != 255)
{
for (i = 0; i < MacFilter.length && null != MacFilter[i]; i++)
{
if (parseInt(MacFilter[i].ulPriority) == parseInt(getElement('PriValue').value)
&& selctIndex != i )
{
AddErrInfo(eval("gErrStr13") + getSafeString(getElement('PriValue').value) + eval("gErrStr14"), 'PriValue');
return false;
}
}
}
if (getElement('Name').value == '')
{
AddErrInfo(eval("gErrStr15"), 'Name');
return false;
}
if (isValidName(getElement('Name').value) == false)
{
AddErrInfo(eval("gErrStr16"), 'Name');
return false;
}
if (getElement('SourceMACAddress').value == ''
&& getElement('DestMACAddress').value == ''
&& getElement('TimeCtl').checked == true)
{
AddErrInfo(eval("gErrStr17"), 'TimeCtl');
return false;
}
if (getElement('SourceMACAddress').value == ''
&& getElement('DestMACAddress').value == '')
{
if (parseInt(getElement('PriValue').value) != 255)
{
AddErrInfo(eval("gErrStr18"), 'PriValue');
return false;
}
}
else
{
if (getElement('PriValue').value == "" || parseInt(getElement('PriValue').value) < 0 || parseInt(getElement('PriValue').value) > 254)
{
AddErrInfo(eval("gErrStr19"), 'PriValue');
return false;
}
}
if (getElement('SourceMACAddress').value != ''
&& isValidSrcMacAddress(getElement('SourceMACAddress').value) == false )
{
AddErrInfo(eval("gErrStr20") + getSafeString(getElement('SourceMACAddress').value) + eval("gErrStr21"), 'SourceMACAddress');
return false;
}
if (getElement('DestMACAddress').value != ''
&& isValidDstMacAddress(getElement('DestMACAddress').value) == false )
{
AddErrInfo(eval("gErrStr22") + getSafeString(DestMACAddress.value) + eval("gErrStr23"), 'DestMACAddress');
return false;
}
if (getElement('TimeCtl').checked == true)
{
var startTimePart = checkTime(getElement('StartTime').value);
if (startTimePart == null)
{
AddErrInfo(eval("gErrStr24") + getSafeString(getElement('StartTime').value) + eval("gErrStr25"), 'StartTime');
return false;
}
var endTimePart = checkTime(getElement('EndTime').value);
if (endTimePart == null)
{
AddErrInfo(eval("gErrStr26") + getSafeString(getElement('EndTime').value) + eval("gErrStr27"), 'EndTime');
return false;
}
if (startTimePart[0] > endTimePart[0])
{
AddErrInfo(eval("gErrStr28"), '');
return false;
}
if (startTimePart[0] == endTimePart[0]
&& startTimePart[1] >= endTimePart[1])
{
AddErrInfo(eval("gErrStr29"), '');
return false;
}
}
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
var days = getElement('day');
var str = '';
if (lang == 'zh')
{
for (var i= 0; i < 7; i++)
{
if (days[i].checked == true)
{
str += 'Y';
}
else
{
str += 'N';
}
days[i].disabled = true;
}
}
else
{
for (var j= 1; j < 7; j++)
{
if (days[j].checked == true)
{
str += 'Y';
}
else
{
str += 'N';
}
days[j].disabled = true;
}
if (days[0].checked == true)
{
str += 'Y';
}
else
{
str += 'N';
}
days[0].disabled = true;
}
SubmitForm.addParameter('x.Name',getValue('Name'));
SubmitForm.addParameter('x.Status',getValue('MacStatus'));
SubmitForm.addParameter('x.Priority',getValue('PriValue'));
SubmitForm.addParameter('x.SourceMACAddress',getValue('SourceMACAddress'));
SubmitForm.addParameter('x.DestMACAddress',getValue('DestMACAddress'));
SubmitForm.addParameter('x.TimeControl',getCheckVal('TimeCtl'));
SubmitForm.addParameter('x.StartTime',getValue('StartTime'));
SubmitForm.addParameter('x.EndTime',getValue('EndTime'));
SubmitForm.addParameter('x.Days',str);
if( selctIndex == -1 )
{
SubmitForm.setAction('addcfg.cgi?x=InternetGatewayDevice.X_FireWall.MacFilter'
+ '&RequestFile=/html/security/macfilter.asp');
}
else
{
SubmitForm.setAction('setcfg.cgi?x='+MacFilter[selctIndex].domain
+ '&RequestFile=/html/security/macfilter.asp');
}
top.aplyLine = selctIndex;
setDisable('btnApply',1);
}
function Cancel()
{
}
function ShowMacStatus(Status)
{
if (Status == 0)
{
return eval("gVarDisable");
}
else if (Status == 1)
{
return eval("gVarAccept");
}
else if (Status == 2)
{
return eval("gVarDeny");
}
}
function setCtlDisplay(record)
{
setText('Name',record.Name);
setText('SourceMACAddress',record.MACAddress);
setText('DestMACAddress',record.DestMACAddress);
setText('StartTime',record.StartTime);
setText('EndTime',record.EndTime);
setSelect('MacStatus',record.Status);
setCheck("TimeCtl",record.TimeCtl);
setText('PriValue',record.ulPriority);
if (lang == 'zh')
{
for(i=0;i<7;i++)
{
if (record.Days.charAt(i) == 'Y')
{
getElement('day')[i].checked = true;
}
else
{
getElement('day')[i].checked = false;
}
}
}
else
{
if (record.Days.charAt(6) == 'Y')
{
getElement('day')[0].checked = true;
}
else
{
getElement('day')[0].checked = false;
}
for (var j=0; j<6; j++)
{
if (record.Days.charAt(j) == 'Y')
{
getElement('day')[j+1].checked = true;
}
else
{
getElement('day')[j+1].checked = false;
}
}
}
if (record.TimeCtl == 1)
{
setDisplay('DivTimeCtl',1);
}
else if (record.TimeCtl == 0)
{
setDisplay('DivTimeCtl',0);
}
}
function ClickTimeCtl()
{
HideSrcDstNode();
with(document.forms[0])
{
if (getElement('TimeCtl').checked == true)
{
setDisplay('DivTimeCtl',1);
}
else
{
setDisplay('DivTimeCtl',0);
}
}
}
function setControl(index)
{
var record;
selctIndex = index;
if (index == -1)
{
record = new stMacFilter('','','0','','','YYYYNNN','00:00','23:59','0','0');
}
else
{
record = MacFilter[index];
}
setCtlDisplay(record);
}
function ShowTimeCtl(TimeCtl)
{
if (TimeCtl == 1)
{
return "Enable";
}
else
{
return "Disable";
}
}
function ShowMacInfo(Index,Flags)
{
document.write('<tr id="record_' + Index
+ '" class="trTabContent" onclick="selectLine(this.id);">');
document.write('<td>' + getSafeString(MacFilter[Index].Name) + '&nbsp;</td>');
if (Flags == 1)
{
document.write('<td>' + MacFilter[Index].MACAddress + '&nbsp;</td>');
document.write('<td>' + MacFilter[Index].DestMACAddress + '&nbsp;</td>');
document.write('<td>' + ShowTimeCtl(MacFilter[Index].TimeCtl) + '&nbsp;</td>');
if (MacFilter[Index].TimeCtl == 1)
{
if (lang == 'zh')
{
document.write('<td>' + MacFilter[Index].Days + '&nbsp;</td>');
}
else
{
var tmpStr = '';
var days = MacFilter[Index].Days;
tmpStr = days.charAt(6)+days.charAt(0)+days.charAt(1)+days.charAt(2)+days.charAt(3)+days.charAt(4)+days.charAt(5);
document.write('<td>' + tmpStr + '&nbsp;</td>');
}
document.write('<td>' + MacFilter[Index].StartTime + '-'
+ MacFilter[Index].EndTime + '&nbsp;</td>');
}
else
{
document.write('<td>&nbsp;</td>');
document.write('<td>&nbsp;</td>');
}
}
else
{
document.write('<td>--&nbsp;</td>');
document.write('<td>--&nbsp;</td>');
document.write('<td>--&nbsp;</td>');
document.write('<td>--&nbsp;</td>');
document.write('<td>--&nbsp;</td>');
}
document.write('<td>' + MacFilter[Index].ulPriority + '&nbsp;</td>');
document.write('<td>' + ShowMacStatus(MacFilter[Index].Status) + '&nbsp;</td>');
document.write('<td>' + '<input type=\'checkbox\' name=\'rml\''
+ ' value=\'' + MacFilter[Index].domain + '\'>' + '</td>');
document.write('</tr>');
}
function clickRemove()
{
removeInst('/html/security/macfilter.asp');
}
function FW_MAC_CfgHeader()
{
writeTabCfgHeaderEx(eval("gVarMacFilter"),'MAC Filter',"90%");
}
</script>

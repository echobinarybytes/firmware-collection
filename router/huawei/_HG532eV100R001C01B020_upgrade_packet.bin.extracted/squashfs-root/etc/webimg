<script language="JavaScript" type="text/javascript">
var url_path = '/html/ntwkall/dhcp.asp';
function dhcpcnst(domain,DHCPServerEnable,dhcpStart,dhcpEnd,DnsServer,LeaseTime
)
{
this.domain = domain;
this.dhcpStart = dhcpStart;
this.dhcpEnd = dhcpEnd;
this.LeaseTime = LeaseTime;
this.DHCPSrvEnable = DHCPServerEnable;
var dns = DnsServer.split(',');
this.DnsSrvOne = dns[0];
if (dns.length > 1)
{
this.DnsSrvTwo = dns[1];
}
else
{
this.DnsSrvTwo = '';
}
}
var dhcpst = <%webGetCfgEntry(InternetGatewayDevice.LANDevice.1.LANHostConfigManagement,DHCPServerEnable|MinAddress|MaxAddress|DNSServers|DHCPLeaseTime
, dhcpcnst);%>;
function dhcppoolcnst(domain,Enable,dhcpStart,dhcpEnd,VendorId,Relay)
{
this.PoolDomain = domain;
this.PoolEnable = Enable;
this.PoolStart = dhcpStart;
this.PoolEnd = dhcpEnd;
this.PoolVendor = VendorId;
this.PoolRelay = Relay;
}
var dhcppool = <%webGetCfgEntry(InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.DHCPConditionalServingPool.{i},Enable|MinAddress|MaxAddress|VendorClassID|X_DeviveDhcpRelay,dhcppoolcnst);%>;
function getDhcpStaticInfo(domain,Chaddr,Yiaddr)
{
this.domain = domain;
this.MacAddr = Chaddr;
this.IpAddr = Yiaddr;
}
var DhcpStatic = <%webGetCfgEntry(InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.DHCPStaticAddress.{i},Chaddr|Yiaddr,getDhcpStaticInfo);%>;
var DeviceInfo =  <%webGetCfgArray(InternetGatewayDevice.LANDevice.1.Hosts.Host.{i},X_DeviceType|IPAddress|MACAddress|LeaseTimeRemaining|Active);%>
function isLeaseIp(ip)
{
for (i = 0; i < DeviceInfo.length - 1; i++)
{
if (ip == DeviceInfo[i][2])
{
return false;
}
}
return true;
}
var i = 0;
var poolIndex = top.lanDevIndex;
var proto = ''//'<ejGetOther(sysInfo, noBrPrtcl)%>';
var natEnbl = 1;
var dhcpSvrEnbl = dhcpst[0].DHCPSrvEnable;
var dhcpStart = dhcpst[0].dhcpStart;
var dhcpEnd = dhcpst[0].dhcpEnd;
var DnsOne = dhcpst[0].DnsSrvOne;
var DnsTwo = dhcpst[0].DnsSrvTwo;
var UseWanData = dhcpst[0].UseWan;
var AssConnData = dhcpst[0].AssConnetion;
var PaMacAddrData = dhcpst[0].PaMacAddr;
var dhcpLease = dhcpst[0].LeaseTime;
if(-1 != dhcpLease)
{
var dhcpDay =  parseInt(eval(dhcpLease/86400));
var dhcpHour = parseInt(eval(dhcpLease%86400/3600));
var dhcpMin = parseInt(eval(dhcpLease%3600/60));
var dhcpSec = eval(dhcpLease%60);
}
else
{
var dhcpDay =  0;
var dhcpHour = 0;
var dhcpMin = 0;
var dhcpSec = 0;
}
var host = <%webGetCfgArray(InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.IPInterface.1,IPInterfaceIPAddress|IPInterfaceSubnetMask
);%>;
var oldAddrPrimary;
var oldMaskPrimary;
if (host.length > 1)
{
oldAddrPrimary = host[0][1];
oldMaskPrimary = host[0][2];
}
var changeAddrFlag = false;
function disableDhcpSrv()
{
with ( document.forms[0] )
{
setCheck("dhcpSrvEnable",0);
dhcpEthStart.disabled = 1;
dhcpEthEnd.disabled = 1;
DnsSrvOne.disabled = 1;
DnsSrvTwo.disabled = 1;
setDisable('permanetlea', 1);
setDisable('LeaseTimeInfo', 1);
setDisable('dhcpLeasedTimeDay', 1);
setDisable('dhcpLeasedTimeHour', 1);
setDisable('dhcpLeasedTimeMin', 1);
setDisable('dhcpLeasedTimeSec', 1);
}
}
function enableDhcpSrv(formLoad)
{
with ( document.forms[0] )
{
setCheck("dhcpSrvEnable",1);
dhcpEthStart.disabled = 0;
dhcpEthEnd.disabled = 0;
if (formLoad == 0)
{
setDhcpAddresses(ethIpAddress.value);
}
else
{
dhcpEthStart.value = dhcpStart;
dhcpEthEnd.value = dhcpEnd;
}
setDisable('DnsSrvOne',0);
setDisable('DnsSrvTwo',0);
setText('dhcpLeasedTimeDay', dhcpDay);
setText('dhcpLeasedTimeHour', dhcpHour);
setText('dhcpLeasedTimeMin', dhcpMin);
setText('dhcpLeasedTimeSec', dhcpSec);
setDisable('dhcpLeasedTimeDay', 0);
setDisable('dhcpLeasedTimeHour', 0);
setDisable('dhcpLeasedTimeMin', 0);
setDisable('dhcpLeasedTimeSec', 0);
setDisable('permanetlea', 0);
}
}
var SelStaticRuleIndex;
function setControl(index)
{
SelStaticRuleIndex = index;
}
function clickAddList(tabTitle)
{
var tab = document.getElementById(tabTitle).getElementsByTagName('table');
var row,col;
var rowLen = tab[0].rows.length;
var firstRow = tab[0].rows[0];
var lastRow = tab[0].rows[rowLen - 1];
var temp = 'record_null' + '_' + tabTitle;
if (lastRow.id == temp)
{
selectLine(temp);
return;
}
row = tab[0].insertRow(rowLen);
var appName = navigator.appName;
if(appName == 'Microsoft Internet Explorer')
{
g_browserVersion = 1; 
row.className = 'trTabContent';
row.id = temp;
row.attachEvent("onclick", function(){selectLine(temp);});
}
else
{
g_browserVersion = 2; 
row.setAttribute('class','trTabContent');
row.setAttribute('id',temp);
row.setAttribute('onclick','selectLine(this.id);');
}
col = row.insertCell(0);
col.innerHTML = '----';
for (var i = 1; i < firstRow.cells.length; i++)
{
col = row.insertCell(i);
col.innerHTML =  '<input type="text" name="newlistvalue'+ i+ '" id="newlistvalue'+ i+ '" maxlength="17">';
}
selectLine(temp);
setDisable('addNewReverve',0);
}
function clickRemove(tabTitle)
{
if (tabTitle == eval("gVarDhcpReverse"))
{
if(SelStaticRuleIndex < 0)
{
alert(eval("gVarSelectAlert"));
return;
}
DelStaticRule();
}
}
function DelStaticRule()
{
var SubmitForm = new webSubmitForm();
SubmitForm.addParameter(DhcpStatic[SelStaticRuleIndex].domain,'');
SubmitForm.setAction('del.cgi?RequestFile=' + url_path);
SubmitForm.submit();
}
function CheckMacInList(NewMac)
{
var LowNewMac = NewMac.toLowerCase();
if(DhcpStatic.length > 1)
{
for (var i = 0; i < DhcpStatic.length - 1; i++)
{
if(LowNewMac == DhcpStatic[i].MacAddr.toLowerCase())
{
return true;
}
}
return false;
}
else
{
return false;
}
}
function CheckIpInList(NewIpAddr)
{
var LowNewIpAddr = NewIpAddr.toLowerCase();
if(DhcpStatic.length > 1)
{
for (var i = 0; i < DhcpStatic.length - 1; i++)
{
if(LowNewIpAddr == DhcpStatic[i].IpAddr.toLowerCase())
{
return true;
}
}
return false;
}
else
{
return false;
}
}
function AddStaticRule()
{
if(getValue('newlistvalue2').length == 0)
{
AddErrInfo(eval("gVarIPReverseEmpty"), "newlistvalue2");
setDisplay('erroinfo', 1)
setErrInfo();
return false;
}
if(getValue('newlistvalue1').length == 0)
{
AddErrInfo(eval("gVarMACReverseEmpty"), "newlistvalue1");
setDisplay('erroinfo', 1)
setErrInfo();
return false;
}
if(isValidIpAddress(getValue('newlistvalue2')) == false)
{
AddErrInfo(eval("gErrStr1") + getValue('newlistvalue2') + eval("gErrStr2"), "newlistvalue2");
setDisplay('erroinfo', 1)
setErrInfo();
return false;
}
if(isSameSubNet(oldAddrPrimary, oldMaskPrimary, getValue('newlistvalue2'), oldMaskPrimary) == false)
{
AddErrInfo(eval("gErrStr1") + getValue('newlistvalue2') + eval("gErrStr2"), "newlistvalue2");
setDisplay('erroinfo', 1)
setErrInfo();
return false;
}
if(isLeaseIp(getValue('newlistvalue2')) == false)
{
AddErrInfo(eval("gErrStr1") + getValue('newlistvalue2') + eval("gErrStr58"), "newlistvalue2");
setDisplay('erroinfo', 1)
setErrInfo();
return false;
}
if(isValidMacAddress(getValue('newlistvalue1')) == false)
{
AddErrInfo(eval("gErrStr22") + getValue('newlistvalue1') + eval("gErrStr23"),"newlistvalue1");
setDisplay('erroinfo', 1)
setErrInfo();
return false;
}
if(DhcpStatic.length > 16)
{
AddErrInfo(eval("gVarOverMaxRule"), "");
setDisplay('erroinfo', 1)
setErrInfo();
return false;
}
if (true == CheckMacInList(getValue('newlistvalue1')))
{
AddErrInfo(eval("gVarMacExists"), "newlistvalue1");
setDisplay('erroinfo', 1)
setErrInfo();
return false;
}
if (true == CheckIpInList(getValue('newlistvalue2')))
{
AddErrInfo(eval("gVarIPExists"), "newlistvalue2");
setDisplay('erroinfo', 1)
setErrInfo();
return false;
}
var AddForm = new webSubmitForm();
AddForm.addParameter('x.Enable','1');
AddForm.addParameter('x.Chaddr',getValue('newlistvalue1'));
AddForm.addParameter('x.Yiaddr',getValue('newlistvalue2'));
AddForm.setAction('addcfg.cgi?'
+ 'x=InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.DHCPStaticAddress'
+ '&RequestFile=' + url_path);
AddForm.submit();
return true;
}
function ModifiedStaticRule()
{
if ('' != top.errStrStr)
{
ClearErro();
}
top.errStrStr = '';
top.errIdCtrlStr = '';
if(SelStaticRuleIndex < 0)
{
AddStaticRule();
}
else
{
return;
}
}
function typeClick(typeid)
{
with ( document.forms[0] )
{
switch(typeid)
{
case 0:
{
if (getCheckVal('dhcpSrvEnable') == 0)
{
disableDhcpSrv();
}
else
{
enableDhcpSrv(0);
}
}
break;
default:
break;
}
}
}
function EnblpermanetLeaChange() {
with ( document.forms[0] )
{
if (getCheckVal('permanetlea') == false)
{
setDisable('dhcpLeasedTimeDay', 0);
setDisable('dhcpLeasedTimeHour', 0);
setDisable('dhcpLeasedTimeMin', 0);
setDisable('dhcpLeasedTimeSec', 0);
setDisplay('LeaseTimeInfo',1);
if (-1 == dhcpLease)
{
setText('dhcpLeasedTimeDay', 1);
setText('dhcpLeasedTimeHour', 0);
setText('dhcpLeasedTimeMin', 0);
setText('dhcpLeasedTimeSec', 0);
}
}
else
{
setDisable('dhcpLeasedTimeDay', 1);
setDisable('dhcpLeasedTimeHour', 1);
setDisable('dhcpLeasedTimeMin', 1);
setDisable('dhcpLeasedTimeSec', 1);
setDisplay('LeaseTimeInfo',0);
}
}
}
function LoadFrame() {
var i = GetmenuName("dhcp.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
with ( document.forms[0] )
{
ethIpAddress.value = oldAddrPrimary;
ethSubnetMask.value = oldMaskPrimary;
dhcpEthStart.value = dhcpStart;
dhcpEthEnd.value = dhcpEnd;
if (proto == 'Bridge')
{
setVisible('dhcpInfo',0);
}
else
{
setVisible('dhcpInfo',1);
if ( dhcpSvrEnbl == '1' )
{
enableDhcpSrv(1);
}
else
{
disableDhcpSrv();
}
DnsSrvOne.value = DnsOne;
DnsSrvTwo.value = DnsTwo;
if(dhcpLease == -1)
{
setCheck('permanetlea',1);
setDisable('dhcpLeasedTimeDay', 1);
setDisable('dhcpLeasedTimeHour', 1);
setDisable('dhcpLeasedTimeMin', 1);
setDisable('dhcpLeasedTimeSec', 1);
setDisplay('LeaseTimeInfo',0);
}
else if(dhcpLease != 60 && dhcpLease != 3600
&& dhcpLease != 86400 && dhcpLease != 604800)
{
setText('dhcpLeasedTimeDay', dhcpDay);
setText('dhcpLeasedTimeHour', dhcpHour);
setText('dhcpLeasedTimeMin', dhcpMin);
setText('dhcpLeasedTimeSec', dhcpSec);
setCheck('permanetlea',0);
setDisable('dhcpLeasedTimeDay', 0);
setDisable('dhcpLeasedTimeHour', 0);
setDisable('dhcpLeasedTimeMin', 0);
setDisable('dhcpLeasedTimeSec', 0);
setDisplay('LeaseTimeInfo',1);
}
IpPoolIndex.selectedIndex = poolIndex;
IpPoolIndexChange(IpPoolIndex);
if (poolIndex == -1)
{
IpPoolIndex.selectedIndex = (IpPoolIndex.options.length - 1);
}
}
}
if(document.cookie.indexOf('DSO=change') > -1){
alert(eval("gVarDNSMessage"));
document.cookie="DSO=unchanged";
}
}
function isEndGTEStart(EndIp, StartIp)
{
addrEnd = EndIp.split('.');
addrStart = StartIp.split('.');
E = parseInt(addrEnd[3]) + 1;
S = parseInt(addrStart[3]) + 1;
if (E < S)
return false;
return true;
}
function ipIsInIP(subStartIp, subEndIp, StartIp, EndIp)
{
addrEnd = EndIp.split('.');
addrStart = StartIp.split('.');
subAddrEnd = subEndIp.split('.');
subAddrStart = subStartIp.split('.');
var okFlag = 0;
for(i = 0; i < 4; i++)
{
seg = parseInt(addrEnd[i]);
subSeg = parseInt(subAddrEnd[i]);
if(subSeg > seg)
{
return false;
}
else if(subSeg<seg)
{
okFlag = 1;
break;
}
}
if( okFlag = 0 )
{
return false;
}
okFlag = 0;
for(i = 0; i < 4; i++)
{
seg = parseInt(addrStart[i]);
subSeg = parseInt(subAddrStart[i]);
if((subSeg > 0) && (subSeg < seg))
{
return false;
}
else if(subSeg>seg)
{
okFlag = 1;
break;
}
}
if( okFlag = 0 )
{
return false;
}
return true;
}
function CheckForm(type)
{
with ( document.forms[0] )
{
switch(type)
{
case 1: 
{
if (Pool_Check() != true)
{
return false;
}
}
break;
case 0: 
{
var DnsSrvOneIp = DnsSrvOne.value;
var DnsSrvTwoIp = DnsSrvTwo.value;
var DSOindex = DnsSrvOneIp.lastIndexOf('.');
var DSTindex = DnsSrvTwoIp.lastIndexOf('.');
if((dhcpEthStart.value != '0.0.0.0')&&(dhcpEthEnd.value != '0.0.0.0'))
{
if (isValidIpAddress(dhcpEthStart.value) == false
|| !(isSameSubNet(ethIpAddress.value, ethSubnetMask.value, dhcpEthStart.value, ethSubnetMask.value)))
{
AddErrInfo(eval("gErrStr7") + getSafeString(dhcpEthStart.value) + eval("gErrStr8"), 'dhcpEthStart');
return false;
}
if(isBroadcastIp(dhcpEthStart.value, ethSubnetMask.value) == true)
{
AddErrInfo(eval("gErrStr9") + getSafeString(dhcpEthStart.value) + eval("gErrStr10"), 'dhcpEthStart');
return false;
}
if ( isValidIpAddress(dhcpEthEnd.value) == false
||!(isSameSubNet(ethIpAddress.value, ethSubnetMask.value, dhcpEthEnd.value, ethSubnetMask.value)))
{
AddErrInfo(eval("gErrStr11") + getSafeString(dhcpEthEnd.value) + eval("gErrStr12"), 'dhcpEthEnd');
return false;
}
if(isBroadcastIp(dhcpEthEnd.value, ethSubnetMask.value) == true)
{
AddErrInfo(eval("gErrStr13") + getSafeString(dhcpEthEnd.value) + eval("gErrStr14"), 'dhcpEthEnd');
return false;
}
if (getElement('dhcpEthStart').value != ""
&& (IpAddress2DecNum(getElement('dhcpEthStart').value) > IpAddress2DecNum(getElement('dhcpEthEnd').value)))
{
AddErrInfo(eval("gErrStr15"), 'dhcpEthStart');
return false;
}
if (dhcpEthStart.value == ethIpAddress.value)
{
AddErrInfo(eval("gErrStr16"), 'dhcpEthStart');
return false;
}
if (dhcpEthEnd.value == ethIpAddress.value)
{
AddErrInfo(eval("gErrStr17"), 'dhcpEthEnd');
return false;
}
}
if(permanetlea.checked == false)
{
if( (dhcpLeasedTimeDay.value != '') &&
((isInteger(dhcpLeasedTimeDay.value) == false)) || (parseInt(dhcpLeasedTimeDay.value) < 0))
{
AddErrInfo(eval("gErrStr53"), 'dhcpLeasedTimeDay');
return false;
}
if( (dhcpLeasedTimeHour.value != '') &&
((isInteger(dhcpLeasedTimeHour.value) == false)) || (parseInt(dhcpLeasedTimeHour.value) < 0) || (parseInt(dhcpLeasedTimeHour.value) >= 24))
{
AddErrInfo(eval("gErrStr54"), 'dhcpLeasedTimeHour');
return false;
}
if( (dhcpLeasedTimeMin.value != '') &&
((isInteger(dhcpLeasedTimeMin.value) == false)) || (parseInt(dhcpLeasedTimeMin.value) < 0) || (parseInt(dhcpLeasedTimeMin.value) >= 60))
{
AddErrInfo(eval("gErrStr55"), 'dhcpLeasedTimeMin');
return false;
}
if( (dhcpLeasedTimeSec.value != '') &&
((isInteger(dhcpLeasedTimeSec.value) == false)) || (parseInt(dhcpLeasedTimeSec.value) < 0) || (parseInt(dhcpLeasedTimeSec.value) >= 60))
{
AddErrInfo(eval("gErrStr56"), 'dhcpLeasedTimeSec');
return false;
}
}
if ( (DnsSrvOne.value != '') && (isAbcIpAddress(DnsSrvOneIp) == false
|| isDeIpAddress(DnsSrvOneIp) == true
|| isBroadcastIpAddress(DnsSrvOneIp) == true
|| isLoopIpAddress(DnsSrvOneIp) == true ) )
{
AddErrInfo(eval("gErrStr18") + getSafeString(DnsSrvOne.value) + eval("gErrStr19"), 'DnsSrvOne');
return false;
}
if ( (DnsSrvTwo.value != '') && ( isAbcIpAddress(DnsSrvTwoIp) == false
|| isDeIpAddress(DnsSrvTwoIp) == true
|| isBroadcastIpAddress(DnsSrvTwoIp) == true
|| isLoopIpAddress(DnsSrvTwoIp) == true ) )
{
AddErrInfo(eval("gErrStr20") + getSafeString(DnsSrvTwo.value) + eval("gErrStr21"), 'DnsSrvTwo');
return false;
}
}
break;
case 2: 
{
if ( isValidIpAddress(ethIpAddress.value) == false )
{
AddErrInfo(eval("gErrStr1") + getSafeString(ethIpAddress.value) + eval("gErrStr2"), 'ethIpAddress');
return false;
}
if ( isValidSubnetMask(ethSubnetMask.value) == false )
{
AddErrInfo(eval("gErrStr3") + getSafeString(ethSubnetMask.value) + eval("gErrStr4"), 'ethSubnetMask');
return false;
}
else
{
var mask = ethSubnetMask.value;
var maskParts = mask.split('.');
if((parseInt(maskParts[0])== 255)
&&(parseInt(maskParts[1])== 255)
&&(parseInt(maskParts[2])== 255)
&&(parseInt(maskParts[3]) > 253))
{
AddErrInfo(eval("gErrStr3") + getSafeString(ethSubnetMask.value) + eval("gErrStr4"), 'ethSubnetMask');
return false;
}
}
if ( isBroadcastIp(ethIpAddress.value, ethSubnetMask.value) == true )
{
AddErrInfo(eval("gErrStr5") + getSafeString(ethIpAddress.value) + eval("gErrStr6"), 'ethIpAddress');
return false;
}
}
break;
default:
break;
}
}
return true;
}
function setDhcpAddresses(lanIp)
{
with ( document.forms[0] )
{
if ( isValidIpAddress(lanIp) == false )
{
return;
}
var lanMask = ethSubnetMask.value;
var Start = dhcpEthStart.value;
var End = dhcpEthEnd.value;
addrParts = lanIp.split('.');
maskParts = lanMask.split('.');
if ( addrParts.length != 4 )
return false;
if ( maskParts.length != 4 )
return false;
t1 = parseInt(addrParts[3]) + 1;
t2 = 254 - parseInt(maskParts[3]);
if(255 <= t2 || t2 < 0)
{
return false;
}
if (dhcpEthStart.value != '' && t1 >= 255)
{
return false;
}
if (DnsSrvOne.value != ethIpAddress.value)
{
if (oldAddrPrimary == DnsOne)
{
DnsSrvOne.value = ethIpAddress.value;
}
}
if (DnsSrvTwo.value != ethIpAddress.value)
{
if (oldAddrPrimary == DnsTwo)
{
DnsSrvTwo.value = ethIpAddress.value;
}
}
}
}
function AddPoolSubmitParam(Form)
{
with (document.forms[0])
{
if (top.lanDevIndex == -1)
{
Form.addParameter('y.Enable',getCheckVal('PoolEnable'));
Form.addParameter('y.MinAddress',getValue('dhcpEthStartFrag'));
Form.addParameter('y.MaxAddress',getValue('dhcpEthEndFrag'));
Form.addParameter('y.VendorClassID',getValue('OptionText'));
Form.addParameter('y.X_DeviveDhcpRelay',getCheckVal('enableRelay'));
Form.setAction('addcfg.cgi?y=InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.DHCPConditionalServingPool'
+ '&RequestFile=' + url_path);
top.lanDevIndex = 0;
}
else
{
switch(top.dhcpOptRedio)
{
case 0:
Form.addParameter('y.Enable',getCheckVal('PoolEnable'));
Form.addParameter('y.MinAddress',getValue('dhcpEthStartFrag'));
Form.addParameter('y.MaxAddress',getValue('dhcpEthEndFrag'));
Form.addParameter('y.VendorClassID',dhcppool[top.lanDevIndex].PoolVendor);
Form.addParameter('y.X_DeviveDhcpRelay',getCheckVal('enableRelay'));
Form.setAction('setcfg.cgi?y='
+ dhcppool[top.lanDevIndex].PoolDomain
+ '&RequestFile=' + url_path);
break;
case 1:
Form.addParameter(dhcppool[top.lanDevIndex].PoolDomain,'');
Form.setAction('del.cgi?RequestFile=' + url_path);
top.lanDevIndex = 0;
break;
case 2:
Form.addParameter('y.Enable',getCheckVal('PoolEnable'));
Form.addParameter('y.MinAddress',getValue('dhcpEthStartFrag'));
Form.addParameter('y.MaxAddress',getValue('dhcpEthEndFrag'));
Form.addParameter('y.VendorClassID',getValue('UpdateOptionName'));
Form.addParameter('y.X_DeviveDhcpRelay',getCheckVal('enableRelay'));
Form.setAction('setcfg.cgi?y='
+ dhcppool[top.lanDevIndex].PoolDomain
+ '&RequestFile=' + url_path);
break;
default:
break;
}
}
}
}
function AddSubmitParam(Form,type)
{
with (document.forms[0])
{
switch(type)
{
case 0:
{
Form.addParameter('y.MinAddress',getValue('dhcpEthStart'));
Form.addParameter('y.MaxAddress',getValue('dhcpEthEnd'));
var DnsStr = '';
DnsStr = '';
var dhcpLeasedTime = 0;
var flag = false;
if (DnsSrvOne.value != '')
{
DnsStr = getValue('DnsSrvOne');
if (DnsSrvTwo.value != '')
{
DnsStr = getValue('DnsSrvOne') + ',' + getValue('DnsSrvTwo');
}
}
else if (DnsSrvTwo.value != '') 
{
DnsStr = getValue('DnsSrvTwo');
}
Form.addParameter('y.DNSServers',DnsStr);
if(getCheckVal('permanetlea') == false)
{
if ('' != getValue('dhcpLeasedTimeDay'))
{
dhcpLeasedTime += parseInt(getValue('dhcpLeasedTimeDay')) * 86400;
flag = true;
}
if ('' != getValue('dhcpLeasedTimeHour'))
{
dhcpLeasedTime += parseInt(getValue('dhcpLeasedTimeHour')) * 3600;
flag = true;
}
if ('' != getValue('dhcpLeasedTimeMin'))
{
dhcpLeasedTime += parseInt(getValue('dhcpLeasedTimeMin')) * 60;
flag = true;
}
if ('' != getValue('dhcpLeasedTimeSec'))
{
dhcpLeasedTime += parseInt(getValue('dhcpLeasedTimeSec'));
flag = true;
}
if((false == flag) || (dhcpLeasedTime == 0))
{
dhcpLeasedTime = -1;
}
}
else
{
dhcpLeasedTime = -1;
}
if(getValue('DnsSrvOne') != DnsOne){
document.cookie="DSO=change";
}
Form.addParameter('y.DHCPLeaseTime',dhcpLeasedTime);
Form.addParameter('y.DHCPServerEnable',getCheckVal('dhcpSrvEnable'));
var url = 'setcfg.cgi?'
+ 'y=InternetGatewayDevice.LANDevice.1.LANHostConfigManagement'
+ '&RequestFile=' + url_path;
Form.setAction(url);
}
break;
case 1:
{
AddPoolSubmitParam(Form);
}
break;
case 2:
{
Form.addParameter('x.IPInterfaceIPAddress',getValue('ethIpAddress'));
Form.addParameter('x.IPInterfaceSubnetMask',getValue('ethSubnetMask'));
Form.addParameter('y.MinAddress', getValue('dhcpEthStart'));
Form.addParameter('y.MaxAddress', getValue('dhcpEthEnd'));
var DnsStr2 = '';
if (DnsSrvOne.value != '')
{
DnsStr2 = getValue('DnsSrvOne');
if (DnsSrvTwo.value != '')
{
DnsStr2 = getValue('DnsSrvOne') + ',' + getValue('DnsSrvTwo');
}
}
else if (DnsSrvTwo.value != '') 
{
DnsStr2 = getValue('DnsSrvTwo');
}
Form.addParameter('y.DNSServers',DnsStr2);
var url = 'setcfg.cgi?'
+ 'x=InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.IPInterface.1'
+ '&y=InternetGatewayDevice.LANDevice.1.LANHostConfigManagement'
+ '&RequestFile=' + url_path;
Form.setAction(url);
}
break;
default:
return;
}
}
setDisable('dhcpSrvEnable',1);
setDisable('btnLANSetSubmit',1);
setDisable('btnDHCPServerSubmit',1);
setDisable('btnDHCPSPoolSubmit',1);
setDisable('btnAddIPReverve',1);
setDisable('tabIPAddressReserve',1);
}
function IpPoolIndexChange(DHCPPoolIndex)
{
top.lanDevIndex = DHCPPoolIndex.selectedIndex;
with ( document.forms[0] ) {
var index = DHCPPoolIndex.selectedIndex;
if ((-1 == index) || (DHCPPoolIndex[index].value == "Other"))
{
top.lanDevIndex = -1;
setDisplay('OptionText',1);
dhcpEthStartFrag.value = '0.0.0.0';
dhcpEthEndFrag.value = '0.0.0.0';
setCheck('enableRelay',0);
setCheck('PoolEnable',1);
document.getElementById("Option_Operation").style.display = 'none';
}
else
{
setDisplay('OptionText',0);
dhcpEthStartFrag.value = dhcppool[index].PoolStart;
dhcpEthEndFrag.value = dhcppool[index].PoolEnd;
setCheck('enableRelay',dhcppool[index].PoolRelay);
setCheck('PoolEnable',dhcppool[index].PoolEnable);
document.getElementById("Option_Operation").style.display = '';
}
setRadio('X_Opt_Operation',0);
DeviceOperation(0);
}
}
function Pool_Judge_Ip(Start,End,IpAddr,endaddr)
{
addrParts = IpAddr.split('.');
StartParts = Start.split('.');
EndParts = End.split('.');
addrendParts = endaddr.split('.');
addr_n = parseInt(addrParts[3]);
start_n = parseInt(StartParts[3]);
end_n = parseInt(EndParts[3]);
addrend_n = parseInt(addrendParts[3]);
if (addr_n == start_n
|| addr_n == end_n)
{
return true;
}
if (addrend_n == start_n
|| addrend_n == end_n)
{
return true;
}
if (addr_n > start_n
&& addr_n < end_n)
{
return true;
}
if (addrend_n > start_n
&& addrend_n < end_n)
{
return true;
}
if (addr_n < start_n
&& addrend_n > end_n)
{
return true;
}
return false;
}
function Pool_Ip_Addr_Exist(ipstartaddr,ipendaddr)
{
var ljIndex;
for (ljIndex = 0; ljIndex < dhcppool.length - 1; ljIndex++)
{
if ( ljIndex != top.lanDevIndex
&& Pool_Judge_Ip(dhcppool[ljIndex].PoolStart,dhcppool[ljIndex].PoolEnd,ipstartaddr,ipendaddr) == true)
{
return true;
}
}
return false;
}
function Pool_Check()
{
with ( document.forms[0])
{
var ljIndex;
if (top.lanDevIndex == -1
&& top.dhcpOptRedio != 0)
{
AddErrInfo(eval("gErrStr43"), 'IpPoolIndex');
return false;
}
if (top.lanDevIndex == -1
&& OptionText.value == '')
{
AddErrInfo(eval("gErrStr44"), 'OptionText');
return false;
}
if (top.lanDevIndex == -1
&& OptionText.value.indexOf('#') != -1)
{
AddErrInfo(eval("gErrStr50"), 'OptionText');
return false;
}
if (top.dhcpOptRedio == 2
&& UpdateOptionName.value.indexOf('#') != -1)
{
AddErrInfo(eval("gErrStr50"), 'UpdateOptionName');
return false;
}
if (top.dhcpOptRedio == 2
&& UpdateOptionName.value == '')
{
AddErrInfo(eval("gErrStr45"), 'UpdateOptionName');
return false;
}
if (top.lanDevIndex == -1
&& dhcppool.length - 1 >= 8)
{
AddErrInfo(eval("gErrStr46"), 'OptionText');
return false;
}
for (ljIndex = 0; ljIndex < dhcppool.length - 1; ljIndex++)
{
if (top.lanDevIndex == -1
&& OptionText.value == dhcppool[ljIndex].PoolVendor)
{
AddErrInfo(eval("gErrStr47"), 'OptionText');
return false;
}
if (top.lanDevIndex != -1
&& top.dhcpOptRedio == 2
&& ljIndex != top.lanDevIndex
&& UpdateOptionName.value == dhcppool[ljIndex].PoolVendor)
{
AddErrInfo(eval("gErrStr48"), 'UpdateOptionName');
return false;
}
}
if ((dhcpEthStartFrag.value == '')
&& (dhcpEthEndFrag.value == ''))
{
dhcpEthStartFrag.value = '0.0.0.0';
dhcpEthEndFrag.value = '0.0.0.0';
}
else if((dhcpEthStartFrag.value != '0.0.0.0')&&(dhcpEthEndFrag.value != '0.0.0.0'))
{
if (isValidIpAddress(dhcpEthStartFrag.value) == false
|| !(isSameSubNet(ethIpAddress.value, ethSubnetMask.value, dhcpEthStartFrag.value, ethSubnetMask.value)))
{
AddErrInfo(eval("gErrStr24") + getSafeString(dhcpEthStartFrag.value) + eval("gErrStr25"), 'dhcpEthStartFrag');
return false;
}
if(isBroadcastIp(dhcpEthStartFrag.value, ethSubnetMask.value) == true)
{
AddErrInfo(eval("gErrStr26") + getSafeString(dhcpEthStartFrag.value) + eval("gErrStr27"), 'dhcpEthStartFrag');
return false;
}
if ( isValidIpAddress(dhcpEthEndFrag.value) == false
|| !(isSameSubNet(ethIpAddress.value, ethSubnetMask.value, dhcpEthEndFrag.value, ethSubnetMask.value)))
{
AddErrInfo(eval("gErrStr28") + getSafeString(dhcpEthEndFrag.value) + eval("gErrStr29"), 'dhcpEthEndFrag');
return false;
}
if(isBroadcastIp(dhcpEthEndFrag.value, ethSubnetMask.value) == true)
{
AddErrInfo(eval("gErrStr30") + getSafeString(dhcpEthEndFrag.value) + eval("gErrStr31"), 'dhcpEthEndFrag');
return false;
}
if (getElement('dhcpEthStartFrag').value != ""
&& (IpAddress2DecNum(getElement('dhcpEthStartFrag').value) > IpAddress2DecNum(getElement('dhcpEthEndFrag').value)))
{
AddErrInfo(eval("gErrStr32"), 'dhcpEthStartFrag');
return false;
}
if (dhcpEthStartFrag.value == ethIpAddress.value)
{
AddErrInfo(eval("gErrStr33"), 'dhcpEthStartFrag');
return false;
}
if (dhcpEthEndFrag.value == ethIpAddress.value)
{
AddErrInfo(eval("gErrStr34"), 'dhcpEthEndFrag');
return false;
}
if (Pool_Ip_Addr_Exist(dhcpEthStartFrag.value,dhcpEthEndFrag.value) == true)
{
AddErrInfo(eval("gErrStr49"), '');
return false;
}
}
else if((dhcpEthStartFrag.value == '0.0.0.0') && (dhcpEthEndFrag.value == '0.0.0.0'))
{
return true;
}
else if((dhcpEthStartFrag.value != '0.0.0.0')&&(dhcpEthEndFrag.value == '0.0.0.0'))
{
AddErrInfo(eval("gErrStr35"), 'dhcpEthEndFrag');
return false;
}
else if((dhcpEthStartFrag.value == '0.0.0.0')&&(dhcpEthEndFrag.value != '0.0.0.0'))
{
AddErrInfo(eval("gErrStr35"), 'dhcpEthStartFrag');
return false;
}
if(!ipIsInIP(dhcpEthStartFrag.value, dhcpEthEndFrag.value, dhcpEthStart.value , dhcpEthEnd.value))
{
AddErrInfo(eval("gErrStr36"), 'dhcpEthStartFrag');
return false;
}
}
return true;
}
function DeviceOperation(Type)
{
switch(Type)
{
case 0:
document.getElementById("TXT_Option_Name").style.display = 'none';
top.dhcpOptRedio = 0;
break;
case 1:
document.getElementById("TXT_Option_Name").style.display = 'none';
top.dhcpOptRedio = 1;
break;
case 2:
if (top.lanDevIndex != -1)
{
setText('UpdateOptionName', dhcppool[top.lanDevIndex].PoolVendor);
}
else
{
setText('UpdateOptionName','');
}
document.getElementById("TXT_Option_Name").style.display = '';
top.dhcpOptRedio = 2;
break;
default:
break;
}
}

var ErrInfo = <%webGetPubInfoIdx();%>;
function GetRebootResult()
{
var xmlhttp = CreateXMLHttp();
xmlhttp.onreadystatechange = function()
{
if (xmlhttp.readyState == 4)
{
if (xmlhttp.status == 200)
{
var result = xmlhttp.responseText;
if (result == "0")
{
top.location.replace("/");
}
}
}
}
xmlhttp.open('post', '/index/getRebootRes.cgi', true);
xmlhttp.send(null);
}
function Init()
{
var ErrID = document.getElementById("err_info");
if (ErrInfo >= gPubInfoArray.length)
{
ErrInfo = 0;
}
ErrID.innerHTML = gPubInfoArray[ErrInfo];
setInterval("GetRebootResult()", 10000);
}
</SCRIPT>
</HEAD>
<BODY onLoad="Init();" topmargin="0" leftmargin="0" marginwidth="0" marginheight="0">

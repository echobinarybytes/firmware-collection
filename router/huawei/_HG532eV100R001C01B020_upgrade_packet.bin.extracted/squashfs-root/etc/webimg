<script language="JavaScript" type="text/javascript">
var selctIndex = -1;
function stDsl(domain,LinkType)
{
this.domain = domain;
var list = domain.split('.');
this.key = 	'.' + list[4] + '.';
this.LinkType = LinkType;
}
function stWanInfo(domain, Name, CntType,ServiceList,Enable, NATEnabled)
{
this.domain = domain;
var list = domain.split('.');
this.key = 	'.' + list[4] + '.';
this.Name = Name;
this.CntType = CntType;
this.LinkType = 'EoA';
this.ServiceList = ServiceList;
this.Enable = getBoolValue(Enable);
this.NATEnabled = NATEnabled;
}
var WanIPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Name|ConnectionType|X_ServiceList|Enable|NATEnabled,stWanInfo);%>;
var WanPPPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|ConnectionType|X_ServiceList|Enable|NATEnabled,stWanInfo);%>;
var WanInfo = new Array();
for (i = 0; i < WanIPInfo.length-1; i++)
{
WanInfo[i] = WanIPInfo[i];
}
for (j = 0; j < WanPPPInfo.length-1; i++,j++)
{
WanInfo[i] =  WanPPPInfo[j];
}
var host = <%webGetCfgArray(InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.IPInterface.1,IPInterfaceIPAddress|IPInterfaceSubnetMask);%>;
var IpAddress = host[0][1];
var SubnetMask = host[0][2];
var tabnum = 0;
function ShowIfName(val)
{
for (var i = 0; i < WanInfo.length; i++)
{
if (WanInfo[i].domain == val)
{
return WanInfo[i].Name;
}
else if ('br0' == val)
{
return 'br0';
}
}
return '&nbsp;';
}
function stStaticRoute(Domain, DestIPAddress, Gateway, mask, Interface)
{
this.Domain = Domain;
this.DestIPAddress = DestIPAddress;
this.Gateway = Gateway;
this.mask = mask;
this.Interface = Interface;
}
var StaticRoute = <%webGetCfgEntry(InternetGatewayDevice.Layer3Forwarding.Forwarding.{i},DestIPAddress|GatewayIPAddress|DestSubnetMask|Interface,stStaticRoute);%>;
var StaticRouteAcc = <%webGetCfgEntryAcc(InternetGatewayDevice.Layer3Forwarding.Forwarding.{i},DestIPAddress|GatewayIPAddress|DestSubnetMask|Interface,stStaticRoute);%>;
function LoadFrame()
{
var i = GetmenuName("route.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_0');
}
}
function AddSubmitParam(SubmitForm,type)
{
}
function WriteOption()
{
for (i = 0; i < WanInfo.length; i++)
{
if ((WanInfo[i].CntType == 'IP_Routed')
&& ((WanInfo[i].ServiceList.indexOf('INTERNET') >= 0) || (WanInfo[i].ServiceList.indexOf('OTHER') >= 0)))
{
document.write('<option id="wan_' + i + '" value="' + WanInfo[i].domain + '">'
+ WanInfo[i].Name + '</option>');
}
}
}
function btnClear()
{
with ( document.forms[tabnum] )
{
DestIPAddress.value = '';
DestSubnetMask.value = '';
GatewayIPAddress.value = '';
}
}
function getMostRightPosOf1(str)
{
for (i = str.length - 1; i >= 0; i--)
{
if (str.charAt(i) == '1')
{
break;
}
}
return i;
}
function getBinaryString(str)
{
var numArr = [128, 64, 32, 16, 8, 4, 2, 1];
var addrParts = str.split('.');
if (addrParts.length < 3)
{
return "00000000000000000000000000000000";
}
var binstr = '';
for (i = 0; i < 4; i++)
{
var num = parseInt(addrParts[i])
for ( j = 0; j < numArr.length; j++ )
{
if ( (num & numArr[j]) != 0 )
{
binstr += '1';
}
else
{
binstr += '0';
}
}
}
return binstr;
}
function isMatchedIpAndMask(ip, mask)
{
var locIp = getBinaryString(ip);
var locMask = getBinaryString(mask);
if (locIp.length != locMask.length)
{
return false;
}
var most_right_pos_1 = getMostRightPosOf1(locMask);
if (most_right_pos_1 == -1)
{
if (loaIp == '00000000000000000000000000000000')
{
return true;
}
else
{
return false;
}
}
for (var i = most_right_pos_1 + 1; i < locIp.length; i++)
{
if (locIp.charAt(i) != '0')
{
return false;
}
}
return true;
}
function clickRemove()
{
removeInst('/html/network/route.asp');
}
function CheckForm(type)
{
for (var i = 0; i < StaticRoute.length - 1; i++)
{
if (StaticRoute[i].DestIPAddress == getValue('DestIPAddress') && selctIndex != i )
{
AddErrInfo(eval("gErrStr0"), 'DestIPAddress');
return false;;
}
}
with ( document.forms[tabnum] )
{
var DestIp = DestIPAddress.value;
var index1 = IpAddress.lastIndexOf('.');
var index2 = DestIp.lastIndexOf('.');
if ( isValidIpAddress(DestIp) == false
|| isDeIpAddress(DestIp) == true
|| isBroadcastIpAddress(DestIp) == true
|| isLoopIpAddress(DestIp) == true )
{
AddErrInfo(eval("gErrStr1") + getSafeString(DestIPAddress.value) + eval("gErrStr2"), 'DestIPAddress');
return false;
}
if (IpAddress == DestIp
|| (DestIp.substr(index1 + 1) == '0' && IpAddress.substr(0,index1)
== DestIp.substr(0,index2)))
{
AddErrInfo(eval("gErrStr3"), 'DestIPAddress');
return false;
}
if ( isValidSubnetMask(DestSubnetMask.value) == false
&&  DestSubnetMask.value != '255.255.255.255')
{
AddErrInfo(eval("gErrStr4") + getSafeString(DestSubnetMask.value) + eval("gErrStr5"), 'DestSubnetMask');
return false;
}
if (isMatchedIpAndMask(DestIPAddress.value, DestSubnetMask.value) == false)
{
AddErrInfo(eval("gErrStr6") + getSafeString(DestIPAddress.value) + eval("gErrStr7") + DestSubnetMask.value + eval("gErrStr8"), 'DestIPAddress')
return false;
}
if (gwStr.checked == false && ifStr.checked == false)
{
AddErrInfo(eval("gErrStr9"), 'gwStr');
return false;
}
if (gwStr.checked == true)
{
if ( isAbcIpAddress(GatewayIPAddress.value) == false )
{
AddErrInfo(eval("gErrStr10") + getSafeString(GatewayIPAddress.value) + eval("gErrStr11"), 'gwStr');
return false;
}
}
if (ifStr.checked == true)
{
var selectObj = getElement('Interface');
var index = parseInt(selectObj.selectedIndex);
var idx = selectObj.options[index].id.split('_')[1];
if (getSelectVal('Interface') != 'br0' && WanInfo[idx].Enable != 1)
{
AddErrInfo(eval("gErrStr13"), 'Interface');
return false;
}
if (getSelectVal('Interface') != 'br0'
&& WanInfo[idx].LinkType == 'EoA'
&& WanInfo[idx].domain.indexOf('IPCon') > -1
&& gwStr.checked == false )
{
AddErrInfo(eval("gErrStr12"), 'ifStr');
return false;
}
}
}
return true;
}
function AddSubmitParam(Form,type)
{
with ( document.forms[tabnum] )
{
Form.addParameter('x.DestIPAddress',DestIPAddress.value);
Form.addParameter('x.DestSubnetMask',DestSubnetMask.value);
if (gwStr.checked == true)
{
Form.addParameter('x.GatewayIPAddress',GatewayIPAddress.value);
}
else
{
Form.addParameter('x.GatewayIPAddress','0.0.0.0');
}
if (ifStr.checked == true)
{
Form.addParameter('x.Interface',getSelectVal('Interface'));
}
else
{
}
}
if (selctIndex == -1)
{
Form.setAction("addcfg.cgi?x=InternetGatewayDevice.Layer3Forwarding.Forwarding"
+ "&RequestFile=/html/network/route.asp");
}
else
{
Form.setAction("setcfg.cgi?x="+ StaticRoute[selctIndex].Domain
+ "&RequestFile=/html/network/route.asp");
}
top.aplyLine = selctIndex;
setDisable('btnApply',1);
}
function setCtlDisplay(record)
{
setText('DestIPAddress',record.DestIPAddress);
setText('GatewayIPAddress',record.Gateway);
setText('DestSubnetMask',record.mask);
setSelect('Interface',record.Interface);
}
function setControl(index)
{
var record;
selctIndex = index;
if (index == -1)
{
record = new stStaticRoute('', '', '', '', '');
setDisable('btnApply',0);
setDisable('GatewayIPAddress',0);
setDisable('Interface',0);
setDisable('gwStr',0);
setDisable('ifStr',0);
setDisable('DestIPAddress', 0);
setDisable('DestSubnetMask', 0);
setCheck('gwStr',0);
setCheck('ifStr',1);
}
else
{
record = StaticRoute[index];
setDisable('btnApply',0);
setDisable('GatewayIPAddress',1);
setDisable('Interface',1);
setDisable('gwStr',1);
setDisable('ifStr',1);
if (record.Gateway == "0.0.0.0")
{
setCheck('gwStr',0);
}
else
{
setCheck('gwStr',1);
}
if (record.Interface == "")
{
setCheck('ifStr',0);
}
else
{
setCheck('ifStr',1);
}
if(StaticRouteAcc[index].DestIPAddress == '')
{
setDisable('DestIPAddress', 1);
}
else
{
setDisable('DestIPAddress', 0);
}
if(StaticRouteAcc[index].mask == '')
{
setDisable('DestSubnetMask', 1);
}
else
{
setDisable('DestSubnetMask', 0);
}
}
setCtlDisplay(record);
}

<script language="JavaScript" type="text/javascript">
var getStateInterval = 0;
function makeConfirm()
{
if(confirm(eval("gVarRemoteMakeSure")))
{
document.getElementById("button").disabled = true;
document.getElementById("progressBar").style.display = "";
document.getElementById("progressBar").innerHTML = '<img id=\"waitIcon\" src=\"/images/progress2M.gif\">';
document.getElementById("stinfo").style.display = "";
document.getElementById("stinfo").innerHTML = gVarRemoteInfo0;
sendUpgradeCgi();
getStateInterval = setInterval("getState();", 500);
}
}
function sendUpgradeCgi()
{
var sendxmlhttp = CreateXMLHttp();
var r = Math.random();
sendxmlhttp.open("post","remoteupg.cgi?r="+r,true);
sendxmlhttp.send(null);
}
function GetRebootResult()
{
var xmlhttpReboot = CreateXMLHttp();
xmlhttpReboot.onreadystatechange = function()
{
if (xmlhttpReboot.readyState == 4)
{
if (xmlhttpReboot.status == 200)
{
var result = xmlhttpReboot.responseText;
if (result == "0")
{
top.location.replace("/");
}
}
}
}
xmlhttpReboot.open('post', '/index/getRebootRes.cgi', true);
xmlhttpReboot.send(null);
}
function getState()
{
var getstatexmlhttp = CreateXMLHttp();
var stateCode;
getstatexmlhttp.onreadystatechange = function()
{
if (getstatexmlhttp.readyState == 4 && getstatexmlhttp.status == 200)
{
stateCode = getstatexmlhttp.responseText;
setMessageOnPage(stateCode);
}
}
var r = Math.random();
getstatexmlhttp.open("post","getstate.cgi?r="+r,true);
getstatexmlhttp.send(null);
}
function LoadFrame()
{
document.getElementById("stinfo").innerHTML ="";
document.getElementById("progressBar").innerHTML = "";
document.getElementById("button").disabled = false;
}
function CheckForm(type)
{
with (getElement ("ConfigForm"))
{
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
}
function setMessageOnPage(stateCode)
{
switch(parseInt(stateCode))
{
case 0:
document.getElementById("stinfo").innerHTML = gVarRemoteInfo0;
break;
case 1:
document.getElementById("stinfo").innerHTML = gVarRemoteInfo1;
clearInterval(getStateInterval);
document.getElementById("progressBar").innerHTML = "";
document.getElementById("button").disabled = false;
break;
case 2:
document.getElementById("stinfo").innerHTML = gVarRemoteInfo2;
break;
case 3:
document.getElementById("stinfo").innerHTML = gVarRemoteInfo3;
clearInterval(getStateInterval);
document.getElementById("progressBar").innerHTML = "";
document.getElementById("button").disabled = false;
break;
case 4:
document.getElementById("stinfo").innerHTML = gVarRemoteInfo4;
clearInterval(getStateInterval);
document.getElementById("progressBar").innerHTML = "";
document.getElementById("button").disabled = false;
break;
case 5:
document.getElementById("stinfo").innerHTML = gVarRemoteInfo5;
break;
case 6:
document.getElementById("stinfo").innerHTML = gVarRemoteInfo6;
clearInterval(getStateInterval);
document.getElementById("progressBar").innerHTML = "";
document.getElementById("button").disabled = false;
break;
case 7:
document.getElementById("stinfo").innerHTML = gVarRemoteInfo7;
clearInterval(getStateInterval);
document.getElementById("progressBar").innerHTML = "";
document.getElementById("button").disabled = false;
break;
case 8:
document.getElementById("stinfo").innerHTML = gVarRemoteInfo8;
break;
case 9:
document.getElementById("stinfo").innerHTML = gVarRemoteInfo9;
clearInterval(getStateInterval);
setInterval("GetRebootResult();", 10000);
break;
default:
document.getElementById("stinfo").innerHTML = gVarRemoteInfo10;
document.getElementById("button").disabled = false;
}
}

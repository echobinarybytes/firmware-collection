<script language="javascript">
if (wlanEnbl == '1')
{
for (i = 0; i < WlanInfo.length-1; i++)
{
if (WlanInfo[i].enable == 1)
{
var Auth;
var Encrypt;
if (WlanInfo[i].BeaconType == 'None')
{
Auth = 'None';
Encrypt = 'None';
}
else if (WlanInfo[i].BeaconType == 'Basic')
{
Auth = WlanInfo[i].BasicAuth;
Encrypt = WlanInfo[i].BasicEncrypt;
}
else if (WlanInfo[i].BeaconType == 'WPA')
{
Auth = WlanInfo[i].WPAAuth;
Encrypt = WlanInfo[i].WPAEncrypt;
}
else if (WlanInfo[i].BeaconType == '11i')
{
Auth = WlanInfo[i].IEEE11iAuth;
Encrypt = WlanInfo[i].IEEE11iEncrypt;
}
else if (WlanInfo[i].BeaconType == 'WPAand11i')
{
Auth = WlanInfo[i].MixedAuth;
Encrypt = WlanInfo[i].MixedEncrypt;
}
document.writeln('<tr class="trTabContent">' + '<td>'
+ 'SSID' + (i+1) + '</td>' + '<td>'
+ getSafeString(WlanInfo[i].ssid) + '&nbsp;</td><td>'
+ Auth.toUpperCase () + '&nbsp;</td>' + '<td'
+ ' align="center">' + Encrypt.toUpperCase()
+ '&nbsp;</td>' + '</tr>');
}
}
}
</script>
</table>

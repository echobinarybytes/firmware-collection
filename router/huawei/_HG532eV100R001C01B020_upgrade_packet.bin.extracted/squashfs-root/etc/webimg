<script language="JavaScript" type="text/javascript">
for (i = 0; i < Wan.length; i++)
{
document.write('<tr class="trTabContent">');
document.write('<td>' + Wan[i].Name + '</td>');
if (Wan[i].xIpv6Addr == '')

<script language="JavaScript" type="text/javascript">
for (k = 0; k < UrlCurrList.length && null != UrlCurrList[k]; k++)
{
if ((AllDropOrAcceptIndex != -1) && (k == AllDropOrAcceptIndex))
{
continue;
}
if (DisplayNumOneIndex == -1)
{
DisplayNumOneIndex = k;
}
document.write('<tr id="record_' + k
+ '" class="trTabContent" onclick="selectLine(this.id);">');
document.write('<TD align="center">' + (DisplayIndex+1) + '</TD>');
document.write('<TD align="center">' + ShowUrl(UrlCurrList[k].Url) + '</TD>');
document.write('<TD align="center">' + UrlCurrList[k].ulPriority + '</TD>');
document.write('<TD align="center">' + ShowUrlStatus(UrlCurrList[k].Status) + '</TD>');
document.write('<TD align="center"><input type="checkbox" name="rml" value="' + UrlCurrList[k].domain + '"></td>\n');
document.write('</TR>');
DisplayIndex ++;
}
if	(AllDropOrAcceptIndex != -1)
{
if (DisplayNumOneIndex == -1)
{
DisplayNumOneIndex = AllDropOrAcceptIndex;
}
document.write('<tr id="record_' + AllDropOrAcceptIndex
+ '" class="trTabContent" onclick="selectLine(this.id);">');
document.write('<TD align="center">' + (DisplayIndex+1) + '</TD>');
document.write('<TD align="center">' + ShowUrl(UrlCurrList[AllDropOrAcceptIndex].Url) + '</TD>');
document.write('<TD align="center">' + UrlCurrList[AllDropOrAcceptIndex].ulPriority + '</TD>');
document.write('<TD align="center">' + ShowUrlStatus(UrlCurrList[AllDropOrAcceptIndex].Status) + '</TD>');
document.write('<TD align="center"><input type="checkbox" name="rml" value="' + UrlCurrList[AllDropOrAcceptIndex].domain + '"></td>\n');
document.write('</TR>');
DisplayIndex ++;
}

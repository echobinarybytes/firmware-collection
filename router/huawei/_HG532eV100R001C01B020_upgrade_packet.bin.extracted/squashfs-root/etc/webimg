<head>
<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/util.js"></script>
<script language="JavaScript" src="/lang/igmpsnooping.res"></script>
<title>IGMP Snooping</title>
<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" type="text/javascript">
function stIGMPCfg(domain,IGMPProxyEnable,IGMPSnoopingEnable,GroupNumber,WanList,DownVersion)
{
this.domain = domain;
this.IGMPProxyEnable = IGMPProxyEnable;
this.IGMPSnoopingEnable = IGMPSnoopingEnable
this.GroupNumber = GroupNumber;
this.WanList = WanList;
this.DownVersion = DownVersion;
}
function stWan(domain,Enable,Name,ConnectionType,ServerList, NATEnabled)
{
this.domain = domain;
this.Enable = Enable;
this.Name = Name;
this.CntType = ConnectionType;
this.ServerList = ServerList;
this.NATEnabled = NATEnabled;
}
var vIgmpProxy = <%webGetCfgEntry(InternetGatewayDevice.Services.X_IPTV,IGMPProxyEnable|IGMPSnoopingEnable|GroupNumber|WanList|DownVersion,stIGMPCfg);%>;
var igmp = vIgmpProxy[0];
var WanPPP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Enable|Name|ConnectionType|X_ServiceList|NATEnabled,stWan);%>;
var WanIP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Enable|Name|ConnectionType|X_ServiceList|NATEnabled,stWan);%>;
var Wan = new Array();
for (i = 0; i < WanPPP.length-1; i++)
{
Wan[i] = WanPPP[i];
}
for (j = 0; j < WanIP.length-1; j++,i++)
{
Wan[i] = WanIP[j];
}
function LoadFrame()
{
var i = GetmenuName("igmpsnooping.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
with ( document.forms[0])
{
setCheck('chkIgmpProxy',igmp.IGMPProxyEnable);
setCheck('chkIgmpSnp',igmp.IGMPSnoopingEnable);
setRadio('DownVersion',igmp.DownVersion);
proxyClick();
if(IGMPWanIsExist())
{
setValueDft('Interface',igmp.WanList);
}
else
{
setValueDft('Interface',"");
}
}
}
function CheckForm(type)
{
if((getSelectVal('Interface')=="")&&(getCheckVal('chkIgmpProxy') == 1))
{
AddErrInfo(eval("gErrStr2"), 'Interface');
return false;
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
var IgmpSnp = getCheckVal('chkIgmpSnp');
SubmitForm.addParameter('InternetGatewayDevice.Services.X_IPTV.IGMPSnoopingEnable',IgmpSnp);
var IgmpProxy = getCheckVal('chkIgmpProxy');
SubmitForm.addParameter('InternetGatewayDevice.Services.X_IPTV.IGMPProxyEnable',IgmpProxy);
var WanInfo = "";
if(IgmpProxy == 1)
{
var WanInfo  = getSelectVal('Interface');
SubmitForm.addParameter('InternetGatewayDevice.Services.X_IPTV.WanList',WanInfo);
}
else
{
SubmitForm.addParameter('InternetGatewayDevice.Services.X_IPTV.WanList',WanInfo);
}
SubmitForm.addParameter('InternetGatewayDevice.Services.X_IPTV.DownVersion' ,getRadioVal('DownVersion'));
SubmitForm.setAction('setcfg.cgi?RequestFile=/html/application/igmpsnooping.asp');
setDisable('btnApply',1);
}
function proxyClick()
{
setDisplay('proxyEnable',getCheckVal('chkIgmpProxy'));
}
function WriteRouteOption()
{
document.write('<option value="" >' + "" + '</option>');
for (i = 0; i < Wan.length; i++)
{
if (Wan[i].CntType.indexOf('IP_Routed') < 0)
{
continue;
}
if (Wan[i].Enable == 0)
{
continue;
}
if (Wan[i].ServerList.indexOf('INTERNET') < 0 && Wan[i].ServerList.indexOf('OTHER') < 0)
{
continue;
}
if (((Wan[i].NATEnabled != 1) && (Wan[i].NATEnabled != 2)))
{
continue;
}
document.write('<option id="wan_' + i + '" value="' + Wan[i].domain + '">'
+ Wan[i].Name + '</option>');
}
}
function IGMPWanIsExist()
{
var IGMPWanList = igmp.WanList;
for (i = 0; i < Wan.length; i++)
{
if(IGMPWanList == Wan[i].domain)
{
if((Wan[i].CntType.indexOf('Bridge') > -1)||(Wan[i].ServerList == 'TR069'))
{
return false;
}
else
{
return true;
}
}
}
return false;
}
function WanChose()
{
with ( document.forms[0])
{
if((Interface.options.length > 0)&&(Interface.options[0].value == ""))
{
Interface.options[0] = null;
}
}
}

<title>Mirror</title>
<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" type="text/javascript">
var enblMirror = '<%webGetParaByDomainName(InternetGatewayDevice.Services.X_Mirror.Enable);%>';
var lanInterface = '<%webGetParaByDomainName(InternetGatewayDevice.Services.X_Mirror.LanInterface);%>';
function MirrorEnableChange(flag)
{
with (getElById('ConfigForm'))
{
if(MirrorEnable.checked == true)
{
setDisplay('mirrorconfig', 1);
WriteLanInterFace(flag);
}
else
{
setDisplay('mirrorconfig', 0);
if (0 == flag)
{
onSubmit();
}
}
}
}
function LoadFrame()
{
setCheck('MirrorEnable', enblMirror);
MirrorEnableChange(1);
}
function AddSubmitParam(Form)
{
Form.addParameter('InternetGatewayDevice.Services.X_Mirror.Enable', getCheckVal('MirrorEnable'));
Form.addParameter('InternetGatewayDevice.Services.X_Mirror.LanInterface', getSelectVal('LanInterface'));
Form.setAction('setcfg.cgi?RequestFile=/html/management/mirror.asp');
}
function stLan(Domain,Name)
{
this.Domain = Domain;
this.Name = Name;
}
var LanList = new Array();
LanList[0] = new stLan("InternetGatewayDevice.LANDevice.1.LANEthernetInterfaceConfig.1","LAN1");
function GetLanName(Domain)
{
for(i=0;i<1;i++)
{
if(Domain == LanList[i].Domain)
{
this.Name = LanList[i].Name;
return this;
}
}
}
function GetLanDomain(Name)
{
for(i=0;i<8;i++)
{
if(Name == LanList[i].Name)
{
this.Domain = LanList[i].Domain;
return this;
}
}
}
function WriteLanInterFace(flag)
{
var loc, k, i;
loc = '<table class="tabInfo" cellpadding="2" cellspacing="0">';
loc = '<table width="100%" border="0">';
loc +='<tr class="trTabConfigure" align="left"><td width="154" align="right">Mirror to:</td>';
loc += '<td width="16">&nbsp;</td>';
loc +='<td colspan="6">';
loc +='<select size="1" id="LanInterface" name="LanInterface">';
if (flag == 0)
{
for(i=0;i<LanList.length;i++)
{
loc +='<option value="'+LanList[i].Domain+'">'+ LanList[i].Name +'</option>';
}
}
else
{
var LanMax = new GetLanName(lanInterface);
for(i=0;i<LanList.length;i++)
{
if (LanList[i].Name == LanMax.Name)
{
loc +='<option value="'+lanInterface+'" selected>'+ LanMax.Name +'</option>';
}
else
{
loc +='<option value="'+LanList[i].Domain+'">'+ LanList[i].Name +'</option>';
}
}
}
loc +='</select></td>';
loc +='</tr>';
loc +='</table>';
getElement('lan').innerHTML = loc;
}
function onSubmit(type)
{
var Form = new webSubmitForm();
AddSubmitParam(Form, type);
Form.submit();
}

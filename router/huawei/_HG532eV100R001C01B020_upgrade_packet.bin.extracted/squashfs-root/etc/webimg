<script language="JavaScript" type="text/javascript">
function WanPPPConstruction(Domain,Username,PasswordIsNull,Enable,ConnectionType)
{
this.Domain = Domain;
this.Username = Username;
this.Enable = Enable;
this.ConnectionType = ConnectionType;
if (PasswordIsNull == 1)
{
this.Password = '';
}
else
{
this.Password = '@1GV)Z<!';
}
}
var WANPPP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.1.WANConnectionDevice.{i}.WANPPPConnection.{i}, Username|X_PasswordIsNull|Enable|ConnectionType, WanPPPConstruction);%>;
var PPPnum = 0;
for (i=0; i<WANPPP.length - 1; ++i)
{
if((1 == WANPPP[i].Enable) && ('IP_Routed' == WANPPP[i].ConnectionType))
{
++PPPnum;
break;
}
}
var QuickConfigured = <%webGetParaByDomainName(InternetGatewayDevice.DeviceInfo.X_ServiceManage.QuickConfigured);%>;
var un,pw;
function stWlan(Domain,BeaconType,SSID,WEPKeyIndex,EncryptionLevel)
{
this.Domain = Domain;
this.BeaconType = BeaconType;
this.SSID = SSID;
this.WEPKeyIndex = WEPKeyIndex;
this.EncypBit = parseInt(EncryptionLevel);
}
var Wlan = <%webGetCfgEntry(InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i},BeaconType|SSID|WEPKeyIndex|WEPEncryptionLevel,stWlan)%>
var keys = <%webGetCfgArray(InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.{i},WEPKey);%>;
function LoadFrame()
{
document.getElementById('txt_Username').focus();
var Username = document.getElementById('txt_Username');
var Password = document.getElementById('txt_Password');
Username.value = Wlan[0].SSID;
if (Wlan[0].BeaconType != 'Basic')
{
Password.value = '@1GV)Z<!';
}
else
{
var i = Wlan[0].WEPKeyIndex - 1;
Password.value = keys[i][1];
}
un = Username.value;
pw = Password.value;
}
function SubmitForm(type)
{
var Username = document.getElementById('txt_Username');
var Password = document.getElementById('txt_Password');
if(Username.value == "")
{
alert(eval("gWLANErr1"));
if (type)
return false;
else
return;
}
if (Password.value == "")
{
alert(eval("gWLANErr2"));
if (type)
return false;
else
return;
}
var ssid = Username.value;
if (isValidAscii(ssid) != '')
{
alert(eval("gWLANErr6") + isValidAscii(ssid) + eval("gWLANErr7"));
return false;
}
if (ssid.charAt(ssid.length - 1) == ' ')
{
alert(eval("gWLANSSIDErr2"));
return false;
}
else if(ssid.charAt(0) == ' ')
{
alert(eval("gWLANSSIDErr1"));
return false;
}
if ((un == Username.value) && (pw == Password.value))
{
if(QuickConfigured == 2)
{
var Form = new webSubmitForm();
var url = "setcfg.cgi?x=InternetGatewayDevice.DeviceInfo.X_ServiceManage&RequestFile=/html/content1.asp"
Form.addParameter('x.QuickConfigured', 0);
Form.setAction(url);
Form.submit();
return;
}
else
{
top.location.replace("/html/content1.asp");
return;
}
}
var Form = new webSubmitForm();
var url;
if( Wlan[0].BeaconType == "Basic")
{
if (isValidWEPKey(Password.value) == false)
{
return;
}
Form.addParameter('x.SSID',Username.value);
if (pw != Password.value)
{
var i=Wlan[0].WEPKeyIndex;
var j='x.WEPKey.'+i+'.WEPKey';
Form.addParameter(j,Password.value);
}
url = "setcfg.cgi?x=InternetGatewayDevice.LANDevice.1.WLANConfiguration.1";
}
else
{
if (isValidWPAPskKey(Password.value) == false)
{
alert(eval("gWLANErr5"));
return;
}
Form.addParameter('x.SSID',Username.value);
url = "setcfg.cgi?x=InternetGatewayDevice.LANDevice.1.WLANConfiguration.1";
if (pw != Password.value)
{
Form.addParameter('y.PreSharedKey',Password.value);
url =url + "&y=InternetGatewayDevice.LANDevice.1.WLANConfiguration.1.PreSharedKey.1";
}
}
url = url + "&RequestFile=/html/content1.asp";
Form.setAction(url);
Form.submit();
}
function onNext()
{
SubmitForm(0);
}
function isValidKey(val, size) {
var ret = false;
var len = val.length;
var dbSize = size * 2;
if ( len == size )
{
ret = true;
}
else if ( len == dbSize )
{
for ( i = 0; i < dbSize; i++ )
if ( isHexaDigit(val.charAt(i)) == false )
{
break;
}
if ( i == dbSize )
{
ret = true;
}
}
else
{
ret = false;
}
return ret;
}
function isValidWEPKey(val)
{
var KeyBit = parseInt(Wlan[0].EncypBit) + 24;
if(KeyBit == 128)
{
if (isValidKey(val, 13) == false)
{
alert(eval("gWLANErr3"));
return false;
}
}
else
{
if (isValidKey(val, 5) == false)
{
alert(eval("gWLANErr4"));
return false;
}
}
return true;
}
function isValidWPAPskKey(val)
{
var ret = false;
var len = val.length;
var maxSize = 64;
var minSize = 8;
if (len >= minSize && len < maxSize)
{
if (isValidAscii(val) == '')
{
ret = true;
}
}
else if (len == maxSize)
{
for (i = 0; i < maxSize; i++)
{
if (isHexaDigit(val.charAt(i)) == false)
{
break;
}
}
if (i == maxSize)
{
ret = true;
}
}
else
{
ret = false;
}
return ret;
}
function advanced()
{
top.location.replace("/html/content1.asp");
}
function onBack()
{
top.location.replace("/html/quickconfig/quicksetppp.asp");
}

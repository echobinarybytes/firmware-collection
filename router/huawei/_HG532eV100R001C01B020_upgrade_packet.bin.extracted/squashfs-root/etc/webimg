var UserLevel = 1;
function stWlan(domain,enable,ssid,mode,channel,AutoChannel,rate,power,wlHide,BeaconType,Encryption,AuthMode,KeyIndex,EncryptionLevel,WPAModes,IEEE11iEModes,WPAWPA2Modes,Country,DeviceNum,TxBurst,IgmpSnooping,AMSDU,DeclineBA,AutoBA,RDG,HtDisallowTKIP,APISOControl,BWControl,GIControl,WMMEnable,TxRxStream,MCSValue)
{
this.domain = domain;
this.enable = enable;
this.ssid = ssid;
this.mode = mode;
this.channel = channel;
this.AutoChannel = AutoChannel;
this.rate = rate;
this.power = power;
this.wlHide = (1-parseInt(wlHide));
this.BeaconType = BeaconType;
this.Encryption = Encryption;
this.AuthMode = AuthMode;
this.KeyIndex = KeyIndex;
this.EncypBit = parseInt(EncryptionLevel);
this.WPAModes = WPAModes;
this.IEEE11iEModes = IEEE11iEModes;
this.WPAWPA2Modes = WPAWPA2Modes;
this.Country = Country;
this.DeviceNum = DeviceNum;
this.TxBurst = TxBurst;
this.IgmpSnooping = IgmpSnooping;
this.AMSDU = AMSDU;
this.DeclineBA = DeclineBA;
this.AutoBA = AutoBA;
this.RDG = RDG;
this.HtDisallowTKIP = HtDisallowTKIP;
this.APISOControl = APISOControl;
this.BWControl = BWControl;
this.GIControl = GIControl;
this.WMMEnable= WMMEnable;
this.TxRxStream = TxRxStream;
this.MCSValue = MCSValue;
}
function stWps(domain,WPSControl,WPSMode,APPinValue,PinValue)
{
this.domain = domain;
this.WPSControl = WPSControl;
this.WPSMode = WPSMode;
this.APPinValue=APPinValue;
this.PinValue = PinValue;
}
var Wlan = <%webGetCfgEntry(InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i},Enable|SSID|Standard|Channel|AutoChannelEnable|MaxBitRate|X_PowerValue|SSIDAdvertisementEnabled|BeaconType|BasicEncryptionModes|BasicAuthenticationMode|WEPKeyIndex|WEPEncryptionLevel|WPAEncryptionModes|IEEE11iEncryptionModes|X_MixedEncryptionModes|RegulatoryDomain|X_AssociateDeviceNum|X_TxBurst|X_IgmpSnooping|X_AMSDU|X_DeclineBA|X_AutoBA|X_RDG|X_HtDisallowTKIP|X_WlanIsolateControl|X_Wlan11NBWControl|X_Wlan11NGIControl|WMMEnable|X_Wlan11NTxRxStream|X_Wlan11NHtMcs,stWlan);%>;
var WPSCfg = <%webGetCfgEntry(InternetGatewayDevice.LANDevice.1.WLANConfiguration.1.WPS,Enable|X_WPSMode|DevicePassword|PinPhrase,stWps);%>;
var keys = <%webGetCfgArray(InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.WEPKey.{i},WEPKey);%>;
var wpaPskKey = <%webGetCfgArray(InternetGatewayDevice.LANDevice.1.WLANConfiguration.{i}.PreSharedKey.1,PreSharedKey);%>;
var enbl = <%webGetParaByDomainName(InternetGatewayDevice.LANDevice.1.X_WLANEnable);%>;
var state = enbl;

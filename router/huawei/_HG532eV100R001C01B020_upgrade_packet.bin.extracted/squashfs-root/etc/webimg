for (var i = 0; i < WAN.length; i++)
{
document.write('<TR id=record_' + i +' class="trTabContent" onclick="selectLine(this.id);">');
document.write('<TD align="center">' + WAN[i].Name + '</TD>');
if ('PPP_Routed' == WAN[i].ConnType)
{
document.write('<TD align="center">PPPoE</TD>');
}
else if ('PPPoA' == WAN[i].ConnType)
{
document.write('<TD align="center">PPPoA</TD>');
}
else if ('IP_Routed' == WAN[i].ConnType)
{
document.write('<TD align="center">IPoE</TD>');
}
else if (('IP_Bridged' == WAN[i].ConnType) || ('PPPoE_Bridged' == WAN[i].ConnType))
{
document.write('<TD align="center">Bridge</TD>');
}
else
{
document.write('<TD align="center">' + WAN[i].ConnType + '</TD>');
}
if ((WAN[i].Status == 'Connected') || (WAN[i].xIpv6Status == 'Connected'))
{
document.write('<TD align="center">' + eval('gVarConnected') + '</TD>');
}
else if ((WAN[i].Status == 'Connecting') || (WAN[i].xIpv6Status == 'Connecting'))
{
document.write('<TD align="center">' + eval('gVarConnecting') + '</TD>');
}
else
{
document.write('<TD align="center">' + eval('gVarDisConnected') + '</TD>');
}
if (("" != WAN[i].IpAddress) && ("" != WAN[i].xIpv6Addr))
{
document.write('<TD align="center">' + WAN[i].IpAddress + '<br>' + WAN[i].xIpv6Addr + '</TD>');
}
else if ("" == WAN[i].xIpv6Addr)
{
document.write('<TD align="center">' + WAN[i].IpAddress + '</TD>');
}
else
{
document.write('<TD align="center">' + WAN[i].xIpv6Addr + '</TD>');
}
document.write('<TD align="center">' + WAN[i].Enable + '</TD>');
document.write('</TR>');
}

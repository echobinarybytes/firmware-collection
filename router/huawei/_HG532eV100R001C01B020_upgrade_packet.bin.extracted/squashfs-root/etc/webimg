<script language="JavaScript" type="text/javascript">
function LoadFrame()
{
if ((QuickConfigured != 1)
)
{
var i;
var j = 0;
for (i = 0; i < WANPPP.length - 1; ++i)
{
if ((1 == WANPPP[i].Enable) && ('IP_Routed' == WANPPP[i].ConnectionType))
{
++j;
}
}
if (j > 0)
{
location.replace('/html/quickconfig/quicksetppp.asp');
}
else
{
location.replace('/html/quickconfig/quicksetwlan.asp');
}
}
else
{
location.replace('/html/content1.asp');
}
}
</script>

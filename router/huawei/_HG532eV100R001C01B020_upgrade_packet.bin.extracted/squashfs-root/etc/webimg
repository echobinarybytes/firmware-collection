function stWlan(domain,enable,ssid,BeaconType,BasicEncrypt,BasicAuth,WPAEncrypt,WPAAuth,IEEE11iEncrypt,IEEE11iAuth,MixedEncrypt,MixedAuth,Channel,ChannelUsed)
{
this.domain = domain;

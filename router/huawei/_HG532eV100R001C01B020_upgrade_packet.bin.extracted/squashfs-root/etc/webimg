function stFWInfo(domain,LevelName,Revisable)
{
this.domain = domain;
this.Name = LevelName;
this.Revisable = Revisable;
}
var FWLevel  = <%webGetCfgEntry(InternetGatewayDevice.X_FireWall.FilterMode.{i},Level|Revisable,stFWInfo);%>;
function stCuLevel(domain,LevelName)
{
this.domain = domain;
this.CurrName = LevelName;
}
var CurrLevel = <%webGetCfgEntry(InternetGatewayDevice.X_FireWall,CurrentLevel,stCuLevel);%>
var i = 0;
var lCurr_Index = -1; 
for (i = 0; i < FWLevel.length - 1; i++)
{
if (FWLevel[i].Name == CurrLevel[0].CurrName)
{
top.g_FWLevel = i;
lCurr_Index = i;
}
}
function FWLevelNameValidate(NewName)
{
for (i = 0; i < FWLevel.length - 1; i++)
{
if (FWLevel[i].Name == NewName)
{
return true;
}
}
return false;
}
function Sun_Submit()
{
var SubmitForm = new webSubmitForm();
var revisable = 1; 
with ( document.forms[0])
{
SubmitForm.addParameter('x.CurrentLevel',FWLevel[top.g_FWLevel].Name);
SubmitForm.setAction('setcfg.cgi?x=InternetGatewayDevice.X_FireWall'
+ '&RequestFile=/html/security/firewall.asp');
SubmitForm.submit();
}
setDisable('Apply',1);
}
function LoadFrame()
{
top.errStrStr = '';
top.errIdCtrlStr = '';
var i = GetmenuName("firewall.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
with(document.forms[0])
{
if (top.g_FWLevel != -1)
{
FireWallLevel.selectedIndex = top.g_FWLevel;
}
else
{
FireWallLevel.selectedIndex = 0;
FireWallLevel.value = "Other";
}
FWLevelChange(FireWallLevel, 0);
}
}
function FWLevelChange(FWLevelList, flag)
{
if('1' == flag)  
{
if("Off" == FWLevelList[FWLevelList.selectedIndex].value)
{
if(! confirm(eval("gVarLevelOffWarning")))
{
FWLevelList.selectedIndex = lCurr_Index;
return;
}
}
}
top.g_FWLevel = FWLevelList.selectedIndex;
if (FWLevelList[FWLevelList.selectedIndex].value == "Other")
{
top.g_FWLevel = -1;
}
var helpinfo = getElement('helpinfo');
if ("Low" == FWLevelList[FWLevelList.selectedIndex].value)
{
helpinfo.innerHTML = eval("gVarInformationOfLow");
}
else if ("High" == FWLevelList[FWLevelList.selectedIndex].value)
{
helpinfo.innerHTML = eval("gVarInformationOfHigh");
}
else if ("Off" == FWLevelList[FWLevelList.selectedIndex].value)
{
helpinfo.innerHTML = eval("gVarInformationOfOff");
}
}
</script>

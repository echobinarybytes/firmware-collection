<script language="JavaScript" type="text/javascript">
function stSyslogCfg(domain,Enable,ServerEnable,Level,DisplayLevel,MainServer,MainServerPort,MinorServer,MinorServerPort)
{
this.domain = domain;
this.Enable = Enable;
this.ServerEnable = ServerEnable;
this.Level = Level;
this.DisplayLevel = DisplayLevel;
this.MainServer = MainServer;
this.MainServerPort = MainServerPort;
this.MinorServer = MinorServer;
this.MinorServerPort = MinorServerPort;
}
var temp = <%webGetCfgEntry(InternetGatewayDevice.X_SyslogConfig,Enable|LogServerEnable|Level|DisplayLevel|MainServer|MainServerPort|MinorServer|MinorServerPort,stSyslogCfg);%>;
var SyslogCfg = temp[0];
var tempAcc = <%webGetCfgEntryAcc(InternetGatewayDevice.X_SyslogConfig,Enable|LogServerEnable|Level|DisplayLevel|MainServer|MainServerPort|MinorServer|MinorServerPort,stSyslogCfg);%>;
var SyslogCfgAcc = tempAcc[0];
var logEnable;
var pageData;
var g_mainServerIsSet;
var g_minorServerIsSet;
function InitServerPara()
{
g_mainServerIsSet = 0;
g_minorServerIsSet = 0;
}
function FinalDisplay()
{
var  ServerEnable;
ServerEnable = getSelectVal('LogServerEnable');
if (0 == ServerEnable)
{
setDisplay('TrPrimaryServerAddr', 0);
setDisplay('TrPrimaryServerPort', 0);
setDisplay('TrMinorServerAddr', 0);
setDisplay('TrMinorServerPort', 0);
}
else if (1 == ServerEnable)
{
setDisplay('TrPrimaryServerAddr', 1);
setDisplay('TrPrimaryServerPort', 1);
setDisplay('TrMinorServerAddr', 1);
setDisplay('TrMinorServerPort', 1);
}
}
function GetPageData()
{
this.xEnable = getCheckVal('Enable');
this.xLevel = getSelectVal('Level');
this.xLogServerEnable = getSelectVal('LogServerEnable');
this.xMainServer = getValue('MainServer');
this.xMainServerPort = getValue('MainServerPort');
this.xMinorServer = getValue('MinorServer');
this.xMinorServerPort = getValue('MinorServerPort');
}
function LoadFrame()
{
var i = GetmenuName("logcfg.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
setCheck('Enable',SyslogCfg.Enable);
setSelect('LogServerEnable',SyslogCfg.ServerEnable);
setSelect('Level',SyslogCfg.Level);
setText('MainServer',SyslogCfg.MainServer);
setText('MainServerPort',SyslogCfg.MainServerPort);
setText('MinorServer',SyslogCfg.MinorServer);
setText('MinorServerPort',SyslogCfg.MinorServerPort);
setDisplay('Filter',SyslogCfg.Enable);
setDisable('Enable',TransAcc(SyslogCfgAcc.Enable));
setDisable('LogServerEnable',TransAcc(SyslogCfgAcc.ServerEnable));
setDisable('Level',TransAcc(SyslogCfgAcc.Level));
setDisable('MainServer',TransAcc(SyslogCfgAcc.MainServer));
setDisable('MainServerPort',TransAcc(SyslogCfgAcc.MainServerPort));
setDisable('MinorServer',TransAcc(SyslogCfgAcc.MinorServer));
setDisable('MinorServerPort',TransAcc(SyslogCfgAcc.MinorServerPort));
FinalDisplay();
}
function CheckForm(checkflag)
{
with (getElement('ConfigForm'))
{
logEnable = getCheckVal('Enable');
if (0 == checkflag)
{
return true;
}
InitServerPara();
pageData = new GetPageData();
if (1 == pageData.xLogServerEnable)
{
if (0 != MainServer.value.length)
{
if ((true == isCorrectIpAddress(MainServer.value))
|| (true == isValidDomain(MainServer.value, 64)))
{
if (0 != MainServerPort.value.length)
{
if(true == isValidPort(MainServerPort.value))
{
g_mainServerIsSet = 1;
}
else
{
AddErrInfo(eval("gErrStr1"), 'MainServerPort');
return false;
}
}
else
{
AddErrInfo(eval("gErrStr1"), 'MainServerPort');
return false;
}
}
else
{
AddErrInfo(eval("gErrStr0"), 'MainServer');
return false;
}
} 
if (0 != MinorServer.value.length)
{
if ((true == isCorrectIpAddress(MinorServer.value))
|| (true == isValidDomain(MinorServer.value, 64)))
{
if (0 != MinorServerPort.value.length)
{
if(true == isValidPort(MinorServerPort.value))
{
g_minorServerIsSet = 1;
}
else
{
AddErrInfo(eval("gErrStr2"), 'MinorServerPort');
return false;
}
}
else
{
AddErrInfo(eval("gErrStr2"), 'MinorServerPort');
return false;
}
}
else
{
AddErrInfo(eval("gErrStr5"), 'MinorServer');
return false;
}
} 
if((1 == g_mainServerIsSet) || (1 == g_minorServerIsSet))
{
return true;
}
else
{
AddErrInfo(eval("gErrStr6"), 'MainServer');
return false;
}
} 
return true;
} 
}
function AddSubmitParam(SubmitForm,type)
{
if (0 == type)
{
SubmitForm.addParameter('x.Enable', logEnable);
}
else
{
SubmitForm.addParameter('x.Enable', pageData.xEnable);
SubmitForm.addParameter('x.Level', pageData.xLevel);
SubmitForm.addParameter('x.LogServerEnable', pageData.xLogServerEnable);
if(1 == pageData.xLogServerEnable)
{
if(1 == g_mainServerIsSet)
{
SubmitForm.addParameter('x.MainServer', pageData.xMainServer);
SubmitForm.addParameter('x.MainServerPort', pageData.xMainServerPort);
}
if(1 == g_minorServerIsSet)
{
SubmitForm.addParameter('x.MinorServer', pageData.xMinorServer);
SubmitForm.addParameter('x.MinorServerPort', pageData.xMinorServerPort);
}
}
}
SubmitForm.setAction('setcfg.cgi?x=InternetGatewayDevice.X_SyslogConfig&RequestFile=/html/management/logcfg.asp');
setDisable('Enable',1);
}
</script>
</head>

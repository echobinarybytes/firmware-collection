function LoadFrame()
{
loadFrameHook();
var i = GetmenuName("cfgfile.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
}
function CheckForm(type)
{
with (getElement ("ConfigForm"))
{
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
}
function VerifyFile(FileObject)
{
var FileName = FileObject.value;
if (FileName.length == 0)
{
AddErrInfo(eval("gErrStr0"), '');
return false;
}
if (FileName.length > 128)
{
AddErrInfo(eval("gErrStr1"), '');
return false;
}
try
{
fso = new ActiveXObject("Scripting.FileSystemObject");
try
{
var FileSize = fso.GetFile(FileName).Size;
var MaxImageFileSize = (1024)*(1024)*(2.5);
if ((FileSize > MaxImageFileSize)||(FileSize <= 0))
{
AddErrInfo(eval("gErrStr2"), '');
return false;
}
}
catch (e)
{
AddErrInfo(eval("gErrStr3") + File + eval("gErrStr4"), '');
return false;
}
}
catch (e)
{
}
return true;
}
function restoreButton()
{
document.getElementById("btnSubmit").disabled = false;
}
function uploadSetting(FileObject, btnSubmitObject)
{
if ('' != top.errStrStr)
{
ClearErro();
}
top.errStrStr = '';
top.errIdCtrlStr = '';
if (VerifyFile(FileObject) == false)
{
if (top.errStrStr != '')
{
setDisplay('erroinfo', 1)
setErrInfo();
}
return false;
}
btnSubmitObject.disabled = true;
top.previousPage = '/html/management/cfgfile.asp';
setTimeout("restoreButton();", 500);
return true;
}
function backupSetting()
{
var form = new webSubmitForm();
form.setAction('downloadconfigfile.conf?RequestFile=/html/management/cfgfile.asp');
form.submit();
}

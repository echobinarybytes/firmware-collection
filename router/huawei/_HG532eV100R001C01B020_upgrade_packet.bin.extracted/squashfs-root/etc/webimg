<script language="JavaScript" type="text/javascript">
function stSntp(domain,Enable, stats, ntp1,ntp2, ntp3, ntp4, ntp5, currtime, ZoneName, label)
{
this.domain = domain;
this.Enable = Enable;
this.stats = stats;
this.ntp1 = ntp1;
this.ntp2 = ntp2;
this.ntp3 = ntp3;
this.ntp4 = ntp4;
this.ntp5 = ntp5;
this.currtime = currtime;
this.ZoneName = ZoneName;
this.label = label;
}
var SntpEntry = <%webGetCfgEntry(InternetGatewayDevice.Time,Enable|Status|NTPServer1|NTPServer2|NTPServer3|NTPServer4|NTPServer5|CurrentLocalTime|LocalTimeZoneName|X_Label,stSntp);%>;
var SntpEntryAcc = <%webGetCfgEntryAcc(InternetGatewayDevice.Time,Enable|Status|NTPServer1|NTPServer2|NTPServer3|NTPServer4|NTPServer5|CurrentLocalTime|LocalTimeZoneName|X_Label,stSntp);%>;
var SntpAcc = SntpEntryAcc[0];
var Sntp = SntpEntry[0];
var dstcfgenable = 0;
var localtimename = '';
var ntpServers = new Array();
ntpServers[0] = 'clock.fmt.he.net';
ntpServers[1] = 'clock.nyc.he.net';
ntpServers[2] = 'clock.sjc.he.net';
ntpServers[3] = 'clock.via.net';
ntpServers[4] = 'ntp1.tummy.com';
ntpServers[5] = 'time.cachenetworks.com';
ntpServers[6] = 'time.nist.gov';
function getTimeZoneIndex(timeZoneName)
{
var i = 0, ret = 8;  
var first = -1;
var result = -1;
if (timeZoneName == 'GMT')
{
timeZoneName = '(GMT)'
}
if (Sntp.label >= 0)
{
if (Sntp.label < timeZonesEng.length)
{
if (timeZonesEng[Sntp.label].indexOf(timeZoneName) != -1)
{
return Sntp.label;
}
}
for ( var i = 0; i < timeZonesEng.length; i++ )
{
if ( timeZonesEng[i].indexOf(timeZoneName) != -1 )
{
if (first == -1)
{
first = i;
}
if (i == Sntp.label)
{
return i;
}
}
}
}
else
{
for (i = 0; i < timeZonesEng.length; i++ )
{
if ( timeZonesEng[i].indexOf(timeZoneName) != -1 )
{
first = i;
break;
}
}
}
if ((first != -1) && (first < timeZonesEng.length))
{
ret = first;
}
return ret;
}
var gmt = '';
var offsetArr  = new Array();
function getdayoffset(str)
{
var len = str.length;
var cunt = 0;
var value = -1;
var i = 0;
if (str.charAt(0) == 'M')
{
str = str.substring(1, len);
}
else
{
return str;
}
len = str.length;
offsetArr[0] = 0;
offsetArr[1] = 0;
offsetArr[2] = 0;
for (i = 0; i < len; i++)
{
if ((str.charAt(i) >= '0') && (str.charAt(i) <= '9'))
{
value = parseInt(str.charAt(i));
i++;
}
if ((str.charAt(i) >= '0') && (str.charAt(i) <= '9'))
{
value = value * 10 + (str.charAt(i) - '0');
i++;
}
if (str.charAt(i) == '.')
{
offsetArr[cunt] = value;
cunt++;
value = -1;
}
if (((str.charAt(i) < '0') ||  (str.charAt(i) > '9')) && (str.charAt(i) != '.'))
{
break;
}
}
if (value != -1)
{
offsetArr[cunt] = value;
}
str = str.substring(i, len);
return str;
}
function setoffset(str)
{
var len = str.length;
var cunt = 0;
var i = 0;
var num = '';
offsetArr[0] = 0;
offsetArr[1] = 0;
offsetArr[2] = 0;
for (i = 0; i < len; i++)
{
if ((str.charAt(i) >= '0') && (str.charAt(i) <= '9'))
{
num += str.charAt(i);
i++;
}
if ((str.charAt(i) >= '0') && (str.charAt(i) <= '9'))
{
num += str.charAt(i);
i++;
}
if (str.charAt(i) == ':')
{
offsetArr[cunt] = parseInt(num);
cunt++;
num = '';
}
if (((str.charAt(i) < '0') ||  (str.charAt(i) > '9')) && (str.charAt(i) != ':'))
{
break;
}
}
if (num != '')
{
offsetArr[cunt] = parseInt(num);
}
str = str.substring(i, len);
return str;
}
function setdstoffset(str, arr, symbol)
{
var len = str.length;
var cunt = 0;
var i = 0;
var num = '';
var zoneoffset = 0;
var dstoffset = 0;
var finaloffset = 0;
var flag = '';
offsetArr[0] = 0;
offsetArr[1] = 0;
offsetArr[2] = 0;
if(str.charAt(0) == '-')
{
flag = '-';
i = 1;
}
for ( ; i < len; i++)
{
if ((str.charAt(i) >= '0') && (str.charAt(i) <= '9'))
{
num += str.charAt(i);
i++;
}
if ((str.charAt(i) >= '0') && (str.charAt(i) <= '9'))
{
num += str.charAt(i);
i++;
}
if (str.charAt(i) == ':')
{
offsetArr[cunt] = eval(num);
cunt++;
num = '';
}
if (((str.charAt(i) < '0') ||  (str.charAt(i) > '9')) && (str.charAt(i) != ':'))
{
break;
}
}
if (num != '')
{
offsetArr[cunt] = parseInt(num);
}
str = str.substring(i, len);
zoneoffset = arr[0] * 3600 + arr[1] * 60;
if(symbol == '-')
zoneoffset = 0 - zoneoffset;
dstoffset = offsetArr[0] * 3600 + offsetArr[1] * 60 + offsetArr[2];
if(flag == '-')
dstoffset = 0 - dstoffset;
finaloffset = zoneoffset - dstoffset;
if(finaloffset < 0)
{
finaloffset = 0 - finaloffset;
}
if(0 != finaloffset % 3600)
offsetArr[0] = Math.ceil(finaloffset / 3600) - 1;
else
offsetArr[0] = Math.ceil(finaloffset / 3600);
if(0 != finaloffset % 60)
offsetArr[1] = Math.ceil((finaloffset % 3600) / 60) - 1;
else
offsetArr[1] = Math.ceil((finaloffset % 3600) / 60);
offsetArr[2] = finaloffset % 60;
return str;
}
function setsel(sid, num)
{
var obj = getElement(sid);
obj.selectedIndex = num;
}
function setselv(sid1, sid2, sid3, flag1, flag2, flag3)
{
setsel(sid1, (offsetArr[0] - flag1));
setsel(sid2, (offsetArr[1] - flag2));
setsel(sid3, (offsetArr[2] - flag3));
}
function setdisptime()
{
var subzone = '';
var len = Sntp.ZoneName.length;
var j = 0;
var x = 0;
var zonetmpstr = new Array();
zonetmpstr[0] = 0;
zonetmpstr[1] = 0;
zonetmpstr[2] = 0;
var symbol = "";
if (Sntp.ZoneName.charAt(0) == '<')
{
for (j = 0; j < len; j++)
{
if (Sntp.ZoneName.charAt(j) == '>')
{
subzone = Sntp.ZoneName.substring(0, j);
break;
}
}
}
else
{
for (j = 0; j < len; j++)
{
if (((Sntp.ZoneName.charAt(j) < 'A') || (Sntp.ZoneName.charAt(j) > 'Z'))
&& ((Sntp.ZoneName.charAt(j) < 'a') || (Sntp.ZoneName.charAt(j) > 'z')))
{
subzone = Sntp.ZoneName.substring(j, len);
break;
}
}
}
if ((subzone.charAt(0) != '-') && (subzone.charAt(0) != '+'))
{
subzone = '+' + subzone;;
}
if ((subzone.charAt(0) == '-') || (subzone.charAt(0) == '+'))
{
gmt += subzone.charAt(0);
symbol += subzone.charAt(0);
if ((subzone.charAt(1) >= '0') && (subzone.charAt(1) <= '9'))
{
if ((subzone.charAt(2) >= '0') && (subzone.charAt(2) <= '9'))
{
gmt += subzone.charAt(1);
gmt += subzone.charAt(2);
if('0' == subzone.charAt(1))
zonetmpstr[0] = parseInt(subzone.charAt(2));
else
zonetmpstr[0] = parseInt(subzone.charAt(1) + subzone.charAt(2));
}
else
{
gmt += '0';
gmt += subzone.charAt(1);
}
}
if (subzone.charAt(3) != ':')
{
gmt  += ':';
gmt += '0';
gmt += '0';
}
else
{
gmt += subzone.charAt(3);
if ((subzone.charAt(4) >= '0') && (subzone.charAt(4) <= '9'))
{
if ((subzone.charAt(5) >= '0') && (subzone.charAt(5) <= '9'))
{
gmt += subzone.charAt(4);
gmt += subzone.charAt(5);
if('0' == subzone.charAt(4))
zonetmpstr[1] = parseInt(subzone.charAt(5));
else
zonetmpstr[1] = parseInt(subzone.charAt(4) + subzone.charAt(5));
}
else
{
gmt += '0';
gmt += subzone.charAt(5);
}
}
}
for (j = 1; j < gmt.length; j++)
{
if (gmt.charAt(j) == '0')
{
continue;
}
}
if (j == gmt.length - 1)
{
gmt = '';
}
}
if ((gmt == '+00:00') || (gmt == '-00:00') || gmt == '00:00')
{
gmt = 'GMT';
}
for (j = 0; j < subzone.length; j++)
{
if ((subzone.charAt(j) == '-') || (subzone.charAt(j) == '+')
|| (subzone.charAt(j) == ':')
|| ((subzone.charAt(j) >= '0') && (subzone.charAt(j) <= '9')))
{
continue;
}
break;
}
x = subzone.length;
subzone = subzone.substring(j, x);
if (subzone == '')
{
dstcfgenable = 0;
}
else
{
dstcfgenable = 1;
for (j = 0; j < subzone.length; j++)
{
if (((subzone.charAt(j) >= 'A') && (subzone.charAt(j) <= 'Z'))
|| ((subzone.charAt(j) >= 'a') && (subzone.charAt(j) <= 'z')))
{
continue;
}
break;
}
x = subzone.length;
subzone = subzone.substring(j, x);
if (subzone.charAt(0) == ',')
{
offsetArr[0] = 1;
offsetArr[1] = 0;
offsetArr[2] = 0;
}
else
{
subzone = setdstoffset(subzone, zonetmpstr, symbol);
}
setselv('offsethh', 'offsetmm', 'offsetss', 0, 0, 0);
if (subzone.charAt(0) == ',')
{
subzone = subzone.substring(1, subzone.length);
if (subzone.charAt(0) != 'M')
{
return ;
}
subzone = getdayoffset(subzone);
setselv('monthstart', 'weekstart', 'daystart', 1, 1, 0);
if (subzone.charAt(0) == ',')
{
offsetArr[0] = 2;
offsetArr[1] = 0;
offsetArr[2] = 0;
setselv('hhstart', 'mmstart', 'ssstart', 0, 0, 0);
}
else if (subzone.charAt(0) == '/')
{
subzone = subzone.substring(1, subzone.length);
subzone = setoffset(subzone);
setselv('hhstart', 'mmstart', 'ssstart', 0, 0, 0);
}
else
{
return ;
}
if (subzone.charAt(0) == ',')
{
subzone = subzone.substring(1, subzone.length);
if (subzone.charAt(0) != 'M')
{
return ;
}
subzone = getdayoffset(subzone);
setselv('monthend', 'weekend', 'dayend', 1, 1, 0);
if (subzone.charAt(0) == ',')
{
offsetArr[0] = 2;
offsetArr[1] = 0;
offsetArr[2] = 0;
setselv('hhend', 'mmend', 'ssend', 0, 0, 0);
}
else if (subzone.charAt(0) == '/')
{
subzone = subzone.substring(1, subzone.length);
subzone = setoffset(subzone);
setselv('hhend', 'mmend', 'ssend', 0, 0, 0);
return ;
}
else
{
return ;
}
}
}
}
}
var label = -1;
function genlocaltimename()
{
var obj = getElement('cboTimeZone');
var index = obj.selectedIndex;
var val = obj.options[index].value;
var zoneoffsetval = 0;
label = index;
localtimename += "GMT";
localtimename += val;
if (val == '')
{
localtimename += '00:00:00';
}
var xx = getCheckVal('dstEnabled');
if (getCheckVal('dstEnabled'))
{
localtimename += 'DST';
{
if(val == '')
{
zoneoffsetval = 0;
}
else
{
var symbol = val.charAt(0);
var zoneoffsethh = eval(val.substr(1, 2));
var zoneoffsetmm = eval(val.substr(4, 2));
zoneoffsetval = zoneoffsethh * 3600 + zoneoffsetmm * 60;
if(symbol == '-')
{
zoneoffsetval = 0 - zoneoffsetval;
}
}
var x = eval(getSelectVal('offsethh'));
var y = eval(getSelectVal('offsetmm'));
var z = eval(getSelectVal('offsetss'));
var dstoffsetval = x * 3600 + y * 60 + z;
var finalval = zoneoffsetval - dstoffsetval;
if(0 != finalval % 3600)
x = Math.ceil(finalval / 3600) - 1;
else
x = Math.ceil(finalval / 3600);
if(0 != finalval % 60)
y = Math.ceil((finalval % 3600) / 60) - 1;
else
y = Math.ceil((finalval % 3600) / 60);
z = finalval % 60;
if(finalval < 0)
{
if(0 != finalval % 3600)
x = x + 1;
if(0 != finalval % 60)
y = y + 1;
localtimename += '-';
}
if (Math.abs(x) < 10)
{
localtimename += '0';
}
localtimename += Math.abs(x);
localtimename += ':';
if (Math.abs(y) < 10)
{
localtimename += '0';
}
localtimename += Math.abs(y);
localtimename += ':';
if (Math.abs(z) < 10)
{
localtimename += '0';
}
localtimename += Math.abs(z);
}
localtimename += ',M';
var obj = getElement('monthstart');
localtimename += (obj.selectedIndex + 1);
localtimename += '.';
obj = getElement('weekstart');
localtimename += (obj.selectedIndex + 1);
localtimename += '.';
obj = getElement('daystart');
localtimename += obj.selectedIndex;
localtimename += '/';
var x = getSelectVal('hhstart');
if (x < 10)
{
localtimename += '0';
}
localtimename += x;
localtimename += ':';
var x = getSelectVal('mmstart');
if (x < 10)
{
localtimename += '0';
}
localtimename += x;
localtimename += ':';
var x = getSelectVal('ssstart');
if (x < 10)
{
localtimename += '0';
}
localtimename += x;
localtimename += ',M';
var obj = getElement('monthend');
localtimename += (obj.selectedIndex + 1);
localtimename += '.';
obj = getElement('weekend');
localtimename += (obj.selectedIndex + 1);
localtimename += '.';
obj = getElement('dayend');
localtimename += obj.selectedIndex;
localtimename += '/';
var x = getSelectVal('hhend');
if (x < 10)
{
localtimename += '0';
}
localtimename += x;
localtimename += ':';
var x = getSelectVal('mmend');
if (x < 10)
{
localtimename += '0';
}
localtimename += x;
localtimename += ':';
var x = getSelectVal('ssend');
if (x < 10)
{
localtimename += '0';
}
localtimename += x;
return false;
}
}
function getTimeZoneName(idx) {
var str = timeZonesEng[idx];
var ret = '';
if ( idx != 13)
ret = str.substr(12);
else
ret = str.substr(6);
return ret;
}
function getTimeZoneOffset(idx)
{
var str = timeZonesEng[idx];
var ret = '';
var offset = '';
var i = 0;
if (str.charAt(0) == '(')
{
for (i = 0; i < str.length; i++)
{
if (str.charAt(i) == ')')
{
break;
}
}
}
if ((i - 1) == 3)
{
ret = '';
}
else
{
ret = str.substr(4, (i - 4));
}
for(i = 0; i < ret.length; i++)
{
if(ret.charAt(i) == '+')
{
offset += '-';
}
else if(ret.charAt(i) == '-')
{
offset += '+';
}
else
{
offset += ret.charAt(i);
}
}
return offset;
return ret;
}
function writecurrtime()
{
var i;
var currtime;
var currtime2;
currtime = Sntp.currtime.split('T');
currtime2 = currtime[1].split('Z');
document.writeln(currtime[0] + " " + currtime2[0]);
}

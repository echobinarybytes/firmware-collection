document.getElementById('dslmessage').innerHTML = '<B><FONT color=blue>' + gVarInfo2 + '</FONT></B>';
document.getElementById('pppmessage').innerHTML = '<B><FONT color=blue>' + gVarInfo2 + '</FONT></B>';
document.getElementById('pingmessage').innerHTML = '<B><FONT color=blue>' + gVarInfo2 + '</FONT></B>';
document.getElementById('pppmessage1').innerHTML = '<B><FONT color=blue>' + gVarInfo40 + '</FONT></B>';
document.getElementById('pingmessage1').innerHTML = '<B><FONT color=blue>' + gVarInfo40 + '</FONT></B>';
var xmlhttp = CreateXMLHttp();
xmlhttp.onreadystatechange = function()
{
if (xmlhttp.readyState == 4)
{
if (xmlhttp.status == 200)
{
var temp = xmlhttp.responseText;
if(temp.indexOf('Up') >= 0)
{
document.getElementById('dslmessage').innerHTML = gVarDSLStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo3 + '</FONT></B>';
PPPDiagnose();
}
if(temp.indexOf('Up') < 0)
{
document.getElementById('dslmessage').innerHTML = gVarDSLStatus + '&nbsp;<B><FONT color=red>' + gVarInfo4 + '</FONT></B>'+ '&nbsp;&nbsp;' + '<a href="/html/status/dslinfo.asp">' + '<B><FONT color=blue>' + gVarInfo5 + '</FONT></B>' + '</a>';
document.getElementById('pppmessage').innerHTML = gVarPPPoEStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo9 + '</FONT></B>';
document.getElementById('pingmessage').innerHTML = gVarInternetStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo9 + '</FONT></B>';
document.getElementById('pppmessage1').innerHTML = gVarPPPoEStatus1 + '&nbsp;<B><FONT color=blue>' + gVarInfo34 + '</FONT></B>';
document.getElementById('pingmessage1').innerHTML = gVarInternetStatus1 + '&nbsp;<B><FONT color=blue>' + gVarInfo34 + '</FONT></B>';
EnableBtn();
return false;
}
}
else
{
document.getElementById('dslmessage').innerHTML = gVarDSLStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo8 + '</FONT></B>';
document.getElementById('pppmessage').innerHTML = gVarPPPoEStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo9 + '</FONT></B>';
document.getElementById('pingmessage').innerHTML = gVarInternetStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo9 + '</FONT></B>';
document.getElementById('pppmessage1').innerHTML = gVarPPPoEStatus1 + '&nbsp;<B><FONT color=blue>' + gVarInfo34 + '</FONT></B>';
document.getElementById('pingmessage1').innerHTML = gVarInternetStatus1 + '&nbsp;<B><FONT color=blue>' + gVarInfo34 + '</FONT></B>';
EnableBtn();
}
}
}
xmlhttp.open('GET','/html/management/dslstatus.asp?r='+Math.random(), true);
xmlhttp.send(null);
}
function PPPDiagnose()
{
var ipv4flag = 0;
var ipv6flag = 0;
var xmlhttp = CreateXMLHttp();
xmlhttp.onreadystatechange = function()
{
if (xmlhttp.readyState == 4)
{
if (xmlhttp.status == 200)
{
var temp = xmlhttp.responseText;
var ppp1sub = temp.substr(temp.indexOf('WanPPP ='));  
if(ppp1sub == 'undefined' && temp.indexOf('Showtime') >= 0)
{
document.getElementById('pppmessage').innerHTML = gVarPPPoEStatus + '&nbsp;<B><FONT color=red>' + gVarInfo10 + '</FONT></B>' +  '&nbsp;&nbsp;' + '<a href="/html/ntwkall/wan.asp">' + '<B><FONT color=blue>' + gVarInfo11 + '</FONT></B>' + '</a>';
document.getElementById('pppmessage1').innerHTML = gVarPPPoEStatus1 + '&nbsp;<B><FONT color=red>' + gVarInfo28 + '</FONT></B>' +  '&nbsp;&nbsp;' + '<a href="/html/ntwkall/wan.asp">' + '<B><FONT color=blue>' + gVarInfo29 + '</FONT></B>' + '</a>';
ppp1sub = temp.substr(temp.indexOf('WanPPP2 ='));
if(ppp1sub == 'undefined')
{
document.getElementById('pppmessage').innerHTML = gVarPPPoEStatus + '&nbsp;<B><FONT color=red>' + gVarInfo10 + '</FONT></B>' +  '&nbsp;&nbsp;' + '<a href="/html/ntwkall/wan.asp">' + '<B><FONT color=blue>' + gVarInfo11 + '</FONT></B>' + '</a>';
document.getElementById('pppmessage1').innerHTML = gVarPPPoEStatus1 + '&nbsp;<B><FONT color=red>' + gVarInfo28 + '</FONT></B>' +  '&nbsp;&nbsp;' + '<a href="/html/ntwkall/wan.asp">' + '<B><FONT color=blue>' + gVarInfo29 + '</FONT></B>' + '</a>';
EnableBtn();
return false;
}
}
else
{
ppp1sub = ppp1sub.substr(ppp1sub.indexOf('('));
}
GetIPAdress(ppp1sub);
if ((IPAdress != '') && (wanname != ''))
{
if (IPAdress != '')
{
document.getElementById('pppmessage').innerHTML = gVarPPPoEStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo12 + '&nbsp;</FONT></B>'+ '<B><FONT color=blue>' + gVarInfo13 + '&nbsp; </FONT></B>' + '<B><FONT color=blue>' + wanname + '</FONT></B>' + '<B><FONT color=blue>'+ '&nbsp;' + gVarInfo14 + ' &nbsp;</FONT></B>' + '<B><FONT color=blue>' + IPAdress + '</FONT></B>' + '.';
if (DefaultIP != '')
{
document.getElementById('pingmessage').innerHTML = gVarInternetStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo15 + '</FONT></B>' + '&nbsp;<B><FONT color=blue>' +  DefaultIP + '</FONT></B>';
PingDiagnose(DefaultIP, 1);
}
else
{
document.getElementById('pingmessage').innerHTML = gVarInternetStatus + '&nbsp;<B><FONT color=red>' + gVarInfo16 + '</FONT></B>'+ '&nbsp;&nbsp;' +  '<a href="/html/status/internetstatus.asp">' + '<B><FONT color=blue>' + gVarInfo17 + '</FONT></B>' + '</a>';
EnableBtn();
}
}
}
else
{
document.getElementById('pppmessage').innerHTML = gVarPPPoEStatus + '&nbsp;<B><FONT color=red>' + gVarInfo18 + '</FONT></B>' + '&nbsp;&nbsp;' + '<a href="/html/ntwkall/wan.asp">' + '<B><FONT color=blue>' + gVarInfo19 +'</FONT></B>' + '</a>';
document.getElementById('pingmessage').innerHTML = gVarInternetStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo9 + '</FONT></B>';
ipv4flag = 1;
}
if ((IPAdress_6 != '') && (wanname != ''))
{
if (IPAdress_6 != '')
{
document.getElementById('pppmessage1').innerHTML = gVarPPPoEStatus1 + '&nbsp;<B><FONT color=blue>' + gVarInfo39 + '&nbsp;</FONT></B>'+ '<B><FONT color=blue>' + gVarInfo13 + '&nbsp; </FONT></B>' + '<B><FONT color=blue>' + wanname + '</FONT></B>' + '<B><FONT color=blue>'+ '&nbsp;' + gVarInfo27 + ' &nbsp;</FONT></B>' + '<B><FONT color=blue>' + IPAdress_6 + '</FONT></B>' + '.';
if (DefaultIP6 != '')
{
document.getElementById('pingmessage1').innerHTML = gVarInternetStatus1 + '&nbsp;<B><FONT color=blue>' + gVarInfo15 + '</FONT></B>' + '&nbsp;<B><FONT color=blue>' +  DefaultIP6 + '</FONT></B>';
PingDiagnose(DefaultIP6, 2);
}
else
{
document.getElementById('pingmessage1').innerHTML = gVarInternetStatus1 + '&nbsp;<B><FONT color=red>' + gVarInfo31 + '</FONT></B>'+ '&nbsp;&nbsp;' +  '<a href="/html/status/internetstatus.asp">' + '<B><FONT color=blue>' + gVarInfo32 + '</FONT></B>' + '</a>';
EnableBtn();
}
}
}
else
{
document.getElementById('pppmessage1').innerHTML = gVarPPPoEStatus1 + '&nbsp;<B><FONT color=red>' + gVarInfo33 + '</FONT></B>' + '&nbsp;&nbsp;' + '<a href="/html/ntwkall/wan.asp">' + '<B><FONT color=blue>' + gVarInfo29 +'</FONT></B>' + '</a>';
document.getElementById('pingmessage1').innerHTML = gVarInternetStatus1 + '&nbsp;<B><FONT color=blue>' + gVarInfo34 + '</FONT></B>';
ipv6flag = 1;
}
if ((ipv4flag == 1) && (ipv6flag == 1))
{
EnableBtn();
return false;
}
}
else
{
document.getElementById('pppmessage').innerHTML = gVarPPPoEStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo8 +'</FONT></B>';
document.getElementById('pingmessage').innerHTML =  gVarInternetStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo9 + '</FONT></B>';
document.getElementById('pppmessage1').innerHTML = gVarPPPoEStatus1 + '&nbsp;<B><FONT color=blue>' + gVarInfo35 +'</FONT></B>';
document.getElementById('pingmessage1').innerHTML =  gVarInternetStatus1 + '&nbsp;<B><FONT color=blue>' + gVarInfo34 + '</FONT></B>';
EnableBtn();
}
}
}
xmlhttp.open('GET','/html/management/pppstatus.asp?r='+Math.random(), true);
xmlhttp.send(null);
}
function PingDiagnose(temp, flag)
{
var xmlhttp = CreateXMLHttp();
xmlhttp.onreadystatechange = function()
{
if (xmlhttp.readyState == 4)
{
EnableBtn();
if (xmlhttp.status == 200)
{
if (flag == 1)
{
var temptext = xmlhttp.responseText;
var pingsub = temptext.substr(temptext.indexOf('PingResult ='));
if ( (pingsub.indexOf('none') != -1) || (pingsub.indexOf('0 packets received') != -1))
{
document.getElementById('pingmessage').innerHTML = gVarInternetStatus + '&nbsp;<B><FONT color=red>' + gVarInfo16 + '</FONT></B>'+ '&nbsp;&nbsp;' +  '<a href="/html/status/internetstatus.asp">' + '<B><FONT color=blue>' + gVarInfo17 + '</FONT></B>' + '</a>';
return false;
}
var pingsub = pingsub.substr(pingsub.indexOf('"'));
if ((pingsub.indexOf('time=') != -1))
{
getElement('pingmessage').innerHTML = gVarInternetStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo21 + '</FONT></B>' + '&nbsp;'+ '<B><FONT color=blue>' + gVarInfo22 + '</FONT></B>';
}
else
{
document.getElementById('pingmessage').innerHTML = gVarInternetStatus + '&nbsp;<B><FONT color=red>' + gVarInfo20 + '</FONT></B>'+ '&nbsp;&nbsp;' +  '<a href="/html/status/internetstatus.asp">' + '<B><FONT color=blue>' + gVarInfo17 + '</FONT></B>' + '</a>';
}
}
else if (flag == 2)
{
var temptext = xmlhttp.responseText;
var pingsub = temptext.substr(temptext.indexOf('PingResult ='));
if ( (pingsub.indexOf('none') != -1) || (pingsub.indexOf('0 packets received') != -1))
{
document.getElementById('pingmessage1').innerHTML = gVarInternetStatus1 + '&nbsp;<B><FONT color=red>' + gVarInfo31 + '</FONT></B>'+ '&nbsp;&nbsp;' +  '<a href="/html/status/internetstatus.asp">' + '<B><FONT color=blue>' + gVarInfo32 + '</FONT></B>' + '</a>';
return false;
}
var pingsub = pingsub.substr(pingsub.indexOf('"'));
if ((pingsub.indexOf('time=') != -1))
{
getElement('pingmessage1').innerHTML = gVarInternetStatus1 + '&nbsp;<B><FONT color=blue>' + gVarInfo37 + '</FONT></B>' + '&nbsp;'+ '<B><FONT color=blue>' + gVarInfo38 + '</FONT></B>';
}
else
{
document.getElementById('pingmessage1').innerHTML = gVarInternetStatus1 + '&nbsp;<B><FONT color=red>' + gVarInfo36 + '</FONT></B>'+ '&nbsp;&nbsp;' +  '<a href="/html/status/internetstatus.asp">' + '<B><FONT color=blue>' + gVarInfo32 + '</FONT></B>' + '</a>';
}
}
}
else
{
document.getElementById('pingmessage').innerHTML = gVarInternetStatus + '&nbsp;<B><FONT color=blue>' + gVarInfo8 +'</FONT></B>';
document.getElementById('pingmessage1').innerHTML = gVarInternetStatus1 + '&nbsp;<B><FONT color=blue>' + gVarInfo35 +'</FONT></B>';
}
}
}
xmlhttp.open('POST','excutecmdfordiagnose.cgi?'+'cmd='+temp+'&RequestFile='+'/html/management/pingstatus.asp', true);
xmlhttp.send(null);
}
function GetIPAdress(temp)
{
IPAdress = '';
wanname = '';
DefaultIP = '';
IPAdress_6 = '';
DefaultIP6 = '';
if(temp == 'undefined')
{
document.getElementById('pppmessage').innerHTML = gVarInternetStatus +  '&nbsp;<B><FONT color=red>' + gVarInfo23 + '</FONT></B>';
document.getElementById('pppmessage1').innerHTML = gVarInternetStatus1 +  '&nbsp;<B><FONT color=red>' + gVarInfo30 + '</FONT></B>';
return false;
}
while (-1 != temp.indexOf('new stWanPPP'))
{
temp = temp.substr(temp.indexOf('new stWanPPP(')+'new stWanPPP('.length);
var subtemp = temp.substr(0, temp.indexOf(')'));
if (-1 == subtemp.indexOf('IP_Routed'))
{
continue;
}
if ((-1 == subtemp.indexOf('INTERNET')) && (-1 == subtemp.indexOf('INTERNET_TR069')))
{
continue;
}
subtemp = subtemp.substr(subtemp.indexOf('InternetGatewayDevice.WANDevice.1.'));
if(subtemp == 'undefined')
{
document.getElementById('pppmessage').innerHTML = gVarInternetStatus +  '&nbsp;<B><FONT color=red>' + gVarInfo23 + '</FONT></B>';
document.getElementById('pppmessage1').innerHTML = gVarInternetStatus1 +  '&nbsp;<B><FONT color=red>' + gVarInfo30 + '</FONT></B>';
return false;
}
var nametemp = subtemp.substr(subtemp.indexOf(','));
if(nametemp == 'undefined')
{
document.getElementById('pppmessage').innerHTML = gVarInternetStatus +  '&nbsp;<B><FONT color=red>' + gVarInfo23 + '</FONT></B>';
document.getElementById('pppmessage1').innerHTML = gVarInternetStatus1 +  '&nbsp;<B><FONT color=red>' + gVarInfo30 + '</FONT></B>';
return false;
}
var nametemp = nametemp.substr(nametemp.indexOf('"'));
if(nametemp == 'undefined')
{
document.getElementById('pppmessage').innerHTML = gVarInternetStatus +  '&nbsp;<B><FONT color=red>' + gVarInfo23 + '</FONT></B>';
document.getElementById('pppmessage1').innerHTML = gVarInternetStatus1 +  '&nbsp;<B><FONT color=red>' + gVarInfo30 + '</FONT></B>';
return false;
}
var nametemp1 = nametemp.substr(nametemp.indexOf('"')+1);
if(nametemp1 == 'undefined')
{
document.getElementById('pppmessage').innerHTML = gVarInternetStatus +  '&nbsp;<B><FONT color=red>' + gVarInfo23 + '</FONT></B>';
document.getElementById('pppmessage1').innerHTML = gVarInternetStatus1 +  '&nbsp;<B><FONT color=red>' + gVarInfo30 + '</FONT></B>';
return false;
}
var nametemp2 = nametemp1.substr(nametemp1.indexOf('"'));
if(nametemp2 == 'undefined')
{
document.getElementById('pppmessage').innerHTML = gVarInternetStatus +  '&nbsp;<B><FONT color=red>' + gVarInfo23 + '</FONT></B>';
document.getElementById('pppmessage1').innerHTML = gVarInternetStatus1 +  '&nbsp;<B><FONT color=red>' + gVarInfo30 + '</FONT></B>';
return false;
}
var namelength = nametemp.length - nametemp2.length;
wanname = nametemp.substring(1,namelength);
if(wanname == 'undefined')
{
document.getElementById('pppmessage').innerHTML = gVarInternetStatus +  '&nbsp;<B><FONT color=red>' + gVarInfo23 + '</FONT></B>';
document.getElementById('pppmessage1').innerHTML = gVarInternetStatus1 +  '&nbsp;<B><FONT color=red>' + gVarInfo30 + '</FONT></B>';
return false;
}
var Iptemp =  nametemp2.substr(nametemp2.indexOf('"')+1);
if(Iptemp == 'undefined')
{
document.getElementById('pppmessage').innerHTML = gVarInternetStatus +  '&nbsp;<B><FONT color=red>' + gVarInfo23 + '</FONT></B>';
return false;
}
var Iptemp1 = Iptemp.substr(Iptemp.indexOf('"'));
if(Iptemp1 == 'undefined')
{
document.getElementById('pppmessage').innerHTML = gVarInternetStatus +  '&nbsp;<B><FONT color=red>' + gVarInfo23 + '</FONT></B>';
return false;
}
Iptemp =  Iptemp1.substr(Iptemp1.indexOf('"')+1);
if(Iptemp == 'undefined')
{
document.getElementById('pppmessage').innerHTML = gVarInternetStatus +  '&nbsp;<B><FONT color=red>' + gVarInfo23 + '</FONT></B>';
return false;
}
var Iptemp2 = Iptemp.substr(Iptemp.indexOf('"'));
var IPlength = Iptemp1.length - Iptemp2.length;
IPAdress = Iptemp1.substring(1,IPlength);
var Default = Iptemp2.substr(Iptemp2.indexOf('"')+1);
var DefaultIP1 = Default.substr(Default.indexOf('"'));
Default = DefaultIP1.substr(DefaultIP1.indexOf('"')+1);
var DefaultIP2 = Default.substr(Default.indexOf('"'));
var Dlength = DefaultIP1.length - DefaultIP2.length;
DefaultIP = DefaultIP1.substring(1,Dlength);
var IPAdress6 = DefaultIP2.substr(DefaultIP2.indexOf('"')+1);
if(IPAdress6 == 'undefined')
{
document.getElementById('pppmessage1').innerHTML = gVarInternetStatus1 +  '&nbsp;<B><FONT color=red>' + gVarInfo30 + '</FONT></B>';
return false;
}
var IPAdress6_1 = IPAdress6.substr(IPAdress6.indexOf('"'));
if(IPAdress6_1 == 'undefined')
{
document.getElementById('pppmessage1').innerHTML = gVarInternetStatus1 +  '&nbsp;<B><FONT color=red>' + gVarInfo30 + '</FONT></B>';
return false;
}
var IPtemp6 = IPAdress6_1.substr(IPAdress6_1.indexOf('"')+1);
if(IPtemp6 == 'undefined')
{
document.getElementById('pppmessage1').innerHTML = gVarInternetStatus1 +  '&nbsp;<B><FONT color=red>' + gVarInfo30 + '</FONT></B>';
return false;
}
var IPAdress6_2 = IPtemp6.substr(IPtemp6.indexOf('"'));
var IPlength6 = IPAdress6_1.length - IPAdress6_2.length;
IPAdress_6 = IPAdress6_1.substring(1,IPlength6);
var Default6 = IPAdress6_2.substr(IPAdress6_2.indexOf('"')+1);
var DefaultIP6_1 = Default6.substr(Default6.indexOf('"'));
Default6 = DefaultIP6_1.substr(DefaultIP6_1.indexOf('"')+1);
var DefaultIP6_2 = Default6.substr(Default6.indexOf('"'));
var Dlength6 = DefaultIP6_1.length - DefaultIP6_2.length;
DefaultIP6 = DefaultIP6_1.substring(1,Dlength6);
if ((IPAdress != '') && (wanname != '') && (DefaultIP != ''))
{
break;
}
if ((IPAdress_6 != '') && (wanname != '') && (DefaultIP6 != ''))
{
break;
}
}
}
var gStrParaCmd;
var gDomainAddress;
function diagCmd(cmd)
{
if ('' != top.errStrStr)
{
ClearErro();
}
DisableBtn();
var address;
if(cmd.indexOf("traceroute") != -1)
{
address = 'IPDomainaddress';
}
else
{
address = 'IPDomainaddressPing';
}
var ip_ctrl = document.getElementById(address);
if (ip_ctrl.value == '')
{
AddErrInfo(eval("gVarDestAddrEmpty"), '');
if (top.errStrStr != '')
{
setDisplay('erroinfo', 1)
setErrInfo();
}
EnableBtn();
return false;
}
var requesthttp = CreateXMLHttp();
var data = "";
data += "address=" + getValue(address)+"&cmd="+cmd;;
gDomainAddress = getValue(address);
if(isIpv6Address(ip_ctrl.value) == true)
{
data +="&ipversion=6";
}
else if(isAbcIpAddress(ip_ctrl.value) == true)
{
data +="&ipversion=4";
}
else if(isValidDomain(ip_ctrl.value) == true)
{
data +="&ipversion=5"; 
}
else
{
AddErrInfo(eval("gErrStr0"), '');
if (top.errStrStr != '')
{
setDisplay('erroinfo', 1)
setErrInfo();
EnableBtn();
}
return false;
}
requesthttp.onreadystatechange = function()
{
if (requesthttp.readyState == 4)
{
if (requesthttp.status == 200)
{
}
else
{
}
}
}
requesthttp.open('POST','StartTracePingCmd.cgi?'+data, true);
requesthttp.send(null);
if (cmd.indexOf("traceroute") != -1)
{
setDisable('canceltrace',0);
setDisable('trace',1);
}
else
{
setDisable('cancelping',0);
setDisable('ping',1);
}
setTimeout("diagGetResult();",100);
cmdFlag = true;
if (cmd.indexOf("traceroute") != -1)
{
gStrParaCmd = "traceroute";
}
else
{
gStrParaCmd = "ping"
}
}
function CheckIPDomain()
{
top.errStrStr = '';
top.errIdCtrlStr = '';
var ip_ctrl = document.getElementById('TraceRouteaddress');
if (isValidCfgStr('', ip_ctrl.value, 256) == false || (ip_ctrl.value == ''))
{
AddErrInfo(eval("gErrStr0"), '');
if (top.errStrStr != '')
{
setDisplay('erroinfo', 1)
setErrInfo();
EnableBtn();
}
return false;
}
if ((isAbcIpAddress(ip_ctrl.value) == false)
&& (isIpv6Address(ip_ctrl.value)) == false)
{
AddErrInfo(eval("gErrStr0"), '');
if (top.errStrStr != '')
{
setDisplay('erroinfo', 1)
setErrInfo();
EnableBtn();
}
return false;
}
return true;
}
var TRresult = "";
function isValidIpAddress(address) {
var i = 0;
if ( address == '0.0.0.0' ||
address == '255.255.255.255' )
return true;
var addrParts = address.split('.');
if ( addrParts.length != 4 )
return false;
for (i = 0; i < 4; i++) {
if (isNaN(addrParts[i]) || addrParts[i] ==""
|| addrParts[i].charAt(0) == '+' ||  addrParts[i].charAt(0) == '-'
|| (addrParts[i].charAt(0) == '0' && addrParts[i].length > 1))
return false;
if (addrParts[i].length > 3 || addrParts[i].length < 1)
{
return false;
}
if (!isInteger(addrParts[i]) || addrParts[i] < 0)
{
return false;
}
num = parseInt(addrParts[i]);
if (i == 0 && num == 0)
{
return false;
}
if ( num < 0 || num > 255 )
return false;
if ((3 == i) && (255 == num))
{
return false;
}
}
return true;
}
function diagGetResult()
{
var responsehttp = CreateXMLHttp();
var Result;
if(gStrParaCmd.indexOf("traceroute") != -1)
{
Result = 'txtTraceRouteResult';
}
else
{
Result = 'txtPingResult';
}
responsehttp.onreadystatechange = function()
{
if (responsehttp.readyState == 4)
{
if (responsehttp.status == 200)
{
var temp = responsehttp.responseText;
if(temp.indexOf("no_tracerouteping_process")==-1)
{
var t,m = "";
t = temp.split('\n');
var i = t.length;
for(n = 0; n<i; ++n)
{
m += t[n] + '<br>';
}
document.getElementById(Result).innerHTML = m;
if (cmdFlag)
{
setTimeout("diagGetResult();",1000);
}
TRresult = m;
}
else
{
cmdFlag = false;
if (TRresult.indexOf(gVarTraceCompleteInfo) == -1 && TRresult.indexOf(gVarCacelTraceInfo) == -1
&&TRresult.indexOf(gVarPingCompleteInfo) == -1 && TRresult.indexOf(gVarCacelPingInfo) == -1)
{
if (gStrParaCmd.indexOf("traceroute") != -1)
{
if (TRresult == "" || TRresult == '<br>')
{
if (isValidIpAddress(gDomainAddress) == false)
{
TRresult = gVarTracerouteFail +"<br>"+"<br>"+gVarTraceCompleteInfo;
}
else
{
TRresult = gVarTracerouteFail2 +"<br>"+"<br>"+gVarTraceCompleteInfo;
}
}
else
{
TRresult = TRresult  + gVarTraceCompleteInfo;
}
}
else
{
if (TRresult == "" || TRresult == '<br>')
{
TRresult = gVarPingFail + "<br>"  +"<br>"+ gVarPingCompleteInfo;
}
else
{
if (TRresult.indexOf("loss") != -1)
{
TRresult = TRresult  + gVarPingCompleteInfo;
}
else
{
TRresult = gVarPingFail2 + "<br>"  +"<br>"+ gVarPingCompleteInfo;
}
}
}
}
document.getElementById(Result).innerHTML = TRresult;
TRresult = "";
if(gStrParaCmd.indexOf("traceroute") != -1)
{
setDisable('canceltrace',1);
EnableBtn();
}
else
{
setDisable('cancelping',1);
EnableBtn();
}
}
}
}
}
responsehttp.open('post','ReadTracePingRes.cgi?cmd='+gStrParaCmd, true);
responsehttp.send(null);
}
function diagCancelCmd()
{
var Result;
if(gStrParaCmd.indexOf("traceroute") != -1)
{
Result = 'txtTraceRouteResult';
setDisable('canceltrace',1);
EnableBtn();
}
else
{
Result = 'txtPingResult';
setDisable('cancelping',1);
EnableBtn();
}
cmdFlag=false;
var requesthttp = CreateXMLHttp();
requesthttp.onreadystatechange = function()
{
if (requesthttp.readyState == 4)
{
if (requesthttp.status == 200)
{
if (TRresult.indexOf(gVarTraceCompleteInfo) == -1 && TRresult.indexOf(gVarCacelTraceInfo) == -1
&&TRresult.indexOf(gVarPingCompleteInfo) == -1 && TRresult.indexOf(gVarCacelPingInfo) == -1)
{
if (gStrParaCmd.indexOf("traceroute") != -1)
{
TRresult = TRresult  + gVarCacelTraceInfo;
}
else
{
TRresult = TRresult + gVarCacelPingInfo;
}
}
document.getElementById(Result).innerHTML = TRresult;
}
else
{
}
}
}
requesthttp.open('post','CancelTraceCmd.cgi?cmd='+gStrParaCmd, true);
requesthttp.send(null);
}

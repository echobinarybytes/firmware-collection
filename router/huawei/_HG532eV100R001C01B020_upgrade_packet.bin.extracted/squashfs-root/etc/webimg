<script language="JavaScript" type="text/javascript">
function stDsl(domain, ConfigMode, X_Sra, X_BitSwap)
{
this.domain = domain;
this.ConfigMode = ConfigMode;
this.X_Sra = X_Sra;
this.X_BitSwap = X_BitSwap;
}
var DSLCfg = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.1.WANDSLInterfaceConfig, X_ConfigMode|X_Sra|X_BitSwap, stDsl);%>;
var DslInfo = DSLCfg[0];
function LoadFrame()
{
if (DslInfo.ConfigMode != null)
{
var list = DslInfo.ConfigMode.split(',');
var cb_Dsl = getElById('cb_dslEnable');
for (var i = 0; i < list.length; i++)
{
for(var j = 0; j < cb_Dsl.length; j++)
{
if (list[i] == cb_Dsl[j].value)
{
cb_Dsl[j].checked = true;
}
}
}
}
if (DslInfo.X_Sra != null )
{
var cb_dslSra = getElById('cb_dslSraEnable');
if ("1" == DslInfo.X_Sra )
{
cb_dslSra.checked = true;
}
else
{
cb_dslSra.checked = false;
}
}
if (DslInfo.X_BitSwap != null )
{
var cb_dslBitSwap = getElById('cb_dslBitSwapEnable');
if ("1" == DslInfo.X_BitSwap )
{
cb_dslBitSwap.checked = true;
}
else
{
cb_dslBitSwap.checked = false;
}
}
}
function allClick()
{
var cb_Dsl = getElById('cb_dslEnable');
if (cb_Dsl[5].checked == true)
{
for(var j = 0; j < cb_Dsl.length; j++)
{
if (j != 5)
{
cb_Dsl[j].checked = false;
}
}
}
}
function annexMClick()
{
var cb_Dsl = getElById('cb_dslEnable');
if (cb_Dsl[6].checked == true)
{
for(var j = 0; j < cb_Dsl.length; j++)
{
if (j != 6)
{
cb_Dsl[j].checked = false;
}
}
}
}
function selClick()
{
var cb_Dsl = getElById('cb_dslEnable');
for(var j = 0; j < cb_Dsl.length-2; j++)
{
if(cb_Dsl[j].checked == true)
break;
}
if (j < cb_Dsl.length-2)
{
cb_Dsl[5].checked = false;
cb_Dsl[6].checked = false;
}
}
function AddSubmitParam(SubmitForm,type)
{
var DslList = "";
var cb_Dsl = getElById('cb_dslEnable');
var DslSraEnable = "1";
var DslBitSwapEnable = "1";
var cb_dslSra = getElById('cb_dslSraEnable');
var cb_dslBitSwap = getElById('cb_dslBitSwapEnable');
for (i = 0; i < cb_Dsl.length; i++)
{
if (cb_Dsl[i].checked == true)
{
if(DslList == "")
{
DslList += cb_Dsl[i].value;
}
else
{
DslList += ',' + cb_Dsl[i].value;
}
}
}
if(false == cb_dslSra.checked)
{
DslSraEnable = "0";
}
if(false == cb_dslBitSwap.checked)
{
DslBitSwapEnable = "0";
}
SubmitForm.addParameter('InternetGatewayDevice.WANDevice.1.WANDSLInterfaceConfig.X_ConfigMode', DslList);
SubmitForm.addParameter('InternetGatewayDevice.WANDevice.1.WANDSLInterfaceConfig.X_Sra', DslSraEnable);
SubmitForm.addParameter('InternetGatewayDevice.WANDevice.1.WANDSLInterfaceConfig.X_BitSwap', DslBitSwapEnable);
SubmitForm.setAction('setcfg.cgi?RequestFile=/html/ntwkall/dsl.asp');
setDisable("buttonDslApply", 1);
}
function CheckForm()
{
var cb_Dsl = getElById('cb_dslEnable');
for(var j = 0; j < cb_Dsl.length; j++)
{
if (cb_Dsl[j].checked == true)
{
break;
}
}
if (j == cb_Dsl.length)
{
AddErrInfo(eval("gDSLErrMsg1"), '');
return false;
}
return true;
}
</script>
</head>

{
document.write('<td>' + eval("gVarConnecting") + '&nbsp;</td>');
}
else if (Wan[i].xIpv6Status == 'Connected')
{
document.write('<td>' + eval("gVarConnected") + '&nbsp;</td>');
}
else if (Wan[i].xIpv6Status == 'PendingDisconnect')
{
document.write('<td>' + eval("gVarPending") + '&nbsp;</td>');
}
else if (Wan[i].xIpv6Status == 'Disconnecting')
{
document.write('<td>' + eval("gVarDisConnecting") + '&nbsp;</td>');
}
else if (Wan[i].xIpv6Status == 'Disconnected')
{
document.write('<td>' + eval("gVarDisConnected") + '&nbsp;</td>');
}
else if (Wan[i].xIpv6Status == 'Authenticating')
{
document.write('<td>' + eval("gVarAuthenticating") + '&nbsp;</td>');
}
if (Wan[i].xIpv6Gateway != '')
{
document.write('<td>' + Wan[i].xIpv6Gateway + '&nbsp;</td>');
}
else
{
document.write('<td>' + Wan[i].xIpv6DynamicGateway + '&nbsp;</td>');
}
}
document.write('</tr>');
}
</script>
</table>

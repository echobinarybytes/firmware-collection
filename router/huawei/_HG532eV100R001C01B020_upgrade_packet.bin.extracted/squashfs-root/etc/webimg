<script language="JavaScript" type="text/javascript">
var OamDiagResult = <%webGetQamDiagResult();%>;
var IPAdress = '';
var IPAdress_6 = '';
var wanname = '';
var DefaultIP = '';
var DefaultIP6 = '';
var cmdFlag = false;
function AtmConst(domain, atmIfName,atmPvc)
{
this.domain = domain;
this.atmIfName = atmIfName;
if(atmPvc.charAt(0) == 'P')
{
atmPvc = atmPvc.substr(4);
}
var pvc = atmPvc.split('/');
this.atmVpi = parseInt(pvc[0],10).toString();
this.atmVci = parseInt(pvc[1],10).toString();
this.atmPvc = this.atmVpi+'.'+this.atmVci;
}
var CntAtm = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.1.WANConnectionDevice.{i},Name|WANDSLLinkConfig.DestinationAddress,AtmConst);%>;
function DisableBtn()
{
setDisable('btnDiagnose',1);
setDisable('ping',1);
setDisable('trace',1);
setDisable('oamf5test',1);
}
function EnableBtn()
{
setDisable('btnDiagnose',0);
setDisable('ping',0);
setDisable('trace',0);
if( CntAtm.length <= 1)
{
setDisable("oamf5test", 1);
}
else
{
setDisable('oamf5test',0);
}
}
function isValidUrlName(url)
{
var i=0;
var invalidArray = new Array();
invalidArray[i++] = "www";
invalidArray[i++] = "com";
invalidArray[i++] = "org";
invalidArray[i++] = "net";
invalidArray[i++] = "edu";
invalidArray[i++] = "www.";
invalidArray[i++] = ".com";
invalidArray[i++] = ".org";
invalidArray[i++] = ".net";
invalidArray[i++] = ".edu";
if (isValidAscii(url) != '')
{
return false;
}
for (i = 0; i < url.length; i++)
{
if (url.charAt(i) == '\\')
{
return false;
}
}
if (url == "")
{
return false;
}
if (url.length < 3)
{
return false;
}
for(j=0; j< invalidArray.length; j++)
{
if (url == invalidArray[j])
{
return false;
}
}
return true;
}
function Startoamf5test()
{
if ('' != top.errStrStr)
{
ClearErro();
}
DisableBtn();
with(getElement('oam'))
{
var Pvc_ctrl = getSelectVal('oaminterface');
getElement('OamResult').innerHTML = '<B><FONT color=blue> '+gVarOamTesting
+ '</FONT><B>';
var form = new webSubmitForm();
form.setAction('/html/management/excuteoamcmd.cgi?pvc=' + Pvc_ctrl + '&RequestFile=/html/management/diagnose.asp');
form.submit();
setDisable("oamf5test", 1);
}
}

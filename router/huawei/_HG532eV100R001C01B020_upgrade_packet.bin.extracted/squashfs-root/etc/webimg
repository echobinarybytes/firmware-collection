function WanPPPConstruction(Domain,Username,PasswordIsNull,Enable,ConnectionType)
{
this.Domain = Domain;
this.Username = Username;
this.Enable = Enable;
this.ConnectionType = ConnectionType;
if (PasswordIsNull == 1)
{
this.Password = '';
}
else
{
this.Password = '@1GV)Z<!';
}
}
var WANPPP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.1.WANConnectionDevice.{i}.WANPPPConnection.{i}, Username|X_PasswordIsNull|Enable|ConnectionType, WanPPPConstruction);%>;
var pw;
var un;
var flag;
function LoadFrame()
{
setDisplay('wlanCfg', 1);
setDisplay('erroinfo', 1);
var Username = document.getElementById('txt_Username');
var Password = document.getElementById('txt_Password');
var i;
var j = 0;
for (i=0; i<WANPPP.length - 1; ++i)
{
if((1 == WANPPP[i].Enable) && ('IP_Routed' == WANPPP[i].ConnectionType))
{
Username.value = WANPPP[i].Username;
un = Username.value;
++j;
break;
}
}
if(j == 0)
{
Username.value = "";
Password.value = "";
flag = 0;
setDisable('txt_Username', 1);
setDisable('txt_Password', 1);
setDisable('btnApply', 1);
setDisplay('warning', 1);
}
else
{
setDisplay('warning', 0);
Password.value = WANPPP[i].Password;
}
pw = Password.value;
}
function CheckValid(val)
{
if ((val.value.length > 64) || (isValidAscii(val.value) != '') || (val.value.indexOf(' ') > -1))
{
return false;
}
return true;
}
function SubmitForm()
{
if ('' != top.errStrStr)
{
ClearErro();
}
top.errStrStr = '';
top.errIdCtrlStr = '';
if (flag == 0)
{
alert(eval("gATMErrStr39"));
return;
}
var Username = document.getElementById('txt_Username');
var Password = document.getElementById('txt_Password');
if ((Username.value == '') || (Password.value == ''))
{
AddErrInfo(eval("gErrStr25"), 'Username');
setErrInfo();
setDisplay('erroinfo', 1);
return;
}
if (false == CheckValid(Username))
{
AddErrInfo(eval("gErrStr26"), 'Username');
setErrInfo();
setDisplay('erroinfo', 1);
return;
}
else if (false == CheckValid(Password))
{
AddErrInfo(eval("gErrStr27"), 'Password');
setErrInfo();
setDisplay('erroinfo', 1);
return;
}
if ((un == Username.value) && (pw == Password.value))
{
top.contentfrm.location.replace("/html/ntwkall/ppp.asp");
return;
}
else
{
var i;
var Form = new webSubmitForm();
var url = "setcfg.cgi?";
var para = 'x';
var paraname;
var parapw;
for (i = 0; i < WANPPP.length - 1; ++i)
{
if((1 == WANPPP[i].Enable) && ('IP_Routed' == WANPPP[i].ConnectionType))
{
para = para + 1;
url = url + para + '=' + WANPPP[i].Domain + '&';
paraname = para + '.Username';
Form.addParameter(paraname, Username.value);
if (Password.value != '@1GV)Z<!')
{
parapw = para + '.Password';
Form.addParameter(parapw, Password.value);
}
}
}
url = url+"RequestFile=/html/ntwkall/ppp.asp";
Form.setAction(url);
Form.submit();
}
}

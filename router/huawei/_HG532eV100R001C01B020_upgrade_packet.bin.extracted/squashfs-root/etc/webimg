<script language="JavaScript" type="text/javascript">
var TABLE_MENU_INTERVAL = 2;
var tab_count=0;
function init()
{
}
function setTabMenuImage(menuItem,imageType)
{
if(menuItem!=null){
var tdTabMenu_1 = getElement('td1_' + menuItem.id);
var tdTabMenu_2 = getElement('td2_' + menuItem.id);
var tdTabMenu_3 = getElement('td3_' + menuItem.id);
if(tdTabMenu_1!=null && tdTabMenu_2 !=null && tdTabMenu_3!=null){
tdTabMenu_1.style.backgroundImage = 'url(/images/' + imageType + 'tableft.gif)';
tdTabMenu_2.style.backgroundImage = 'url(/images/' + imageType + 'tabmid.gif)';
tdTabMenu_3.style.backgroundImage = 'url(/images/' + imageType + 'tabright.gif)';
}
}
}
function clickTabMenu(tabMenuId)
{
if (tabMenuId.indexOf("td_link") != -1)
{
tabMenuId = tabMenuId.substr(3);
}
var menuItem = top.g_oMenuTree.curMenuItem;
setTabMenuTextColor(menuItem,'#000000'); 
setTabMenuImage(menuItem,'init');
top.errStrStr = '';
top.errIdCtrlStr = '';
if (menuItem.link.indexOf('pin.') > -1)
{
var temp = menuItem.link.lastIndexOf('/') + 1;
var subutl = menuItem.link.substr(temp);
top.g_url = subutl;
}
top.Link = '';
var menuID = tabMenuId.substr(tabMenuId.indexOf('_') + 1);
menuItem = top.g_oMenuTree.getMenuById(menuID);
top.g_oMenuTree.curMenuItem = menuItem;
var menuIDtem = menuItem.id;
var menus = menuIDtem.split('_');
g_firstmenu = menus[0] + '_' + menus[1]; 
g_secmenu = menus[0] + '_' + menus[1] + '_' + menus[2]; 
top.g_secondmenu = g_secmenu;
g_thirdmenu = menuIDtem;
top.g_thirdmenu = g_thirdmenu;
refreshMenu(g_firstmenu,g_secmenu,g_thirdmenu);
top.menufrm.expandMenuById(menuItem.id);
setTabMenuTextColor(menuItem,'#990000'); 
setTabMenuImage(menuItem,'click');
var tdPath = getElement('tdMenuPath');
if (menuItem.parent.nChildCount > 1)
{
if(tdPath!=null){
tdPath.innerHTML = menuItem.parent.parent.text
+ ' > ' + menuItem.parent.text
+ ' > ' + menuItem.text;
}
}
else
{
if(tdPath!=null){
tdPath.innerHTML = menuItem.parent.parent.text + ' > ' + menuItem.text;
}
}
}
function setTabMenuTextColor(menuItem, color)
{
if(menuItem!=null){
var tdMenuTab = 'link_' + menuItem.id;
if(getElement(tdMenuTab)!=null){
getElement(tdMenuTab).style.color = color;
}
}
}
function mouseOverLink(xitem)
{
if (xitem.indexOf("td_link") != -1)
{
xitem = xitem.substr(3);
}
getElement(xitem).style.color="#990000";
}
function mouseOutLink(xitem)
{
if (xitem.indexOf("td_link") != -1)
{
xitem = xitem.substr(3);
}
var index = xitem.indexOf('_');
var tdID = 'td2_' + xitem.substr(index + 1);
if(document.getElementById(tdID).style.backgroundImage.indexOf('clicktabmid.gif') > -1)
{
document.getElementById(xitem).style.color="#990000";
}
else
{
document.getElementById(xitem).style.color="#000000";
}
}
function refreshMenu(firmenu,secmenu,thirdmenu)
{
var cookie = "FirstMenu=" + firmenu + "; path=/";
document.cookie = cookie;
var cookie = "SecondMenu=" + secmenu + "; path=/";
document.cookie = cookie;
var cookie = "ThirdMenu=" + thirdmenu + "; path=/";
document.cookie = cookie;
}
function makeTableHTML()
{
var menuItem = top.g_oMenuTree.curMenuItem;
var tabMenu = getElement('tdTableMenu');
var tdPath = getElement('tdMenuPath');
if(menuItem !=null){
if (menuItem.parent.nChildCount > 1)
{
if(tdPath!=null){
tdPath.innerHTML = menuItem.parent.parent.text
+ ' > ' + menuItem.parent.text
+ ' > ' + menuItem.text;
}
}
else
{
if(tdPath!=null){
tdPath.innerHTML = menuItem.parent.parent.text + ' > ' + menuItem.text;
}
}
var html = '<table cellpadding="0" cellspacing="0"><tr>';
for (var i = 0; i < menuItem.parent.nChildCount; i++)
{
menuItem = menuItem.parent.children[i];
if (menuItem.link.indexOf("../management/upgrade.asp")!= -1 && top.isShowRemoteUpgrade == 0)
{
continue;
}
{
html += '<td  valign="bottom" id = "td_link_' + menuItem.id +'" onClick = "clickTabMenu(this.id);" onmouseover="mouseOverLink(this.id);" onmouseout="mouseOutLink(this.id)"><table cellpadding="0" cellspacing="0" width="160" height="25" class="TabMenu">'
+ '<td width="10"  background="/images/inittableft.gif" '
+ 'id="td1_' + menuItem.id + '">&nbsp;</td>'
+	'<td  align="center" width="138" background="/images/inittabmid.gif" class="MenuLink_Lvl_3'
+ '" id="td2_' + menuItem.id + '"><a hidefocus="hidefocus" href="'
+ menuItem.link + '" target="contentfrm" id="link_'
+ menuItem.id + '">'
+ menuItem.text + '</a></td>'
+ '<td  width="11" background="/images/inittabright.gif" id="td3_' + menuItem.id
+ '">&nbsp;</td>'
+	'</table></td>';
}
html += '<td width="' + TABLE_MENU_INTERVAL + '"></td>';
}
html += '</tr></table>';
if(tabMenu!=null){
tabMenu.innerHTML = html;
}
setTabMenuTextColor(top.g_oMenuTree.curMenuItem,'#990000');
setTabMenuImage(top.g_oMenuTree.curMenuItem,'click');
}
}
function makeBackTable(menuName,ThirdMenu,type)
{
var temp = top.g_oMenuTree.getMenuById(menuName);
top.g_oMenuTree.curMenuItem = temp;
var menuItem =  top.g_oMenuTree.getMenuById(ThirdMenu);
var tabMenu = getElement('tdTableMenu');
var tdPath = getElement('tdMenuPath');
if (temp.parent.nChildCount > 1)
{
if(tdPath!=null){
tdPath.innerHTML = temp.parent.parent.text
+ ' > ' + temp.parent.text
+ ' > ' + temp.text;
}
}
else
{
if(tdPath!=null){
tdPath.innerHTML = temp.parent.parent.text + ' > ' + temp.text;
}
}
if(type == 1)
{
setTabMenuTextColor(menuItem,'#000000'); 
setTabMenuImage(menuItem,'init');
setTabMenuTextColor(top.g_oMenuTree.curMenuItem,'#990000');
setTabMenuImage(top.g_oMenuTree.curMenuItem,'click');
}
else
{
var menus = menuName.split('_');
var menuFir = menus[0] + '_' + menus[1];
var menuSec = menuFir + '_' + menus[2];
var menuThr = menuSec + '_0';
var menuItem = top.g_oMenuTree.getMenuById(menuThr);
top.menufrm.expandMenu(menuFir)
setTabMenuTextColor(menuItem,'#000000'); 
setTabMenuImage(menuItem,'init');
setTabMenuTextColor(top.g_oMenuTree.curMenuItem,'#990000');
setTabMenuImage(top.g_oMenuTree.curMenuItem,'click');
}
}
function SubmitKeyClick(e)
{
if (e.keyCode == 8)
{
return false;
}
}
</script>

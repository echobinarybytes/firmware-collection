var WanIndex = -1;
var selIndex = 0;
function stWanInfo(domain,Name,Enable,NATEnabled,ConnectionType,ServiceList,DMZEnable,DMZIP, ConnectionType)
{
this.domain = domain;
this.key = this.domain;
this.Name = Name;
this.ConnectionType = ConnectionType;
this.ServiceList = ServiceList;
this.DMZEnable = DMZEnable;
this.DMZIP = DMZIP;
this.Enable = Enable;
this.NATEnabled = NATEnabled;
this.CntStatus = '';
}
var WanIPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Name|Enable|NATEnabled|ConnectionType|X_ServiceList|X_DMZ.DMZEnable|X_DMZ.DMZHostIPAddress|ConnectionType,stWanInfo);%>;
var WanIPInfoAcc = <%webGetCfgEntryAcc(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Name|Enable|NATEnabled|ConnectionType|X_ServiceList|X_DMZ.DMZEnable|X_DMZ.DMZHostIPAddress|ConnectionType,stWanInfo);%>;
var WanPPPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|Enable|NATEnabled|ConnectionType|X_ServiceList|X_DMZ.DMZEnable|X_DMZ.DMZHostIPAddress|ConnectionType,stWanInfo);%>;
var WanPPPInfoAcc = <%webGetCfgEntryAcc(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|Enable|NATEnabled|ConnectionType|X_ServiceList|X_DMZ.DMZEnable|X_DMZ.DMZHostIPAddress|ConnectionType,stWanInfo);%>;
function stCntStatus(domain,CntStatus)
{
this.key = domain;
this.CntStatus = CntStatus;
}
var WanIPCntStatusInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},ConnectionStatus,stCntStatus);%>;
var WanPPPCntStatusInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},ConnectionStatus,stCntStatus);%>;
var CntStatusInfo = new Array();
for (i = 0; i < WanIPCntStatusInfo.length-1; i++)
{
CntStatusInfo[i] = WanIPCntStatusInfo[i];
}
for (j = 0; j < WanPPPCntStatusInfo.length-1; j++,i++)
{
CntStatusInfo[i] = WanPPPCntStatusInfo[j];
}
var WanInfo = new Array();
var WanInfoAcc = new Array();
for (i = 0; i < WanIPInfo.length-1; i++)
{
WanInfo[i] = WanIPInfo[i];
WanInfoAcc[i] = WanIPInfoAcc[i];
}
for (j = 0; j < WanPPPInfo.length-1; j++,i++)
{
WanInfo[i] = WanPPPInfo[j];
WanInfoAcc[i] = WanPPPInfoAcc[j];
}
AssociateParam('WanInfo','CntStatusInfo','CntStatus');
function AddSubmitParam(SubmitForm,type)
{
var Interface = getElement('DMZInterface');
var optionID = Interface.options[Interface.selectedIndex].id;
var index = optionID.split('_')[1];
setDisable('DMZInterface',1);
SubmitForm.addDiv('dmzForm','x');
SubmitForm.setAction('setcfg.cgi?x=' + WanInfo[index].domain + '.X_DMZ'
+ '&RequestFile=/html/application/dmz.asp');
top.aplyLine = selIndex;
setDisable('btnApply',1);
}
function CheckForm(type)
{
return CheckDMZ();
}
function CheckDMZ()
{
if (getElement('DMZInterface').length == 0)
{
AddErrInfo(eval("gErrStr0"), '');
return false;
}
with (getElement('dmzForm'))
{
if ((getElement('DMZHostIPAddress').value != ''
&& isAbcIpAddress(getElement('DMZHostIPAddress').value) == false)
|| (getElement('DMZHostIPAddress').value == '' && getCheckVal('DMZEnable') == 1) )
{
AddErrInfo(eval("gErrStr1"), '');
return false;
}
}
return true;
}
function LoadFrame()
{
var i = GetmenuName("dmz.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_' + WanIndex);
}
}
function ShowDMZ()
{
var Interface = getElement('DMZInterface');
var optionID = Interface.options[Interface.selectedIndex].id;
var index = optionID.split('_')[1];
setCheck('DMZEnable',WanInfo[index].DMZEnable);
setText('DMZHostIPAddress',WanInfo[index].DMZIP);
if (WanInfo[index].Enable == 0 || WanInfo[index].NATEnabled == 0)
{
setDisable('DMZEnable',1);
setDisable('DMZHostIPAddress',1);
}
else
{
setDisable('DMZEnable',TransAcc(WanInfoAcc[index].DMZEnable));
setDisable('DMZHostIPAddress',TransAcc(WanInfoAcc[index].DMZIP));
}
}
function clearAll()
{
}
function trimspace(str)
{
var strTemp = new String(str);
while (-1 != strTemp.search(" "))
{
strTemp = strTemp.replace(" ", "");
}
return strTemp;
}
function setCtlDisplay(record)
{
setSelect('DMZInterface',record.key);
setCheck('DMZEnable',record.DMZEnable);
setText('DMZHostIPAddress',record.DMZIP);
}
function setControl(index)
{
var record;
selIndex = index;
if (index == -1)
{
record = new stWanInfo('','','','','','','','');
setDisable('DMZEnable', 0);
setDisable('DMZHostIPAddress', 0);
}
else
{
record = WanInfo[index];
setDisable('DMZEnable', TransAcc(WanInfoAcc[index].DMZEnable));
setDisable('DMZHostIPAddress', TransAcc(WanInfoAcc[index].DMZIP));
}
setCtlDisplay(record);
}

{
document.write('<TD>' + 'Path1' + '</TD>');
}
else
{
document.write('<TD>' + 'Path0&1' + '</TD>');
}
if ((1 == CntPtm[i].ptmPriorityLow) && (1 == CntPtm[i].ptmPriorityHigh))
{
document.write('<TD>' + 'Normal&High' + '</TD>');
}
else if (1 == CntPtm[i].ptmPriorityLow)
{
document.write('<TD>' + 'Normal' + '</TD>');
}
else if (1 == CntPtm[i].ptmPriorityHigh)
{
document.write('<TD>' + 'High' + '</TD>');
}
document.write('<TD>' + CntPtm[i].vlanId + '</TD>');
document.write('<TD>' + CntPtm[i].priority + '</TD>');
var index = CntPtm[i].domain.lastIndexOf('.');
var domain = CntPtm[i].domain.substr(0, index);
document.write('<TD align="center"><input type="checkbox"' + 'name="rml"'
+ ' value="' + domain + '"></td>\n');
document.write('</TR>');
}

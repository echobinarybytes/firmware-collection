<script language="JavaScript" type="text/javascript">
var WanIndex = -1;
var selctIndex = -1;
function stWanInfo(domain,Name,Enable,NATEnabled,ConnectionType,ServiceList,DMZEnable,DMZIP)
{
this.domain = domain;
this.key = this.domain;
this.Name = Name;
this.ConnectionType = ConnectionType;
this.ServiceList = ServiceList;
this.DMZEnable = DMZEnable;
this.DMZIP = DMZIP;
this.Enable = Enable;
this.NATEnabled = NATEnabled;
this.CntStatus = '';
}
var WanIPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Name|Enable|NATEnabled|ConnectionType|X_ServiceList|X_DMZ.DMZEnable|X_DMZ.DMZHostIPAddress,stWanInfo);%>;
var WanPPPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|Enable|NATEnabled|ConnectionType|X_ServiceList|X_DMZ.DMZEnable|X_DMZ.DMZHostIPAddress,stWanInfo);%>;
function stCntStatus(domain,CntStatus)
{
this.key = domain;
this.CntStatus = CntStatus;
}
var WanIPCntStatusInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},ConnectionStatus,stCntStatus);%>;
var WanPPPCntStatusInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},ConnectionStatus,stCntStatus);%>;
var CntStatusInfo = new Array();
for (i = 0; i < WanIPCntStatusInfo.length-1; i++)
{
CntStatusInfo[i] = WanIPCntStatusInfo[i];
}
for (j = 0; j < WanPPPCntStatusInfo.length-1; j++,i++)
{
CntStatusInfo[i] = WanPPPCntStatusInfo[j];
}
var WanInfo = new Array();
for (i = 0; i < WanIPInfo.length-1; i++)
{
WanInfo[i] = WanIPInfo[i];
}
for (j = 0; j < WanPPPInfo.length-1; j++,i++)
{
WanInfo[i] = WanPPPInfo[j];
}
AssociateParam('WanInfo','CntStatusInfo','CntStatus');
function stPortMap(domain,ProtMapEnabled,Protocol,RemoteHost,ExPort, ExPortEnd, InPort,InClient,Description)
{
this.domain = domain;
this.key = this.domain;
this.ProtMapEnabled = ProtMapEnabled;
this.Protocol = Protocol;
this.RemoteHost = RemoteHost;
this.ExPort = ExPort;
this.ExPortEnd = ExPortEnd;
this.InPort = InPort;
this.InClient = InClient;
this.Description = Description;
var index = domain.lastIndexOf('PortMapping');
this.Interface = domain.substr(0,index - 1);
this.NATEnabled = '';
this.Enable = '';
this.Name = '';
}
var WanIPPortMapping = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i}.PortMapping.{i},PortMappingEnabled|PortMappingProtocol|RemoteHost|ExternalPort|ExternalPortEndRange|InternalPort|InternalClient|PortMappingDescription,stPortMap);%>;
var WanIPPortMappingAcc = <%webGetCfgEntryAcc(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i}.PortMapping.{i},PortMappingEnabled|PortMappingProtocol|RemoteHost|ExternalPort|ExternalPortEndRange|InternalPort|InternalClient|PortMappingDescription,stPortMap);%>;
var WanPPPPortMapping = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i}.PortMapping.{i},PortMappingEnabled|PortMappingProtocol|RemoteHost|ExternalPort|ExternalPortEndRange|InternalPort|InternalClient|PortMappingDescription,stPortMap);%>;
var WanPPPPortMappingAcc = <%webGetCfgEntryAcc(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i}.PortMapping.{i},PortMappingEnabled|PortMappingProtocol|RemoteHost|ExternalPort|ExternalPortEndRange|InternalPort|InternalClient|PortMappingDescription,stPortMap);%>;
var PortMapping = new Array();
var PortMappingAcc = new Array();
for (i = 0; i < WanIPPortMapping.length-1; i++)
{
PortMapping[i] = WanIPPortMapping[i];
PortMappingAcc[i] = WanIPPortMappingAcc[i];
}
for (j = 0; j < WanPPPPortMapping.length-1; j++,i++)
{
PortMapping[i] = WanPPPPortMapping[j];
PortMappingAcc[i] = WanPPPPortMappingAcc[j];
}
if (PortMapping.length > 0)
{
AssociateParam('PortMapping','WanInfo','NATEnabled|Enable|id|Name');
}
function AddSubmitParam(SubmitForm,type)
{
var Interface = getSelectVal('PortMappingInterface');
setDisable('PortMappingInterface',1);
getElementByName('PortMappingEnabled').value = 1;
SubmitForm.addDiv('PortMappingDiv','x');
SubmitForm.addParameter('x.InternalPort',getValue('InternalPort'));
if( selctIndex == -1 )
{
SubmitForm.setAction('addcfg.cgi?x=' + Interface + '.PortMapping'
+'&RequestFile=/html/application/portmapping.asp');
}
else
{
SubmitForm.setAction('setcfg.cgi?x=' + PortMapping[selctIndex].domain
+'&RequestFile=/html/application/portmapping.asp');
}
top.aplyLine = selctIndex;
setDisable('btnApply',1);
}
function CheckForm(type)
{
switch (type)
{
case 3:
return CheckPortMapping();
break;
}
return true;
}
var cwmpConnReqPort = <%webGetParaByDomainName(InternetGatewayDevice.ManagementServer.X_ConnReqPort);%>;
function CheckPortMapping()
{
if((selctIndex == -1) && (PortMapping.length >= 32))
{
AddErrInfo(eval("gErrStr0"), '')
return false;
}
if (getElement('PortMappingInterface').length == 0)
{
AddErrInfo(eval("gErrStr1"), 'PortMappingInterface');
return false;
}
with (getElById('PortMappingForm'))
{
if (RemoteHost.value != "")
{
if (isAbcIpAddress(RemoteHost.value) == false)
{
AddErrInfo(eval("gErrStr2"), 'RemoteHost');
return false;
}
}
if (ExternalPort.value == "")
{
AddErrInfo(eval("gErrStr3"), 'ExternalPort');
return false;
}
if ((parseInt(ExternalPort.value) == cwmpConnReqPort)
|| (parseInt(ExternalPort.value) == 8082))
{
AddErrInfo(eval("gErrStr21"), 'ExternalPort');
return false;
}
if (ExternalPortEndRange.value == '')
{
setValueDft('ExternalPortEndRange', ExternalPort.value);
}
if (ExternalPortEndRange.value != '')
{
if ((parseInt(ExternalPortEndRange.value) != 0)
&& (isValidPort(ExternalPortEndRange.value) == false) )
{
AddErrInfo(eval("gErrStr8"), 'ExternalPortEndRange');
return false;
}
if (parseInt(ExternalPortEndRange.value) != 0)
{
if (parseInt(ExternalPortEndRange.value) < parseInt(ExternalPort.value))
{
AddErrInfo(eval("gErrStr17"), 'ExternalPort');
return false;
}
if (((parseInt(ExternalPort.value) <= cwmpConnReqPort) && (parseInt(ExternalPortEndRange.value) >= cwmpConnReqPort))
|| ((parseInt(ExternalPort.value) <= 8082) && (parseInt(ExternalPortEndRange.value) >= 8082)))
{
AddErrInfo(eval("gErrStr21"), 'ExternalPort');
return false;
}
}
}
if (InternalPort.value == "")
{
AddErrInfo(eval("gErrStr4"), 'InternalPort');
return false;
}  
if (InternalClient.value == "")
{
AddErrInfo(eval("gErrStr5"), 'InternalClient');
return false;
}
if (isAbcIpAddress(InternalClient.value) == false)
{
AddErrInfo(eval("gErrStr5"), 'InternalClient');
return false;
}
if (PortMappingDescription.value == "")
{
AddErrInfo(eval("gErrStr6"), 'PortMappingDescription');
return false;
}
if (isAbcIpAddress(InternalClient.value) == false)
{
AddErrInfo(eval("gErrStr7"), 'InternalClient');
return;
}
if (isValidPort(ExternalPort.value) == false )
{
AddErrInfo(eval("gErrStr8"), 'ExternalPort');
return false;
}
if (isValidPort(InternalPort.value) == false
&& (InternalPort.value != 0))
{
AddErrInfo(eval("gErrStr9"), 'InternalPort');
return false;
}
if (isValidName(PortMappingDescription.value) == false)
{
AddErrInfo(eval("gErrStr10"), 'PortMappingDescription');
return false;
}
for (i = 0; i < PortMapping.length; i++)
{
if(selctIndex != i)
{
if (PortMapping[i].Description == PortMappingDescription.value)
{
AddErrInfo(eval("gErrStr11") + getSafeString(getValue('PortMappingDescription')) + eval("gErrStr12"), 'PortMappingDescription');
return false;
}
if (PortMapping[i].Interface ==  getSelectVal('PortMappingInterface')
&& PortMapping[i].ExPort == ExternalPort.value
&& PortMapping[i].ExPortEnd == ExternalPortEndRange.value
&& (PortMapping[i].Protocol == PortMappingProtocol.value
||PortMappingProtocol.value == 'TCP/UDP'
||PortMapping[i].Protocol == 'TCP/UDP'))
{
AddErrInfo(eval("gErrStr13"), 'ExternalPort');
return false;
}
}
}
var portsum = 0;
for (i = 0; i < PortMapping.length; i++)
{
if(selctIndex != i)
{
if (parseInt(PortMapping[i].ExPortEnd) == 0)
{
portsum += 1;
}
else
{
portsum += (parseInt(PortMapping[i].ExPortEnd)
- parseInt(PortMapping[i].ExPort) + 1);
}
}
}
if (parseInt(ExternalPortEndRange.value) == 0)
{
portsum += 1;
}
else
{
portsum += (parseInt(ExternalPortEndRange.value)
- parseInt(ExternalPort.value) + 1);
}
if (portsum > 256)
{
AddErrInfo(eval("gErrStr20"), 'ExternalPort');
return false;
}
var flag = 0;
var xextenalport = parseInt(ExternalPort.value);
var xextenalportEnd = 0;
if (ExternalPortEndRange.value != '')
{
xextenalportEnd = parseInt(ExternalPortEndRange.value);
}
var xportmapi = 0;
var xportmapiend = 0;
for (i = 0; i < PortMapping.length; i++)
{
if (flag)
{
break;
}
if(selctIndex != i)
{
if (PortMapping[i].Interface !=  getSelectVal('PortMappingInterface'))
{
continue;
}
if ((PortMapping[i].Protocol != PortMappingProtocol.value)
&& (PortMappingProtocol.value != 'TCP/UDP')
&& (PortMapping[i].Protocol != 'TCP/UDP'))
{
continue;
}
xportmapi = parseInt(PortMapping[i].ExPort);
xportmapiend = parseInt(PortMapping[i].ExPortEnd);
if (xextenalportEnd == 0)
{
if (xportmapiend == 0)
{
if (xextenalport != xportmapi)
{
continue;
}
flag = 1;
}
else
{
if ((xextenalport < xportmapi) || (xextenalport > xportmapiend))
{
continue;
}
flag = 1;
}
}
else
{
if (xportmapiend == 0)
{
if ((xextenalport > xportmapi)
|| (xextenalportEnd < xportmapi))
{
continue;
}
flag = 1;
}
else
{
if ((xextenalportEnd < xportmapi) || (xextenalport > xportmapiend))
{
continue;
}
flag = 1;
}
}
}
}
if (flag)
{
AddErrInfo(eval("gErrStr13"), 'ExternalPort');
return false;
}
var ret = true;
if ( (("21" == getValue("ExternalPort")) || ("21" == getValue("ExternalPortEndRange"))
|| ((21 >= parseInt(getValue("ExternalPort"))) && (21 <= parseInt(getValue("ExternalPortEndRange")))))
&& ((PortMappingProtocol.value == 'TCP/UDP') || (PortMappingProtocol.value == 'TCP'))
)
{
ret = confirm(eval('gErrStr14'));
if (ret != true)
{
return ret;
}
}
if ( (("22" == getValue("ExternalPort")) || ("22" == getValue("ExternalPortEndRange"))
|| ((22 >= parseInt(getValue("ExternalPort"))) && (22 <= parseInt(getValue("ExternalPortEndRange")))))
&& ((PortMappingProtocol.value == 'TCP/UDP') || (PortMappingProtocol.value == 'TCP'))
)
{
ret = confirm(eval('gErrStr18'));
if (ret != true)
{
return ret;
}
}
if ( (("23" == getValue("ExternalPort")) || ("23" == getValue("ExternalPortEndRange"))
|| ((23 >= parseInt(getValue("ExternalPort"))) && (23 <= parseInt(getValue("ExternalPortEndRange")))))
&& ((PortMappingProtocol.value == 'TCP/UDP') || (PortMappingProtocol.value == 'TCP')))
{
ret = confirm(eval('gErrStr15'));
if (ret != true)
{
return ret;
}
}
if ( (("80" == getValue("ExternalPort")) || ("80" == getValue("ExternalPortEndRange"))
|| ((80 >= parseInt(getValue("ExternalPort"))) && (80 <= parseInt(getValue("ExternalPortEndRange")))))
&& ((PortMappingProtocol.value == 'TCP/UDP') || (PortMappingProtocol.value == 'TCP')))
{
ret = confirm(eval('gErrStr16'));
if (ret != true)
{
return ret;
}
}
}
return true;
}
function LoadFrame()
{
var i = GetmenuName("portmapping.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_0');
}
}
function ShowPortMapping()
{
var Interface = getElement('PortMappingInterface');
var optionID = Interface.options[Interface.selectedIndex].id;
var index = optionID.split('_')[1];
if (Interface.length > 0)
{
if (WanInfo[index].Enable == 0 || WanInfo[index].NATEnabled == 0)
{
setDisable('btn1',1);
}
else
{
setDisable('btn1',0);
}
}
var html = '<table border="1" cellpadding="0" cellspacing="0" width="70%" style="table-layout:fixed;word-break:break-all">'
+ '<tr align="middle">'
+  '<td width="9%" class="table_title">Prptocol</td>'
+  '<td width="13%" class="table_title">Source IP Address</td>'
+  '<td width="16%" class="table_title">External Port</td>'
+  '<td width="16%" class="table_title">External End Port</td>'
+  '<td width="16%" class="table_title">Internal Port</td>'
+  '<td width="13%" class="table_title">Internal Host</td>'
+  '<td width="15%" class="table_title">Mapping Name</td>'
+  '<td width="10%" class="table_title">Enable</td>'
+  '<td width="10%" class="table_title">Remove</td>'
+ '</tr>'
for (i = 0; i < PortMapping.length; i++)
{
if (PortMapping[i].domain.indexOf(getSelectVal('PortMappingInterface'))
> -1)
{
html += '<TR align="middle">'
html +=  '<TD align="center">' + PortMapping[i].Protocol + '&nbsp;</TD>';
html += '<TD align="center">' + PortMapping[i].RemoteHost + '&nbsp;</TD>';
html += '<TD align="center">' + PortMapping[i].ExPort + '&nbsp;</TD>';
if (PortMapping[i].ExPortEnd == 0)
{
html += '<TD align="center">' + PortMapping[i].ExPort + '&nbsp;</TD>';
}
else
{
html += '<TD align="center">' + PortMapping[i].ExPortEnd + '&nbsp;</TD>';
}
html += '<TD align="center">' + PortMapping[i].InPort + '&nbsp;</TD>';
html += '<TD align="center">' + PortMapping[i].InClient + '&nbsp;</TD>';
html += '<TD align="center">'
+ getDisplayText(PortMapping[i].Description) + '&nbsp;</TD>';
if (PortMapping[i].ProtMapEnabled == 1)
{
html += '<TD align="center">' + eval('gVarPubEnable') + '&nbsp;</TD>';
}
else
{
html += '<TD align="center">' + eval('gVarPubDisable') + '&nbsp;</TD>';
}
html += '<TD align="center"><input type="checkbox" name="rml" value="'
+ PortMapping[i].domain + '"></TD>';
html += '</TR>';
}
}
html += '</table>'
}
function ShowPortMap()
{
var html = ''
for (i = 0; i < PortMapping.length; i++)
{
for(var j=0;j<WanInfo.length;j++)
{
if(WanInfo[j].Name == PortMapping[i].Name)
{
break;
}
}
if(WanInfo.length == j)
{
break;
}
if ( (WanInfo[j].ConnectionType != 'IP_Routed') || (WanInfo[j].ServiceList == 'TR069') )
{
continue;
}
{
html += '<TR id=record_' + i
+' class="trTabContent" onclick="selectLine(this.id);">'
html += '<TD align="center">' + getSafeString(PortMapping[i].Description) + '&nbsp;</TD>';
html +=  '<TD align="center">' + getSafeString(PortMapping[i].Name) + '&nbsp;</TD>';
html +=  '<TD align="center">' + PortMapping[i].Protocol + '&nbsp;</TD>';
html += '<TD align="center">' + PortMapping[i].RemoteHost + '&nbsp;</TD>';
html += '<TD align="center">' + PortMapping[i].ExPort + '&nbsp;</TD>';
{
html += '<TD align="center">' + PortMapping[i].ExPortEnd + '&nbsp;</TD>';
}
html += '<TD align="center">' + PortMapping[i].InPort + '&nbsp;</TD>';
html += '<TD align="center">' + PortMapping[i].InClient + '&nbsp;</TD>';
if (PortMapping[i].ProtMapEnabled == 1)
{
html += '<TD align="center">' + eval('gVarPubEnable') + '&nbsp;</TD>';
}
else
{
html += '<TD align="center">' + eval('gVarPubDisable') + '&nbsp;</TD>';
}
html += '<TD align="center"><input type="checkbox" name="rml" value="'
+ PortMapping[i].domain + '"></TD>';
html += '</TR>';
}
}
document.write(html);
}
TOTAL_APP = 15;
FIRST_APP = "...";
var v = new Array(TOTAL_APP);
v[0] = new cV("Web Server (HTTP)",1);
v[0].e[0] = new iVe("80", "80", "1", "80", "80");
v[1] = new cV("FTP Server",1);
v[1].e[0] = new iVe("21", "21", "1", "21", "21");
v[2] = new cV("Telnet Server",1);
v[2].e[0] = new iVe("23", "23", "1", "23", "23");
v[3] = new cV("Mail (POP2)",1);
v[3].e[0] = new iVe("109", "109", "1", "109", "109");
v[4] = new cV("Mail (POP3)",1);
v[4].e[0] = new iVe("110", "110", "1", "110", "110");
v[5] = new cV("Mail (SMTP)",1);
v[5].e[0] = new iVe("25", "25", "1", "25", "25");
v[6] = new cV("NTP",1);
v[6].e[0] = new iVe("123", "123", "2", "123", "123");
v[7] = new cV("PPTP",1);
v[7].e[0] = new iVe("1723", "1723", "1", "1723", "1723");
v[8] = new cV("Remote desktop",1);
v[8].e[0] = new iVe("3389", "3389", "1", "3389", "3389");
v[9] = new cV("Apple Remote desktop",1);
v[9].e[0] = new iVe("3283", "3283", "0", "3283", "3283");
v[10] = new cV("Remotely any where",1);
v[10].e[0] = new iVe("2000", "2000", "0", "2000", "2000");
v[11] = new cV("ShoutCast",1);
v[11].e[0] = new iVe("8000", "8000", "0", "8000", "8000");
v[12] = new cV("Imap",1);
v[12].e[0] = new iVe("143", "143", "1", "143", "143");
v[13] = new cV("Lap Link",1);
v[13].e[0] = new iVe("1547", "1547", "1", "1547", "1547");
v[14] = new cV("My SQL Server",1);
v[14].e[0] = new iVe("3306", "3306", "0", "3306", "3306");
function cV(name, entryNum)
{
this.name = name;
this.eNum = entryNum;
this.e = new Array(5);
}
function iVe(eStart, eEnd, proto, iStart, iEnd)
{
this.eStart = eStart;
this.eEnd = eEnd;
this.proto = proto;
this.iStart = iStart;
this.iEnd = iEnd;
}
function clearAll()
{
}
function trimspace(str)
{
var strTemp = new String(str);
while (-1 != strTemp.search(" "))
{
strTemp = strTemp.replace(" ", "");
}
return strTemp;
}
function appSelect(sName)
{
clearAll();
with (getElement('PortMappingForm'))
{
if (sName == FIRST_APP)
{
return;
}
for(i = 0; i < TOTAL_APP; i++)
{
if(v[i].name == sName)
{
switch (v[i].e[0].proto)
{
case '0':
setSelect('PortMappingProtocol','TCP/UDP');
break;
case '1':
setSelect('PortMappingProtocol','TCP');
break;
case '2':
setSelect('PortMappingProtocol','UDP');
break;
}
getElement('RemoteHost').value = "";
getElement('ExternalPort').value = v[i].e[0].eStart;
getElement('ExternalPortEndRange').value = v[i].e[0].eEnd;
getElement('InternalPort').value = v[i].e[0].iStart;
getElement('PortMappingDescription').value = trimspace(sName);
}
}
}
}
function radioClick()
{
if(getElement("radiosrv")[0].checked == true)
{
setDisable("constsrvName",1);
}
else
{
setDisable("constsrvName",0);
}
}
function setCtlDisplay(record)
{
var endIndex = record.domain.lastIndexOf('PortMapping') - 1;
var Interface = record.domain.substring(0,endIndex);
setSelect('PortMappingInterface',Interface);
setSelect('PortMappingEnabled',record.ProtMapEnabled);
setSelect('PortMappingProtocol',record.Protocol);
setText('RemoteHost',record.RemoteHost);
setText('ExternalPort',record.ExPort);
{
setText('ExternalPortEndRange',record.ExPortEnd);
}
setText('InternalPort',record.InPort);
setText('InternalClient',record.InClient);
setText('PortMappingDescription',record.Description);
}
function  setCtlDisable(index)
{
if(-1 == index)
{
setDisable('PortMappingProtocol', 0);
setDisable('RemoteHost', 0);
setDisable('ExternalPort', 0);
setDisable('ExternalPortEndRange', 0);
setDisable('InternalPort', 0);
setDisable('InternalClient', 0);
setDisable('PortMappingDescription', 0);
}else{
setDisable('PortMappingProtocol', TransAcc(PortMappingAcc[index].Protocol));
setDisable('RemoteHost', TransAcc(PortMappingAcc[index].RemoteHost));
setDisable('ExternalPort', TransAcc(PortMappingAcc[index].ExPort));
setDisable('ExternalPortEndRange', TransAcc(PortMappingAcc[index].ExPortEnd));
setDisable('InternalPort', TransAcc(PortMappingAcc[index].InPort));
setDisable('InternalClient', TransAcc(PortMappingAcc[index].InClient));
setDisable('PortMappingDescription', TransAcc(PortMappingAcc[index].Description));
}
}
function setControl(index)
{
var record;
selctIndex = index;
if (index == -1)
{
record = new stPortMap('','','','','', '', '','','');
setDisable('PortMappingInterface',0);
}
else
{
record = PortMapping[index];
setDisable('PortMappingInterface',1);
}
setCtlDisplay(record);
setCtlDisable(index);
}
function clickRemove()
{
removeInst('/html/application/portmapping.asp');
}

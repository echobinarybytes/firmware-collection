<script language="JavaScript" type="text/javascript">
var selctIndex = -1;
var AppFltList = new Array();
function stAppInfo(Name,Protocol,Port,Status)
{
this.Name = Name;
this.Protocol = Protocol;
this.ulPort = Port;
this.Status = Status;
}
var AppNameConstEng = new Array();
AppNameConstEng[0] = new stAppInfo('FTP','1','21','1');
AppNameConstEng[1] = new stAppInfo('SSH','1','22','1');
AppNameConstEng[2] = new stAppInfo('Telnet','1','23','1');
AppNameConstEng[3] = new stAppInfo('SMTP','1','25','1');
AppNameConstEng[4] = new stAppInfo('DNS','2','53','1');
AppNameConstEng[5] = new stAppInfo('HTTP','1','80','1');
AppNameConstEng[6] = new stAppInfo('POP3','1','110','1');
AppNameConstEng[7] = new stAppInfo('HTTPS','1','443','1');
AppNameConstEng[8] = new stAppInfo('HTTP Proxy','1','8080','1');
AppNameConstEng[9] = new stAppInfo('File Share','0','137-139,445','1');
function stAppFilter(domain,Name,Protocol,Status,Port)
{
this.domain = domain;
this.Name = Name;
this.Protocol = Protocol;
this.Status = Status;
this.Port = Port;
}
var AppFltList = <%webGetCfgEntry(InternetGatewayDevice.X_FireWall.AppFilter.{i},Name|Protocol|Status|Port,stAppFilter);%>
function ShowAppStatus(Status)
{
if (Status == 0)
{
return eval("gVarDisable");
}
else if (Status == 1)
{
return eval("gVarAccept");
}
else if (Status == 2)
{
return eval("gVarDeny");
}
}
function ShowAppProtocol(Protocol)
{
return Protocol;
}
function CheckForm()
{
with(document.forms[0])
{
if (getElement('AppName').value == "FIRST_APP")
{
AddErrInfo(eval("gErrStr7"), 'AppName');
return false;
}
for (i = 0; i < AppFltList.length && null != AppFltList[i]; i++)
{
if (AppFltList[i].Name == getElement('AppName').value && selctIndex != i)
{
AddErrInfo(eval("gErrStr8") + getSafeString(AppName.value) + eval("gErrStr9"), 'AppName');
return false;
}
}
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
SubmitForm.addParameter('x.Protocol',getSelectVal('AppProtocol'));
SubmitForm.addParameter('x.Name',getSelectVal('AppName'));
SubmitForm.addParameter('x.Status',getSelectVal('AppStatus'));
SubmitForm.addParameter('x.Port',getValue('AppPort'));
if( selctIndex == -1 )
{
SubmitForm.setAction('addcfg.cgi?x=InternetGatewayDevice.X_FireWall.AppFilter'
+ '&RequestFile=/html/security/appfilter.asp');
}
else
{
SubmitForm.setAction('setcfg.cgi?x='+AppFltList[selctIndex].domain
+ '&RequestFile=/html/security/appfilter.asp');
}
top.aplyLine = selctIndex;
setDisable('btnApply',1);
}
function setCtlDisplay(record)
{
with(document.forms[0])
{
setSelect('AppName',record.Name);
setSelect('AppProtocol',record.Protocol);
setSelect('AppStatus',record.Status);
setText('AppPort',record.Port);
AppPort.disabled = 1;
AppProtocol.disabled = 1;
}
}
function setControl(index)
{
var record;
selctIndex = index;
if (index == -1)
{
record = new stAppFilter('','FIRST_APP', '', '0', '');
}
else
{
record = AppFltList[index];
}
setCtlDisplay(record);
}
function AppSelect(sName)
{
with (document.forms[0])
{
getElement('AppPort').value = '';
setSelect('AppProtocol','');
setSelect('AppStatus','0');
AppPort.disabled = 1;
AppProtocol.disabled = 1;
if (sName == "FIRST_APP")
{
return;
}
for(i = 0; i < AppNameConstEng.length; i++)
{
if(AppNameConstEng[i].Name == sName)
{
switch (AppNameConstEng[i].Protocol)
{
case '0':
setSelect('AppProtocol','TCP/UDP');
break;
case '1':
setSelect('AppProtocol','TCP');
break;
case '2':
setSelect('AppProtocol','UDP');
break;
default:
setSelect('AppProtocol','');
break;
}
getElement('AppPort').value = AppNameConstEng[i].ulPort;
setSelect('AppStatus',AppNameConstEng[i].Status);
break;
}
}
}
}
function LoadFrame()
{
top.errStrStr = '';
top.errIdCtrlStr = '';
with(document.forms[0])
{
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_0');
}
AppPort.disabled = 1;
AppProtocol.disabled = 1;
}
}
function clickRemove()
{
removeInst('/html/security/appfilter.asp');
}
function FW_APP_CfgHeader()
{
writeTabCfgHeaderEx(eval("gVarAppFilter"),'App Filter',"90%");
}

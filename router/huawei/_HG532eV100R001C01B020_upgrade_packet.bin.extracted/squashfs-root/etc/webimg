function expandMenu(firstMenuID)
{
if (firstMenuID == 'status')
{
displayStatus();
}
else if(firstMenuID == 'basic')
{
displayBasic();
}
else if(firstMenuID == 'advance')
{
displayAdvance();
}
else if(firstMenuID == 'management')
{
displayMan();
}
return;
}
function openAllHelp(postion)
{
var IDFromPostion = '';
hiddenStatusContent();
hiddenBasicContent();
hiddenAdvanceContent();
getElement(globalSecItem).style.color = '#000000';
var IDPose = postion.split('_');
if (IDPose.length > 3)
{
IDFromPostion = IDPose[0] + '_' + IDPose[1] + '_' + IDPose[2];
}
else
{
IDFromPostion = postion;
}
getElement(IDFromPostion).style.color = '#990000';
if (top.Link != '' && top.Link != 'undefined')
{
expandMenu(IDPose[0]);
}
switch(postion)
{
case 'status_Link_1':
case 'status_Link_1_Device':
getElement('Device_Div').style.display = "";
break;
case 'status_Link_2_Internet':
getElement('WAN_Internet_Div').style.display = "";
break;
case 'status_Link_2_DSL':
getElement('WAN_DSL_Div').style.display = "";
break;
case 'status_Link_2_Ethernet':
break;
case 'status_Link_2_UMTS':
break;
case 'status_Link_2':
getElement('WAN_Internet_Div').style.display = "";
getElement('WAN_DSL_Div').style.display = "";
break;
case 'status_Link_3_LANH':
getElement('LAN_Network_Div').style.display = "";
break;
case 'status_Link_3_LANStatus':
getElement('LAN_Ethernet_Div').style.display = "";
break;
case 'status_Link_4_WLAN':
case 'status_Link_4_Wireless':
case 'status_Link_4_SSID':
case 'status_Link_4':
getElement('LAN_WLAN_Div').style.display = "";
break;
case 'status_Link_3':
getElement('LAN_Network_Div').style.display = "";
getElement('LAN_Ethernet_Div').style.display = "";
break;
case 'status_Link_4_Call':
getElement('VOIP_Call_Div').style.display = "";
break;
case 'status_Link_4_Port':
getElement('VOIP_Port_Div').style.display = "";
break;
case 'basic_Link_1' :
case 'basic_Link_1_WAN' :
getElement('WAN_Basic').style.display = "";
break;
case 'basic_Link_2_LANInfo':
getElement('LAN_Basic_ADiv').style.display = "";
break;
case 'basic_Link_2_DHCPInfo':
getElement('LAN_Basic_BDiv').style.display = "";
break;
case 'basic_Link_2_ReserveIP':
getElement('LAN_Basic_SDiv').style.display = "";
break;
case 'basic_Link_2_DHCPServingPool':
getElement('LAN_Basic_EDiv').style.display = "";
break;
case 'basic_Link_2_DHCPv6Server':
getElement('LAN_Basic_GDiv').style.display = "";
break;
case 'basic_Link_2_SLAAC':
getElement('LAN_Basic_HDiv').style.display = "";
break;
case 'basic_Link_3_WLANInfo':
getElement('LAN_Basic_CDiv').style.display = "";
break;
case 'basic_Link_3_WLANFilter':
getElement('LAN_Basic_DDiv').style.display = "";
break;
case 'basic_Link_4':
case 'basic_Link_4_DSL':
getElement('DSL_Basic_Div').style.display = "";
break;
case 'basic_Link_2':
getElement('LAN_Basic_ADiv').style.display = "";
getElement('LAN_Basic_BDiv').style.display = "";
getElement('LAN_Basic_SDiv').style.display = "";
getElement('LAN_Basic_EDiv').style.display = "";
getElement('LAN_Basic_GDiv').style.display = "";
getElement('LAN_Basic_HDiv').style.display = "";
break;
case 'basic_Link_3':
getElement('LAN_Basic_CDiv').style.display = "";
getElement('LAN_Basic_DDiv').style.display = "";
break;
case 'advance_Link_1_StaticRoute':
getElement('Advance_Route1_div').style.display = "";
break;
case 'advance_Link_1_RIP':
getElement('Advance_Route2_div').style.display = "";
break;
case 'advance_Link_1_IPv6StaticRoute':
getElement('Advance_Route3_div').style.display = "";
break;
case 'advance_Link_1':
getElement('Advance_Route1_div').style.display = "";
getElement('Advance_Route2_div').style.display = "";
getElement('Advance_Route3_div').style.display = "";
break;
case 'advance_Link_2_Firewall':
getElement('Advance_Firewall_div').style.display = "";
break;
case 'advance_Link_17_MACFilter':
getElement('Advance_MAC_div').style.display = "";
break;
case 'advance_Link_17_IPFilter':
getElement('Advance_IP_div').style.display = "";
break;
case 'advance_Link_17_URLFilter':
getElement('Advance_URL_div').style.display = "";
break;
case 'advance_Link_17_APPFilter':
getElement('Advance_APP_div').style.display = "";
break;
case 'advance_Link_17':
getElement('Advance_MAC_div').style.display = "";
getElement('Advance_IP_div').style.display = "";
getElement('Advance_URL_div').style.display = "";
getElement('Advance_APP_div').style.display = "";
break;
case 'advance_Link_18':
getElement('Advance_ACL_div').style.display = "";
break;
case 'advance_Link_2_DosAttack':
getElement('Advance_DOS_div').style.display = "";
break;
case 'advance_Link_18_ACL':
getElement('Advance_ACL_div').style.display = "";
break;
case 'advance_Link_2':
getElement('Advance_Firewall_div').style.display = "";
getElement('Advance_DOS_div').style.display = "";
break;
case 'advance_Link_3_ALG':
getElement('Advance_ALG_div').style.display = "";

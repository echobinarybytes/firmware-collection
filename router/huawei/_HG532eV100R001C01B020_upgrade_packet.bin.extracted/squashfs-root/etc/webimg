<html>
<head>
<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/util.js"></script>
<script language="JavaScript" src="/lang/account.res"></script>
<title>Account</title>
<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" type="text/javascript">
var ErrInfo = <%webGetMsgRetCode();%>;
function stUserInfo(domain,name)
{
this.domain = domain;
this.name = name;
}
var UserInfo = <%webGetCfgEntry(InternetGatewayDevice.UserInterface.X_Web.UserInfo.{i},Username,stUserInfo);%>;
var sptUserName = UserInfo[0].name;
var sysUserName = UserInfo[1].name;
var curUserName = "<%webGetUserName();%>";
var curUserType = "<%webGetUserType();%>";   
var sptUserType = 0;
var sysUserType = 1;
function LoadFrame()
{
var i = GetmenuName("account.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
with (getElement('ConfigForm'))
{
if (curUserName == sysUserName)
{
secUsername.style.display = "none";
trNewUsername.style.display = "";
}
else
{
trNewUsername.style.display = "none";
}
newUsername.value = oldUsername.value;
}
if ((ErrInfo < gPubErrStrArray.length) && (ErrInfo != '0'))
{
alert(gPubErrStrArray[ErrInfo]);
if (ErrInfo == 15)
{
top.location = '/html/index.asp';
}
}
}
function oldUsernameChange()
{
with (ConfigForm)
{
if (oldUsername.value == sptUserName
)
{
TroldPassword.style.display = "";
trNewUsername.style.display = "none";
}
else if (oldUsername.value == sysUserName)
{
TroldPassword.style.display = "none";
trNewUsername.style.display = "";
}
newUsername.value = oldUsername.value;
}
}
function CheckForm()
{
with (ConfigForm)
{
if (newUsername.value == "" && trNewUsername.style.display != "none")
{
AddErrInfo(eval("gErrStr0"), 'newUsername');
return false;
}
if (curUserName != sptUserName && oldPassword.value == ""
)
{
AddErrInfo(eval("gErrStr1"), 'oldPassword');
return false;
}
if ((newUsername.value.indexOf('\\') != -1 ) || (newUsername.value.indexOf('\'') != -1 ) || (newUsername.value.indexOf('"') != -1 ))
{
AddErrInfo(eval("gErrStr9"), 'newUsername');
return false;
}
if (getValue('oldPassword').length > 16)
{
AddErrInfo(eval("gErrStr6"), 'oldPassword');
return false;
}
if (getValue('newPassword').length > 16)
{
AddErrInfo(eval("gErrStr7"), 'newPassword');
return false;
}
if (getValue('cfmPassword').length > 16)
{
AddErrInfo(eval("gErrStr8"), 'cfmPassword');
return false;
}
if (newPassword.value == "")
{
AddErrInfo(eval("gErrStr2"), 'newPassword');
return false;
}
if (cfmPassword.value != newPassword.value)
{
AddErrInfo(eval("gErrStr3"), 'cfmPassword');
return false;
}
if(newUsername.value == sptUserName && curUserName == sysUserName)
{
AddErrInfo(eval("gErrStr4"), 'newUsername');
return false;
}
if(newUsername.value == sptUserName && curUserName == sptUserName && oldUsername.value == sysUserName)
{
AddErrInfo(eval("gErrStr5"), 'newUsername');
return false;
}
var level = checkStrong(newPassword.value);
if(1 == level)
{
var choice = confirm(eval("gErrStr12") + '\n' + eval("gErrStr13"));
if(choice == false)
return false;
}
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
var cfgUsername = getSelectVal('oldUsername');
SubmitForm.addParameter('cfgUsername',cfgUsername);
SubmitForm.addParameter('Userpassword', base64encode(SHA256(getValue('cfmPassword'))));
if (cfgUsername == sysUserName)
{
SubmitForm.addParameter('Username',getValue('newUsername'));
if (curUserName != sptUserName
)
{
SubmitForm.addParameter('OldPassword', base64encode(SHA256(getValue('oldPassword'))));
}
}
else
{
SubmitForm.addParameter('OldPassword', base64encode(SHA256(getValue('oldPassword'))));
}
SubmitForm.setAction('chgacount.cgi?RequestFile=/html/management/account.asp');
top.previousPage = 	'/html/management/account.asp';
}
</script>
</head>

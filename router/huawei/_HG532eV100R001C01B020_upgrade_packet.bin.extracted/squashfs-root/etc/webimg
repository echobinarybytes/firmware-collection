function stSTUN(domain,Enable,SrvAddress,SrvPort,Username,Password,MaximumPeriod,MinimumPeriod)
{
this.domain = domain;
this.Enable = Enable;
this.SrvAddress = SrvAddress;
this.SrvPort = SrvPort;
this.Username = Username;
this.Password = Password;
this.MaximumPeriod = MaximumPeriod;
this.MinimumPeriod = MinimumPeriod;
}
var temp = <%webGetCfgEntry(InternetGatewayDevice.ManagementServer,STUNEnable|STUNServerAddress|STUNServerPort|STUNUsername|STUNPassword|STUNMaximumKeepAlivePeriod|STUNMinimumKeepAlivePeriod,stSTUN);%>;
var STUN = temp[0];
function LoadFrame()
{
var i = GetmenuName("tr111.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
setCheck('STUNEnable',STUN.Enable);
setText('STUNServerAddress',STUN.SrvAddress);
setText('STUNServerPort',STUN.SrvPort);
setText('STUNUsername',STUN.Username);
setText('STUNPassword',STUN.Password);
setText('STUNMaximumKeepAlivePeriod',STUN.MaximumPeriod);
setText('STUNMinimumKeepAlivePeriod',STUN.MinimumPeriod);
}
function CheckForm(type)
{
with (getElement('ConfigForm'))
{
if (isPlusInteger(STUNMinimumKeepAlivePeriod.value) == false)
{
AddErrInfo(eval("gErrStr0"), 'STUNMinimumKeepAlivePeriod');
return false;
}
if (isPlusInteger(STUNMaximumKeepAlivePeriod.value) == false)
{
AddErrInfo(eval("gErrStr1"), 'STUNMinimumKeepAlivePeriod');
return false;
}
if ( parseInt(STUNMaximumKeepAlivePeriod.value) < parseInt(STUNMinimumKeepAlivePeriod.value))
{
AddErrInfo(eval("gErrStr2") + eval("gErrStr3"), 'STUNMaximumKeepAlivePeriod');
return false;
}
if (isValidCfgStr('STUN Server Address',STUNServerAddress.value,255)
== false)
{
return false;
}
if (isValidPort(STUNServerPort.value) == false)
{
AddErrInfo(eval("gErrStr4") + STUNServerPort.value + eval("gErrStr5"), 'STUNServerPort');
return false;
}
if (isValidCfgStr('STUN Username',STUNUsername.value,255)
== false)
{
return false;
}
if (isValidCfgStr('STUN Password',STUNPassword.value,255)
== false)
{
return false;
}
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
SubmitForm.addForm('ConfigForm','x');
SubmitForm.delParameter('x.STUNPassword');
if (getValue('STUNPassword') != '@1GV)Z<!')
{
SubmitForm.addParameter('x.STUNPassword', getValue('STUNPassword'));
}
SubmitForm.setAction('setcfg.cgi?x=InternetGatewayDevice.ManagementServer&'
+ 'RequestFile=/html/network/tr111.asp');
setDisable('btnApply',1);
}

document.writeln("    <td>" + PacketInfo[i][6] + "&nbsp;</td>");
document.writeln("    <td>" + PacketInfo[i][7] + "&nbsp;</td>");
document.writeln("    <td>" + PacketInfo[i][8] + "&nbsp;</td>");
document.writeln("  </tr>");
}
}
}
</script>
</table>

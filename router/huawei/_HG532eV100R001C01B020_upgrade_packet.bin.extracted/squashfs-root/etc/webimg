function writehhfordst()
{
for(var i = 0; i < 4; i++)
document.writeln("<option value=" + i + ">" + i + "</option>");
}
function writehh()
{
for( var i = 0; i < 24; i++ )
document.writeln("<option value=" + i + ">" + i + "</option>");
}
function writemm()
{
for( var i = 0; i < 60; i++ )
document.writeln("<option value=" + i + ">" + i + "</option>");
}
function writess()
{
for( var i = 0; i < 60; i++ )
document.writeln("<option value=" + i + ">" + i + "</option>");
}
function writeTimeZoneList()
{
for( var i = 0; i < timeZonesEng.length; i++ )
document.writeln("<option value=" + getTimeZoneOffset(i) + ">" + timeZonesEng[i] + "</option>");
}
function ntpChange(optionlist,textbox)
{
if( optionlist[optionlist.selectedIndex].value == "Other" )
{
setDisplay(textbox.name, 1);
}
else
{
textbox.value = "";
setDisplay(textbox.name, 0);
}
}
function writeNtpList(needed)
{
if(!needed)
{
document.writeln("<option value=None>" + eval('gVarNone') + "</option>");
}
for( var i = 0; i < ntpServers.length; i++ )
{
document.writeln("<option value=" + ntpServers[i] + ">" + ntpServers[i] + "</option>");
}
document.writeln("<option value=Other>" + eval('gVarOther') + "</option>");
}
function LoadFrame()
{
var i = GetmenuName("sntp.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
var i = 0;
var ntp1 = Sntp.ntp1 ;
var ntp2 = Sntp.ntp2;
var ntp3 = Sntp.ntp3;
var ntp4 = Sntp.ntp4;
var ntp5 = Sntp.ntp5;
var ntp_enabled = Sntp.Enable;
var tz_name = Sntp.ZoneName;
with (document.forms[0])
{
setCheck('ntpEnabled',ntp_enabled);
if( ntp1.length )
{
for( i = 0; i < ntpServers.length; i++ )
{
if( ntp1 == ntpServers[i] )
{
ntpServer1.selectedIndex = i;
break;
}
}
if( i == ntpServers.length )
{
ntpServer1.selectedIndex = i; 
ntpServerOther1.value = ntp1;
}
}
ntpChange(ntpServer1,ntpServerOther1);
if( ntp2.length )
{
for( i = 0; i < ntpServers.length; i++ )
{
if( ntp2 == ntpServers[i] )
{
ntpServer2.selectedIndex = i+1;
break;
}
}
if( i == ntpServers.length )
{
ntpServer2.selectedIndex = i+1; 
ntpServerOther2.value = ntp2;
}
}
ntpChange(ntpServer2,ntpServerOther2);
if( ntp3.length )
{
for( i = 0; i < ntpServers.length; i++ )
{
if( ntp3 == ntpServers[i] )
{
ntpServer3.selectedIndex = i+1;
break;
}
}
if( i == ntpServers.length )
{
ntpServer3.selectedIndex = i+1; 
ntpServerOther3.value = ntp3;
}
}
ntpChange(ntpServer3,ntpServerOther3);
if( ntp4.length )
{
for( i = 0; i < ntpServers.length; i++ )
{
if( ntp4 == ntpServers[i] )
{
ntpServer4.selectedIndex = i+1;
break;
}
}
if( i == ntpServers.length )
{
ntpServer4.selectedIndex = i+1; 
ntpServerOther4.value = ntp4;
}
}
ntpChange(ntpServer4,ntpServerOther4);
if( ntp5.length )
{
for( i = 0; i < ntpServers.length; i++ )
{
if( ntp5 == ntpServers[i] )
{
ntpServer5.selectedIndex = i+1;
break;
}
}
if( i == ntpServers.length )
{
ntpServer5.selectedIndex = i+1; 
ntpServerOther5.value = ntp5;
}
}
ntpChange(ntpServer5,ntpServerOther5);
setDisplay('ntpConfig', getCheckVal('ntpEnabled'));
setdisptime();
cboTimeZone.selectedIndex = Sntp.label;
setCheck('dstEnabled', dstcfgenable);
setDisplay('dstcfgenable', 	1);
setDisplay('dstcfgstart', dstcfgenable);
setDisplay('dstcfgend', dstcfgenable);
setDisplay('dstcfgoffset', dstcfgenable);
}
}
function writesynstatus()
{
if (Sntp.stats == "Disabled")
{
document.write(eval("gVarSntpdisabled"));
}
else if (Sntp.stats == "Unsynchronized")
{
document.write(eval("gVarSntpunsync"));
}
else if (Sntp.stats == "Synchronized")
{
document.write(eval("gVarSntpsynced"));
}
else if (Sntp.stats == "Error_FailedToSynchronize")
{
document.write(eval("gVarSntpsyncerror"));
}
else
{
document.write(eval("gVarSntpdisabled"));
}
}
function isTValidName(name) {
var i = 0;
var unsafeString = "\"<>%\\^[]`\+\$\,='#&:;*/{} \t";
for ( i = 0; i < name.length; i++ ) {
for( j = 0; j < unsafeString.length; j++)
if ( (name.charAt(i)) == unsafeString.charAt(j) )
return false;
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
with(document.forms[0])
{
if (1 == getCheckVal('ntpEnabled'))
{
SubmitForm.addParameter('x.Enable', 1);
if(ntpServer1.selectedIndex == ntpServers.length)
{
SubmitForm.addParameter('x.NTPServer1', ntpServerOther1.value)
}
else
{
SubmitForm.addParameter('x.NTPServer1', ntpServer1[ntpServer1.selectedIndex].value);
}
if(ntpServer2.selectedIndex == ntpServers.length+1)
{
SubmitForm.addParameter('x.NTPServer2', ntpServerOther2.value);
}
else
{
if( ntpServer2.selectedIndex > 0)
{
SubmitForm.addParameter('x.NTPServer2', ntpServer2[ntpServer2.selectedIndex].value);
}
else
{
SubmitForm.addParameter('x.NTPServer2', '');
}
}
if(ntpServer3.selectedIndex == ntpServers.length+1)
{
SubmitForm.addParameter('x.NTPServer3', ntpServerOther3.value);
}
else
{
if( ntpServer3.selectedIndex > 0)
{
SubmitForm.addParameter('x.NTPServer3', ntpServer3[ntpServer3.selectedIndex].value);
}
else
{
SubmitForm.addParameter('x.NTPServer3', '');
}
}
if(ntpServer4.selectedIndex == ntpServers.length+1)
{
SubmitForm.addParameter('x.NTPServer4', ntpServerOther4.value);
}
else
{
if( ntpServer4.selectedIndex > 0)
{
SubmitForm.addParameter('x.NTPServer4',ntpServer4[ntpServer4.selectedIndex].value);
}
else
{
SubmitForm.addParameter('x.NTPServer4', '');
}
}
if(ntpServer5.selectedIndex == ntpServers.length+1)
{
SubmitForm.addParameter('x.NTPServer5',ntpServerOther5.value);
}
else
{
if( ntpServer5.selectedIndex > 0)
{
SubmitForm.addParameter('x.NTPServer5',ntpServer5[ntpServer5.selectedIndex].value);
}
else
{
SubmitForm.addParameter('x.NTPServer5', '');
}
}
genlocaltimename();
SubmitForm.addParameter('x.LocalTimeZoneName', localtimename);
SubmitForm.addParameter('x.X_Label', label);
}
else
{
SubmitForm.addParameter('x.Enable',0);
}
SubmitForm.setAction('setcfg.cgi?x=InternetGatewayDevice.Time&RequestFile=/html/ntwkall/sntp.asp');
setDisable('ntpEnabled',1);
setDisable('btnApply',1);
}
}
function CheckForm(type)
{
with( document.forms[0] )
{
if (1 == getCheckVal('ntpEnabled'))
{
if( ntpServer1.selectedIndex == ntpServers.length )
{
if( ntpServerOther1.value.length == 0 )
{
AddErrInfo(eval("gErrStr1"), 'ntpServerOther1');
return;
}
if(false == isValidIPDomain(ntpServerOther1.value,64) && false == isGlobalIpv6Address(ntpServerOther1.value))
{
AddErrInfo(eval("gErrStr1"), 'ntpServerOther1');
return;
}
}
if( ntpServer2.selectedIndex == ntpServers.length+1 )
{
if( ntpServerOther2.value.length == 0 )
{
AddErrInfo(eval("gErrStr2"), 'ntpServerOther2');
return;
}
if(false == isValidIPDomain(ntpServerOther2.value,64) && false == isGlobalIpv6Address(ntpServerOther2.value))
{
AddErrInfo(eval("gErrStr2"), 'ntpServerOther2');
return;
}
}
if( ntpServer3.selectedIndex == ntpServers.length+1 )
{
if( ntpServerOther3.value.length == 0 )
{
AddErrInfo(eval("gErrStr2"), 'ntpServerOther3');
return;
}
if(false == isValidIPDomain(ntpServerOther3.value,64) && false == isGlobalIpv6Address(ntpServerOther3.value))
{
AddErrInfo(eval("gErrStr2"), 'ntpServerOther3');
return;
}
}
if( ntpServer4.selectedIndex == ntpServers.length+1 )
{
if( ntpServerOther4.value.length == 0 )
{
AddErrInfo(eval("gErrStr2"), 'ntpServerOther4');
return;
}
if(false == isValidIPDomain(ntpServerOther4.value,64) && false == isGlobalIpv6Address(ntpServerOther4.value))
{
AddErrInfo(eval("gErrStr2"), 'ntpServerOther4');
return;
}
}
if( ntpServer5.selectedIndex == ntpServers.length+1 )
{
if( ntpServerOther5.value.length == 0 )
{
AddErrInfo(eval("gErrStr2"), 'ntpServerOther5');
return;
}
if(false == isValidIPDomain(ntpServerOther5.value,64) && false == isGlobalIpv6Address(ntpServerOther5.value))
{
AddErrInfo(eval("gErrStr2"), 'ntpServerOther5');
return;
}
}
}
}
}
function clickAction()
{
if(getCheckVal('ntpEnabled') == 0)
{
setDisplay('ntpConfig',0);
Submit();
}
else
{
setDisplay('ntpConfig',1);
}
}
function clickDstAction()
{
if(getCheckVal('dstEnabled') == 0)
{
setDisplay('dstcfgstart',0);
setDisplay('dstcfgend',0);
setDisplay('dstcfgoffset',0);
}
else
{
setDisplay('dstcfgstart',1);
setDisplay('dstcfgend',1);
setDisplay('dstcfgoffset',1);
}
}

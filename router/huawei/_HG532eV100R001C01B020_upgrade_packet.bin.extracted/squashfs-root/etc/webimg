<head>
<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/util.js"></script>
<script language="JavaScript" src="/lang/alg.res"></script>
<title>ALG</title>
<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" type="text/javascript">
function stAlg(domain,L2TPEnable,IPSECEnable,PPTPEnable,H323Enable,RTSPEnable,SIPEnable,SIPPort,SIPWanPath)
{
this.domain = domain;
this.L2TPEnable = L2TPEnable;
this.IPSECEnable = IPSECEnable;
this.PPTPEnable = PPTPEnable;
this.H323Enable = H323Enable;
this.RTSPEnable = RTSPEnable;
this.SIPEnable = SIPEnable;
this.SIPPort = SIPPort;
this.SIPWanPath = SIPWanPath;
}
var algArray = <%webGetCfgEntry(InternetGatewayDevice.Services.X_ALGAbility,L2TPEnable|IPSECEnable|PPTPEnable|H323Enable|RTSPEnable|SIPEnable|SIPPort|SIPWanPath,stAlg);%>;
var alg = algArray[0];
var algArrayAcc = <%webGetCfgEntryAcc(InternetGatewayDevice.Services.X_ALGAbility,L2TPEnable|IPSECEnable|PPTPEnable|H323Enable|RTSPEnable|SIPEnable|SIPPort|SIPWanPath,stAlg);%>;
var algAcc = algArrayAcc[0];
var WanIndex = -1;
function stWanInfo(domain,Name,Enable,NATEnabled,ConnectionType,ServiceList,DMZEnable,DMZIP)
{
this.domain = domain;
this.key = this.domain;
this.Name = Name;
this.ConnectionType = ConnectionType;
this.ServiceList = ServiceList;
this.DMZEnable = DMZEnable;
this.DMZIP = DMZIP;
this.Enable = Enable;
this.NATEnabled = NATEnabled;
this.CntStatus = '';
}
var WanIPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Name|Enable|NATEnabled|ConnectionType|X_ServiceList|X_DMZ.DMZEnable|X_DMZ.DMZHostIPAddress,stWanInfo);%>;
var WanPPPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|Enable|NATEnabled|ConnectionType|X_ServiceList|X_DMZ.DMZEnable|X_DMZ.DMZHostIPAddress,stWanInfo);%>;
function stCntStatus(domain,CntStatus)
{
this.key = domain;
this.CntStatus = CntStatus;
}
var WanIPCntStatusInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},ConnectionStatus,stCntStatus);%>;
var WanPPPCntStatusInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},ConnectionStatus,stCntStatus);%>;
var CntStatusInfo = new Array();
for (i = 0; i < WanIPCntStatusInfo.length-1; i++)
{
CntStatusInfo[i] = WanIPCntStatusInfo[i];
}
for (j = 0; j < WanPPPCntStatusInfo.length-1; j++,i++)
{
CntStatusInfo[i] = WanPPPCntStatusInfo[j];
}
var WanInfo = new Array();
for (i = 0; i < WanIPInfo.length-1; i++)
{
WanInfo[i] = WanIPInfo[i];
}
for (j = 0; j < WanPPPInfo.length-1; j++,i++)
{
WanInfo[i] = WanPPPInfo[j];
}
AssociateParam('WanInfo','CntStatusInfo','CntStatus');
function AddSubmitParam(SubmitForm,type)
{
SubmitForm.addForm('algForm',alg.domain);
SubmitForm.setAction('setcfg.cgi?RequestFile=/html/application/alg.asp');
setDisable('btnApply',1);
}
function sipClick()
{
setDisplay('sipproxyport',getCheckVal('SIPEnable'));
}
function isOptionValid(portValue)
{
if(isPlusInteger(portValue) == false)
{
return false;
}
if(isValidPort(portValue) == false)
{
return false;
}
return true;
}
function CheckForm(type)
{
return CheckAlg();
}
function CheckAlg()
{
if (isOptionValid(getValue('SIPPort')) == false)
{
AddErrInfo(eval("gErrStr4"), '');
return false;
}
if (getCheckVal('SIPEnable') == 1 && WanIndex > -1)
{
if (WanInfo[WanIndex].NATEnabled == 0 || WanInfo[WanIndex].Enable == 0
|| WanInfo[WanIndex].ConnectionType != 'IP_Routed')
{
if (confirm('Noticethe WAN Connection has been invalidthe configure of sip can not take effect after modify SIP configureAre you sure you want to modify configure?') == false)
return false;
}
}
return true;
}
function LoadFrame()
{
var i = GetmenuName("alg.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
setCheck('L2TPEnable',alg.L2TPEnable);
setCheck('IPSECEnable',alg.IPSECEnable);
setCheck('PPTPEnable', alg.PPTPEnable);
setCheck('H323Enable',alg.H323Enable);
setCheck('RTSPEnable',alg.RTSPEnable);
setCheck('SIPEnable',alg.SIPEnable);
setText('SIPPort',alg.SIPPort);
setSelect('SIPWanPath',alg.SIPWanPath);
sipClick();
setDisable('L2TPEnable', TransAcc(algAcc.L2TPEnable));
setDisable('IPSECEnable', TransAcc(algAcc.IPSECEnable));
setDisable('PPTPEnable',  TransAcc(algAcc.PPTPEnable));
setDisable('H323Enable', TransAcc(algAcc.H323Enable));
setDisable('RTSPEnable', TransAcc(algAcc.RTSPEnable));
setDisable('SIPEnable', TransAcc(algAcc.SIPEnable));
setDisable('SIPPort', TransAcc(algAcc.SIPPort));
setDisable('SIPWanPath', TransAcc(algAcc.SIPWanPath));
}

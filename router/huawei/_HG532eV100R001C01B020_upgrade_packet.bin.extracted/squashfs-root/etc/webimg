function VerifyFile(FileObject)
{
var FileName = FileObject.value;
if (FileName.length == 0)
{
AddErrInfo(eval("gErrStr0"), '');
setDisplay('erroinfo', 1);
setErrInfo();
return false;
}
if (FileName.length > 128)
{
AddErrInfo(eval("gErrStr1"), '');
setDisplay('erroinfo', 1);
setErrInfo();
return false;
}
return true;
}
function uploadImage(FileObject, btnSubmitObject)
{
if ('' != top.errStrStr)
{
ClearErro();
}
top.errStrStr = '';
top.errIdCtrlStr = '';
if (VerifyFile(FileObject) == false)
{
return false;
}
btnSubmitObject.disabled = true;
top.previousPage = '/html/management/firmware.asp';
top.g_url = '';
return true;
}

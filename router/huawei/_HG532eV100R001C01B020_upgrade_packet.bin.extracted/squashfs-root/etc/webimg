var index=-1;
function clickLogout()
{
var form = new webSubmitForm();
form.setAction('/index/logout.cgi');
form.submit();
}
function moveOnLogout()
{
}
function clickWizard()
{
var menuItem = top.g_oMenuTree.getMenuByName(eval('Wizard'));
g_firstmenu = menuItem.id;  
g_secmenu = g_firstmenu + '_0';
top.g_secondmenu = g_secmenu;
g_thirdmenu = g_secmenu + '_0';
top.g_thirdmenu = g_thirdmenu;
refreshMenu(g_firstmenu,g_secmenu,g_thirdmenu);
top.menufrm.expandMenuById(menuItem.id);
}
function clickHelp()
{
var menuItem = top.g_oMenuTree.getMenuByName(eval('Help'));
g_firstmenu = menuItem.id; 
g_secmenu = g_firstmenu + '_0'; 
top.g_secondmenu = g_secmenu;
g_thirdmenu = g_secmenu + '_0'; 
top.g_thirdmenu = g_thirdmenu;
refreshMenu(g_firstmenu,g_secmenu,g_thirdmenu);
top.menufrm.expandMenuById(menuItem.id);
top.Link = '';
}
function closeBrowser(e)
{
}
function mouseOverLink(item)
{
item.style.color="#990000";
}
function mouseDownLink(item)
{
item.style.color="#990000";
}
function mouseUpLink(item)
{
item.style.color = '#000000';
}
function mouseOutLink(item)
{
item.style.color = '#000000';
}
function refreshMenu(firmenu,secmenu,thirdmenu)
{
var cookie = "FirstMenu=" + firmenu + "; path=/";
document.cookie = cookie;
var cookie = "SecondMenu=" + secmenu + "; path=/";
document.cookie = cookie;
var cookie = "ThirdMenu=" + thirdmenu + "; path=/";
document.cookie = cookie;
var xmlhttp = CreateXMLHttp();
}
function get_cookie (cookie_name)
{
var results = document.cookie.match ( '(^|;) ?' + cookie_name + '=([^;]*)(;|$)' );
if (results)
{
return (results[2]);
}
else
{
return "";
}
}

<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" src="/js/util.js"></script>
<script language="JavaScript" type="text/javascript">
var selIndex = 0;
var i = 0;
var j = 0;
function stRip(domain,Name,Operation,Proto,ConnectionType,ServiceList, NATEnabled)
{
this.domain = domain;
this.key = domain.substr(domain.indexOf('WANDevice'));
this.Name = Name;
this.Operation = Operation;
this.Protocol = Proto;
this.ConnectionType = ConnectionType;
this.ServiceList = ServiceList;
this.NATEnabled = NATEnabled;
}
function stRipLan(domain, Operation,Protocol)
{
this.domain = domain;
this.key = domain.substr(domain.indexOf('LANDevice'));
this.Operation = Operation;
this.Protocol = Protocol;
}
var WanIPRip = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Name|X_RIPOperation|RouteProtocolRx|ConnectionType|X_ServiceList|NATEnabled,stRip);%>;
var WanPPPRip =<%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|X_RIPOperation|RouteProtocolRx|ConnectionType|X_ServiceList|NATEnabled, stRip);%>;
var LanIPRip = <%webGetCfgEntry(InternetGatewayDevice.LANDevice.{i}.LANHostConfigManagement.IPInterface.{i},X_RIPOperation|X_RouteProtocolRx, stRipLan);%>;
var Rip = new Array();
for (i = 0; i < WanIPRip.length-1; i++)
{
Rip[i] = WanIPRip[i];
}
for (j = 0; j < WanPPPRip.length-1; i++,j++)
{
Rip[i] =  WanPPPRip[j];
}
function getLanDevName(domain)
{
for (var i= 0; i < LanIPRip.length - 1; i++)
{
if (LanIPRip[i].domain == domain)
{
return LanIPRip[i].X_RipLanBrName;
}
}
}
function ShowIfName(val,arrMsgInfo)
{
for (i = 0; i < Rip.length; i++)
{
if (Rip[i].domain == val)
{
return Rip[i].Name;
}
else if ('br0' == val)
{
return 'br0';
}
}
return '&nbsp;';
}
function LoadFrame()
{
var i = GetmenuName("rip.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
for (var i = 0; i < Rip.length; i++)
{
if(null != getElement('record_'+i))
{
selectLine('record_'+i);
break;
}
}
}
}
function ShowProtocol(Version)
{
if (Version == "RIPv1")
{
return eval("gVarVerOne");
}
else if (Version == "RIPv2")
{
return eval("gVarVerTwo");
}
else if (Version == "RIPv1/v2")
{
return eval("gVarBoth");
}
else if (Version == "Off")
{
return eval("gVarOff");
}
else
{
return "";
}
}
function ShowOperation(OptValue)
{
if (OptValue == "Active")
{
return eval("gVaractive");
}
else if (OptValue == "Passive")
{
return eval("gVarpassive");
}
else
{
return "";
}
}
function CheckForm()
{
if (getElement('sel_Interface').length == 0)
{
AddErrInfo(eval("gErrStr0"), '');
return false;
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
top.aplyLine = selIndex;
var domain = getSelectVal('sel_Interface');
if (domain.indexOf('LANDevice') > -1)
{
SubmitForm.addParameter('x.X_RouteProtocolRx', getSelectVal('RouteProtocolRx')) ;
}
else
{
SubmitForm.addParameter('x.RouteProtocolRx', getSelectVal('RouteProtocolRx')) ;
}
SubmitForm.addParameter('x.X_RIPOperation', getSelectVal('X_RIPOperation')) ;
SubmitForm.setAction('setcfg.cgi?x=' + domain + '&RequestFile=/html/network/rip.asp');
setDisable('btnApply1',1);
}
function ChangeInterface(Interface)
{
var ripInfo = Rip[Interface.selectedIndex];
setSelect('X_RIPOperation',ripInfo.Operation);
setSelect('RouteProtocolRx',ripInfo.Protocol);
top.ripIndex = Interface.selectedIndex;
}
function setCtlDisplay(record)
{
setSelect('sel_Interface',record.domain);
setSelect('X_RIPOperation',record.Operation);
setSelect('RouteProtocolRx',record.Protocol);
}
function setControl(index)
{
var record;
selIndex = index;
if (index == -1)
{
record = new stRip('','','','');
}
else
{
if (index < Rip.length)
{
record = Rip[index];
}
else
{
var lanRipIndex = (index - Rip.length);
record = LanIPRip[lanRipIndex];
}
}
setCtlDisplay(record);
}

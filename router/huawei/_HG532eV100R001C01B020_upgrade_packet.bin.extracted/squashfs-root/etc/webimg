break;
case 'advance_Link_6':
getElement('Advance_QoSGlobal_div').style.display = "";
getElement('Advance_QoSClass_div').style.display = "";
break;
case 'advance_Link_7_FTP':
getElement('Advance_FTP_div').style.display = "";
break;
case 'advance_Link_7_FTPRecord':
getElement('Advance_Rec_div').style.display = "";
break;
case 'advance_Link_7_Samba':
getElement('Advance_Samba_div').style.display = "";
break;
case 'advance_Link_7' :
getElement('Advance_FTP_div').style.display = "";
getElement('Advance_Rec_div').style.display = "";
getElement('Advance_Samba_div').style.display = "";
break;
case 'advance_Link_8' :

<head>
<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/util.js"></script>
<script language="JavaScript" src="/lang/radvd.res"></script>
<title>RADVD Configure</title>
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" type="text/javascript">
function radvdcnst(domain,Enable,AllocateWan,AssConn,Prefix,PrefixLength,Preferredlifetime,Validlifetime,ULAAllocatedMode,ULAPrefix,ULAPrefixLength,ULAPreferredlifetime,ULAValidlifetime)
{
this.domain            = domain;
this.allocateWan       = AllocateWan;
this.AssConn           = AssConn;
this.Prefix            = Prefix;
this.PrefixLength      = PrefixLength;
this.Enable            = Enable;
this.Preferredlifetime = Preferredlifetime;
this.Validlifetime     = Validlifetime;
this.ULAAllocatedMode   = ULAAllocatedMode;
this.ULAPrefix          = ULAPrefix;
this.ULAPrefixLength    = ULAPrefixLength;
this.ULAPreferredlifetime = ULAPreferredlifetime;
this.ULAValidlifetime   = ULAValidlifetime;
}
var radvdst = <%webGetCfgEntry(InternetGatewayDevice.LANDevice.{i}.LANHostConfigManagement.X_SLAAC,RouterAdvertisementEnable|UseAllocatedWAN|AssociatedConnection|Prefix|PrefixLength|Preferredlifetime|Validlifetime|ULAAllocatedMode|ULAPrefix|ULAPrefixLength|ULAPreferredlifetime|ULAValidlifetime,radvdcnst);%>;
var radvdEnbl = radvdst[0].Enable;
var UseAllc  = radvdst[0].allocateWan;
var AssCon   = radvdst[0].AssConn;
var Prefix   = radvdst[0].Prefix;
var Prelen   = radvdst[0].PrefixLength;
var Time1    = radvdst[0].Preferredlifetime;
var Time2    = radvdst[0].Validlifetime;
var UseULAMd = radvdst[0].ULAAllocatedMode;
var ULAPrefix = radvdst[0].ULAPrefix;
var ULAPrelen = radvdst[0].ULAPrefixLength;
var ULATime1 = radvdst[0].ULAPreferredlifetime;
var ULATime2 = radvdst[0].ULAValidlifetime;
var changeAddrFlag = false;
function WanIPConstruction(domain, xIpv6Addr,xIpv6PrefixLen)
{
this.domain            = domain;
this.xIpv6Addr       = xIpv6Addr;
this.xIpv6PrefixLen = xIpv6PrefixLen;
}
var WANIP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},X_IPv6Address|X_IPv6PrefixLength,WanIPConstruction);%>;
function WanPPPConstruction(domain, xIpv6Addr,xIpv6PrefixLen)
{
this.domain            = domain;
this.xIpv6Addr       = xIpv6Addr;
this.xIpv6PrefixLen = xIpv6PrefixLen;
}
var WANPPP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i}, X_IPv6Address|X_IPv6PrefixLength,WanPPPConstruction);%>;
var WAN = Array();
for (var i = 0; i < WANIP.length-1; i++)
{
WAN[i] = WANIP[i];
WAN[i].index = i;
}
for (var j = 0; j < WANPPP.length-1; j++,i++)
{
WAN[i] = WANPPP[j];
WAN[i].index = i;
}
function disableDhcpSrv()
{
with ( document.forms[0] )
{
setCheck('radvdSrvType' ,0);
setDisplay('AssConnection', 0);
setDisplay('RadvdPrefixCfg',0);
setDisplay('ULAConnection', 0);
setDisplay('ULAPrefixCfg',0);
}
}
function enableDhcpSrv(formLoad)
{
with ( document.forms[0] )
{
setCheck('radvdSrvType',1);
setDisplay('AssConnection', 1);
setDisplay('ULAConnection', 1);
if ( "Normal" == UseWan.value )
{
setDisplay('RadvdPrefixCfg', 1);
}
if ( "Manual" == UseULA.value )
{
setDisplay('ULAPrefixCfg', 1);
}
}
}
function UseWanChange()
{
with ( document.forms[0] )
{
if ( "Normal" == UseWan.value && 1 == getCheckVal('radvdSrvType') )
{
setDisplay('RadvdPrefixCfg', 1);
}
else
{
setDisplay('RadvdPrefixCfg', 0);
}
}
}
function UseULAModeChange()
{
with ( document.forms[0] )
{
if ( "Manual" == UseULA.value && 1 == getCheckVal('radvdSrvType') )
{
setDisplay('ULAPrefixCfg', 1);
}
else
{
setDisplay('ULAPrefixCfg', 0);
}
}
}
function typeClick()
{
with ( document.forms[0] )
{
if (getCheckVal('radvdSrvType') == 0)
{
disableDhcpSrv();
}
else
{
enableDhcpSrv(1);
}
}
}
function LoadFrame()
{
var i        = GetmenuName("radvd.asp");
var menuName = UpdateMenuByFileName(i);
var menuItem = top.g_oMenuTree.curMenuItem;
var cookie   = document.cookie;
ReLocation(cookie,menuName);
with ( document.forms[0] )
{
if ( 1 == radvdEnbl )
{
enableDhcpSrv(1);
}
else
{
setCheck("radvdSrvType",0);
disableDhcpSrv();
}
UseWan.value = UseAllc;
UseWanChange();
radvdPrefix.value = Prefix;
PrefixLen.value  = Prelen;
t1.value = Time1;
t2.value = Time2;
UseULA.value = UseULAMd;
UseULAModeChange();
ulaPrefix.value = ULAPrefix;
ulaPrefixLen.value = ULAPrelen;
ulat1.value = ULATime1;
ulat2.value = ULATime2;
}
}
function CheckPrefix(Prefix,type)
{
var IpAddress1 = Prefix.split(':');
var IpAddress3 = Prefix;
if ( 1 == type && '' == IpAddress3)
{
return true;
}
if ( true != isGlobalIpv6Address(IpAddress3) )
{
return false;
}
return true;
}
function isValidFDPrefix(address)
{
var tempAddress = address.toLowerCase();
if (tempAddress.substring(0, 2) != 'fd')
{
return false;
}
return true;
}
function CheckLength(Length)
{
var TemLen = parseInt(Length);
if ( true == isNaN(TemLen) || TemLen > 64 || TemLen < 16)
{
return false;
}
return true;
}
function CheckHex(Hex)
{
var TemHex  = Hex;
var Loopj   = 0;
var Tempi   = 0;
var Tempj   = 1;
var section = 0;
var num     = 0;
for ( Loopj = 0 ; Loopj < TemHex.length ; Loopj++ )
{
section = TemHex.substring(Tempi, Tempj);
num = parseInt(section,16);
if ( true == isNaN(num) )
{
return false;
}
Tempi++;
Tempj++;
}
return true;
}
function CheckTime(Time1,Time2)
{
var TemTime1 = Time1;
var TemTime2 = Time2;
if ( TemTime1.length > 10 || '' == TemTime1 )
{
return 1;
}
if ( TemTime2.length > 10 || '' == TemTime2 )
{
return 2;
}
if ( true != isPlusInteger(TemTime1))
{
return 1;
}
if ( true != isPlusInteger(TemTime2))
{
return 2;
}
TemTime1 = parseInt(Time1);
TemTime2 = parseInt(Time2);
if ( TemTime1 > 4294967295 || TemTime1 < 600 )
{
return 1;
}
if ( TemTime2 > 4294967295 || TemTime2 < 600 )
{
return 2;
}
if ( TemTime2 < TemTime1 )
{
return 3;
}
return true;
}
function CheckForm(type)
{
with ( document.forms[0] )
{
switch(type)
{
case 0:
{
if ( 1 != getCheckVal('radvdSrvType') ||( "Normal" != UseWan.value && "Manual" != UseULA.value))
{
return true;
}
if( "Normal" == UseWan.value)
{
if ( true != CheckPrefix(radvdPrefix.value,0) )
{
AddErrInfo(eval("gErrStr0") + getSafeString(radvdPrefix.value) + eval("gErrStr1"), 'radvdPrefix');
return false;
}
if ( true != CheckLength(PrefixLen.value) )
{
AddErrInfo(eval("gErrStr2") + getSafeString(PrefixLen.value) + eval("gErrStr3"), 'PrefixLen');
return false;
}
var maxlen = 0;
for( i =0;i < WAN.length; i++)
{
if ( radvdst.PrefixLength >= WAN[i].xIpv6PrefixLen )
{
maxlen =  radvdst.PrefixLength;
}
else
{
maxlen =  WAN[i].xIpv6PrefixLen;
}
if (true == CompareTwoAddress(radvdPrefix.value, maxlen, WAN[i].xIpv6Addr, maxlen))
{
AddErrInfo("the prefix can not be same with wan", 'radvdPrefix');
return false;
}
}
switch ( CheckTime(t1.value,t2.value) )
{
case 1 :
{
AddErrInfo(eval("gErrStr4") + getSafeString(t1.value) + eval("gErrStr5"), 't1');
return false;
}
case 2 :
{
AddErrInfo(eval("gErrStr4") + getSafeString(t2.value) + eval("gErrStr5"), 't2');
return false;
}
case 3 :
{
AddErrInfo(eval("gErrStr6") + getSafeString(t2.value) + eval("gErrStr7") + eval("gErrStr8") + t1.value, 't2');
return false;
}
default:
break;
}
}
if ( "Manual" == UseULA.value )
{
if ( true != CheckPrefix(ulaPrefix.value,0) )
{
AddErrInfo(eval("gErrStr10") + getSafeString(ulaPrefix.value) + eval("gErrStr11"), 'ulaPrefix');
return false;
}
if ( true != isValidFDPrefix(ulaPrefix.value) )
{
AddErrInfo(eval("gErrStr10") + getSafeString(ulaPrefix.value) + eval("gErrStr19"), 'ulaPrefix');
return false;
}
if ( true != CheckLength(ulaPrefixLen.value) )
{
AddErrInfo(eval("gErrStr12") + getSafeString(ulaPrefixLen.value) + eval("gErrStr13"), 'ulaPrefixLen');
return false;
}
switch ( CheckTime(ulat1.value,ulat2.value) )
{
case 1 :
{
AddErrInfo(eval("gErrStr14") + getSafeString(ulat1.value) + eval("gErrStr15"), 'ulat1');
return false;
}
case 2 :
{
AddErrInfo(eval("gErrStr14") + getSafeString(ulat2.value) + eval("gErrStr15"), 'ulat2');
return false;
}
case 3 :
{
AddErrInfo(eval("gErrStr16") + getSafeString(ulat2.value) + eval("gErrStr17") + eval("gErrStr18") + ulat1.value, 'ulat2');
return false;
}
default:
break;
}
}
break;
}
case 1:
{
}
break;
case 2:
{
}
break;
default:
break;
}
}
return true;
}
function AddSubmitParam(Form,type)
{
with (document.forms[0])
{
switch(type)
{
case 0:
{
Form.addParameter('y.RouterAdvertisementEnable',getCheckVal('radvdSrvType'));
if ( 1 == getCheckVal('radvdSrvType') )
{
Form.addParameter('y.UseAllocatedWAN',getValue('UseWan'));
Form.addParameter('y.ULAAllocatedMode',getValue('UseULA'));
}
if ( 1 == getCheckVal('radvdSrvType') && "Normal" == UseWan.value )
{
Form.addParameter('y.Prefix',getValue('radvdPrefix'));
Form.addParameter('y.PrefixLength',getValue('PrefixLen'));
Form.addParameter('y.Preferredlifetime',getValue('t1'));
Form.addParameter('y.Validlifetime',getValue('t2'));
}
if ( 1 == getCheckVal('radvdSrvType') && "Manual" == UseULA.value )
{
Form.addParameter('y.ULAPrefix',getValue('ulaPrefix'));
Form.addParameter('y.ULAPrefixLength',getValue('ulaPrefixLen'));
Form.addParameter('y.ULAPreferredlifetime',getValue('ulat1'));
Form.addParameter('y.ULAValidlifetime',getValue('ulat2'));
}
var RequestFile = '/html/ntwkall/radvd.asp';
var url = 'setcfg.cgi?'
+ 'y=InternetGatewayDevice.LANDevice.1.LANHostConfigManagement.X_SLAAC'
+ '&RequestFile=' + RequestFile;
Form.setAction(url);
break;
}
case 1:
break;
case 2:
break;
default:
return;
}
}
}

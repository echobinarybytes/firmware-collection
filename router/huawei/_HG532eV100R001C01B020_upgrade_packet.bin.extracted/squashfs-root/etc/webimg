<script language="JavaScript" type="text/javascript">
var jump = 0;
for (i = 0; i < IpFltCurrList.length && null != IpFltCurrList[i]; i++)
{
jump = 0;
for (j = 0; j < AllDropOrAcceptArray.length; j++)
{
if (i == AllDropOrAcceptArray[j].Index)
{
jump = 1;
break;
}
}
if (1 == jump)
{
continue;
}
jump = 0;
for (j = 0; j < InterfaceArray.length; j++)
{
if (i == InterfaceArray[j].Index)
{
jump = 1;
break;
}
}
if (1 == jump)
{
continue;
}
if (DisplayNumOneIndex == -1)
{
DisplayNumOneIndex = i;
}
ShowIpFltInfo(i,1);
}
for (i = 0; i < InterfaceArray.length; i++)
{
if (DisplayNumOneIndex == -1)
{
DisplayNumOneIndex = InterfaceArray[i].Index;
}
ShowIpFltInfo(InterfaceArray[i].Index,0);
}
for (i = 0; i < AllDropOrAcceptArray.length; i++)
{
if (DisplayNumOneIndex == -1)
{
DisplayNumOneIndex = AllDropOrAcceptArray[i].Index;
}
ShowIpFltInfo(AllDropOrAcceptArray[i].Index,0);
}
</script>

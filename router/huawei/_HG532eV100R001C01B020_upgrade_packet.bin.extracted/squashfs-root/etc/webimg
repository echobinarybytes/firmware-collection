<head>
<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/util.js"></script>
<script language="JavaScript" src="/lang/multinat.res"></script>
<title>Multi-NAT</title>
<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" type="text/javascript">
var WanIndex = -1;
var selctIndex = -1;
function stWanInfo(domain,Name,Enable,NATEnabled,ConnectionType,ServiceList)
{
this.domain = domain;
this.key = this.domain;
this.Name = Name;
this.ConnectionType = ConnectionType;
this.ServiceList = ServiceList;
this.Enable = Enable;
this.NATEnabled = NATEnabled;
this.CntStatus = '';
}
var WanIPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Name|Enable|NATEnabled|ConnectionType|X_ServiceList,stWanInfo);%>;
var WanPPPInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|Enable|NATEnabled|ConnectionType|X_ServiceList,stWanInfo);%>;
function stCntStatus(domain,CntStatus)
{
this.key = domain;
this.CntStatus = CntStatus;
}
var WanIPCntStatusInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},ConnectionStatus,stCntStatus);%>;
var WanPPPCntStatusInfo = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},ConnectionStatus,stCntStatus);%>;
var CntStatusInfo = new Array();
for (i = 0; i < WanIPCntStatusInfo.length-1; i++)
{
CntStatusInfo[i] = WanIPCntStatusInfo[i];
}
for (j = 0; j < WanPPPCntStatusInfo.length-1; j++,i++)
{
CntStatusInfo[i] = WanPPPCntStatusInfo[j];
}
var WanInfo = new Array();
for (i = 0; i < WanIPInfo.length-1; i++)
{
WanInfo[i] = WanIPInfo[i];
}
for (j = 0; j < WanPPPInfo.length-1; j++,i++)
{
WanInfo[i] = WanPPPInfo[j];
}
AssociateParam('WanInfo','CntStatusInfo','CntStatus');
function stMultiNat(domain,Enable,Type,LanStartIP,LanEndIP,WanStartIP,WanEndIP)
{
this.domain = domain;
this.key = this.domain;
this.Enable = Enable;
this.Type = Type;
this.LanStartIP = LanStartIP;
this.LanEndIP = LanEndIP;
this.WanStartIP = WanStartIP;
this.WanEndIP = WanEndIP;
var index = domain.lastIndexOf('MultiNat');
this.Interface = domain.substr(0,index - 1);
}
var WanIPMultiNat = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i}.MultiNat.{i},Enabled|Type|LocalStartIP|LocalEndIP|PublicStartIP|PublicEndIP,stMultiNat);%>;
var WanPPPMultiNat = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i}.MultiNat.{i},Enabled|Type|LocalStartIP|LocalEndIP|PublicStartIP|PublicEndIP,stMultiNat);%>;
var MultiNat = new Array();
for (i = 0; i < WanIPMultiNat.length-1; i++)
{
MultiNat[i] = WanIPMultiNat[i];
}
for (j = 0; j < WanPPPMultiNat.length-1; j++,i++)
{
MultiNat[i] = WanPPPMultiNat[j];
}
if (MultiNat.length > 0)
{
AssociateParam('MultiNat','WanInfo','NATEnabled|Enable|id|Name');
}
function AddSubmitParam(SubmitForm,type)
{
var Interface = getSelectVal('MultiNatInterface');
setDisable('MultiNatInterface',1);
SubmitForm.addParameter('x.Enabled', '1');
SubmitForm.addParameter('x.Type', getSelectVal('MultiNatType'));
SubmitForm.addParameter('x.LocalStartIP', getValue('LocalStartIP'));
SubmitForm.addParameter('x.LocalEndIP', getValue('LocalEndIP'));
SubmitForm.addParameter('x.PublicStartIP', getValue('PublicStartIP'));
SubmitForm.addParameter('x.PublicEndIP', getValue('PublicEndIP'));
if( selctIndex == -1 )
{
SubmitForm.setAction('addcfg.cgi?x=' + Interface + '.MultiNat'
+'&RequestFile=/html/application/multinat.asp');
}
else
{
SubmitForm.setAction('setcfg.cgi?x=' + MultiNat[selctIndex].domain
+'&RequestFile=/html/application/multinat.asp');
}
top.aplyLine = selctIndex;
setDisable('btnApply',1);
}
function CheckForm(type)
{
return CheckMultiNat();
}
function GetNumberOfIpAddr(address)
{
var addrParts = address.split('.');
return addrParts[0]*256*256*256 + addrParts[1]*256*256 + addrParts[2]*256 + addrParts[3];
}
function CheckMultiNat()
{
if(MultiNat.length >= 16)
{
AddErrInfo(eval("gErrStr0"), '')
return false;
}
if (getElement('MultiNatInterface').length == 0)
{
AddErrInfo(eval("gErrStr1"), 'MultiNatInterface');
return false;
}
with (getElById('MultiNatForm'))
{
var type = getSelectVal('MultiNatType');
if (type == 1) 
{
if (isValidIpAddress(LocalStartIP.value) == false)
{
AddErrInfo(eval("gErrStr2"), 'LocalStartIP');
return false;
}
if (isValidIpAddress(PublicStartIP.value) == false)
{
AddErrInfo(eval("gErrStr3"), 'PublicStartIP');
return false;
}
}
else if (type == 2) 
{
if (isValidIpAddress(LocalStartIP.value) == false)
{
AddErrInfo(eval("gErrStr2"), 'LocalStartIP');
return false;
}
if (isValidIpAddress(LocalEndIP.value) == false)
{
AddErrInfo(eval("gErrStr4"), 'LocalEndIP');
return false;
}
if (isValidIpAddress(PublicStartIP.value) == false)
{
AddErrInfo(eval("gErrStr3"), 'PublicStartIP');
return false;
}
}
else if (type == 3) 
{
if (isValidIpAddress(LocalStartIP.value) == false)
{
AddErrInfo(eval("gErrStr2"), 'LocalStartIP');
return false;
}
if (isValidIpAddress(LocalEndIP.value) == false)
{
AddErrInfo(eval("gErrStr4"), 'LocalEndIP');
return false;
}
if (isValidIpAddress(PublicStartIP.value) == false)
{
AddErrInfo(eval("gErrStr3"), 'PublicStartIP');
return false;
}
if (isValidIpAddress(PublicEndIP.value) == false)
{
AddErrInfo(eval("gErrStr5"), 'PublicEndIP');
return false;
}
var iLocalStart = GetNumberOfIpAddr(LocalStartIP.value);
var iLocalEnd = GetNumberOfIpAddr(LocalEndIP.value);
var iWanStart = GetNumberOfIpAddr(PublicStartIP.value);
var iWanEnd = GetNumberOfIpAddr(PublicEndIP.value);
if((iWanEnd - iWanStart) != (iLocalEnd - iLocalStart))
{
AddErrInfo(eval("gErrStr5"), 'PublicEndIP');
return false;
}
}
else if (type == 5) 
{
if (isValidIpAddress(PublicStartIP.value) == false)
{
AddErrInfo(eval("gErrStr3"), 'PublicStartIP');
return false;
}
}
else 
{
if (isValidIpAddress(LocalStartIP.value) == false)
{
AddErrInfo(eval("gErrStr2"), 'LocalStartIP');
return false;
}
if (isValidIpAddress(LocalEndIP.value) == false)
{
AddErrInfo(eval("gErrStr4"), 'LocalEndIP');
return false;
}
if (isValidIpAddress(PublicStartIP.value) == false)
{
AddErrInfo(eval("gErrStr3"), 'PublicStartIP');
return false;
}
if (isValidIpAddress(PublicEndIP.value) == false)
{
AddErrInfo(eval("gErrStr5"), 'PublicEndIP');
return false;
}
}
if (type == 2 || type == 4 || type == 3)
{
if (IpAddress2DecNum(getElement('LocalStartIP').value) > IpAddress2DecNum(getElement('LocalEndIP').value))
{
AddErrInfo(eval("gErrStr6"), 'LocalStartIP');
return false;
}
}
if (type == 3 || type == 4)
{
if (IpAddress2DecNum(getElement('PublicStartIP').value) > IpAddress2DecNum(getElement('PublicEndIP').value))
{
AddErrInfo(eval("gErrStr7"), 'PublicStartIP');
return false;
}
}
}
return true;
}
function LoadFrame()
{
var i = GetmenuName("multinat.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_0');
}
if (MultiNat.length > 0)
{
setControl(0);
}
else
{
setControl(-1);
}
}
function ShowMultiNat()
{
}
function selectChangedType()
{
with (getElById('MultiNatForm'))
{
var type = getSelectVal('MultiNatType');
if (type == 1) 
{
setDisplay('IDLocalTR',1 );
setText('LocalEndIP','');
setText('PublicEndIP','');
setDisplay('localend1',0 );
setDisplay('localend3',0 );
setDisplay('publicend1',0 );
setDisplay('publicend3',0 );
}
else if (type == 2) 
{
setDisplay('IDLocalTR',1 );
setText('PublicEndIP','');
setDisplay('localend1',1 );
setDisplay('localend3',1 );
setDisplay('publicend1',0 );
setDisplay('publicend3',0 );
}
else if (type == 3) 
{
setDisplay('IDLocalTR',1 );
setDisplay('localend1',1 );
setDisplay('localend3',1 );
setDisplay('publicend1',1 );
setDisplay('publicend3',1 );
}
else if (type == 5) 
{
setDisplay('IDLocalTR',0 );
setText('LocalStartIP','');
setText('LocalEndIP','');
setText('PublicEndIP','');
setDisplay('publicend1',0 );
setDisplay('publicend3',0 );
}
else 
{
setDisplay('IDLocalTR',1 );
setDisplay('localend1',1 );
setDisplay('localend3',1 );
setDisplay('publicend1',1 );
setDisplay('publicend3',1 );
}
}
}
function ShowMultiNatAll()
{
var html = '';
var mnattype = '';
for (i = 0; i < MultiNat.length; i++)
{
for (var j = 0; j < WanInfo.length; j++)
{
if (WanInfo[j].Name == MultiNat[i].Name)
{
break;
}
}
if(WanInfo.length == j)
{
break;
}
if ( (WanInfo[j].ConnectionType != 'IP_Routed') || (WanInfo[j].ServiceList == 'TR069') )
{
continue;
}
{
html += '<TR id=record_' + i
+' class="trTabContent" onclick="selectLine(this.id);">'
html += '<TD align="center">' + MultiNat[i].Name + '&nbsp;</TD>';
if (MultiNat[i].Type == 1)
{
mnattype = eval("gVarType1");
}
else if (MultiNat[i].Type == 2)
{
mnattype = eval("gVarType2");
}
else if (MultiNat[i].Type == 3)
{
mnattype = eval("gVarType3");
}
else if (MultiNat[i].Type == 5)
{
mnattype = eval("gVarType5");
}
else
{
mnattype = eval("gVarType4");
}
html += '<TD align="center">' + mnattype + '&nbsp;</TD>';
html += '<TD align="center">' + MultiNat[i].LanStartIP + '&nbsp;</TD>';
html += '<TD align="center">' + MultiNat[i].LanEndIP + '&nbsp;</TD>';
html += '<TD align="center">' + MultiNat[i].WanStartIP + '&nbsp;</TD>';
html += '<TD align="center">' + MultiNat[i].WanEndIP + '&nbsp;</TD>';
if (MultiNat[i].Enable == 1)
{
html += '<TD align="center">' + eval("gVarEnable") + '&nbsp;</TD>';
}
else
{
html += '<TD align="center">' + eval("gVarDisable") + '&nbsp;</TD>';
}
html += '<TD align="center"><input type="checkbox" name="rml" value="'
+ MultiNat[i].domain + '"></TD>';
html += '</TR>';
}
}
document.write(html);
}
function clearAll()
{
}
function setCtlDisplay(record)
{
var endIndex = record.domain.lastIndexOf('MultiNat') - 1;
var Interface = record.domain.substring(0, endIndex);
setSelect('MultiNatInterface', Interface);
setSelect('MultiNatType',record.Type);
setText('LocalStartIP',record.LanStartIP);
setText('LocalEndIP',record.LanEndIP);
setText('PublicStartIP',record.WanStartIP);
setText('PublicEndIP',record.WanEndIP);
selectChangedType();
}
function setControl(index)
{
var record;
selctIndex = index;
if (index == -1)
{
record = new stMultiNat('','','','','','','');
setDisable('MultiNatInterface',0);
}
else
{
record = MultiNat[index];
setDisable('MultiNatInterface',1);
}
setCtlDisplay(record);
}
function clickRemove()
{
removeInst('/html/application/multinat.asp');
}

<html>
<head>
<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/util.js"></script>
<script language="JavaScript" src="/lang/ptm.res"></script>
<title>Internet Connection</title>
<script language="JavaScript" src="/js/tabinfo.js"></script>
<style type="text/css">
.firstCol
{
width:24%;
text-align:right ;
}
.alignledge
{
width:3%;
}
</style>
<script language="JavaScript" type="text/javascript">
function PtmConstruction(domain, ptmPortId, ptmPriorityLow, ptmPriorityHigh, vlanId, priority)
{
this.domain = domain;
this.ptmId = this.domain;
var list = domain.split('.');
this.key = '.' + list[4] + '.';
this.WdDomain = list[0] + '.' + list[1] + '.' + list[2];
this.ptmPortId = ptmPortId;
this.ptmPriorityLow = ptmPriorityLow;
this.ptmPriorityHigh = ptmPriorityHigh;
this.ptmIfName = '';
this.vlanId    = vlanId;
this.priority  = priority;
}
var WANLINK = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPTMLinkConfig,X_PortId|X_PTMPriorityLow|X_PTMPriorityHigh|X_VLANIDMark|X_802-1pMark, PtmConstruction);%>;
function WanConnDevConstruction(Domain, Name)
{
this.Domain = Domain;
this.Name = Name;
}
var WANCONNDEV = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}, Name, WanConnDevConstruction);%>;
var WD = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANCommonInterfaceConfig, WANAccessType, WanWdConstruction);%>;
function WanWdConstruction(Domain, AccessType)
{
this.WdDomain = Domain;
var list = Domain.split('.');
this.WdInstId = list[2];
this.AccessType = AccessType;
}
function GetWdDomain()
{
for (var i = 0; i < WD.length - 1; i++)
{
if (WD[i].AccessType == 'VDSL')
{
return 'InternetGatewayDevice.WANDevice.' + WD[i].WdInstId;
}
}
return 'InternetGatewayDevice.WANDevice.4';
}
var CntPtm = Array();
for (var i = 0, j = 0; i < WANLINK.length-1; i++)
{
if (GetWdDomain() == WANLINK[i].WdDomain)
{
CntPtm[j] = WANLINK[i];
CntPtm[j].ptmIfName = WANCONNDEV[i].Name;
j++;
}
}
CntPtm[j] = WANLINK[i];
var AddFlag = false;
var SelPTMIndex = -1;
function LoadFrame()
{
var i = GetmenuName("ptm.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
with (getElement('ConfigForm'))
{
ptmId.selectedIndex = 0;
if (CntPtm.length > 1)
{
selectLine('record_0');
}
else
{
btnAddPTMCnt();
}
}
}
function btnAddPTMCnt()
{
AddFlag = true;
with (getElement('ConfigForm'))
{
AddOption(ptmId, -1, 'New PTM', true);
setCheck('path0', '1');
setCheck('path1', '0');
setCheck('priorityNorm', "1");
setCheck('priorityHigh', "0");
setDisplay('seclEnableVlan',1);
setDisplay('secVlan',0);
setDisplay('sec8021p',0);
setText('vlan',-1);
setText('v8021p',-1);
setCheck('enbl8021q', '0');
SelPTMIndex = -1;
}
}
function Enbl8021qChange()
{
with (getElement('ConfigForm'))
{
if (enbl8021q.checked == true)
{
setDisplay('secVlan',1); 
setDisplay('sec8021p',1);
}
else
{
setDisplay('secVlan',0);
setDisplay('sec8021p',0);
setText('vlan',-1);
setText('v8021p',-1);
}
}
}
function AddOption(selItem,value,text,ifSelected)
{
var option = document.createElement("option");
option.innerText = text;
option.value = value;
option.selected = ifSelected;
selItem.appendChild(option);
}
function PTMIdChange()
{
with (getElement('ConfigForm'))
{
if (AddFlag == true)
{
CancelAddCnt();
}
var si = ptmId.selectedIndex;
ptmId.selectedIndex = si;
var index;
for (var i = 0; i < CntPtm.length-1; i++)
{
if (CntPtm[i].ptmId == ptmId.value)
{
index = i;
break;
}
}
SelPTMIndex = index;
if(0 == CntPtm[index].ptmPortId)
{
setCheck('path0', '1');
setCheck('path1', '0');
}
else if (1 == CntPtm[index].ptmPortId)
{
setCheck('path0', '0');
setCheck('path1', '1');
}
else
{
setCheck('path0', '1');
setCheck('path1', '1');
}
if(1 == CntPtm[index].ptmPriorityLow)
{
setCheck('priorityNorm', '1');
}
else
{
setCheck('priorityNorm', '0');
}
if (1 == CntPtm[index].ptmPriorityHigh)
{
setCheck('priorityHigh', '1');
}
else
{
setCheck('priorityHigh', '0');
}
if (CntPtm[index].vlanId >= 0 && CntPtm[index].vlanId <= 4094
&& CntPtm[index].priority >= 0 && CntPtm[index].priority <= 7)
{
setCheck('enbl8021q', '1');
vlan.value     = CntPtm[index].vlanId;
v8021p.value   = CntPtm[index].priority;
}
else
{
setCheck('enbl8021q', '0');
vlan.value     = -1;
v8021p.value   = -1 ;
}
Enbl8021qChange();
}
}
function AddSubmitParam(Form,type)
{
var path0Val = getCheckVal('path0');
var path1Val = getCheckVal('path1');
Form.usingPrefix('y');
Form.addParameter('Enable', '1');
if ((path0Val == 1) && (path1Val == 1))
{
Form.addParameter('X_PortId', '4');
}
else if (path0Val == 1)
{
Form.addParameter('X_PortId', '0');
}
else
{
Form.addParameter('X_PortId', '1');
}
Form.addParameter('X_PTMPriorityLow', getCheckVal('priorityNorm'));
Form.addParameter('X_PTMPriorityHigh', getCheckVal('priorityHigh'));
Form.addParameter('X_VLANIDMark', getValue('vlan'));
Form.addParameter('X_802-1pMark', getValue('v8021p'));
Form.endPrefix();
top.aplyLine = SelPTMIndex;
if( SelPTMIndex == -1 ) 
{
Form.setAction('addcfg.cgi?x=' + GetWdDomain() +'.WANConnectionDevice' +
'&y=WANPTMLinkConfig&RequestFile=/html/ntwkall/ptm.asp');
}
else 
{
Form.setAction('setcfg.cgi?y=' + CntPtm[SelPTMIndex].domain
+ '&RequestFile=/html/ntwkall/ptm.asp');
}
setDisable('btnApply', 1);
}
var MAX_LAYER2_CONN_COUNT = 4;
function CheckForm()
{
with (getElement('ConfigForm'))
{
if (ptmId.length == 0)
{
AddErrInfo(eval("gErrStr1"), '');
return false;
}
var configPort;
if ((getCheckVal('path0') == 1) && (getCheckVal('path1') == 1))
{
configPort = '4';
}
else if (getCheckVal('path0') == 1)
{
configPort = '0';
}
else
{
configPort = '1';
}
for (var i = 0; i < CntPtm.length - 1; i++)
{
if ((CntPtm[i].ptmPortId == configPort) ||
(CntPtm[i].ptmPortId == '0' && configPort == '4') ||
(CntPtm[i].ptmPortId == '1' && configPort == '4') ||
(CntPtm[i].ptmPortId == '4' && configPort == '0') ||
(CntPtm[i].ptmPortId == '4' && configPort == '1'))
{
if (i != SelPTMIndex)
{
if((CntPtm[i].ptmPriorityLow == getCheckVal('priorityNorm')) ||
(CntPtm[i].ptmPriorityHigh == getCheckVal('priorityHigh')))
{
AddErrInfo(eval("gErrStr5"), '');
return false;
}
}
}
}
if (enbl8021q.checked == true)
{
var v = vlan.value;
if(isPlusInteger(v) == false)
{
AddErrInfo(eval("gErrStr21"), 'vlan');
return false;
}
else
{
if ((v == "") || (v < 0) || (v > 4094))
{
AddErrInfo(eval("gErrStr21"), 'vlan');
return false;
}
if (v.length > 1 && v.charAt(0) == '0')
{
AddErrInfo(eval("gErrStr21"), 'vlan');
return false;
}
}
var v = v8021p.value;
if(isPlusInteger(v) == false)
{
AddErrInfo(eval("gErrStr22"), 'v8021p');
return false;
}
else
{
if ((v == "") || (v < 0) || (v > 7))
{
AddErrInfo(eval("gErrStr22"), 'v8021p');
return false;
}
if (v.length > 1 && v.charAt(0) == '0')
{
AddErrInfo(eval("gErrStr22"), 'v8021p');
return false;
}
}
}
if ((CntPtm.length - 1 >= MAX_LAYER2_CONN_COUNT)
&& (SelPTMIndex == -1))
{
AddErrInfo(eval("gErrStr6"), '');
return false;
}
}
return true;
}
function clickRemove()
{
if ((getDelCnt() == 0) || confirm(eval('gVarDeleteLinkConfirm')))
{
removeInst('/html/ntwkall/ptm.asp');
}
}
function CancelAddCnt()
{
with (getElement('ConfigForm'))
{
RemoveOption(ptmId, ptmId.length - 1);
AddFlag = false;
}
}
function setControl(index)
{
var record;
if (index == -1)
{
btnAddPTMCnt();
SelPTMIndex = -1;
}
else
{
getElement('ptmId').selectedIndex = index;
PTMIdChange();
getElement('tdHead').innerHTML = CntPtm[SelPTMIndex].ptmIfName;
}
}

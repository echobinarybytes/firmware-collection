<html>
<head>
<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="stylesheet" href="/css/contentstyle.css" type="text/css">
<script language="JavaScript" src="/js/util.js"></script>
<script language="JavaScript" src="/lang/ipfilter.res"></script>
<script language="JavaScript" src="/lang/firewall.res"></script>
<title>Ip Filter</title>
<script language="JavaScript" src="/js/tabinfo.js"></script>
<script language="JavaScript" type="text/javascript">
var selctIndex = -1;
var IpFltCurrList = new Array();
function stWanInfo(domain,Name,Enable,ConnectionType,ServiceList)
{
this.domain = domain;
this.Name = Name;
this.Enable = Enable;
this.ConnectionType = ConnectionType;
this.ServiceList = ServiceList;
}
var WanIP  = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANIPConnection.{i},Name|Enable|ConnectionType|X_ServiceList,stWanInfo);%>;
var WanPPP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.{i}.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|Enable|ConnectionType|X_ServiceList,stWanInfo);%>;
var WanInfo = new Array();
j = 0;
for (i = 0; i < WanIP.length-1; i++)
{
if (WanIP[i].ConnectionType == 'IP_Routed'
&& WanIP[i].ServiceList != 'TR069'
&& WanIP[i].ServiceList != 'VOICE'
&& WanIP[i].ServiceList != 'VOICE_TR069'
&& WanIP[i].Enable == 1)
{
WanInfo[j++] = WanIP[i];
}
}
for (i = 0; i < WanPPP.length-1; i++)
{
if (WanPPP[i].ConnectionType == 'IP_Routed'
&& WanPPP[i].ServiceList != 'TR069'
&& WanPPP[i].ServiceList != 'VOICE'
&& WanPPP[i].ServiceList != 'VOICE_TR069'
&& WanPPP[i].Enable == 1)
{
WanInfo[j++] =  WanPPP[i];
}
}
function ShowIface(Interface)
{
var k = 0;
for (k = 0; k < WanInfo.length; k++)
{
if (WanInfo[k].domain == Interface)
{
return WanInfo[k].Name;
}
}
if ('br0' == Interface)
{
return 'br0';
}
if ('ALL' == Interface)
{
return 'ALL';
}
return '&nbsp;';
}
function ShowIpProtocol(Protocol)
{
if ('ALL' == Protocol)
{
return 'ALL';
}
return Protocol;
}
function ShowIpFltStatus(Status)
{
if (Status == 0)
{
return eval("gVarDisable");
}
else if (Status == 1)
{
return eval("gVarAccept");
}
else if (Status == 2)
{
return eval("gVarDeny");
}
}
function stNewIndex(Index)
{
this.Index = Index;
}
function stIpFilter(domain, Name, Protocol, SourceIPStart, SourcePortStart, DestIPStart, DestPortStart, Status, SourceIPEnd, SourcePortEnd, DestIPEnd, DestPortEnd, Interface,Priority)
{
this.domain = domain;
this.Name = Name;
this.Protocol = Protocol;
this.SourceIPStart = SourceIPStart;
if (SourcePortStart != '0')
this.SourcePortStart = SourcePortStart;
else
this.SourcePortStart = '';
this.DestIPStart = DestIPStart;
if (DestPortStart != '0')
this.DestPortStart = DestPortStart;
else
this.DestPortStart = '';
this.Status = Status;
this.SourceIPEnd = SourceIPEnd;
if (SourcePortEnd != '0')
this.SourcePortEnd = SourcePortEnd;
else
this.SourcePortEnd = '';
this.DestIPEnd = DestIPEnd;
if (DestPortEnd != '0')
this.DestPortEnd = DestPortEnd;
else
this.DestPortEnd = '';
this.Interface = Interface;
this.ulPriority = Priority;
}
var IpFltCurrList = <%webGetCfgEntry(InternetGatewayDevice.X_FireWall.IpFilter.{i},Name|Protocol|SourceIPStart|SourcePortStart|DestIPStart|DestPortStart|Status|SourceIPEnd|SourcePortEnd|DestIPEnd|DestPortEnd|Interface|Priority,stIpFilter);%>;
var j = 0;
var IpfltSwapData = new Array();
for (i = 1; i < IpFltCurrList.length && null != IpFltCurrList[i]; i++)
{
if (parseInt(IpFltCurrList[i].ulPriority) < parseInt(IpFltCurrList[i-1].ulPriority))
{
IpfltSwapData[0] = IpFltCurrList[i];
j = i - 1;
do
{
IpFltCurrList[j+1] = IpFltCurrList[j];
j--;
}while((j >= 0) && (parseInt(IpfltSwapData[0].ulPriority) < parseInt(IpFltCurrList[j].ulPriority)));
if (j < 0)
{
IpFltCurrList[0] = IpfltSwapData[0];
}
else
{
IpFltCurrList[j+1] = IpfltSwapData[0];
}
}
}
var AllDropOrAcceptArray = new Array();
var DisplayNumOneIndex = -1;
var InterfaceArray = new Array();
var InterfaceIndex = 0;
var AllDropIndex = 0;
for (i = 0; i < IpFltCurrList.length && null != IpFltCurrList[i]; i++)
{
if (IpFltCurrList[i].SourceIPStart == ''
&& IpFltCurrList[i].SourceIPEnd == ''
&& IpFltCurrList[i].SourcePortStart == ''
&& IpFltCurrList[i].SourcePortEnd == ''
&& IpFltCurrList[i].DestIPStart == ''
&& IpFltCurrList[i].DestIPEnd == ''
&& IpFltCurrList[i].DestPortStart == ''
&& IpFltCurrList[i].DestPortEnd == '')
{
if (IpFltCurrList[i].Protocol == "ALL")
{
if (IpFltCurrList[i].Interface == "ALL")
{
AllDropOrAcceptArray[AllDropIndex++] = new stNewIndex(i);
}
else
{
InterfaceArray[InterfaceIndex++] = new stNewIndex(i);
}
}
}
}
function clickRemove()
{
removeInst('/html/security/ipfilter.asp');
}
function LoadFrame()
{
top.errStrStr = '';
top.errIdCtrlStr = '';
with(document.forms[0])
{
if(null != getElement('record_'+top.aplyLine))
{
selectLine('record_'+top.aplyLine);
top.aplyLine = -1;
}
else
{
selectLine('record_'+DisplayNumOneIndex);
}
}
}
function setCtlDisplay(record)
{
setText('Name',record.Name);
setSelect('Protocol',record.Protocol);
setText('SourceIPStart',record.SourceIPStart);
setText('SourceIPEnd',record.SourceIPEnd);
setText('DestIPStart',record.DestIPStart);
setText('DestIPEnd',record.DestIPEnd);
setText('SourcePortStart',record.SourcePortStart);
setText('SourcePortEnd',record.SourcePortEnd);
setText('DestPortStart',record.DestPortStart);
setText('DestPortEnd',record.DestPortEnd);
setSelect('IpStatus',record.Status);
setSelect('Interface',record.Interface);
setText('PriValue',record.ulPriority);
}
function setControl(index)
{
var record;
selctIndex = index;
if (index == -1)
{
record = new stIpFilter('', '', 'ALL', '', '', '', '', '0', '', '', '', '', 'ALL','0');
}
else
{
record = IpFltCurrList[index];
}
setDisable('Name',0);
setDisable('Protocol',0);
setDisable('SourceIPStart',0);
setDisable('SourceIPEnd',0);
setDisable('DestIPStart',0);
setDisable('DestIPEnd',0);
setDisable('SourcePortStart',0);
setDisable('SourcePortEnd',0);
setDisable('DestPortStart',0);
setDisable('DestPortEnd',0);
setDisable('IpStatus',0);
setDisable('Interface',0);
setDisable('PriValue',0);
setCtlDisplay(record);
}
function CheckForm()
{
with(document.forms[0])
{
if (selctIndex == -1 && IpFltCurrList.length > 32)
{
AddErrInfo(eval("gErrStrLast"), '');
return false;
}
for (i = 0; i < IpFltCurrList.length && null != IpFltCurrList[i]; i++)
{
if (IpFltCurrList[i].Name == getElement('Name').value && selctIndex != i)
{
AddErrInfo(eval("gErrStr7") + getSafeString(Name.value) + eval("gErrStr8"), 'Name');
return false;
}
}
if (getElement('Name').value == '' ||(getElement('Name').value.length > 32))
{
AddErrInfo(eval("gErrStr37"), 'Name');
return false;
}
if (isValidName(getElement('Name').value) == false )
{
AddErrInfo(eval("gErrStr38"), 'Name');
return false;
}
if (isPlusInteger(getElement('PriValue').value) == false)
{
AddErrInfo(eval("gErrStr9"), 'PriValue');
return;
}
if (getElement('SourceIPStart').value == ""
&& getElement('SourceIPEnd').value == ""
&& getElement('SourcePortStart').value == ""
&& getElement('SourcePortEnd').value == ""
&& getElement('DestIPStart').value == ""
&& getElement('DestIPEnd').value == ""
&& getElement('DestPortStart').value == ""
&& getElement('DestPortEnd').value == "")
{
if (parseInt(getElement('PriValue').value) != 255)
{
AddErrInfo(eval("gErrStr10"), 'PriValue');
return false;
}
}
else
{
if (getElement('PriValue').value == "" || parseInt(getElement('PriValue').value) < 0 || parseInt(getElement('PriValue').value) > 254)
{
AddErrInfo(eval("gErrStr11"), 'PriValue');
return false;
}
}
if (parseInt(getElement('PriValue').value) != 0
&& parseInt(getElement('PriValue').value) != 255)
{
for (i = 0; i < IpFltCurrList.length && null != IpFltCurrList[i]; i++)
{
if (parseInt(getElement('PriValue').value) == parseInt(IpFltCurrList[i].ulPriority)
&& selctIndex != i)
{
AddErrInfo(eval("gErrStr13") + getSafeString(getElement('PriValue').value) + eval("gErrStr14"), 'PriValue');
return false;
}
}
}
if (getElement('SourceIPStart').value != ""  && (isAbcIpAddress(getElement('SourceIPStart').value) == false) )
{
AddErrInfo(eval("gErrStr15") + getSafeString(getElement('SourceIPStart').value) + eval("gErrStr16"), 'SourceIPStart');
return false;
}
if ( getElement('SourceIPEnd').value != ""
&& (isAbcIpAddress(getElement('SourceIPEnd').value) == false))
{
AddErrInfo(eval("gErrStr17") + getSafeString(getElement('SourceIPEnd').value) + eval("gErrStr18"), 'SourceIPEnd');
return false;
}
if ( getElement('DestIPStart').value != ""
&& (isAbcIpAddress(getElement('DestIPStart').value) == false))
{
AddErrInfo(eval("gErrStr19") + getSafeString(getElement('DestIPStart').value) + eval("gErrStr20"), 'DestIPStart');
return false;
}
if ( getElement('DestIPEnd').value != ""
&& (isAbcIpAddress(getElement('DestIPEnd').value) == false))
{
AddErrInfo(eval("gErrStr21") + getSafeString(getElement('DestIPEnd').value) + eval("gErrStr22"), 'DestIPEnd');
return false;
}
if (getElement('SourcePortStart').value != "" && isValidPort(getElement('SourcePortStart').value) == false )
{
AddErrInfo(eval("gErrStr27") + getSafeString(getElement('SourcePortStart').value) + eval("gErrStr28"), 'SourcePortStart');
return false;
}
if (getElement('SourcePortEnd').value != "" && isValidPort(getElement('SourcePortEnd').value) == false )
{
AddErrInfo(eval("gErrStr29") + getSafeString(getElement('SourcePortEnd').value) + eval("gErrStr28"), 'SourcePortEnd');
return false;
}
if (getElement('DestPortStart').value != "" && isValidPort(getElement('DestPortStart').value) == false )
{
AddErrInfo(eval("gErrStr30") + getSafeString(getElement('DestPortStart').value) + eval("gErrStr28"), 'DestPortStart');
return false;
}
if (getElement('DestPortEnd').value != "" && isValidPort(getElement('DestPortEnd').value) == false )
{
AddErrInfo(eval("gErrStr31") + getSafeString(getElement('DestPortEnd').value) + eval("gErrStr28"), 'DestPortEnd');
return false;
}
if ((getElement('SourcePortStart').value != "" || getElement('SourcePortEnd').value != ""
|| getElement('DestPortStart').value != "" || getElement('DestPortEnd').value != "" )
&&  (getElement('Protocol').selectedIndex == 0 || getElement('Protocol').selectedIndex == 4) )
{
AddErrInfo(eval("gErrStr32"), '');
return false;
}
if (getElement('SourceIPEnd').value != ""
&& (IpAddress2DecNum(getElement('SourceIPStart').value) > IpAddress2DecNum(getElement('SourceIPEnd').value)))
{
AddErrInfo(eval("gErrStr23"), 'SourceIPStart');
return false;
}
if (getElement('DestIPEnd').value != ""
&& (IpAddress2DecNum(getElement('DestIPStart').value) > IpAddress2DecNum(getElement('DestIPEnd').value)))
{
AddErrInfo(eval("gErrStr24"), 'DestIPStart');
return false;
}
if (getElement('SourcePortEnd').value != ""
&& parseInt(getElement('SourcePortStart').value) > parseInt(getElement('SourcePortEnd').value))
{
AddErrInfo(eval("gErrStr25"), 'SourcePortStart');
return false;
}
if (getElement('DestPortEnd').value != ""
&& parseInt(getElement('DestPortStart').value) > parseInt(getElement('DestPortEnd').value))
{
AddErrInfo(eval("gErrStr26"), 'DestPortStart');
return false;
}
if (getElement('SourceIPStart').value == "" && getElement('SourceIPEnd').value != "" )
{
AddErrInfo(eval("gErrStr33"), 'SourceIPStart');
return false;
}
if (getElement('DestIPStart').value == "" && getElement('DestIPEnd').value != "" )
{
AddErrInfo(eval("gErrStr34"), 'DestIPStart');
return false;
}
if (getElement('SourcePortStart').value == "" && getElement('SourcePortEnd').value != "" )
{
AddErrInfo(eval("gErrStr35"), 'SourcePortStart');
return false;
}
if (getElement('DestPortStart').value == "" && getElement('DestPortEnd').value != "" )
{
AddErrInfo(eval("gErrStr36"), 'DestPortStart');
return false;
}
if (getElement('SourceIPStart').value != ""
&& getElement('SourceIPEnd').value == "")
{
SourceIPEnd.value = SourceIPStart.value;
}
if (getElement('SourcePortStart').value != ""
&& getElement('SourcePortEnd').value == "")
{
SourcePortEnd.value = SourcePortStart.value;
}
if (getElement('DestIPStart').value != ""
&& getElement('DestIPEnd').value == "")
{
DestIPEnd.value = DestIPStart.value;
}
if (getElement('DestPortStart').value != ""
&& getElement('DestPortEnd').value == "")
{
DestPortEnd.value = DestPortStart.value;
}
}
return true;
}
function AddSubmitParam(SubmitForm,type)
{
SubmitForm.addParameter('x.Protocol',getSelectVal('Protocol'));
SubmitForm.addParameter('x.Interface',getSelectVal('Interface'));
SubmitForm.addParameter('x.Name',getValue('Name'));
SubmitForm.addParameter('x.Status',getSelectVal('IpStatus'));
SubmitForm.addParameter('x.SourceIPStart',getValue('SourceIPStart'));
SubmitForm.addParameter('x.SourceIPEnd',getValue('SourceIPEnd'));
if (getValue('SourcePortStart') == '')
{
SubmitForm.addParameter('x.SourcePortStart','0');
}
else
{
SubmitForm.addParameter('x.SourcePortStart',getValue('SourcePortStart'));
}
if (getValue('SourcePortEnd') == '')
{
SubmitForm.addParameter('x.SourcePortEnd','0');
}
else
{
SubmitForm.addParameter('x.SourcePortEnd',getValue('SourcePortEnd'));
}
SubmitForm.addParameter('x.DestIPStart',getValue('DestIPStart'));
SubmitForm.addParameter('x.DestIPEnd',getValue('DestIPEnd'));
if (getValue('DestPortStart') == '')
{
SubmitForm.addParameter('x.DestPortStart','0');
}
else
{
SubmitForm.addParameter('x.DestPortStart',getValue('DestPortStart'));
}
if (getValue('DestPortEnd') == '')
{
SubmitForm.addParameter('x.DestPortEnd','0');
}
else
{
SubmitForm.addParameter('x.DestPortEnd',getValue('DestPortEnd'));
}
if (getValue('PriValue') == '')
{
SubmitForm.addParameter('x.Priority','0');
}
else
{
SubmitForm.addParameter('x.Priority',getValue('PriValue'));
}
if( selctIndex == -1 )
{
SubmitForm.setAction('addcfg.cgi?x=InternetGatewayDevice.X_FireWall.IpFilter'
+ '&RequestFile=/html/security/ipfilter.asp');
}
else
{
SubmitForm.setAction('setcfg.cgi?x='+IpFltCurrList[selctIndex].domain
+ '&RequestFile=/html/security/ipfilter.asp');
}
top.aplyLine = selctIndex;
setDisable('btnApply',1);
}
function ShowIpFltInfo(Index,Flags)
{
if (IpFltCurrList[Index].Interface != 'br0' && IpFltCurrList[Index].Interface != "ALL")
{
for (j = 0; j < WanInfo.length; j++)
{
if (WanInfo[j].domain == IpFltCurrList[Index].Interface)
{
break;
}
}
if(WanInfo.length == j)
{
return;
}
if ( (WanInfo[j].ConnectionType != 'IP_Routed') || (WanInfo[j].ServiceList == 'TR069') )
{
return;
}
}
document.write('<tr id="record_' + Index
+ '" class="trTabContent" onclick="selectLine(this.id);">');
document.write('<td>' + getSafeString(IpFltCurrList[Index].Name) + '&nbsp;</td>');
document.write('<td>' + ShowIface(IpFltCurrList[Index].Interface) + '&nbsp;</td>');
document.write('<td>' + ShowIpProtocol(IpFltCurrList[Index].Protocol) + '&nbsp;</td>');
if (1 == Flags)
{
if( IpFltCurrList[Index].SourceIPEnd=='')
{
document.write('<td>' + IpFltCurrList[Index].SourceIPStart
+ IpFltCurrList[Index].SourceIPEnd + '&nbsp;</td>');
}
else
{
document.write('<td>' + IpFltCurrList[Index].SourceIPStart + '-'
+ IpFltCurrList[Index].SourceIPEnd + '&nbsp;</td>');
}
if(IpFltCurrList[Index].SourcePortEnd=='')
{
document.write('<td>' + IpFltCurrList[Index].SourcePortStart
+ IpFltCurrList[Index].SourcePortEnd + '&nbsp;</td>');
}
else
{
document.write('<td>' + IpFltCurrList[Index].SourcePortStart + '-'
+ IpFltCurrList[i].SourcePortEnd + '&nbsp;</td>');
}
if(IpFltCurrList[Index].DestIPEnd=='')
{
document.write('<td>' + IpFltCurrList[Index].DestIPStart
+ IpFltCurrList[Index].DestIPEnd + '&nbsp;</td>');
}
else
{
document.write('<td>' + IpFltCurrList[Index].DestIPStart + '-'
+ IpFltCurrList[Index].DestIPEnd + '&nbsp;</td>');
}
if(IpFltCurrList[Index].DestPortEnd=='')
{
document.write('<td>' + IpFltCurrList[Index].DestPortStart
+ IpFltCurrList[Index].DestPortEnd + '&nbsp;</td>');
}
else
{
document.write('<td>' + IpFltCurrList[Index].DestPortStart + '-'
+ IpFltCurrList[Index].DestPortEnd + '&nbsp;</td>');
}
}
else
{
document.write('<td>--&nbsp;</td>');
document.write('<td>--&nbsp;</td>');
document.write('<td>--&nbsp;</td>');
document.write('<td>--&nbsp;</td>');
}
document.write('<td>' + IpFltCurrList[Index].ulPriority + '&nbsp;</td>');
document.write('<td>' + ShowIpFltStatus(IpFltCurrList[Index].Status) + '&nbsp;</td>');
document.write('<td>' + '<input type="checkbox" name="rml"'  + ' value="'
+ IpFltCurrList[Index].domain  + '">' + '</td>');
document.write('</tr>');
}
function FW_IP_CfgHeader()
{
writeTabCfgHeaderEx(eval("gVarIPFilter"),'Ip Filter',"90%","140");
}

<script language="JavaScript" type="text/javascript">
var selctIndex = -1;
var glbChange = false;
var queueChange = false;
var policerChange = false;
function stGloable(domain,Enable,Bandwith,QueueType)
{
this.domain = domain;
this.Bandwith = Bandwith;
this.Enable = Enable;
this.QueueType = QueueType;
}
var GloableData = <%webGetCfgEntry(InternetGatewayDevice.QueueManagement,Enable|X_BandWidth|X_QueueType,stGloable)%>;
var GloableDataAcc = <%webGetCfgEntryAcc(InternetGatewayDevice.QueueManagement,Enable|X_BandWidth|X_QueueType,stGloable)%>;
function stQueque(domain,Enable,QueWeight)
{
this.domain = domain;
this.Enable = Enable;
this.QueWeight = QueWeight;
}
var QueueData = <%webGetCfgEntry(InternetGatewayDevice.QueueManagement.Queue.{i},QueueEnable|QueueWeight,stQueque)%>;
var QueueDataAcc = <%webGetCfgEntryAcc(InternetGatewayDevice.QueueManagement.Queue.{i},QueueEnable|QueueWeight,stQueque)%>;
function stPolicer(domain,Enable,CommittedRate,CommittedBurstSize,PeakRate,PeakBurstSize)
{
this.domain = domain;
this.Enable = Enable;
this.CommittedRate = CommittedRate;
this.CommittedBurstSize = CommittedBurstSize;
this.PeakRate = PeakRate;
this.PeakBurstSize = PeakBurstSize;
}
var PolicerData = <%webGetCfgEntry(InternetGatewayDevice.QueueManagement.Policer.{i},PolicerEnable|CommittedRate|CommittedBurstSize|PeakRate|PeakBurstSize,stPolicer)%>;
var PolicerDataAcc = <%webGetCfgEntryAcc(InternetGatewayDevice.QueueManagement.Policer.{i},PolicerEnable|CommittedRate|CommittedBurstSize|PeakRate|PeakBurstSize,stPolicer)%>;
function SetPQValue()
{
setValueDft('PQEbable1', QueueData[0].Enable);
setValueDft('PQEbable2', QueueData[1].Enable);
setValueDft('PQEbable3', QueueData[2].Enable);
setValueDft('PQEbable4', QueueData[3].Enable);
setDisable('PQEbable1', TransAcc(QueueDataAcc[0].Enable));
setDisable('PQEbable2', TransAcc(QueueDataAcc[1].Enable));
setDisable('PQEbable3', TransAcc(QueueDataAcc[2].Enable));
setDisable('PQEbable4', TransAcc(QueueDataAcc[3].Enable));
}
function SetWRRValue()
{
setValueDft('WRREbable1', QueueData[0].Enable);
setValueDft('WRREbable2', QueueData[1].Enable);
setValueDft('WRREbable3', QueueData[2].Enable);
setValueDft('WRREbable4', QueueData[3].Enable);
setDisable('WRREbable1', TransAcc(QueueDataAcc[0].Enable));
setDisable('WRREbable2', TransAcc(QueueDataAcc[1].Enable));
setDisable('WRREbable3', TransAcc(QueueDataAcc[2].Enable));
setDisable('WRREbable4', TransAcc(QueueDataAcc[3].Enable));
setValueDft('QueueWeight1', QueueData[0].QueWeight);
setValueDft('QueueWeight2', QueueData[1].QueWeight);
setValueDft('QueueWeight3', QueueData[2].QueWeight);
setValueDft('QueueWeight4', QueueData[3].QueWeight);
setDisable('QueueWeight1', TransAcc(QueueDataAcc[0].QueWeight));
setDisable('QueueWeight2', TransAcc(QueueDataAcc[1].QueWeight));
setDisable('QueueWeight3', TransAcc(QueueDataAcc[2].QueWeight));
setDisable('QueueWeight4', TransAcc(QueueDataAcc[3].QueWeight));
}
function SetPolicerValue()
{
setValueDft('CAREbable1', PolicerData[0].Enable);
setValueDft('CAREbable2', PolicerData[1].Enable);
setValueDft('CAREbable3', PolicerData[2].Enable);
setValueDft('CAREbable4', PolicerData[3].Enable);
setDisable('CAREbable1', TransAcc(PolicerDataAcc[0].Enable));
setDisable('CAREbable2', TransAcc(PolicerDataAcc[1].Enable));
setDisable('CAREbable3', TransAcc(PolicerDataAcc[2].Enable));
setDisable('CAREbable4', TransAcc(PolicerDataAcc[3].Enable));
setValueDft('QueueCIR1', PolicerData[0].CommittedRate);
setValueDft('QueuePIR1', PolicerData[0].PeakRate);
setValueDft('QueueCBS1', PolicerData[0].CommittedBurstSize);
setValueDft('QueueMBS1', PolicerData[0].PeakBurstSize);
setDisable('QueueCIR1', TransAcc(PolicerDataAcc[0].CommittedRate));
setDisable('QueuePIR1', TransAcc(PolicerDataAcc[0].PeakRate));
setDisable('QueueCBS1', TransAcc(PolicerDataAcc[0].CommittedBurstSize));
setDisable('QueueMBS1', TransAcc(PolicerDataAcc[0].PeakBurstSize));
setValueDft('QueueCIR2', PolicerData[1].CommittedRate);
setValueDft('QueuePIR2', PolicerData[1].PeakRate);
setValueDft('QueueCBS2', PolicerData[1].CommittedBurstSize);
setValueDft('QueueMBS2', PolicerData[1].PeakBurstSize);
setDisable('QueueCIR2', TransAcc(PolicerDataAcc[1].CommittedRate));
setDisable('QueuePIR2', TransAcc(PolicerDataAcc[1].PeakRate));
setDisable('QueueCBS2', TransAcc(PolicerDataAcc[1].CommittedBurstSize));
setDisable('QueueMBS2', TransAcc(PolicerDataAcc[1].PeakBurstSize));
setValueDft('QueueCIR3', PolicerData[2].CommittedRate);
setValueDft('QueuePIR3', PolicerData[2].PeakRate);
setValueDft('QueueCBS3', PolicerData[2].CommittedBurstSize);
setValueDft('QueueMBS3', PolicerData[2].PeakBurstSize);
setDisable('QueueCIR3', TransAcc(PolicerDataAcc[2].CommittedRate));
setDisable('QueuePIR3', TransAcc(PolicerDataAcc[2].PeakRate));
setDisable('QueueCBS3', TransAcc(PolicerDataAcc[2].CommittedBurstSize));
setDisable('QueueMBS3', TransAcc(PolicerDataAcc[2].PeakBurstSize));
setValueDft('QueueCIR4', PolicerData[3].CommittedRate);
setValueDft('QueuePIR4', PolicerData[3].PeakRate);
setValueDft('QueueCBS4', PolicerData[3].CommittedBurstSize);
setValueDft('QueueMBS4', PolicerData[3].PeakBurstSize);
setDisable('QueueCIR4', TransAcc(PolicerDataAcc[3].CommittedRate));
setDisable('QueuePIR4', TransAcc(PolicerDataAcc[3].PeakRate));
setDisable('QueueCBS4', TransAcc(PolicerDataAcc[3].CommittedBurstSize));
setDisable('QueueMBS4', TransAcc(PolicerDataAcc[3].PeakBurstSize));
}
function SetWFQPQValue()
{
setValueDft('WFQPQEbable1', QueueData[0].Enable);
setValueDft('WFQPQEbable2', QueueData[1].Enable);
setValueDft('WFQPQEbable3', QueueData[2].Enable);
setValueDft('WFQPQEbable4', QueueData[3].Enable);
setDisable('WFQPQEbable1', TransAcc(QueueDataAcc[0].Enable));
setDisable('WFQPQEbable2', TransAcc(QueueDataAcc[1].Enable));
setDisable('WFQPQEbable3', TransAcc(QueueDataAcc[2].Enable));
setDisable('WFQPQEbable4', TransAcc(QueueDataAcc[3].Enable));
setValueDft('WFQPQQueueWeight1', QueueData[0].QueWeight);
setValueDft('WFQPQQueueWeight2', QueueData[1].QueWeight);
setValueDft('WFQPQQueueWeight3', QueueData[2].QueWeight);
setValueDft('WFQPQQueueWeight4', QueueData[3].QueWeight);
setDisable('WFQPQQueueWeight1', TransAcc(QueueDataAcc[0].QueWeight));
setDisable('WFQPQQueueWeight2', TransAcc(QueueDataAcc[1].QueWeight));
setDisable('WFQPQQueueWeight3', TransAcc(QueueDataAcc[2].QueWeight));
setDisable('WFQPQQueueWeight4', TransAcc(QueueDataAcc[3].QueWeight));
}
function LoadFrame()
{
var i = GetmenuName("qos.asp");
var menuName = UpdateMenuByFileName(i);
menuItem = top.g_oMenuTree.curMenuItem;
var cookie = document.cookie;
ReLocation(cookie,menuName);
setValueDft('Enable', GloableData[0].Enable);
setValueDft('QosCIR', GloableData[0].Bandwith);
setValueDft('QueueType', GloableData[0].QueueType);
setDisable('Enable', TransAcc(GloableDataAcc[0].Enable));
setDisable('QosCIR', TransAcc(GloableDataAcc[0].Bandwith));
setDisable('QueueType', TransAcc(GloableDataAcc[0].QueueType));
ChangeQueType();
setDisplay('QosConfig', 1);
setDisplay('QosEnableDisplayTr1', getCheckVal('Enable'));
setDisplay('QosEnableDisplayTr2', getCheckVal('Enable'));
setDisplay('PolicerQueque', getCheckVal('Enable'));
setDisplay('appOption', getCheckVal('Enable'));
setDisplay('QosQueue', getCheckVal('Enable'));
if (getValueDft('QueueType') == 'PQ')
{
setDisplay('PQueque', getCheckVal('Enable'));
setDisplay('WRRQueque', 0);
setDisplay('WFQPQQueque', 0);
SetPQValue();
}
else if (getValueDft('QueueType') == 'WFQ')
{
setDisplay('PQueque', 0);
setDisplay('WRRQueque', getCheckVal('Enable'));
setDisplay('WFQPQQueque', 0);
SetWRRValue();
}
else if (getValueDft('QueueType') == 'WFQ,PQ')
{
setDisplay('PQueque', 0);
setDisplay('WRRQueque', 0);
setDisplay('WFQPQQueque', getCheckVal('Enable'));
SetWFQPQValue();
}
SetPolicerValue();
}
function ChangeQueType()
{
var type = getElement('QueueType')
if (type.value == 'WFQ')
{
setDisplay('ForceWeight', 0);
setDisplay('WRRQueque', 1);
setDisplay('PQueque', 0);
setDisplay('WFQPQQueque', 0);
SetWRRValue();
}
else if (type.value == 'PQ')
{
setDisplay('ForceWeight', 0);
setDisplay('PQueque', 1);
setDisplay('WRRQueque', 0);
setDisplay('WFQPQQueque', 0);
SetPQValue();
}
else if (type.value == 'WFQ,PQ')
{
setDisplay('ForceWeight', 0);
setDisplay('PQueque', 0);
setDisplay('WRRQueque', 0);
setDisplay('WFQPQQueque', 1);
SetWFQPQValue();
}
}
function CheckForm(type)
{
switch(type)
{
case 0: 
CheckGlobleData();
break;
case 1: 
CheckQueueData();
break;
default:
break;
}
}
function CheckGlobleData()
{
with (document.forms[0])
{
if (isInteger(QosCIR.value) == false)
{
AddErrInfo(eval("gErrStr0"), 'QosCIR');
return false;
}
if (parseInt(QosCIR.value) < 0
|| parseInt(QosCIR.value) > 200000)
{
AddErrInfo(eval("gErrStr3"), 'QosCIR');
return false;
}
return true;
}
}
function CheckQueueData()
{
with (document.forms[0])
{
if (getValueDft('QueueType') == 'WFQ')
{
if (QueueWeight1.value == ''
|| isInteger(QueueWeight1.value) == false)
{
AddErrInfo(eval("gErrStr1"), 'QueueWeight1');
return false;
}
if (parseInt(QueueWeight1.value) < 0
|| parseInt(QueueWeight1.value) > 10000)
{
AddErrInfo(eval("gErrStr4"), 'QueueWeight1');
return false;
}
if (QueueWeight2.value == ''
|| isInteger(QueueWeight2.value) == false)
{
AddErrInfo(eval("gErrStr1"), 'QueueWeight2');
return false;
}
if (parseInt(QueueWeight2.value) < 0
|| parseInt(QueueWeight2.value) > 10000)
{
AddErrInfo(eval("gErrStr4"), 'QueueWeight2');
return false;
}
if (QueueWeight3.value == ''
|| isInteger(QueueWeight3.value) == false)
{
AddErrInfo(eval("gErrStr1"), 'QueueWeight3');
return false;
}
if (parseInt(QueueWeight3.value) < 0
|| parseInt(QueueWeight3.value) > 10000)
{
AddErrInfo(eval("gErrStr4"), 'QueueWeight3');
return false;
}
if (QueueWeight4.value == ''
|| isInteger(QueueWeight4.value) == false)
{
AddErrInfo(eval("gErrStr1"), 'QueueWeight4');
return false;
}
if (parseInt(QueueWeight4.value) < 0
|| parseInt(QueueWeight4.value) > 10000)
{
AddErrInfo(eval("gErrStr4"), 'QueueWeight4');
return false;
}
}
else if (getValueDft('QueueType') == 'WFQ,PQ')
{
if (WFQPQQueueWeight1.value == ''
|| isInteger(WFQPQQueueWeight1.value) == false)
{
AddErrInfo(eval("gErrStr1"), 'WFQPQQueueWeight1');
return false;
}
if (parseInt(WFQPQQueueWeight1.value) < 0
|| parseInt(WFQPQQueueWeight1.value) > 10000)
{
AddErrInfo(eval("gErrStr4"), 'WFQPQQueueWeight1');
return false;
}
if (WFQPQQueueWeight2.value == ''
|| isInteger(WFQPQQueueWeight2.value) == false)
{
AddErrInfo(eval("gErrStr1"), 'WFQPQQueueWeight2');
return false;
}
if (parseInt(WFQPQQueueWeight2.value) < 0
|| parseInt(WFQPQQueueWeight2.value) > 10000)
{
AddErrInfo(eval("gErrStr4"), 'WFQPQQueueWeight2');
return false;
}
if (QueueWeight3.value == ''
|| isInteger(WFQPQQueueWeight3.value) == false)
{
AddErrInfo(eval("gErrStr1"), 'WFQPQQueueWeight3');
return false;
}
if (parseInt(WFQPQQueueWeight3.value) < 0
|| parseInt(WFQPQQueueWeight3.value) > 10000)
{
AddErrInfo(eval("gErrStr4"), 'WFQPQQueueWeight3');
return false;
}
if (QueueWeight4.value == ''
|| isInteger(WFQPQQueueWeight4.value) == false)
{
AddErrInfo(eval("gErrStr1"), 'WFQPQQueueWeight4');
return false;
}
if (parseInt(WFQPQQueueWeight4.value) < 0
|| parseInt(WFQPQQueueWeight4.value) > 10000)
{
AddErrInfo(eval("gErrStr4"), 'WFQPQQueueWeight4');
return false;
}
}
return true;
}
}

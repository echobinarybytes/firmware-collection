<script language="JavaScript" type="text/javascript">
function stWanPPP(domain,name,IpAddress,RemoteIPAddress,IPv6Address,IPv6DefaultGateway,ConnectionType)
{
this.domain = domain;
var list = domain.split('.');
this.key = 	list[4] + '.P.' + list[6];
this.name = name;
this.IpAddress = IpAddress;
this.RemoteIPAddress = RemoteIPAddress;
this.IPv6Address = IPv6DefaultGateway;
this.IPv6DefaultGateway = IPv6DefaultGateway;
this.ConnectionType = ConnectionType;
}
var WanPPP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.1.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|ExternalIPAddress|RemoteIPAddress|X_IPv6Address|X_IPv6DefaultGateway|ConnectionType|X_ServiceList,stWanPPP);%>;
var WanPPP2 = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.2.WANConnectionDevice.{i}.WANPPPConnection.{i},Name|ExternalIPAddress|RemoteIPAddress|X_IPv6Address|X_IPv6DefaultGateway|ConnectionType|X_ServiceList,stWanPPP);%>;
</script>

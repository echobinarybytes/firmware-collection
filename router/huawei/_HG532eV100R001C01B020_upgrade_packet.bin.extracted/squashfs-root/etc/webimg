var un,pw;
var QuickConfigured = <%webGetParaByDomainName(InternetGatewayDevice.DeviceInfo.X_ServiceManage.QuickConfigured);%>;
function WanPPPConstruction(Domain,Username,PasswordIsNull,Enable,ConnectionType)
{
this.Domain = Domain;
this.Username = Username;
this.Enable = Enable;
this.ConnectionType = ConnectionType;
if (PasswordIsNull == 1)
{
this.Password = '';
}
else
{
this.Password = '@1GV)Z<!';
}
}
var WANPPP = <%webGetCfgEntry(InternetGatewayDevice.WANDevice.1.WANConnectionDevice.{i}.WANPPPConnection.{i}, Username|X_PasswordIsNull|Enable|ConnectionType, WanPPPConstruction);%>;
function LoadFrame()
{
document.getElementById('txt_Username').focus();
var Username = document.getElementById('txt_Username');
var Password = document.getElementById('txt_Password');
var i;
var j = 0;
for (i=0; i<WANPPP.length - 1; ++i)
{
if((1 == WANPPP[i].Enable) && ('IP_Routed' == WANPPP[i].ConnectionType))
{
Username.value = WANPPP[i].Username;
un = Username.value
++j;
break;
}
}
if(j == 0)
{
Username.value = '***********';
Password.value = '***********';
alert(eval("gPPPErr1"));
top.location.replace("/html/content1.asp");
}
else
{
Password.value = WANPPP[i].Password;
}
pw = Password.value
}
function SubmitForm(type)
{
var Username = document.getElementById('txt_Username');
var Password = document.getElementById('txt_Password');
if(Username.value == "")
{
alert(eval("gPPPErr2"));
if (type)
return false;
else
return;
}
if (Password.value == "")
{
alert(eval("gPPPErr3"));
if (type)
return false;
else
return;
}
if (checkValid(Username, Password) == false)
{
return false;
}
if ((un == Username.value) && (pw == Password.value))
{
if (QuickConfigured != 1)
{
var Form = new webSubmitForm();
var url = "setcfg.cgi?x=InternetGatewayDevice.DeviceInfo.X_ServiceManage&RequestFile=/html/quickconfig/quicksetwlan.asp"
Form.addParameter('x.QuickConfigured', 2);
Form.setAction(url);
Form.submit();
return;
}
else
{
top.location.replace("/html/quickconfig/quicksetwlan.asp");
return;
}
}
var i;
var Form = new webSubmitForm();
var url = "setcfg.cgi?";
var para = 'x';
var paraname;
var parapw;
for (i = 0; i < WANPPP.length - 1; ++i)
{
if((1 == WANPPP[i].Enable) && ('IP_Routed' == WANPPP[i].ConnectionType))
{
para = para + 1;
url = url + para + '=' + WANPPP[i].Domain + '&';
paraname = para + '.Username';
Form.addParameter(paraname, Username.value);
if (Password.value != '@1GV)Z<!')
{
parapw = para + '.Password';
Form.addParameter(parapw, Password.value);
}
}
}
url = url+"RequestFile=/html/quickconfig/quicksetwlan.asp"
Form.setAction(url);
Form.submit();
}
function checkValid(v1, v2)
{
if ((v1.value.length > 64)
|| (isValidAscii(v1.value) != '') || (v1.value.indexOf(' ') > -1))
{
alert(eval("gPPPErr4"));
return false;
}
if ((v2.value.length > 64)
|| (isValidAscii(v2.value) != '')|| (v2.value.indexOf(' ') > -1))
{
alert(eval("gPPPErr5"));
return false;
}
return true;
}
function onNext()
{
SubmitForm(0);
}
function advanced()
{
top.location.replace("/html/content1.asp");
}
